@charset "utf-8";

/* CSS reset */

body,
input,
textarea,
div,
a,
table,
tr,
td,
th {
	font-family: "Segoe UI", "Lucida Grande", Helvetica, Arial, "Microsoft YaHei", "SimSun", "Tahoma", sans-serif!important;
	font-size: 14px;
}

body {
	color: #333;
	-webkit-text-size-adjust: none;
}

body,
div,
dl,
dt,
dd,
ul,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
th,
td {
	margin: 0;
	padding: 0;
}

ul,
li {
	padding: 0;
	margin: 0;
	list-style: none;
}

/*-------------------------- 浏览器版本验证  --------------------------*/
.browserTips_body{ background:#f0f0f0; min-height: 100%;}
.browserTips_html{ min-height: 100%;}
.browserTips_containter{ background:#fff; width: 650px; height: 422px; 
position: absolute; left: 0; right: 0; top:0; bottom:0; margin:auto;
box-shadow: 0  0 10px rgba(0,0,0,0.2);
}

.browserTips_header:after{ display: table; content:""; clear: both;}
.browserTips_header{ margin:0 30px; border-bottom:1px solid #e7e7e7; padding-top:30px; padding-bottom: 30px;}
.browserTips_header .browserTips_icon{float: left; text-align: center; width: 80px; height: 38px; line-height: 38px;}
.browserTips_header .browserTips_icon .icon_warning{ display: inline-block; width: 46px; height: 38px; vertical-align: middle; background:url(../images/icon_warnning.png) no-repeat left top;}
.browserTips_header p{ margin-left: 80px; line-height: 26px;}

.browserTips_content{ text-align: center;}
.browserTips_content .browserTips_title{ font-weight: normal; margin-top:36px; margin-bottom: 26px;}
.browserTips_list ul{ width: 100%;}
.browserTips_list ul:after{ display: table; content:""; clear: both;}
.browserTips_list ul li{ display: inline-block; width: 20%; float: left; margin-bottom:10px;}
.browserTips_list ul li .browser_logo{ display: inline-block; background:url(../images/browser_logo_sprite.png) no-repeat left top; width: 73px; height: 73px;}

.browserTips_list ul li .logo_chrome{ background-position: 0 0;}
.browserTips_list ul li .logo_opera{ background-position: -75px 0;}
.browserTips_list ul li .logo_ie{ background-position: -225px 0;}
.browserTips_list ul li .logo_ie10{ background-position: -300px 0;}
.browserTips_list ul li .logo_edge{ background-position: -375px 0;}
.browserTips_list ul li .logo_firefox{ background-position: -450px 0;}
.browserTips_list ul li .logo_safari{ background-position: -525px 0;}
.browserTips_list ul li .logo_other{ background-position: -600px 0;}

.browserTips_list p{padding-top:5px;/* height: 13px; */ font-size:12px; background-repeat: no-repeat;background-position: center bottom;}
/*.browserTips_list p.text_chrome{ background-image:url(../images/text_chrome.png);}
.browserTips_list p.text_opera{ background-image:url(../images/text_opera.png);}
.browserTips_list p.text_ie{ background-image:url(../images/text_ie9.png);}
.browserTips_list p.text_firefox{ background-image:url(../images/text_firefox.png);}
.browserTips_list p.text_safari{ background-image:url(../images/text_safari.png);}*/

.browserTips_footer{ padding-top:66px;}
.browserTips_footer p{ text-align: center; color:#999;}

.browserTips_containter\ { position: absolute; left: 50%; top:50%; margin-top:-211px; margin-left: -325px;} /* ie7 */

/* 浏览器禁止访问 */
.browserTips_forbid{ display: inline-block; font-size: 20px; vertical-align: middle; color:#ff502f;}
.browserTips_forbid em{ color:#333; font-style: normal;}
.icon_forbid{ display:inline-block; width:38px; height:38px; margin-right: 15px; background:url(../images/icon_forbid.png) no-repeat center; vertical-align: middle;}
.browserTips_forbid_wrap{ line-height: 422px;}

/*屏幕高度小于等于422*/
@media screen and (max-height: 422px) {
	.browserTips_containter{ top:0; bottom:auto;}
}

/*---------- 浏览器设置 ----------*/
.custom_body{ background:#f1f1f1;}
.custom_main_content{ width: 90%; min-width: 920px; max-width: 1160px; margin:20px auto; background:#fff; padding:20px;}
.custom_title{ font-size: 18px; font-weight: normal; text-align: center; margin:0px 0 15px;}

