/* 下拉框 */
.dropdown {
  text-align: left;
  color: #333;
  font-size: 12px;
  outline: none;
  background-color: #fff;
  padding: 0px 0px;
  padding-right: 10px;
  height: 26px;
  line-height: 26px;
  display: inline-block;
  border: 1px solid #fff;
  border-bottom: none;
  position: absolute;
  right: 36px;
  bottom: 0px;
  z-index: 102;
}
.dropdown.current {
  border-color: #d6d6d6;
}
.dropdown .dropdown-icon {
  text-align: center;
}
.dropdown .dropdown-icon i {
  display: inline-block;
  width: 0px;
  height: 0px;
  border: 3px solid transparent;
  border-top-color: #333;
}
.dropdown .dropdown-input {
  display: inline-block;
  padding-left: 10px;
}
.dropdown-options {
  z-index: 101;
}
.dropdown-options li {
  list-style: none;
  font-size: 16px;
  margin-bottom: 2px;
}
.dropdown-options li a {
  color: #333;
  cursor: default;
  font-size: 12px;
  padding: 0px 10px;
  display: block;
  width: 65px;
  height: 26px;
  line-height: 26px;
  white-space: nowrap;
}
.dropdown-options li a:hover {
  color: #a20c16;
  cursor: pointer;
  text-decoration: none;
  background: #f7f7f7;
}
/*.dropdown .icon,.dropdown-options .icon{ display: inline-block; margin-right:7px; width:16px; height:11px;}*/
.dropdown-options {
  padding: 0px;
  list-style: none;
  background: #fff;
  border: 1px solid #d6d6d6;
}
.dropdown .icon_ch,
.dropdown-options .icon_ch {
  background: url(../images/lan_ch.png) no-repeat left top;
}
.dropdown .icon_en,
.dropdown-options .icon_en {
  background: url(../images/lan_en.png) no-repeat left top;
}
.dropdown .icon_hk,
.dropdown-options .icon_hk {
  background: url(../images/lan_hk.png) no-repeat left top;
}
/* 验证码 */
.lui_login_input_div input.lui_login_input_vcode {
  vertical-align: top;
  width: 50%;
  display: inline-block;
}
.lui_login_input_div img {
  position: absolute;
  width: 50%;
  height: 36px;
  right: 0;
  top: 5px;
}
.lui_login_input_div select,
.login_radio_wrap {
  position: absolute;
  left: 0px;
  z-index: -1;
}
.login_radio_wrap {
  display: none;
}
.lui_login_message_td {
  padding: 10px 35px 12px;
  height: auto;
}
.lui_login_message_div {
  font-size: 12px;
  color: #ee5555;
  margin-bottom: 8px;
}
.lui_login_message_div.tip_message {
  background-color: transparent;
  word-break: break-all;
}
.lui_login_input_div .icon_del {
  display: none;
  width: 16px;
  height: 16px;
  background: url(../images/icon_del.png) no-repeat left top;
  position: absolute;
  right: 10px;
  top: 16px;
  cursor: pointer;
}
.lui_login_input_div .icon_del:hover {
  background: url(../images/icon_del_cur.png) no-repeat left top;
}
.lui_login_input_div.show .icon_del {
  display: inline-block;
}

.tipsClass {
  display: none;
  position: absolute;
  border: 1px solid #f6c8b5;
  font-size: 12px;
  padding: 8px 10px;
  color: #ff7753;
  margin-left: 20px;
  background: #fff;
}
.tipsClass:before {
  position: absolute;
  left: 15px;
  top: -16px;
  content: "";
  width: 0px;
  height: 0px;
  border: 8px solid transparent;
  border-bottom-color: #f6c8b5;
}
.tipsClass:after {
  position: absolute;
  left: 15px;
  top: -15px;
  content: "";
  width: 0px;
  height: 0px;
  border: 8px solid transparent;
  border-bottom-color: #fff;
}

/*第三方登录*/
.third_login_form:after {
  content: "";
  display: block;
  clear: both;
}
.third_login_form {
}
.third_login_header {
  margin-bottom: 5px;
  position: relative;
  float: left;
}
.third_login_header:before {
  left: 0;
}
.third_login_header:after {
  right: 0;
}

.third_login_header span {
  display: block;
  text-align: left;
  font-size: 14px;
}
.third_login_header span:after{
  content: "\FF1A"
}
.third_login_list:after {
  content: "";
  display: table;
  visibility: hidden;
  clear: both;
}

.third_login_list {
  margin: 0;
  padding: 0;
  list-style: none;
  float: left;
}

.third_login_list li {
  display: inline-block;
}

.third_login_list li a {
  margin: 0 5px;
  width: 20px;
  height: 20px;
