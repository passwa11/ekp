/* 登录框布局 starts */
.login_iframe {
  width: 56%;
  max-width: 1039px;
  height: 100%;
  border-radius: 8px;
}
.login_iframe_wrap {
  display: table;
  width: 100%;
  height: 100%;
}
.login_iframe_wrap > div {
  display: table-cell;
  vertical-align: middle;
}
.login_iframe_content {
  position: relative;
  padding: 60px 0 20px;
  min-width: 608px;
}
.form_content {
  width: 85%;
  margin: 0 auto;
}
/* 登录框布局 ends */

/* 登录框内容 starts */
.lui_login_form_list_top {
  position: relative;
  margin-bottom: 25px;
}
.lui_login_form_list_top_left,
.lui_login_form_list_top_right {
  display: inline-block;
  vertical-align: bottom;
}
.lui_logo_img {
  font-size: 0;
  height: 48px;
  line-height: 48px;
  margin-right: 20px;
}
.lui_login_form_list {
  list-style: none;
}
.lui_login_form_list:after,
.lui_login_form_list_lang > div:after,
.lui_login_form_list_top_right:after,
.lui_login_form_list_top:after,
.lui_login_form_list_center:after,
.lui_login_form_list_center_left_wrap > div:after,
.lui_login_form_list_bottom:after,
.lui_login_form_list_bottom_left_wrap > div:after,
.lui_login_form_list_bottom_part2:after {
  content: "";
  display: block;
  clear: both;
}
.lui_login_form_list_center_left,
.lui_login_form_list_bottom_left {
  float: left;
  width: 100%;
  margin-right: -150px;
}
.lui_login_form_list_center_left_wrap,
.lui_login_form_list_bottom_left_wrap {
  padding-right: 150px;
  margin-left: -5px;
}
.lui_login_form_list_center_right {
  width: 150px;
  float: left;
  position: relative;
}
.lui_login_form_list_bottom {
  margin-top: 12px;
  height: 50px;
}
/* 登录框内容 ends */

/* 登录框元素 starts */
/* 多语言切换 */
.lui_login_form_list_lang{
  position: absolute;
  top: 20px;
  right: 0;
}
.header_lan {
  list-style: none;
}
.header_lan:after {
  content: "";
  display: block;
  clear: both;
}
.header_lan > li {
  float: left;
}
.header_lan a{
  display: block;
  padding: 0 20px;  
  height: 1em;
  line-height: 1em;
  font-size: 12px;
}
.header_lan > li:first-child >a {
  border-right: 1px solid transparent;
}
/* logo */
.lui_logo_img img{
  max-height: 100%;
  vertical-align: bottom;
}
/* 登录头部*/
.login_iframe .header {
  display: inline-block;
  vertical-align: bottom;
  clear: both;
  padding-left: 20px;
  font-size: 24px;
  text-align: left;
  height: 1em;
  line-height: 1em;
  text-align: center;
}
.muilti_eng .login_iframe .header {
  font-size: 14px;
  color: #555;
}
/* 验证码 */
.input_verity {
  display: inline-block;
  vertical-align: top;
  margin-right: 10px;
}
.verity_pic {
  display: inline-block;
  vertical-align: top;
}
.login_iframe .login_btn:hover {
  border-color: #47b552;
  background-color: #27b037;
}
.login_iframe .bottom {
  text-align: center;
  color: #999;
  padding: 0 25px 15px;
}
.login_iframe .bottom a {
  text-decoration: none;
  color: #999;
  cursor: pointer;
}
.login_iframe .bottom a:hover {
  text-decoration: underline;
}
.login_iframe .bottom em {
  margin: 0px 8px;
}
/* 忘记密码，kk下载 */
.lui_login_form_opt {
  display: inline-block;
  vertical-align: bottom;
  height: 1em;
  line-height: 1em;
  margin-left: 20px;
  padding-top: 7px;
}
.lui_login_form_opt a:hover {
  text-decoration: none;
  transition: color 0.3s;
  -webkit-transition: color 0.3s;
}
.kkDownloadLink {
  font-size: 14px;
}

.kkDownloadLink:hover,
.forgetPswLink:hover {
  color: #4285f4;
}
.login_iframe .form_bottom {
  text-align: center;
}
/* 第三方登录 */
body .third_login_item_icon{
  display:inline-block;
  width: 18px;
  height: 18px;
  border:1px solid transparent;
  border-radius:50%;
  line-height:20px;
  font-size:14px;
  transition: all .3s;
  -webkit-transition: all .3s;
}
/* 登录框元素 ends */

.login_iframe .login_btn {
  cursor: pointer;
  display: inline-block;
  color: #fff;
  height: 35px;
  line-height: 35px;
  width: 100%;
  text-align: center;
  border: 1px solid #6ec477;
  background-color: #54b65f;
  border-radius: 5px;
  font-size: 18px;
  word-spacing: 4px;
  text-shadow: 1px 1px 0 rgba(0, 0, 0, 0.35);
  transition: all 0.3s ease-in-out;
}

.login_iframe .form_bottom a:hover {
  text-decoration: underline;
}
.login_iframe .form_bottom em {
  margin: 0px 8px;
}
.login_iframe .form_bottom .kk_logo {
  display: inline-block;
  margin-right: 5px;
  vertical-align: middle;
  width: 16px;
  height: 16px;
  background: url(../images/kk_logo.png) no-repeat left top;
}

.main_content {
  /* width: 1200px; */
  min-height: 1px;
  margin: 0px auto;
  /*position: relative;*/
  padding: 0 30px;
}
.login_footer {
  position: absolute;
  bottom: 0px;
  left: 0px;
  width: 100%;
  font-size: 12px;
  color: #fff;
  line-height: 24px;
  padding-bottom: 15px;
  text-align: center;
  opacity: 0.8;
  color: #fff;
  text-shadow: 1px 1px 5px rgba(0,0,0,.9);
  -webkit-text-shadow: 1px 1px 5px rgba(0,0,0,.9);
}
.login_footer p {
  margin: 0;
}

/*背景图片*/
.login-background-img {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: 50%;
  background-size: cover;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  -webkit-animation: fadeIn 0.8s ease-in-out 0s 1;
  -moz-animation: fadeIn 0.8s ease-in-out 0s 1;
  -o-animation: fadeIn 0.8s ease-in-out 0s 1;
  -ms-animation: fadeIn 0.8s ease-in-out 0s 1;
  animation: fadeIn 0.8s ease-in-out 0s 1;
  animation-fill-mode: both;
}
.login-background-img > img {
  width: 100%;
  height: 100%;
}
#login-bgImg {
  position: absolute;
  z-index: -1;
}

/*下拉弹出窗*/
.login-dropbox {
  position: absolute;
  right: -1px;
  top: -1px;
}
.login-dropbox-toggle {
  width: 45px;
  height: 38px;
  border-top-left-radius: 0;
  border-bottom-left-radius: 100% 50%;
  overflow: hidden;
  cursor: pointer;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 1001;
}
.login-dropbox-toggle .iconbox {
  width: 90px;
  height: 38px;
  position: absolute;
  left: 0;
  top: 0;
}
.login-dropbox-toggle .iconbox .icon-PC,
.login-dropbox-toggle .iconbox .icon-QRcode {
  width: 45px;
  height: 38px;
  border-top-right-radius: 8px;
  background-repeat: no-repeat;
  background-position: 50%;
  display: block;
  float: left;
}
.login-dropbox-toggle .iconbox .icon-PC {
  background-image: url(../images/login_icon_PC.png);
}
.login-dropbox-toggle .iconbox .icon-QRcode {
  background-image: url(../images/login_icon_QR.png);
}

.login-dropbox-menu {
  display: none;
  padding: 0px 10px;
  width: 510px;
  min-height: 225px;
  border-radius: 8px;
  border: 1px solid #d6d6d6;
  /*background: url(../images/login_dropdown_bg.png) repeat 50%;*/
  background-color: #fff;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1000;
}
.login_iframe .login-dropbox-menu .header {
  /*margin-bottom: 10px;*/
  padding-top: 10px;
  padding: 2px 15px;
  text-align: left;
  background: none;
}

.login-dropbox-QRcode-list {
  margin: 0;
  padding: 10px 0;
  list-style: none;
}
.login-dropbox-QRcode-list:before,
.login-dropbox-QRcode-list:after {
  display: table;
  content: " ";
}
.login-dropbox-QRcode-list:after {
  clear: both;
}
.login-dropbox-QRcode-list > li {
  width: 33.33%;
  min-height: 1px;
  float: left;
}
.login-QRcode-box {
  margin: 0 8px;
  text-align: center;
}
.login-QRcode-box .title {
  margin: 5px 0 12px;
  font-size: 16px;
  font-weight: normal;
  white-space: nowrap;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
}
.login-QRcode-box .QRcode {
  margin: 0 auto;
  width: 100px;
  height: 100px;
}
.login-QRcode-box .QRcode > img {
  width: 100%;
}
.login-QRcode-box .depict {
  margin-top: 15px;
  font-size: 13px;
  font-weight: normal;
}
.login-warningbar {
  margin-top: -5px;
  margin-bottom: 12px;
  padding: 0 10px;
  height: 20px;
  text-align: center;
  color: #ff7753;
}
.lui_login_form_table {
  width: 100%;
  padding-top: 20px;
}
.lui_login_message_td {
  text-align: left;
  width: 100%;
  padding: 10px 35px 12px;
  height: 30px;
  line-height: 30px;
  color: #ff7753;
}

.lui_login_input_title {
  display: none;
}
.lui_login_form_list_item {
  float: left;
