  left: 50%;
}

.third_login_list:after {
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}

.third_login_list {
  margin: 0;
  padding: 0;
  list-style: none;
  text-align: center;
  font-size: 0;
