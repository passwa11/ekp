    border: 1px solid #fff;
    border-bottom: none;
    position: absolute;
    right: 36px;
    bottom: 0px;
    z-index: 102;
}

.dropdown.current {
    border-color: #d6d6d6;
}

.dropdown .dropdown-icon {
    text-align: center;
}

.dropdown .dropdown-icon i {
    display: inline-block;
    width: 0px;
    height: 0px;
    border: 3px solid transparent;
    border-top-color: #333;
}

