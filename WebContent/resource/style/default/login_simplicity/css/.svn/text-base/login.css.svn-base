}

.dropdown-options li {
    list-style: none;
    font-size: 16px;
    margin-bottom: 2px;
}

.dropdown-options li a {
    color: #333;
    cursor: default;
    font-size: 12px;
    padding: 0px 10px;
    display: block;
    width: 65px;
    height: 26px;
    line-height: 26px;
