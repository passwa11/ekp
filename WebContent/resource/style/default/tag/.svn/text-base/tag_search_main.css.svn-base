.frame_size {
	width: 90%;
	BORDER-LEFT: #eaebef 1px solid;
	border-right: #eaebef 1px solid;
	BORDER-BOTTOM: #eaebef 1px solid;
	border-top: #eaebef 1px solid;
	height: 650px;
	margin-top:0px;
}
.top_stick {
	width: 100%;
	height: 90;
	text-align: center;
}

.imgtb {
	background: url("glass.gif") no-repeat right;
}

.tab {
	margin-top: 25px;
	width: 100%;
	height: 50px;
	VERTICAL-ALIGN: top;
	font-size: 13px;
	text-align: left;
}
.tab img {border:none}
.tab a {
	background: url("btn_left.jpg") no-repeat left top;
	margin: 0;
	padding: 0 0 0 4px;
	text-decoration: none;
	display: block;
	width: 80px;
}

.tab a span {
	display: block;
	background: url("btn_right.jpg") no-repeat right top;
	padding: 2px 10px 2px 6px;
	color: #595959;
	cursor: pointer;
	text-align: center;
}

.searchInput {
	BORDER-RIGHT: #7f9db9 1px solid;
