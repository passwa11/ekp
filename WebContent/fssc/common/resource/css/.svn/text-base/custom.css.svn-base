/* 发票start */
.lui_fssc_iframe_invoice {
    background-color: #fff;
    border-radius: 8px;
    /* height: 312px; */

}
.lui_fssc_invoice_card_header {
    padding:10px 15px;
}
.lui_fssc_invoice_card_header_title {
    font-size: 16px;
    color: #333333;
    letter-spacing: 1px;
    font-weight: 500;
    display: inline-block;
    background-color: #fff;
}
.lui_fssc_invoice_card_header_button {
    float: right;
}
.lui_fssc_invoice_card_header_button {
    display: inline-block;
    vertical-align: middle;
}
.lui_fssc_invoice_card_header_button a {
    color: #333333;
    border-radius: 2px;
    border: 1px solid #DDDDDD;
    background-color: #fff;
    padding:5px 15px;
    text-align: center;
    min-width: 86px;
}
.lui_fssc_invoice_card_header_button a:hover {
    color: #fff;
    background-color:#2778FF;
    cursor: pointer;
}
.lui_fssc_invoice_add {
    
}
.lui_fssc_invoice_table {
    width: 100%;
    text-align: left;
    border-spacing: 0px;
}
.lui_fssc_invoice_table tr:first-child {
    background-color: #F8F8F8;
}
.lui_fssc_invoice_table th {
    color: #354052;
}


.lui_fssc_invoice_table th,.lui_fssc_invoice_table td {
    padding: 8px 15px;
    max-width: 160px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.lui_fssc_invoice_table td {
    font-size: 12px;
    color: #666666;
    letter-spacing: 0;
    line-height: 12px;
    font-weight: 400;
}
.lui_fssc_invoice_table tr {
    border-bottom: 1px solid #dddddd;
    border-top: 1px solid #dddddd;
}

.lui_fssc_invoice_checkout_container {
    width: 100px;
    display: inline-block;
    border-radius: 12px;
}

.lui_fssc_invoice_checkout {
   
    line-height: 22px;
    padding:1px 16px;
    /* padding-left: 16px; */
}
.check_primary {
    border:1px solid #4285f4;
    color: #4285f4;
}
.check_warning {
    border:1px solid #FF943E;
    color: #FF943E;
}
.check_danger {
    border:1px solid #F25643;
    color: #F25643;
}


.lui_fssc_invoice_checkout i {
    height: 14px;
    width: 14px;
    display: inline-block;
    
    border-radius: 50%;
    vertical-align: -2px;
    margin-right: 4px;
}

.lui_fssc_invoice_checkout_container.check_warning  .lui_fssc_invoice_checkout i {
    background:url(../images/icon-check-wait.png);
}
.lui_fssc_invoice_checkout_container.check_primary  .lui_fssc_invoice_checkout i {
    background:url(../images/icon-check-pass.png);
}
.lui_fssc_invoice_checkout_container.check_danger  .lui_fssc_invoice_checkout i {
    background:url(../images/icon-check-failure.png);
}
/* 发票end */

