/* vux按钮样式 Starts */
body .weui-btn {
  border-radius: 0;
  font-size: 18px;
  height: 50px;
  line-height: 50px;
  background-color: #ccc;
}
body .weui-btn:after {
  display: none;
}
body .weui-btn_primary:not(.weui-btn_disabled):active,
body .weui-btn_primary {
  background-color: #4285f4;
}
body .weui-btn_disabled.weui-btn_primary {
  background-color: rgba(66, 133, 244, 0.4);
}
body .weui-btn_disabled {
  color: #fff;
}
/* vux表单样式 Start */
body .weui-cell {
  font-size: 16px;
  padding: 14px 15px !important;
}
.vux-datetime,
.weui-label,
.vux-cell-primary {
  color: #191F25;
  font-size: 16px;
}
.weui-cell__bd .weui-textarea {
  color: #333;
  font-size: 16px;
  text-align: right;
  background: #fff;
  padding: 0;
}
.weui-input::-webkit-input-placeholder,
.weui-textarea::-webkit-input-placeholder {
  color: #999;
}
.weui-cells .weui-cell::before,
body .vux-cell-box:not(:first-child):before {
  border-color: #d4d4d4;
  transform: scaleY(1);
}
/* 必填的输入 Start */
.vux-x-input.required {
  position: relative;
  padding-left: 26px !important;
}
.vux-x-input.required:after {
  content: '*';
  position: absolute;
  top: 13px;
  left: 15px;
  color: #EA4335;
  display: block;
}
/* 必填的输入 End */
/* 带箭头的输入 Start */
.is-link.vux-x-input:active {
  background: #ececec;
}
.is-link .weui-input::-webkit-input-placeholder {
  padding-right: 15px;
}
.is-link .weui-cell__primary {
  position: relative;
}
.is-link .weui-cell__primary:after {
  content: " ";
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 1px 1px 0 0;
  border-color: #C8C8CD;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -2px;
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 2px;
}
/* 带箭头的输入 End */
body .vux-x-input.disabled .weui-input {
  -webkit-text-fill-color: #999;
}
.weui-cell__bd .weui-input {
  text-align: right;
  font-size: 16px;
  padding: 0;
  color: #191F25;
}
.weui-cell_access .weui-cell__ft {
  color: #191F25;
  font-size: 16px;
  padding-right: 15px !important;
}
.weui-cell_access .weui-cell__ft:after {
  height: 8px !important;
  width: 8px !important;
  border-width: 1px 1px 0 0 !important;
}
.weui-cells:after {
  display: none;
}
.vux-cell-box:not(:first-child):before,
.weui-cell:before {
  left: 0 !important;
}
.vux-no-group-title::before {
  display: none;
}
.weui-cells__title {
  font-size: 13px !important;
  color: #666 !important;
  margin: 14px 0 !important;
}
/* vux表单样式 End */
/* 底部tabbar */
body .fs-tabbar {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}
/* popup弹窗样式 Start */
body .vux-popup-dialog {
  background: #fff;
}
body .vux-popup-header {
  background-color: #fff;
}
body .vux-popup-header-right {
  color: #4285f4;
}
body .scroller-item {
  background: #fff;
}
/* popup弹窗样式 End */
/* vux 底部选项卡样式 Starts */
body .fs-tabbar {
  background-color: #fff;
}
body .fs-tabbar .weui-tabbar__label {
  margin-top: 0;
  line-height: 20px;
  font-size: 12px;
}
body .weui-tabbar:before {
  border-color: #ebebeb;
}
body .weui-tabbar__item {
  padding-top: 5px;
  padding-bottom: 3px;
}
body .weui-tabbar__item.weui-bar__item_on .weui-tabbar__icon,
body .weui-tabbar__item.weui-bar__item_on .weui-tabbar__icon > i,
body .weui-tabbar__item.weui-bar__item_on .weui-tabbar__label {
  color: #4285f4;
}
body i.weui-tabbar__icon,
body .weui-tabbar__icon > i {
  font-size: 20px;
  color: #979797;
}
body .weui-tabbar__icon {
  width: 20px;
  height: 20px;
  margin-bottom: 2px;
}
body .weui-tabbar__item:first-child i.weui-tabbar__icon,
body .weui-tabbar__item:first-child .weui-tabbar__icon > i {
  font-size: 18px;
  position: relative;
  top: 2px;
}
/* vux 对话框样式重置 */
body .weui-dialog__btn:after {
  top: 10px;
  bottom: 15px;
  border-color: #e5e5e5;
}
body .weui-dialog__ft:after {
  border-color: #e5e5e5;
}
body .weui-dialog__title {
  font-size: 20px;
}
body .weui-dialog__hd {
  padding-bottom: 20px;
}
body .weui-dialog__bd {
  font-size: 14px;
  color: #333;
}
/* 日历控件 */
body .dp-header .dp-item.dp-right {
  color: #4285f4;
}
body .vux-cell-value {
  color: #191F25;
}
/* vux按钮样式 */
body .weui-dialog__btn_primary {
  color: #4285f4;
}
body .weui-btn_default {
  background: #ccc;
  color: #fff;
}
/* vux底部按钮公共样式 */
.fs-bottom-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2;
}
/* vux 搜索框样式 */
body .weui-search-bar__cancel-btn {
  color: #4285f4;
}
body .weui-search-bar__label {
  line-height: 26px;
}
body .weui-search-bar__box .weui-search-bar__input {
  color: #333;
}
/* element 附件上传 */
body .fs-upload-wrap .body {
  padding: 0 15px 10px;
}
.el-upload-list__item {
  padding: 7px 0;
}
.el-upload-list__item .el-icon-close,
.el-upload-list__item .el-icon-upload-success {
  font-family: "fs-iconfont" !important;
  color: #fff;
  background: #F27474;
  border-radius: 50%;
  font-size: 12px;
  width: 14px;
  height: 14px;
  line-height: 14px;
  text-align: center;
  opacity: 1;
}
.el-icon-close:before,
.el-upload-list__item .el-icon-upload-success:before {
  content: "\e807";
  transform: scale(0.7);
  display: block;
}
.el-upload-list__item:hover {
  background: #fff;
}
.el-upload-list__item.is-success .el-upload-list__item-name:hover {
  color: #606266;
  background: #fff;
}
.el-upload-list__item-status-label {
  top: 7px;
}
.el-upload-list__item:after {
  content: '';
  height: 1px;
  background: #eee;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  transform: scaleY(0.5);
}
.el-upload-list__item {
  margin-top: 0;
}
.el-upload-list__item:first-child:after {
  left: -15px;
  right: -15px;
}
.el-upload-list__item:last-child {
  padding: 5px 0 0;
}
.view .el-upload-list.is-disabled .el-upload-list__item .el-upload-list__item-status-label {
  display: none;
}
/* 表格样式重置 */
table {
  border-collapse: collapse;
}
body .vux-timeline {
  background-color: #fff;
  padding: 25px;
}
body .weui-icon-success-no-circle {
  color: #4285f4;
}
body .vux-timeline-item-tail {
  background-color: #eee;
}
body .vux-timeline-item-color {
  background: #eee;
}
body .vux-timeline-item-head-first {
  background: #4285f4;
}
.vux-timeline-item-content {
  color: #666;
  font-size: 12px;
}
.vux-timeline-item-content h4 {
  color: #666;
  line-height: 18px;
}
.vux-timeline-item-content p {
  margin-top: 8px;
  color: #999;
}
.vux-timeline-item-content p.recent {
  color: #666;
  font-size: 14px;
  margin-top: 5px;
}
.vux-timeline .vux-timeline-item:first-child h4 {
  color: #191F25;
}
.is-link .weui-input {
  padding-right: 15px;
  box-sizing: border-box;
}
.is-link.empty .weui-input {
  padding-right: 0;
}
body .weui-switch:checked,
body .weui-switch-cp__input:checked ~ .weui-switch-cp__box {
  border-color: #4285f4;
  background-color: #4285f4;
}
.fs-no-data {
  display: flex;
  align-items: center;
  width: 100%;
  color: #4285f4;
  position: absolute;
  top: 0;
  bottom: 0;
  text-align: center;
}
.fs-no-data .fs-icon {
  display: inline-block;
  margin: auto;
}
.fs-no-data .fs-icon img {
  width: 140px;
  height: 140px;
}
.fs-no-data .fs-icon i {
  font-size: 70px;
  margin-bottom: 30px;
}
.fs-no-data .fs-title {
  font-size: 17px;
  margin-top: 14px;
  color: #aeaeae;
  line-height: 18px;
}
.fs-fee-list .fs-no-data,
.fs-expense-list .fs-no-data {
  padding-bottom: 50px;
}
.fs-cell-required .weui-cell__hd .weui-label:after {
  content: "*";
  color: #f00;
  margin-left: 3px;
}
.fs-cell-required-s .vux-cell-primary .vux-label:after {
  content: "*";
  color: #f00;
  margin-left: 3px;
}
.fs-cell-required.vux-datetime > div p:after {
  content: "*";
  color: #f00;
  margin-left: 3px;
}
