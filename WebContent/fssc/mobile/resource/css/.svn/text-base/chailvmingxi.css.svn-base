/*费用明细 */
.ld-expense-detail .ld-expense-detail-persionlInfo {
       border-top: 1px solid #eee;
       padding-left: 0.3rem;
   }
   
   .ld-expense-detail-costInfo {
       padding-left: 0.3rem;
   }
   
   .ld-expense-detail-body .ld-info-item>div {
       display: flex;
       align-items: center;
   }
   
   .ld-expense-detail-body .ld-info-item i {
       height: 0.12rem;
       width: 0.12rem;
       border-right: 1PX solid #434343;
       border-top: 1PX solid #434343;
       transform: rotate(45deg);
       margin-left: 0.1rem;
   }
   
   .ld-line20px {
       height: 0.2rem;
       background: #F2F2F2;
   }
   
   .ld-expense-detail .ld-expense-detail-persionlInfo>div,
   .ld-expense-detail-costInfo>div {
       display: flex;
       align-items: center;
       justify-content: space-between;
       height: 1rem;
       padding-right: 0.3rem;
       border-top: 1px solid #eee;
       font-size: 0.28rem;
       color: #434343;
   }
   
   .ld-expense-detail-persionlInfo>div:first-child,
   .ld-expense-detail-costInfo>div:first-child {
       border-top: none;
   }
   
   .ld-expense-detail .ld-expense-detail-costInfo>div:last-child {
       border-bottom: 1px solid #eee;
   }
   
   .ld-expense-detail-persionlInfo>div>span:first-child {
       font-size: 0.28rem;
       color: #999999;
   }
   
   .ld-expense-detail-persionlInfo>div>span:last-child {
       font-size: 0.28rem;
       color: #434343;
   }
   
   .ld-expense-detail-costInfo .time span.span2,
   .ld-expense-detail-costInfo .city span.span2 {
       margin-left: 1.5rem;
       position: relative;
   }
   
   .ld-expense-detail-costInfo .time span.span2:before,
   .ld-expense-detail-costInfo .city span.span2:before {
       content: '';
       position: absolute;
       width: 0.6rem;
       height: 5px;
       background: url(../images/date_arrow.png) no-repeat center center;
       background-size: contain;
       left: -0.3rem;
       top: 50%;
       transform: translate(-100%, -50%);
   }
   
   .ld-expense-detail-costInfo .amount span:first-child {
       font-size: 0.28rem;
       color: #999999;
   }
   
   .ld-expense-detail-costInfo .amount input {
       height: 100%;
       border: none;
       outline-style: none;
       text-align: right;
       background: transparent;
   }
   
   .ld-expense-detail-costInfo .amount input::placeholder {
       font-size: 0.28rem;
       color: #999999;
   }
   
   .ld-expense-save-btn {
       background: #4285F4;
       border-radius: 0.08rem;
       height: 0.88rem;
       width: 7.1rem;
       margin: 1rem auto 0;
       line-height: 0.88rem;
       text-align: center;
       color: white;
       font-size: 0.32rem;
   }
   /* input */
      .ld-info-item div {
          height: 100%;
      }
      
      .ld-info-item input {
          height: 100%;
          border: none;
          outline: none;
          background: transparent;
          text-align: right;
          max-width: 120px;
          overflow: hidden;
          text-overflow: ellipsis;
          white-space: nowrap;
      }
      
      .ld-expense-detail-body {
          display: none;
          overflow: hidden;
      }
      
      .ld-expense-detail-body.ld-expense-detail-body-show {
          display: inline-block;
      }
      
      .ld-expense-detail-body {
          position: fixed;
          width: 100%;
          height: 100%;
          top: 0;
          left: 0;
          background: white;
          z-index: 10000;
      }
	.ld-expense-detail-body .amount {
          border-bottom: 1px solid #eee;
      }
      /* 去掉主页面的滚动事件 */
   .ld-newApplicationForm-hidden.ld-newApplicationForm {
       height: 0;
       overflow: hidden;
   }
   
