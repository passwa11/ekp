 /*时间与城市*/
 
 .ld-travel-detail .ld-info-item>div {
    display: flex;
    align-items: center;
}

.ld-travel-detail .ld-info-item i {
    height: 0.12rem;
    width: 0.12rem;
    border-right: 1px solid #434343;
    border-top: 1px solid #434343;
    transform: rotate(45deg);
    margin-left: 0.1rem;
}

.ld-travel-detail .ld-info-item input {
    border: 0;
    background: transparent;
    outline: none;
}

.ld-travel-detail .ld-info-item.time input,
.ld-travel-detail .ld-info-item.city input {
    width: 1.2rem;
}

.ld-line20px {
    height: 0.2rem;
    background: #F2F2F2;
}

.ld-travel-detail .ld-travel-detail-persionlInfo>div {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 1rem;
    padding-right: 0.3rem;
    border-bottom: 1px solid #eee;
    font-size: 0.28rem;
    color: #434343;
}

.ld-travel-detail .ld-travel-detail-persionlInfo>div>span:first-child {
    font-size: 0.28rem;
    color: #999999;
}

.ld-travel-detail .ld-travel-detail-persionlInfo>div>span:last-child {
    font-size: 0.28rem;
    color: #434343;
}

.ld-travel-detail .ld-info-item b {
    width: 0.8rem;
    height: 5px;
    background: url(../images/date_arrow.png) no-repeat center center;
    background-size: cover;
    margin: 0 0.2rem;
}

.ld-travel-detail .ld-info-item input::placeholder {
    color: #333;
}

.ld-travel-detail .ld-info-item.time input.rightInp,
.ld-travel-detail .ld-info-item.city input.rightInp {
    text-align: right;
    color: #333;
    width: 1.4rem;
}