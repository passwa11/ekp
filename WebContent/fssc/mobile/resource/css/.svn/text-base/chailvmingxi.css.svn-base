   /* 费用明细 */
    .ld-newApplicationForm-costInfo ul li {
        border: 1px solid #ccc;
        border-radius: 0.1rem;
        padding: 0.3rem;
        margin-bottom: 0.3rem;
        min-height: 2.11rem;
        box-sizing: border-box;
        margin-top: 0.2rem;
    }
    
    .ld-newApplicationForm-costInfo ul li .ld-notSubmit-list-top {
        display: flex;
        align-items: center;
        padding-bottom: 0.28rem;
        border-bottom: 1px solid #eee;
    }
    
    .ld-newApplicationForm-costInfo ul li .ld-notSubmit-list-top img {
        width: 0.32rem;
        height: 0.32rem;
        object-fit: cover;
        margin-right: 0.16rem;
    }
    
    .ld-newApplicationForm-costInfo ul li .ld-notSubmit-list-top span {
        color: #434343;
        line-height: 0.28rem;
        font-size: 0.28rem;
    }
    
    .ld-newApplicationForm-costInfo ul li .ld-notSubmit-list-top .ld-notSubmit-entryType {
        border: 1px solid #ccc;
        border-radius: 0.02rem;
        background: #f0f0f0;
        font-size: 0.18rem;
        margin-left: 0.14rem;
        height: 0.28rem;
        line-height: 0.26rem;
        box-sizing: border-box;
        padding: 0 0.05rem;
    }
    
    .ld-notSubmit-list-bottom {
        margin-top: 0.3rem;
    }
    
    .ld-notSubmit-list-bottom-info {
        display: flex;
        justify-content: space-between;
        align-items: center;
        height: 0.37rem;
    }
    
    .ld-notSubmit-list-bottom-info>div {
        display: flex;
        align-items: center;
    }
    
    .ld-notSubmit-list-bottom p {
        font-size: 0.26rem;
        height: 0.37rem;
        line-height: 0.37rem;
        color: #999999;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        margin-top: 0.15rem;
    }
  .ld-newApplicationForm-costInfo {
    background: white;
    padding: 0.3rem;
    box-sizing: border-box;
