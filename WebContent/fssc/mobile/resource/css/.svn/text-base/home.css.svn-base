/* home */
.ld-feesCharged-home-main{
    width: 100%;
    box-sizing: border-box;
    margin-bottom: 1.41rem;
}
.ld-feesCharged-mine{
    width: 100%;
    box-sizing: border-box;
    margin-bottom: 1.41rem;
    background:#e2e2e2;
}
.ld-feesCharged-hidden{
	display:none;
}
.ld-feesCharged-mine-header{
	width:100%;
	height:1.41rem;
	background:#fff;
	margin-bottom:0.1rem;
}
.ld-feesCharged-mine-header-icon{
	heigth:100%;
	position:relative;
}
.ld-feesCharged-mine-header-icon img{
	height:1.21rem;
	width:1.21rem;
	top:50%; 
    left:0.41rem;
	border-radius:50%;
	position:absolute;
}
.ld-feesCharged-mine-header-info{
	height:100%
	width:80%;
	float:left;
	margin-left:2.11rem;
}
.ld-feesCharged-mine-header-info h3{
	font-weight:bold;
	font-size:0.38rem;
}
.ld-feesCharged-mine-cell{
	width:100%;
	height:1.41rem;
	background:#fff;
	line-height:1.41rem;
	position:relative;
	border-bottom:1px solid #ddd;
}
.ld-feesCharged-mine-cell img{
	position:absolute;
	top:50%; 
    left:0.41rem;
    transform: translate(-50%,-50%);
	width:0.81rem;
	height:0.81rem;
}
.ld-feesCharged-mine-cell span{
	margin-left:0.81rem;
	width:2rem;
}
.ld-feesCharged-mine-cell-left{
	width:80%;
	float:left;
}
.ld-feesCharged-mine-cell-right{
	width:20%;
	float:right;
}
.ld-feesCharged-mine-cell-right span{
	display:block;
	width:0.3rem;
	height:0.3rem;
	border-top:1px solid #a1a1a1;
	border-right:1px solid #a1a1a1;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg); 	/* IE 9 */
	-moz-transform:rotate(45deg); 	/* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 和 Chrome */
	-o-transform:rotate(45deg); 	/* Opera */
	margin-top:.55rem;
}
.ld-feesCharged-home-main-top{
    padding: 0.2rem 0.3rem;
    background-image: linear-gradient(180deg, #F2F2F2 65%, #E8E8E8 94%);
}
.ld-feesCharged-home-main-notSubmit{
    height:2.68rem;
    background:url(../images/notSubmitBg.png) no-repeat center center ;
    background-size: cover;
    padding: 0 0.3rem;
}
.ld-feesCharged-home-main-notSubmit-top{
    display: flex;
    justify-content: flex-end;
    height:0.92rem;
    line-height: 0.92rem;
    align-items: center;
    border-bottom: 1px dashed #5792f5;
    color:white;
    font-size:0.28rem;
}
.swiper-wrapper#personal li{
   	padding:0 0.15rem;
}
.swiper-wrapper#personal li div{
	background-color:#edf2fc;
	border-radius:0.15rem;
}
.ld-feesCharged-home-main-notSubmit-top span{
    vertical-align:middle;
}
.ld-feesCharged-home-main-notSubmit-top i{
    display: inline-block;
    vertical-align: middle;
    height:0.4rem;
    width:0.4rem;
    background:url(../images/arrow_right.png) no-repeat center center;
    background-size: cover;
    /* margin-left: 0.1rem; */
}
.notSubmitNum{
    position: relative;
    padding: 0 0.2rem;
}
.notSubmitNum:after{
    content: "";
    position: absolute;
    top:0.32rem;
    right:0.1rem;
    height:0.08rem;
    width:0.08rem;
    background: #F25643;
    border-radius: 0.04rem;
}

.ld-feesCharged-home-main-notSubmit-bottom p:first-child{
    line-height: 0.28rem;
    margin:0.3rem 0 0.1rem;
    font-size: 0.28rem;
    color:white;
}
.ld-feesCharged-home-main-notSubmit-bottom p:last-child{
    font-family: DINAlternate-Bold;
    font-size: 0.68rem;
    color:white;
}
/* btn */
.ld-feesCharged-home-btns{
    background:white;
    padding: 0.4rem 0.3rem;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
    margin-top: -0.2rem;
}
.ld-feesCharged-home-btns h3{
    font-family: PingFangSC-Medium;
    font-size: 0.32rem;
    color: #434343;
    letter-spacing: 0;
}
.ld-feesCharged-home-btns ul {
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}
.ld-feesCharged-home-btns li {
    width:25%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    margin: 0.2rem 0;
}
.ld-feesCharged-home-btns li img{
    border-radius: 16px;
    height:0.88rem;
    width:0.88rem;
}

.ld-feesCharged-home-btns li span {
    margin-top: 0.2rem;
    font-size: 0.24rem;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    line-clamp: 2;
    -webkit-box-orient: vertical;
    text-align:center;
}
.ld-feesCharged-home-btns .ld-feesCharged-commonUsedLink li img {
    border-radius: 0.16rem;
}
.ld-feesCharged-home-btns .ld-feesCharged-otheruse li img {
 width:0.6rem;
 height:0.6rem;   
}
.ld-feesCharged li.ld-forward span{
    color:#ccc;
}
/* 横线 */
.ld-line{
    width:100%;
    height:1px;
    background: #eee;
    margin:0.2rem 0 0.4rem;
}
/* swiper */
.swiper-container{
    padding-bottom: 0.2rem;
}
.swiper-pagination{
    bottom:0!important;
}
.swiper-pagination .swiper-pagination-bullet{
    height:0.06rem;
    width:0.06rem;
    border-radius: 0.03rem;
    /* background: #ddd; */
}
.swiper-pagination .swiper-pagination-bullet-active{
    opacity: 0.7;
    background: #4285F4;
    border-radius: 0.03rem;
    width:0.3rem;
    height:0.06rem;
}
/* 记一笔模态框 */
.ld-remember-modal.ld-remember-modal-show{
    height:100%;
}
.ld-remember-modal{
    position: fixed;
    width:100%;
    height:0;
    left: 0;
    top:0;
    z-index: 99;
    background:rgba(0,0,0,.5);
    overflow: hidden;
}
.ld-remember-modal-main{
    width:6.3rem;
    position:absolute;
    top:50%;
    left:50%;
    transform: translate(-50%,-50%);
    background: white;
    border-radius: 0.2rem;
}
.ld-remember-modal-main ul {
    padding:0.7rem 0.5rem;
    box-sizing: border-box;
}
.ld-remember-modal-main ul li {
    height:1.4rem;
    border-radius: 0.2rem;
    background: #f7f7f7;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0.28rem 0.3rem 0.28rem 0.2rem;
    box-sizing: border-box;
    margin-bottom: 0.3rem;
}
.ld-remember-modal-main ul li:last-child{
    margin-bottom: 0;
}
.ld-remember-modal-main ul li > i {
    height:0.18rem;
    width:0.18rem;
    background: url(../images/arrow_right_gray.png) no-repeat center center;
    background-size: contain;
}
.ld-remember-modal-main ul li > div {
    display: flex;
    align-items: center;
}
.ld-remember-modal-main ul li > div > span {
    height:0.77rem;
    width:0.77rem;
    margin-right: 0.16rem;
}
.ld-remember-modal-main ul li > div span.ld-manual-invoice{
    background: url(../images/manual.png) no-repeat center center;
    background-size: cover;
}
.ld-remember-modal-main ul li > div span.ld-picture-invoice{
    background: url(../images/camera.png) no-repeat center center;
    background-size: cover;
}
.ld-remember-modal-main ul li > div span.ld-saomiao-invoice{
    background: url(../images/saomiao.png) no-repeat center center;
    background-size: cover;
}
.ld-remember-modal-main ul li > div span.ld-chooseCost-invoice{
    background: url(../images/shooseCost.png) no-repeat center center;
    background-size: cover;
}
.ld-remember-modal-main ul li > div span.ld-select-travel{
    background: url(../images/pleaseForward.png) no-repeat center center;
    background-size: cover;
}
.ld-remember-modal-text p{
    font-weight: 700;
}
.ld-remember-modal-text p:first-child{
    font-size: 0.28rem;
    color: #434343; 
    margin-bottom: 0.06rem;
}
.ld-remember-modal-text p:last-child{
    font-size: 0.22rem;
    color: #999; 
}
.ld-remember-modal-cancel{
    height:1rem;
    border-top:1px solid #eee;
    text-align: center;
    line-height: 1rem;
    font-size: 0.32rem;
    color: #434343;
}
.didi-order-list{
	position:fixed;
	left:0;
	top:100%;
	transition:all 0.3s;
	width: 100%;
    height: 100%;
    background: #fff;
    z-index: 999;
}
.didi-order-list-show{
	top:0;
}
.ld-costDetail-btn-cancel{
	background: #fff;
    border-radius: 10px;
    border:1px solid #4285F4;
    height: 0.88rem;
    width: 2.7rem;
    text-align: center;
    line-height: 0.88rem;
    color: #4285F4;
    font-size: 0.32rem;
}