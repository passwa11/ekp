.fssc-mobile-links{
	height:100%;
	width:100%;
}
.fssc-mobile-links-link{
	height:1.6rem;
	width:1.6rem;
	float:left;
	text-align:center;
	font-size:0.2rem;
	border:1px dashed #9e9e9e;
	margin-top:0.3rem;
	margin-left:0.2rem;
	position:relative;
}
.fssc-mobile-links-link img{
	height:1rem;
	width:1rem;
	position:absolute;
	top:0;
	left:0.25rem;
}
.fssc-mobile-links-link span{
	height:0.6rem;
	width:100%;
	display:block;
	text-align:center;
	margin-top:1rem;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.fssc-mobile-links-link i{
	height:0.4rem;
	width:0.4rem;
	display:block;
	position:absolute;
	left:1.36rem;
	top:-0.2rem;
	color:#fff;
	background:red;
	z-index:90;
	font-size:0.4rem;
	border-radius:50%;
	line-height:0.4rem;
}
.fssc-mobile-footer{
	width:100%;
	height:1.2rem;
	box-shadow:0 -5px 5px -5px #f2f2f2;
	position:fixed;
	bottom:0;
}
.fssc-mobile-footer-save{
	width: 90%;
    height: 0.8rem;
    color: #fff;
    background: #5087ec;
    margin: 0.2rem 5%;
    font-size: 0.4rem;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 0.8rem;
}
.fssc-mobile-link-add{
	position:fixed;
	width:100%;
	height:100%;
	left:0;
	top:0;
	transition: left 0.6s;
	-moz-transition: left 0.6s;
	-webkit-transition: left 0.6s;
	-o-transition: left 0.6s;
	z-index:99;
	background:#fff;
}
.fssc-mobile-link-hidden{
	left:100%;
}

.ld-info-item{
	width:100%;
	height:1rem;
	font-size:0.3rem;
	line-height:1rem;
	padding-left:0.2rem;
	border-bottom:1px solid #eee;
	color:#999;
}
.ld-info-item .title{
	height:100%;
	width:20%;
	float:left;
	color:#999;
}
.ld-info-item .content{
	height:100%;
	width:75%;
	float:left;
	float:right;
	text-align:right;
	margin-right:0.3rem;
}
.ld-info-item div.icon{
	height:0.8rem;
	width:0.8rem;
	float:left;
	background-size: 100%;
    margin-top: 0.1rem;
    margin-left: 81%;
}
.ld-info-item div.icon-none{
	display:none;
}
.ld-info-item .content input{
	height:100%;
	width:95%;
	text-align:right;
	font-size:0.3rem;
	border:none;
	background:transparent;
}
.ld-info-item i{
	height:0.2rem;
	width:0.2rem;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg);
	-moz-transform:rotate(45deg);
	-webkit-transform:rotate(45deg);
	-o-transform:rotate(45deg);
	display:block;
	margin-top:0.4rem;
	border-top:1px solid #aaa;
	border-right:1px solid #aaa;
	float:right;
}
.fssc-mobile-footer-cancel{
	width: 40%;
    height: 0.8rem;
    color: #5087ec;
    background: #fff;
    margin: 0.2rem 4%;
    font-size: 0.3rem;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 0.8rem;
    border:1px solid #5087ec;
    float:left;
}
.fssc-mobile-footer-add{
	width: 40%;
    height: 0.8rem;
    color: #fff;
    background: #5087ec;
    margin: 0.2rem 4%;
    font-size: 0.3rem;
    border-radius: 0.2rem;
    text-align: center;
    border:1px solid #5087ec;
    line-height: 0.8rem;
    float:left;
}
.fssc-mobile-link-icons{
	position:fixed;
	width:100%;
	height:100%;
	left:0;
	top:0;
	transition: top 0.3s;
	-moz-transition: top 0.3s;
	-webkit-transition: top 0.3s;
	-o-transition: top 0.3s;
	z-index:100;
	background:#fff;
}
.fssc-mobile-link-icons-box{
	width:1.3rem;
	height:1.3rem;
	margin:0.1rem 0.1rem;
	text-align:center;
	float:left;
}
.fssc-mobile-link-icons-box div{
	width:0.3rem;
	height:0.3rem;
	background:url('../images/radio_normal.png');
	background-size:100%;
	margin-left:0.5rem;
	margin-top: -0.1rem;
}
.fssc-mobile-link-icons-box div.active{
	background:url('../images/radio.png');
	background-size:100%;
}
.fssc-mobile-link-icons-box img{
	width:1rem;
	height:1rem;
}
.fssc-mobile-link-icons-hidden{
	top:100%;
}
.fssc-mobile-icons-footer{
	width:100%;
	height:1.2rem;
	box-shadow:0 -5px 5px -5px #f2f2f2;
	position:fixed;
	bottom:0;
	transition: bottom 0.3s;
	-moz-transition: bottom 0.3s;
	-webkit-transition: bottom 0.3s;
	-o-transition: bottom 0.3s;
}
.fssc-mobile-icons-footer-hidden{
	bottom:-100%;
}
.fssc-mobile-icons-footer-cancel{
	width: 40%;
    height: 0.8rem;
    color: #5087ec;
    background: #fff;
    margin: 0.2rem 4%;
    font-size: 0.3rem;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 0.8rem;
    border:1px solid #5087ec;
    float:left;
}
.fssc-mobile-icons-footer-add{
	width: 40%;
    height: 0.8rem;
    color: #fff;
    background: #5087ec;
    margin: 0.2rem 4%;
    font-size: 0.3rem;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 0.8rem;
    border:1px solid #5087ec;
    float:left;
}
