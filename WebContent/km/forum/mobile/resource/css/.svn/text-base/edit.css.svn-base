@charset "utf-8";
@IMPORT url("../../../../../sys/mobile/css/themes/default/header.css");
@IMPORT url("../../../../../sys/mobile/css/themes/default/nav.css");
*{
	padding: 0;
	margin: 0;
}

ol, ul {
	list-style: none;
}

.clear{
	clear: both;
}

.muiEditor .muiEditorTextContainer .muiEditorTextarea{
	color: black;
	// border-top: 1px solid #d5d5d5;
}

/** 编辑  **/

#personView,#dateView,#syncView{
	background-color: #fff;
	border-bottom: 1px solid #d9d7d7;
	/** 修改 #54103**/
/* 	z-index : -1; */
}

.muiImportBox{
	background-color: #fff;
	padding-top: 1rem;
}
.muiImportBox .muiImportContent{ 
	background-color: #fff;
}

.attachmentView{
	/** 默认隐藏 **/
	display: none;
	-webkit-transition: all 200ms linear;
	background-color: #fff;
}

.attachmentView .muiAttachmentItemT{
	background-color: white;
}

.muiImportBox .sysTaskSubjectInput{
	background-color: #fff;
}
.muiImportBox .muiValidate{
	margin:-1.1rem 0 0;
	padding:.5rem 0 0 0;
}

.muiImportBottomBar {
	height: 3.8rem;
	border:1px solid #EFEFEF;
	position: relative;
}

.muiImportBox .muiFieldItem.edit{ 
	padding-top:1rem; 
	padding-bottom:0.5rem;
}

.muiImportIconBar {
	display: table;
}

.muiImportIconBar li {
	display: table-cell;
	padding: 3px 2px;
}

.muiImportIconBar li a, .muiImportIconBar li span {
	width: 3.2rem;
	height: 3.2rem;
	line-height: 3.2rem;
	text-align: center;
	color: #848484;
	font-size: 2rem;
	display: block;
	overflow: hidden;
	cursor: pointer;
}

.muiNavbarContainer{
	height: 4rem;
	line-height: 4rem;
	background-color: #f7f7f7;
}

.muiTaskNavBar ul{
	width: 100%;
	display: table;
	border-bottom: 1px solid #d9d7d7;
	background-color: #fafafa;
}

.muiTaskNavBar ul li {
	text-align: center;
	vertical-align: top;
	color: #848484;
	font-size: 1.2rem;
	cursor: pointer;
	padding-top: 1rem;
	padding-bottom: 1rem;
}

.muiTaskNavBar ul li .muiNavitemSpan{
	height:100%;
}
.muiTaskNavBar ul li .muiNavitemBottom {
	display: none;
}

.muiTaskNavBar ul li .mui {
	display: inline-block;
	color: inherit;
	font-size: 2.4rem;
	font-family: FontMui;
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	position: relative;
	top:-1px;
	left: 0px;
}

.muiTaskNavBar ul li .icon {
	margin-right: 3px;
	padding: 0;
	width: 4rem;
	height: 4rem;
	line-height: 4rem;
	text-align: center;
	color: #848484;
	border: 1px solid #848484;
	border-radius: 50%;
	display: inline-block;
	position: relative;
}

.muiTaskNavBarSelected {
	position: absolute;
	bottom: -2px;
	left: 0;
	background-color: white;
	-webkit-transition: 300ms linear;
	z-index: 99;
}

.muiTaskHeader .muiTaskNavBarSelected .arrM {
	bottom: 1px;
	border-bottom-color: #d9d7d7;
}

.muiTaskHeader .muiTaskNavBarSelected .arrC {
	bottom:0;
	border-bottom-color: #fff;
}

.muiTaskNavBarSelected .arr {
	position: absolute;
	left: 50%;
	/*margin-left: -7.4rem;*/ margin-left:-2.1rem;
	border-color: transparent;
	border-style: dashed dashed solid;
	border-width: 6px;
}

.muiTaskNavBar .muiNavitem.muiNavitemSelected span{
	color: #37aee9;
	font-weight:normal;
	min-width: 0;
}

.muiTaskNavBar .muiNavitem.muiNavitemSelected span.muiNavitemSpan,
.muiTaskNavBar .muiNavitem.muiNavitemSelected span.txt{
	border: 0;
}

.muiTaskNavBar .muiNavitem.muiNavitemSelected .icon{
	color: #37aee9;
	border-color: #37aee9;
}

.muiTaskNavBar ul li .txt {
	display: inline-block;
	font-size: 1.4rem;
}

/** 时间 **/
.muiTaskDatetimeContainer{
	padding: 0 0 1rem 0;
}

.muiTaskDatetime,.muiTaskWeek{
	display: inline-block;
}

.muiTaskDatetimeContainer .newMui.muiFormEleWrap.popup,
.muiTaskDatetimeContainer .newMui.muiFormEleWrap.showTitle {
	padding: 1rem 0;
	height: 6rem;
	padding-left:1.2rem;
}
.muiTaskDatetimeContainer .newMui.muiFormEleWrap .muiFormRequired {
	top: 0;
	font-size: 1.4rem;
}
.muiTaskDatetimeContainer .newMui.muiFormEleWrap.showTitle .muiFormEleTip {
	top: 1rem;
}
.muiEditorTextContainer .muiFormEleWrap.muiFormStatusEdit {
	padding-bottom: 1rem;
}
.muiEditorTextContainer .muiValidateShape{
	top: 1.6rem;
}
.muiTaskWeek{
	font-size:1.4rem;
	font-weight:normal;
	margin-top:.6rem;
}

.muiTaskDatetime .muiField{
	border:1px solid #ececec;
	background-color: #fff;
	min-width: 8rem;
	padding: 5px 4px;
	width: auto;
}

/** 人员 **/
.muiSettingPeopleList {
	padding-top: 0.5rem;
}

.muiSettingPeopleList>li {
	color: #898989;
	font-size: 1.4rem;
	position: relative;
	font-weight:normal;
	padding: 1rem 0 1rem 0;
}
.muiSettingPeopleList>li .newMui.muiFormEleWrap:after {
	bottom: -1rem;
}
.muiSettingPeopleList>li:last-child{ border-bottom: 0;}
.muiSettingPeopleList>li .muiField{
	padding:0 0 0 1.2rem;
}
.muiSettingPeopleList  .newMui.showTitle .muiCateFiledShow {
	margin-top: 1rem;
}
.syncView .muiFormOptionsField{
	margin-left: -240px;
	left: 25%;
	padding-bottom: 0;
	position: relative;
	display:table-cell;
	width: inherit;
}
.muiAgendaNormalTr>td {
	padding-left:1.2rem;
}
.syncView .muiFormOptionsField .muiFieldText{
	border: 0;
	padding-bottom: 5px;
	padding-top:5px;
}
.muiFieldValue .muiCateFiledShow .muiAddressOrgEdit { }
.muiFieldOpt i.mui-address { }
.syncView .muiFormOptionsField .muiFieldOptions .muiFormRadio{
	top: -8px;
}

.syncView .muiFormOptionsField .muiFieldOptions > .muiFieldText{
	padding-top:2px;
}
/** 反馈编辑  **/
.muiTaskTitle{
	color:#555e67;
	padding: 1rem;
	border-bottom: 1px #e0e0e0 solid;
}

.muiProgressBox{
	position:relative;
	padding-bottom:3rem;
	border-top: 1px solid #e0e0e0;
	overflow: hidden;
}

.muiTaskStatusTag{
	padding: 0 3rem;
	width: 2.6rem;
	height: 4rem;
	line-height: 6rem;
	font-size: 1.2rem;
	position: absolute;
	left: -3.6rem;
	top: -0.8rem;
	color: #fff;
	text-align: center;
	-webkit-transform: rotate(-40deg);
	-moz-transform: rotate(-40deg);
	-ms-transform: rotate(-40deg);
	-o-transform: rotate(-40deg);
	white-space: nowrap;
	overflow: hidden;
}

.muiTaskStatusTag.muiTaskProgress{ background-color:#51b749; } /*进行中*/
.muiTaskStatusTag.muiTaskInactive{ background-color:#efad4d; } /*未启动*/
.muiTaskStatusTag.muiTaskTerminate{ background-color:#4dbeba; } /*暂停*/
.muiTaskStatusTag.muiTaskOverrule{ background-color:#eb1d68; } /*驳回*/
.muiTaskStatusTag.muiTaskClose{ background-color:#c0c0c0; } /*已结项*/
.muiTaskStatusTag.muiTaskComplete{ background-color:#3fa9f5; } /*完成*/


.muiProgressBox .pastDue.past{
	background-color: #ff8585;
}

.muiProgressBox .pastDue.notpast{
	background-color: #84c138;
}


.muiProgressBox .numberBox{
	margin:0 0.3rem;
	padding: 0 3px;
	height: 20px;
	line-height: 20px;
	min-width: 22px;
	text-align: center;
	background-color: #999;
	color: #fff;
	border-radius:3px;
}

.muiProgressBox .overday,.muiProgressBox .time{
	float:right;
	margin: 1rem;
}

.muiProgressBox .progress{
	background-color: #bbbfc1;
	height: 10px;
	border-radius: 5px;
	margin: 8rem auto 1rem;
	position: relative;
}

.muiProgressBox .progressAuto{
	margin: 0rem 1.2rem;
}

.muiProgressBox .progressValueText{
	position:absolute;
	margin-top:-2.5rem;
	margin-left: -1.6rem;
}

.muiProgressBox .progressValueTextInner{
	background-color: #18b4ed;
	text-align: center;
	padding: 0.3rem 0.8rem;
	margin-top: -2rem;
	font-size: 1.2rem;
	border-radius:5px;
	color: white;
}

.muiProgressBox .progressTriangle{
	width:0;
    height:0;
    border-width:5px;
    border-style:solid dashed dashed dashed;
    border-color:#18b4ed transparent transparent transparent;
    margin: 0 auto;
}

.muiProgressBox .mblSliderProgressBar{
	background-color: #18b4ed;
}

.muiProgressBox .progressAutoText{
	padding:0.5rem 0 0 1.7rem;
	width: 85%;
	color: #A9A9A9;
	font-size:1.4rem;
}
/** 灯泡  **/
.muiProgressBox .progressAutoText .mui{
	margin:-0.4rem 0 0 1rem;
	color: #F9803F;
}

/** 评价编辑  **/
.muiEvaluateBox{
	background-color:white;
	margin-top:1rem;
	padding:0.5rem;
	position: relative;
	overflow: hidden;
}

.muiEvaluateBox .EvaluateStatus{
	width: 5.5rem;
	height: 1rem;
	font-size: 1.2rem;
	position: absolute;
	left: -1.6rem;
	top: 0.6rem;
	padding: 5px 0px;
	color: #fff;
	text-align: center;
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	overflow: hidden;
	background-color: green;
}

.muiEvaluateBox .EvaluateApprove{
	padding: 5rem 1rem;
}

.mblScrollableViewContainer .muiHeader{border-top:1px #d9d7d7 solid;box-shadow:none;}
.muiHeader.muiTaskHeader{ overflow: visible;}
.muiHeader.muiTaskHeader .muiNavbar{ overflow: visible!important;}

.muiNavbar.sysTaskNavBar .muiNavitem span{ font-size: 1.4rem;}
.muiEditorTextContainer .muiFieldItem{width:96%;}
.muiFormUploadWrap .mui-close:before {
	transform: scale(0.7);
	-webkit-transform: scale(0.7);
	display: inline-block;
}
.newMui .muiTaskDatetimeContainer .muiFormEleWrap:after {
    content: '';
    height: 1px;
    position: absolute;
    bottom: 0;
    background-color: #d5d5d5;
    left: 1.2rem;
    right: 0;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
    z-index: 2;
}

.oldMui form[name=sysTaskFeedbackForm] .muiEditor .muiEditorTextContainer .muiEditorTextarea {
	padding: 1rem;
}
.oldMui form[name=sysTaskFeedbackForm] .muiFormUploadWrap {
	padding-left: 1rem;
}
.oldMui form[name=sysTaskFeedbackForm] .muiFormUploadWrap:after {
	content: '';
	display: table;
	visibility: hidden;
	clear: both;
	padding-left: 1rem;
}
.newMui .muiDetailTableNo span {
	padding-left: 0;
}
.newMui.muiEditor .muiEditorTextContainer .muiEditorTextarea {
	padding: 1rem 0;
}
#syncView .newMui.muiFormEleWrap,
#personView .newMui.muiFormEleWrap {
	padding-left: 1rem;
}
.muiTaskCategory {
	display: block;
	opacity: 1;
	background-color: #fff;
	padding-left: 1rem;
}
.muiTaskCategory .newMui.muiFormEleWrap:after {
	content: '';
	position: absolute;
	left: 1rem;
	right: 0;
	bottom: 0;
	border-bottom: 0;
	transform: scaleY(0.5);
	-webkit-transform: scaleY(0.5);
}
