<div class="muiEkpSubContainer">
    <div class="muiEkpSubForumManagement">
        <div class="muiEkpSubHeadicon" style="background: url(${imgUrl});background-size: cover;"></div>
        <ul class="muiEkpSubForumInfo">
            <li class="muiEkpSubBold">${name}</li>
            <li>
                <div class="muiEkpSubLevel muiFontColorImp">
                    <i class="fontmuis muis-level"></i>
                    LV${level}
                </div>
            </li>
        </ul>
        <ul class="muiEkpSubForumDetail">
            <li>
                <div class="muiFontColorImp">${pcount}</div>
                <span>!{postCount}</span>
            </li>
            <li>
                <div>${rcount}</div>
                <span>!{replyCount}</span>
            </li>
            <li>
                <div class="muiFontColorImp">${score}</div>
                <span>!{score}</span>
            </li>
        </ul>
    </div>
</div>