@CHARSET "UTF-8";

.mblTabBarButton{
	width:100%;
}
.muiCateItem:active {
	background-color: #dfe1e0;
}

.muiCateItem .muiCateSel {
	position: relative;
	font-size: 0.8rem;
	line-height: 1.2rem;
	text-align: center;
}

.muiCateSeled .muiCateSel::after {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	content: '\2713';
	color: white;
}

.muiCateSeled .muiCateSel{
	background-color: #20A0FF;
	border: 0.1rem solid #20A0FF;
}