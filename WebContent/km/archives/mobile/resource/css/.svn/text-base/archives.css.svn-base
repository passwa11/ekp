@CHARSET "UTF-8";


.mblBackground{
	background-color: #f7f7f7;
}

.muiArchivesItem {
	position: relative;
	display: block;
	background: white;
}

.muiArchivesItem:not(:last-child) .muiArchivesWrap{
	border-bottom: 1px solid #e5e5e5;	
}

.muiListItem {
	padding: 0 1rem;
	border-bottom: none;
}

.muiArchivesWrap {
	display: block;
	position: relative;
	text-decoration: none;
	color: black;
}

.muiArchivesMain {
	flex-flow: row nowrap;
	display: flex;
	justify-content: space-between;
	padding: 1rem 0;
}

.muiArchivesMain_sec_left {
	max-width: 23rem;
}

.muiArchivesMain_sec_right {
	text-align: center;
	font-size: 13px;
	color: #999;
}

.muiArchivesMain_sec_left_top {
	display: flex;
	flex-flow: row nowrap;
	align-items: center;
	margin-bottom: 0.8rem;
}
.muiArchivesMain_sec_left_bottom {
	position: relative;
}

.muiArchivesMain_sec_left_bottom .mui-post{
	font-size: 18px;
	position: absolute;
	left: 0;
	top: 4px;
}

.muiArchivesMain_sec_left_bottom span{
	color: #999;
	font-size: 1.3rem;
}

.muiArchivesMain_sec_left_bottom ul{
	padding-left: 2rem;
	position: relative;
}

.muiArchivesMain_sec_left_bottom ul li {
	display: inline-block;
	text-decoration: underline;
	margin: 0.2rem 0.4rem;
}

.muiArchivesSign {
	display: inline-block;
	margin-right: 1.2rem;
	border: 1px solid green;
	color: green;
	padding: .1rem .3rem;
	white-space: nowrap;
	font-size: 1rem;
	border-radius: .3rem;
}

.muiArchivesSign[data-status="1"] {
	border-color: orange;
	color: orange;
}

.muiArchivesSign[data-status="40"] {
	border-color: cornflowerblue;
	color: cornflowerblue;
}

.muiArchivesSign[data-status="60"] {
	border-color: #999;
	color: #999;
}

.muiArchivesSign[data-status="2"],
.muiArchivesSign[data-status="11"],
.muiArchivesSign[data-status="60"] {
	border-color: red;
	color: red;
}

.muiArchivesTitle {
	font-size: 1.7rem;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	max-width: 18rem;
}

.muiArchivesItem_icon {
	display: inline-block;
	width: 3.5rem;
	height: 3.5rem;
	background-size: cover;
	background-position: 50% 50%;
	background-repeat: no-repeat;
	
}

.muiArchivesItem_icon_1 {
	width: 	6rem;
	height: 4.5rem;
}

.muiArchivesItem_icon_2 {
	border-radius: 50%;
}

.simpleListItem {
	padding: 1.2rem 0;
    text-align: center;
    border-bottom: 1px solid #ccc;
}

.simpleListItem:active {
	background: #f5f5f5;
}

.muiListNoDataArea {
	margin-top: 64px;
}

.borrowCertItem {
	font-size: 1.1rem;
}
#btnAddBorrowApply,#btnAddCertification{
	width:100%!important;
}

