.ekp-archives-remind-list:after {
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.ekp-archives-remind-list > li {
	width: 33.33%;
	padding: .5rem 0;
	text-align: center;
	color: #fff;
	font-size: 1.4rem;
	position: relative;
	z-index: 2;
	-webkit-box-sizing: border-box;
			box-sizing: border-box;
	-webkit-flex: 1;
			flex: 1;
	float: left;	
}
