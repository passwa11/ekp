.ekp-archives-remind-list > li:active{
	background-color: rgba(255, 255, 255, .15);
}
.ekp-archives-remind-list > li[data-href]:active {
	border-radius: .3rem;
	background-color: #f8f8f8;
}
.ekp-archives-remind-list > li .num,
.ekp-archives-remind-list > li .txt {
	display: block;
}
.ekp-archives-remind-list > li .num {
	margin-bottom: .5rem;
	color: #fff;
	font-family: Arial;
	font-size: 2.85em;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

	/* 顶部提醒栏 end */
	
		/* 列表导航 begin */
.ekp-archives-nav-panel > .ekp-archives-nav-list{ margin-bottom: .8rem; }
.ekp-archives-nav-list{
	margin: 0;
	padding: 0;
	list-style: none;
	background-color: #fff;
	-webkit-box-shadow: 0 0 3px -1px rgba(0,0,0,.055);
    			box-shadow: 0 0 3px -1px rgba(0,0,0,.055);
}
.ekp-archives-nav-list > li {
	padding-left: 6rem;
	font-size: 1.4rem;
	position: relative;
}
.ekp-archives-nav-list > li:after{
	content: '';
	display: table;
	visibility: hidden;
	clear: both;
}
.ekp-archives-nav-list > li:active,
.ekp-archives-nav-list > li[data-href]:active{ background-color: #f2f2f2; }
.ekp-archives-nav-list > li .iconbox{
	width: 4rem;
	height: 4rem;
	line-height: 4rem;
	text-align: center;
	color: #fff;
	font-size: 2.2rem;
	border-radius: 50%;
	background-color: #ccc;
	float: left;
	margin-left: -5rem;
	margin-top: .9rem;
}
.ekp-archives-nav-list:nth-child(1) > li:nth-child(4n+1) .iconbox{ background-color: #ea4335; }
.ekp-archives-nav-list:nth-child(1) > li:nth-child(4n+2) .iconbox{ background-color: #4285f4; }
.ekp-archives-nav-list:nth-child(1) > li:nth-child(4n+3) .iconbox{ background-color: #47b9f0; }
.ekp-archives-nav-list:nth-child(1) > li:nth-child(4n+4) .iconbox{ background-color: #f9905a; }

.ekp-archives-nav-list:nth-child(2) > li:nth-child(4n+1) .iconbox{ background-color: #4285f4; }
.ekp-archives-nav-list:nth-child(2) > li:nth-child(4n+2) .iconbox{ background-color: #47b9f0; }
.ekp-archives-nav-list:nth-child(2) > li:nth-child(4n+3) .iconbox{ background-color: #f9905a; }
.ekp-archives-nav-list:nth-child(2) > li:nth-child(4n+4) .iconbox{ background-color: #ea4335; }

.ekp-archives-nav-list:nth-child(3) > li:nth-child(4n+1) .iconbox{ background-color: #f9905a; }
.ekp-archives-nav-list:nth-child(3) > li:nth-child(4n+2) .iconbox{ background-color: #3CCC50; }
.ekp-archives-nav-list:nth-child(3) > li:nth-child(4n+3) .iconbox{ background-color: #4285f4; }
.ekp-archives-nav-list:nth-child(3) > li:nth-child(4n+4) .iconbox{ background-color: #47b9f0; }

.ekp-archives-nav-list:nth-child(4) > li:nth-child(4n+1) .iconbox{ background-color: #47b9f0; }
.ekp-archives-nav-list:nth-child(4) > li:nth-child(4n+2) .iconbox{ background-color: #f9905a; }
.ekp-archives-nav-list:nth-child(4) > li:nth-child(4n+3) .iconbox{ background-color: #ea4335; }
.ekp-archives-nav-list:nth-child(4) > li:nth-child(4n+4) .iconbox{ background-color: #4285f4; }

.ekp-archives-nav-caption{
	padding: .8rem 2rem .8rem 0;
	min-height: 4rem;
  -webkit-display: flex;
          display: flex;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-align-items: start;
          align-items: start;
}
.ekp-archives-nav-list > li:not(:last-child) .ekp-archives-nav-caption{ border-bottom: 1px solid #e0e0e0; }
@media screen and (-webkit-min-device-pixel-ratio: 2){
	.ekp-archives-nav-list > li:not(:last-child) .ekp-archives-nav-caption{
		border: 0;
    background-repeat: repeat-x;
    background-size: 100% 1px;
    background-position: left bottom;
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #e0e0e0), to(#e0e0e0));

	}
}
.ekp-archives-nav-caption-title{
	margin: 0;
	color: #333;
	font-size: 1.6rem;
	font-weight: normal;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.ekp-archives-nav-caption-subhead{
	margin-top: .3rem;
	color: #999;
	font-size: 1.2rem;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.ekp-archives-nav-list > li .mui.mui-forward{
	color: #999;
	position: absolute;
	right: 1rem;
	top: 50%;
	margin-top: -.7rem;
}

.ekp-archives-nav-list a {
	display: block;
}
.ekp-archives-nav-icon>span {
	color: #fff;
	font-size: 0.4rem;
	display: block;
	text-align: center;
	line-height: 0.8rem;
}
.mui-numerical-statement:before { content: "\C238"; }/* 统计报表 */
	/* 列表导航 end */
	