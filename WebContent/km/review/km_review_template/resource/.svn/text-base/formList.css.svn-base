/* ------------ 01-热门商城模板 Starts ------------ */
div[id*="template_listview_"] {
  height: 450px;
}

div[id="template_listview_Reuse"] {
  height: 500px;
}

.lui_template_tabs [data-lui-type='lui/panel!Content'] {
  padding: 0px;
}

body {
  overflow-x: hidden;
}

.lui_template_tabs {
  position: absolute;
  width: 100%;
  padding: 5px;
}

.lui_mode_content {
  padding-left: 10px;
}

.cm-hot-template {
  width: 920px;
}

.cm-hot-template .hot-template-top {
  height:200px;
}

.cm-hot-template .hot-template-top .card-box {
  float: left;
  width: 160px;
  height: 178px;
  background: #FFFFFF;
  border:1px solid #eee;
/*   -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16); */
  border-radius: 4px;
  padding: 14px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  text-align: center;
  margin: 20px 10px;
  cursor: pointer;
}
.cm-hot-template .hot-template-top .card-box:last-child span.thirdMall{
  margin-top:10px
}
.cm-hot-template .hot-template-top .card-box p {
  font-size: 14px!important;
  color: #333;
  line-height: 18px;
  margin-bottom: 4px;
  opacity: 1 !important;
}

.cm-hot-template .hot-template-top .card-box span {
  font-size: 12px;
  line-height: 17px;
  color: #999;
}

.cm-hot-template .hot-template-top .card-box div {
  font-size: 12px;
  color: #333333;
  text-align: center;
  opacity: 0.8;
}

.cm-hot-template .hot-template-top .card-box .top {
  margin-bottom: 14px;
}

.cm-hot-template .hot-template-top .card-box .icon-3 {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/icon_add@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  width: 40px\9;
  height: 40px\9;
}

.cm-hot-template .hot-template-top .card-box .icon-1 {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/icon_text@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  background: url("../resource/img/icon_text.png")\9;
}

.cm-hot-template .hot-template-top .card-box .icon-4 {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/icon_duobiaodan@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  background: url("../resource/img/icon_duobiaodan.png")\9;
}

.cm-hot-template .hot-template-top .card-box .icon-5 {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/icon_word@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  background: url("../resource/img/icon_word.png")\9;
}

.cm-hot-template .hot-template-top .card-box .icon-thirdMall-success {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/img_1@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  background: url("../resource/img/img_1.png")\9;
}

[data-formid="thirdMall-success"] p {
  font-size: 12px !important;
  /* color: #666666 !important; */
  text-align: center !important;
  opacity: 0.8 !important;
}

.cm-hot-template .hot-template-top .card-box .icon-thirdMall-noAuth {
  width: 34px;
  margin-top: 23px;
  background: url("../resource/img/icon_store@2x.png");
  background-size: 34px 34px;
  height: 34px;
  display: inline-block;
  background: url("../resource/img/icon_store.png")\9;
}

[data-formid="thirdMall-noAuth"] p{
  font-size: 12px !important;
  color: #333333 !important;
  text-align: center !important;
  opacity: 0.8 !important;
}

.cm-hot-template .hot-template-top .card-box .icon-thirdMall-noNetWork {
  width: 80px;
  margin-top: 23px;
  background: url("../resource/img/img_2_small@2x.png");
  background-size: 80px 40px;
  height: 40px;
  display: inline-block;
  background: url("../resource/img/img_2_small.png")\9;
}

[data-formid="thirdMall-noNetWork"] p {
  font-size: 12px !important;
  color: #333333 !important;
  text-align: center !important;
  opacity: 0.8 !important;
}

.cm-hot-template .hot-template-top .card-box .icon-word,
.cm-hot-template .hot-template-top .card-box .icon-text,
.cm-hot-template .hot-template-top .card-box .icon-duobiaodan {
  width: 34px;
  margin-top: 28px;
}

.cm-hot-template .hot-template-top .card-box .icon-img_1 {
  width: 44px;
  margin-top: 9px;
}

.cm-hot-template .hot-template-top .card-box .icon_store {
  width: 34px;
  margin-top: 18px;
}

.cm-hot-template .hot-template-top .card-box .img_2_small {
  width: 80px;
  margin-top: 24px;
}

.cm-hot-template .hot-template-top .card-box a {
  display: block;
  font-size: 12px;
  color: #4285F4;
  margin-top: 10px;
}

.cm-hot-template .hot-template-main {
  content: '';
  display: block;
  clear: both;
}

.cm-hot-template .hot-template-main .cascade {
  padding: 0 10px;
  font-size: 12px;
  color: #333;
}

.cm-hot-template .hot-template-main .cascade .first-level {
  margin-bottom: 8px;
  width: 100%;
}

.cm-hot-template .hot-template-main .cascade .first-level span {
  margin-right: 20px;
  padding-bottom: 14px;
  line-height: 30px;
}

.cm-hot-template .hot-template-main .cascade .first-level .active {
  color: #4285F4;
  border-bottom: 2px solid #4285F4;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv {
  float: right;
  width: 207px;
  height: 28px;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  padding: 3px 8px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv input {
  border: 0;
  width: 165px;
  max-width: 165px;
  font-size: 12px;
  color: #333;
  line-height: 20px;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv input::-webkit-input-placeholder {
  font-size: 12px;
  color: #999999;
  line-height: 18px;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv::after {
  content: "";
  display: inline-block;
  width: 14px;
  height: 14px;
  background: url("../images/hypermarket/search.png") no-repeat;
  background-size: 14px 14px;
  margin: 3px 0 0 5px;
  vertical-align: top;
}

.cm-hot-template .hot-template-main .cascade p {
  height: 1px;
  background: #EEEEEE;
}

.cm-hot-template .hot-template-main .cascade .second-level {
  margin: 20px 0;
}

.cm-hot-template .hot-template-main .cascade .second-level .active {
  color: #4285F4;
}

.cm-hot-template .hot-template-main .cm-items {
  float: left;
  margin: 10px;
  width: 160px;
  height: 226px;
  overflow: hidden;
  -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  border-radius: 4px;
  position: relative;
  z-index: -99;
}

.cm-hot-template .hot-template-main .cm-items .item-box {
  position: absolute;
  width: 100%;
  height: 100%;
}

.cm-hot-template .hot-template-main .cm-items .item-box img {
  width: 160px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content {
  padding: 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .title {
  font-size: 14px;
  color: #333333;
  line-height: 22px;
  margin-bottom: 16px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews {
  margin-bottom: 10px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews img {
  width: 12px;
  vertical-align: middle;
  margin-right: 2px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews span {
  display: inline-block;
  max-width: 20px;
  font-size: 12px;
  line-height: 18px;
  color: #999999;
  margin-right: 3px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews p {
  float: right;
  width: 56px;
  height: 16px;
  line-height: 16px;
  background: #fff;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #4285F4;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  font-size: 12px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account {
  padding-top: 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border-top: 1px solid #ddd;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account span {
  font-size: 12px;
  color: #333333;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right {
  float: right;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right p {
  display: inline-block;
  width: 49px;
  height: 18px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right img {
  width: 20px;
  height: 20px;
  -o-object-fit: full;
  object-fit: full;
  border-radius: 50%;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .more {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.cm-hot-template .hot-template-main .cm-items .item-box .more span {
  font-size: 14px;
  color: #333333;
}

.cm-hot-template .hot-template-main .cm-items .item-box .more img {
  width: 16px;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-mask {
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  border-radius: 4px;
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content a {
  display: block;
  width: 64px;
  height: 24px;
  line-height: 24px;
  background: #4285F4;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #fff;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  font-size: 12px;
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content a:first-child {
  margin-bottom: 10px;
}

.cm-hot-template .hot-template-main .cm-items:hover .item-mask {
  display: block;
}

/* ------------ 01-热门商城模板 Ends ------------ */
