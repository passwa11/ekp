}
div.muiSimpleCateContainer{
    border: none;
}
/* 排序的icon */
.muiHeaderView{
    padding: 0 15px!important;
}
.muiHeaderView .muiSort span.muiSortIcon:before{
    top:1px;
    border-left: 1px solid #999;
    border-top: 1px solid #999;
    border-bottom:none;
    border-right: none;
    transform: rotate(45deg);
    width: 3px;
    height: 3px;
} 
.muiHeaderView .muiSort span.muiSortIcon:after{
    top:5px;
    border-right: 1px solid #999;
    border-top: 1px solid #999;
    border-bottom:none;
    border-left: none;
    transform: rotate(135deg);
    width: 3px;
    height: 3px;
} 
.muiHeaderView .muiSort.down .muiSortIcon:after {
    border-color:#0089FF;
}
.muiHeaderView .muiSort.up .muiSortIcon:before{
    border-color:#0089FF;
}

/* 分类 */
.muiSimpleCateContainer .muiSimpleCateContent:first-child{
    box-shadow: none!important;
    border-right:1px solid #eee;
    background-color:white!important;
    box-sizing: border-box;
}
.muiCateItem.muiCategoryEntered .muiCateSelArea{
    opacity: 0;
}
.muiCateItem.muiCategoryEntered .muiCateInfo .muiCateName{
    color:#0089FF;
    line-height: 48px!important;
}
.muiCateInfo div.muiCateName{
    color:#111F2C;
    line-height: 48px!important;
}
.muiCateItem.muiCategoryEntered .muiCateMore i.mui-forward:before{
    color:#0089FF;
}
.muiCateSelArea div.muiCateSeled {
    background-color: white;
    border:4px solid #0089FF;
    box-sizing: border-box;
}
.muiCateItem.animated{
    height:48px!important;
    line-height:48px;
}
.muiCatePath{
    background:#F2F2F6!important;
}
.muiCatePathContent{
    background:transparent!important;
}
.muiCateSelArea .muiCateSel i.mui-checked.muiCateSelected:before{
    display: none;
}
.muiCatePathItem:after, 
.muiCatePathTitleItem:after,
.muiCatePathContent .mblScrollableViewContainer>div:after, 
.muiCatePathContent>div:after{
