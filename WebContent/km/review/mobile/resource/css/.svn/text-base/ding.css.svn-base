    padding-top:0;
    margin-bottom: .3rem;
}
.muiKmReviewItem .muiKmReviewListItem .muiKmReviewListProcessContainer>div:not(:first-child):before {
    background-color: #fff;
}
.muiKmReviewItem .muiKmReviewListItem .muiKmReviewListProcessContainer {
    margin-left: 0;
    margin-top: 0rem;
    margin-bottom: 1.8rem;
}
.muiKmReviewItem .muiKmReviewListItem .muiKmReviewListProcessContainer>div:not(:first-child) {
    height: 2rem;
}
.muiKmReviewListBottom .muiKmReviewListPersonHeadIcon img {
    height: 20px;
    width: 20px;
    vertical-align: middle;
    margin-right: 7px;
    border-radius: 20px;
}
.muiKmReviewListBottom .muiKmReviewListPersonHeadIcon span {
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #111F2C;
    letter-spacing: 0;
    vertical-align: middle;
    line-height: 20px;
}
.muiProcessStatusBorder {
	background-color: #fff;
}
