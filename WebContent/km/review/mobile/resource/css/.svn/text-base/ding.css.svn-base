.mblBackground{
	background: #f7f7f8;
}
.clearfix:after {
    content: "";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both
}
.clearfix {
    zoom: 1
}
.mui_review_platform_home{
	padding: 12px;
    color: #111F2C;
    
}
.mui-review-platform-nav{
	background: #FFFFFF;
    margin-bottom: 12px;
    padding: 20px 0 17px 0;
    border-radius: 16px;
}
.mui-review-platform-nav .clearfix {
    zoom: 1;
}
.mui-review-platform-nav ul li {
    float: left;
    width: 25%;
    text-align: center;
}
.mui-review-platform-nav ul li .mui-review-platform-nav-top {
    position: relative;
    text-align: center;
}
.mui-review-platform-nav ul li .mui-review-platform-nav-top img {
    height: 25px;
    width: 20px;
}
.mui-review-platform-nav ul li .mui-review-platform-nav-top span {
    position: absolute;
    height: 16px;
    width: 16px;
    border-radius: 8px;
    background: #FB602D;
    color: white;
    vertical-align: middle;
    font-size: 12px;
    line-height: 16px;
    top: -8px;
    right: 16px;
    display:none;
}
.mui-review-platform-nav ul li p {
    font-family: PingFangSC-Regular;
    font-size: 16px;
    color: #111F2C;
    letter-spacing: 0;
}

.mui-review-platform-template-item {
    border-bottom: 1px solid #ededee;
    margin-bottom: 10px;
    background: #FFFFFF;
    border-radius: 16px;
}
.mui-review-platform-template-item-head {
    overflow: hidden;
    position: relative;
    padding: 15px 15px 12px 15px;
}
.mui-review-platform-template-item-head .mui-review-head-l {
    padding-right: 30px;
    box-sizing: border-box;
}
.mui-review-menu-frame-nav {
    line-height: 24px;
}
.mui-review-menu-frame-item {
    display: inline-block;
    font-size: 16px;
}
.mui-review-menu-frame-icon-arrow {
    width: 16px;
    height: 16px;
    background: url(../images/icon-arrow-r@2x.png) no-repeat center center;
    background-size: contain;
    vertical-align: middle;
    position: relative;
    top: -2px;
    padding: 0 2px;
}
.mui-review-menu-frame-item p {
    display: inline-block;
    table-layout: fixed;
    white-space: normal!important;
    word-wrap: break-word!important;
}
.mui-review-menu-frame-nav em {
    font-style: normal;
    color: #999;
    margin-left: 5px;
}
.mui-review-platform-template-item-head .mui-review-head-r {
    position: absolute;
    top: 17px;
    right: 15px;
}
.mui-review-platform-template-item-head .mui-review-head-r .mui-icon-toggle-up {
    background-image: url(../images/icon-toggle-up@2x.png);
}
.mui-review-platform-template-item-head .mui-review-head-r .mui-icon-toggle-down {
    background-image: url(../images/icon-toggle-down@2x.png);
}
.mui-review-platform-template-item-head .mui-review-head-r i {
    display: inline-block;
    width: 14px;
    height: 14px;
    background-position: center;
    background-repeat: no-repeat;
    background-size: 14px auto;
    cursor: pointer;
}
.mui-review-platform-template-item-content {
    transition: max-height 2s;
    overflow: hidden;
}
.mui-review-platform-template-item-content.unexpanded {
    display:none;
}
.mui-review-platform-template-item-content ul {
    border-top: 1px solid rgba(17,31,44,0.08);
    list-style: none;
    padding: 0 10px 25px 10px;
}
.mui-review-platform-template-item-content li {
    float: left;
    border-radius: 8px;
    width: 25%;
    margin-top: 25px;
    height:90px;
}
.mui-review-platform-template-item-content li>a {
    display: inline-block;
    width: 100%;
    height: 100%;
    text-align: center;
}
.mui-review-platform-template-item-content li>a img {
    height: 40px;
    width: 40px;
    border-radius: 17px;
    margin-bottom: 7px;
}
.mui-review-platform-template-title {
    padding-right: 10px;
}
.mui-review-platform-template-title p {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #333333;
    letter-spacing: 0;
    text-align: center;
    width: 67px;
    margin: 0 auto;
    max-height: 66px;
}
/* 列表页 */
.muiNavHeader,.muiHeaderNav{
	background: #fff;
}
.muiKmReviewItem {
    position: relative;
    margin-top: .1.2rem;
    border-radius: .6rem;
}
.muiList {
    padding: 0 1.2rem;
}
.muiSearchBar {
    padding-left:0;
}
.muiHeaderCategoryWrap{
