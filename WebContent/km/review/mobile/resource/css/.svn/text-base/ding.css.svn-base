    justify-content: space-between;
    align-items: center;
}
div.muiCateSecRight span {
    width:160px;
    height:50px;
    border-radius: 8px;
    line-height:50px;
    font-size: 17px;
}
div.muiCateSecRight span.muiCateClearBtn{
    border: 1px solid #E8EAEB;
    color: rgba(17,31,44,0.56);
}
div.muiCateSecRight span.muiCateSecBtn{
    background: #0089FF;
}

/* 筛选下的按钮 */
div.filterlayer_bottom_buttons{
    width:100%;
    padding: 0 25px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height:75px;
    box-sizing:border-box;
}
div.filterlayer_bottom_buttons div {
    width:160px;
    height:50px;
    border-radius: 8px;
    line-height:50px;
    font-size: 17px;
}
div.filterlayer_bottom_buttons div.filterlayer_bottom_button.bg_1{
    border: 1px solid #E8EAEB;
    color: rgba(17,31,44,0.56);
    margin-right: 10px;
}
div.filterlayer_bottom_buttons div.filterlayer_bottom_button.bg_2{
    background: #0089FF;
}

/* 筛选弹窗 */
div.filterlayer_content{
    padding-bottom: 150px;
    border-radius: 8px 8px 0 0;
}
div.filterlayer_content .mui-review-head{
    height:44px;
    line-height: 44px;
    font-size: 17px;
    position: relative;
    padding-bottom: 8px;
    margin-bottom: 10px;
}
div.filterlayer_content .mui-review-head:after{
    position: absolute;
    content: '';
    height: 8px;
    width: 100%;
    background: #F2F3F5;
    bottom: 0;
    left: 0;
}
div.filterlayer_content .mui-review-head .mui-icon-toggle-down{
    position: absolute;
    height: 10px;
    width: 10px;
    left: 20px;
    top: 14px;
    transform: rotate(-45deg);
    border-bottom: 1px solid #111F2C;
    border-left: 1px solid #111F2C;
}
.filterlayer_content div.filterItem{
    padding-bottom: 24px;
}
.filterlayer_content h4.filterItemTitle{
    font-family: PingFangSC-Regular;
    font-size: 15px;
    color: #111F2C;
    letter-spacing: 0;
    padding:0 0 0 9px;
    margin-bottom: -3px;
}
.filterlayer_content div.filterItem{
    border-bottom:none;
}
div.filterlayer_content ul li{
    width:50%;
    padding:0 9px;
    height:36px;
    line-height: 36px;
    border-radius: 8px;
    margin:15px 0 0 0;
    background: #FFFFFF;
}
div.filterlayer_content ul li a{
    font-family: PingFangSC-Regular;
    font-size: 15px;
    color: rgba(17,31,44,0.56);
    letter-spacing: 0;
    text-align: center;
    border: 1px solid #E2E3E5;
    background:white;
    box-sizing: border-box;
    height:36px;
    line-height: 34px;
    border-radius: 8px;
}
div.filterlayer_content ul li.selected a, .filterlayer_content ul li:active a{
    background: #0089FF;
    color:white;
    border:none;
    line-height: 36px;
    border:none;
}
.filterlayer_content ul li.selected a::after{
    display: none!important;
}


.filterlayer_content span.filterItemPicker_split{
    background-color:#E2E3E5;
}
.muiDocViewHeaderStatus.processStatusDone,
.muiDocViewHeaderStatus.processStatusDiscard{
    bottom:-6rem;
}

/* 审批页面 */
.muiCheckItem.hasText{
