    border-radius: 5px;
    padding: 5px;
}
.muiHeaderCategoryTxt{
	display: table-cell;
    text-align: center;
    outline: 0;
    vertical-align: middle;
}
.muiKmReviewItem .muiKmReviewListItem {
    border-radius: .6rem;
    padding: 1.2rem;
}
.muiKmReviewItem .muiKmReviewListItem .muiKmReviewListTitle {
