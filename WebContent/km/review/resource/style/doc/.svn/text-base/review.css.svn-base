/* 样式主体Start */

body{
    /*background-color: #e9ecef;*/
    height: fit-content;
}

/* 流程统计Start */
.lui_review_statist_data {
    position: relative;
    margin: 6px 0;
}
.lui_review_statist_echarts {
    /*height: 375px;*/
    background-color: #FFFFFF;
    margin-left: 340px;
    border-radius: 4px;
    height: 337px;
    overflow: hidden;
}

.lui_review_statist_panel {
    float: left;
    width: 300px;
    height: 337px;
    padding: 0px 16px;
    background: #FFFFFF;
    box-shadow: 0 0 6px 0 rgba(0,0,0,0.06);
    border-radius: 4px;
}
.lui_review_statist_panel li {
    border-bottom: 1px dotted #eeeeee ;
}
.lui_review_statist_panel li:last-child {
    border-bottom: none;
}

.lui_review_statist_panel li p {
    margin-bottom: 10px;
    line-height: 24px;
    font-size: 12px;
    color: #666666;
    font-weight: 400;
}
.lui_review_statist_panel h3 {
    margin-top: 19px;
    margin-bottom: 13px;
    font-size: 14px;
    color: #333333;
    line-height: 14px;
    font-weight: 400;
}
.lui_review_statist_item {
    width: 50%;
    display: inline-block;
}

.lui_review_statist_item.portlet_item {
    display: inline-block !important;
    width: 146px !important;
}

.lui_review_statist_item a {
    margin-left: 8px;
    font-size: 16px;
    color: #2778FF;
    font-weight: 400;
}

.lui_review_statist_icon {
    width: 16px;
    height: 16px;
    display: inline-block;
    margin-right: 9px;
    vertical-align: middle;
}

.lui_review_statist_icon_todo {
    background: url(../images/icon_pending.png) no-repeat center;
}

.lui_review_statist_icon_done {
    background: url(../images/icon_finished.png) no-repeat center;
}

.lui_review_statist_icon_launch {
    background: url(../images/icon_launch.png) no-repeat center;
}
.lui_review_statist_icon_lib {
    background: url(../images/icon_process_lab.png) no-repeat center;
}

/* 流程统计End */

/* 流程分类Start */
.lui_review_cateTemplate_list {
    overflow: hidden;
    /* width: 1228px; */
    /* padding: 16px; */
    background-color: #FFFFFF;
    margin: 5px 9px;
}

.lui_review_cateTemplate_list li {
    width: 186px;
    height: 64px;
    float: left;
    margin-right: 16px;
    margin-bottom: 16px;
    background: #FFFFFF;
    border: 1px solid #EEEEEE;
    border-radius: 2px;
}
.lui_review_cateTemplate_list li i{
    float: left;
}
.lui_review_cateTemplate_list li img{
    width: 40px;
    height: 40px;
    margin: 12px;
}

.lui_review_cateTemplate_list .lui_review_cate_item_content {
    margin-left: 64px;
    margin-top: 14px;
    margin-bottom: 14px;
}
.lui_review_cateTemplate_list .lui_review_cate_item_content .lui_review_cateTemplate_name {
    font-size: 12px;
    color: #999999;
    line-height: 12px;
    font-weight: 400;
    margin-bottom: 11px;
    width: 110px;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.lui_review_cateTemplate_list .lui_review_cate_item_content p {
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}

.lui_review_cateTemplate_info {

    margin: 6px 0;
}

.lui_review_echarts_table {
    height: 300px;
}

.lui_review_cateTemplate_list .lui_review_cate_item_content .lui_review_cateTemplate_cate {

    font-size: 12px;
    color: #999999;
    line-height: 12px;
    font-weight: 400;
    width: 110px;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
/* 流程分类End */
/* 样式主体End */