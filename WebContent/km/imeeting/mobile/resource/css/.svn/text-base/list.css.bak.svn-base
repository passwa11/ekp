@charset "utf-8";
*{
	padding: 0;
	margin: 0;-webkit-tap-highlight-color:rgba(255,255,255,0);
}

ol, ul {
	list-style: none;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

address, caption, cite, code, dfn, em, strong, th, var {
	font-weight: normal;
	font-style: normal;
}
/*** 会议列表 ***/
.muiImeetingItem{ border-bottom:none;}

.ImeetingListView .mblEdgeToEdgeList .muiListItem{ margin:0; padding-left:0.8rem; padding-right:0.8rem;}

.ImeetingScrollableView,.ImeetingList{
	background: #f0eff5;
}

.ImeetingList .summary{
	margin-bottom: 0!important;
}

.calendarButton {
	height: 3.8rem;
	line-height: 3.8rem;
	display: table-cell;
	text-align: center;
	color: #4285f4;
	min-width: 4rem;
	height: 100%;
	cursor: pointer;
	width: 4rem;
}

li.muiImeetingItem{
	padding-top:0px; padding-bottom:0px;
	padding-left: 2.8rem!important;
	position: relative;  
}
.ImeetingListView .mblEdgeToEdgeList .muiImeetingItem{  padding-bottom:0px; bottom:0px;}
.muiImeetingItem .arrow_dot {
	width: 1.5rem;
	display: block;
	text-align: center;
	z-index: 11;
	position: absolute;
	left: -1rem;
	top: 1.7rem;
	color: #4285f4;
}
.muiImeetingItem .muiMeetingListContent .arrow_dot .mui{
	font-size: 2.2rem;
	margin-left: 1px;
	line-height: 1;
	background-color: #fff;
 	border-radius: 50%;
}
.muiImeetingItem .muiMeetingListContent{
	border-left: 1px solid #4285f4;
	padding-left: 1.6rem;
	position: relative; -webkit-tap-highlight-color:rgba(255,255,255,0);
}
.muiImeetingItem .muiMeetingListContent .address .mui{ font-size:1.5rem;  margin-right:2px; position:relative; top:-1px;}

.muiImeetingItem p.title a {
	color: #0c0c0c;
	display: inline-block;
	font-size: 1.6rem;
	margin: 0px 0 2px;
	width: 97%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.muiImeetingItem .date {
	padding-top: 2.3rem; font-size:1.2rem; margin-bottom:5px; color:#7e7e7e; -webkit-tap-highlight-color:rgba(255,255,255,0);
}

.muiImeetingItem .address {
	font-size:1.2rem;
	color: #7e7e7e;
	padding-bottom: 1rem;
}

.muiImeetingItem .user {
	padding-bottom: 1rem;
	border-bottom: 1px solid #e5e5e5;
	line-height: 2.8rem;
	max-height: 2.8rem; 
	padding-bottom:8px; 
}
.muiImeetingItem:last-child .user{ border-bottom:0px;}
.muiImeetingItem .user a{ font-size:1.2rem; color:#7e7e7e; }
.muiImeetingItem .figure {
	float: left;
	width: 2.8rem;
	height: 2.8rem;
	text-align: center;
	margin-right: 5px;
	border-radius: 50%;
	display: inline-block;
	overflow: hidden;
}

.muiImeetingItem .figure img {
	width: 100%;
	display: block;
}

.muiImeetingItem .muiMeetingStatus.unHold {
	background-color: #51b749;
}

.muiImeetingItem .muiMeetingStatus.holding {
	background-color: #5484ed;
}

.muiImeetingItem .muiMeetingStatus.hold {
	background-color: #f0ad4e;
}

.muiImeetingItem .muiMeetingStatus {
	display: inline-block;
	padding: 0px 5px;
	color: #fff; font-size:1.2rem;
	border-radius: 3px;
	background: #cfcfcf;
	height: 20px;
	line-height: 20px;
	position: absolute;
	right: 5px;
	top: 17px;
}


.rightContent p.title a {
	color: #454545;
	display: inline-block;
	width: 98%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.rightContent p.user .mui{ font-size:1.6rem; margin-top:-0.2rem;}
.rightContent p.place .mui{ font-size:1.7rem; margin-top:-0.2rem;}
.rightContent p.user,.rightContent p.place {
	font-size: 1.3rem;
	color: #898989; 
}

.muiMeetingCardItem .muiCardIcoList {
	bottom: -2px;
}
.muiMeetingCardItem .muiListSummary{ border-top:0px;}
.muiMeetingCardItem .figure.noStatusFigure{ padding-right:0rem;}
.muiMeetingCardItem .muiCardSummary{ padding-top:0px;}
.muiMeetingCardItem .muiCardIcoList li{ float:none;}
.muiMeetingStatusTag{
	padding: 0;
	text-align: center;
	font-size: 1.0rem;
	padding: 4px 4px 2px;
	margin-right: 0.5rem;
	position: relative;
	top: -2px;
	display: inline;
}
.muiCalendarContainer .muiListNoData{
	background-color: #F8F8F8;
}

.muiMeetingStatusTag.muiMeetingCancel,
.muiMeetingStatusTag.muiMeetingDraft,
.muiMeetingStatusTag.muiMeetingUnhold{
	border-color: #999;
	color: #999;
	background-color: #f5f4f3;
}
.muiMeetingStatusTag.muiMeetingExamine,
.muiMeetingStatusTag.muiMeetingHolding {
	border-color: #4285f4;
	color: #4285f4;
	background-color: #eaf6fc;
}
.muiMeetingStatusTag.muiMeetingDiscard {
	border-color: #F29A5F;
	color: #F29A5F;
	background-color: #fff4ed;
}
.muiMeetingStatusTag.muiMeetingRefuse {
	border-color: #F56B6B;
	color: #F56B6B;
	background-color: #fff0ee;
}
.muiMeetingStatusTag.muiMeetingHold {
	border-color: #68CE67;
	color: #68CE67;
	background-color: #eefaee;
}
@media screen and (-webkit-min-device-pixel-ratio: 2) {
	.muiMeetingStatusTag {
		position: relative;
		border: 0;
	}
	.muiMeetingStatusTag:before {
		content: "";
		width: 200%;
		height: 200%;
		position: absolute;
		top: 0;
		left: 0;
		-webkit-transform: scale(0.5);
						transform: scale(0.5);
		-webkit-transform-origin: 0 0;
						transform-origin: 0 0;
		padding: 1px;
		border-radius: 0.3rem;
		-webkit-box-sizing: border-box;
						box-sizing: border-box;
	}
	.muiMeetingStatusTag.muiMeetingDiscard:before {
		border: 1px solid #F29A5F;
	}
	.muiMeetingStatusTag.muiMeetingExamine:before,
	.muiMeetingStatusTag.muiMeetingHolding:before {
		border: 1px solid #4285f4;
	}
	.muiMeetingStatusTag.muiMeetingHold:before {
		border: 1px solid #68CE67;
	}
	.muiMeetingStatusTag.muiMeetingRefuse:before {
		border: 1px solid #F56B6B;
	}
	.muiMeetingStatusTag.muiMeetingCancel:before,
	.muiMeetingStatusTag.muiMeetingDraft:before,
	.muiMeetingStatusTag.muiMeetingUnhold:before {
		border: 1px solid #999;
	}
}