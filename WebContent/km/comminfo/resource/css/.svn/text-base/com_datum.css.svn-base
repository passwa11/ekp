/*********************** 常用资料 *******************************/
 .lui_form_content{border:0px!important;}
.com_datum_wrapper{ width:100%; height:100%; background:url(../images/blue_paper.jpg) repeat; position:absolute;}
.com_datum_content{ width:1087px; margin:28px auto 0px; min-height:715px; position:relative;}
.shadow_L{ position:absolute; left:0px; bottom:0px; background:url(../images/shadow_L.png) no-repeat left bottom; width:39px; height:360px;}
.com_datum_contentC{ position:absolute; left:39px; top:0px; background:#fff; border:12px solid #f1f1f1; border-width:10px 12px; width:980px; min-height:690px; height:100%;}
.shadow_R{ position:absolute; left:1043px; bottom:0px; background:url(../images/shadow_R.png) no-repeat left bottom; width:44px; height:360px;}


.com_datum_contentLeft{ width:716px; height:100%; overflow:hidden; float:left;border:1px solid #dcdcdc; border-right:0px; min-height:690px; position:relative;}
.com_datum_contentList{ width:260px; height:100%; float:left;  background:#f9f9f9; position:relative; border:1px solid #dcdcdc; border-left:0px;}
.com_datum_contentListC{ padding:10px 20px;word-break:break-all;}
.com_datum_listItem{ margin-bottom:20px;word-break:break-all;}
.com_datum_listItem h3{ margin-bottom:15px; font-size:16px; color:#f99f47; padding-left:39px; line-height:35px;  font-weight:700; border-bottom:1px solid #d5d5d5; position:relative;}
.com_datum_listItem h3 .ico_head{ display:inline-block; width:33px; height:35px; background:#f9f9f9 url(../images/com_datum_item_headS.png) no-repeat left top; position:absolute; bottom:-1px; left:0px;}
.com_datum_listItem ul{
	margin: 0;
	padding: 4px 0;
	list-style: none;
}
.com_datum_listItem ul li {
	padding-left: 16px;
	line-height: 20px;
	position: relative;
}
.com_datum_listItem ul li:before{
	content: '';
	display: inline-block;
	width: 16px;
	height: 16px;
	background: url(../images/dot_gray.png) no-repeat 50%;
	position: absolute;
	left: 0;
	top: 2px;
}
.com_datum_listItem ul li a {
	color: #5a5a5a;
	word-break: break-all;
	word-wrap: break-word
}
.com_datum_listItem ul li.current:before,
.com_datum_listItem ul li:hover:before{
	background-image: url(../images/dot_gray_on.png);
}
.com_datum_listItem ul li.current a,
.com_datum_listItem ul li:hover a {
	color: #0d99e1;
	word-break: break-all;
	word-wrap: break-word
}

.com_datum_arrowL,
.com_datum_arrowR {
	position: fixed;
	width: 46px;
	height: 100%;
	z-index: 30;
}
.com_datum_arrowL{ left:0px;}
.com_datum_arrowR{right:0px;}
.com_datum_arrowL span,.com_datum_arrowR span{ display:none; width:46px; height:104px; position:absolute; cursor:pointer; top:240px;}
.com_datum_contentLeft:hover .com_datum_arrowL span,.com_datum_contentLeft:hover .com_datum_arrowR span{ display:block;}
.com_datum_arrowL span{ background:url(../images/com_arrow_norL.png) no-repeat center; }
.com_datum_arrowR span{  background:url(../images/com_arrow_norR.png) no-repeat center;}
.com_datum_arrowL span:hover{ background:url(../images/com_arrow_curL.png) no-repeat center;}
.com_datum_arrowR span:hover{  background:url(../images/com_arrow_curR.png) no-repeat center;}
.com_datum_arrowL.disable span{ background:url(../images/com_arrow_unL.png) no-repeat center; cursor:default;}
.com_datum_arrowR.disable span{  background:url(../images/com_arrow_unR.png) no-repeat center; cursor:default;}
.com_datum_content_w{ position:absolute; top:0px; left:0px; width:100%; height:100%;}

