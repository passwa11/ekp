/* CSS reset */

fieldset,img { border: 0;}
ul,dl{ list-style:none;}
abbr,acrnym { border: 0;}
input::-ms-clear{ display:none }    /*输入框的小叉叉图标消失*/
*:focus { outline: none; }/* 去掉输入框点击时的边框 */

/*系统全局功能*/
.textEllipsis {width: 99%;overflow: hidden;display: block;white-space: nowrap;text-overflow: ellipsis;}
.clr {clear: both}
.clearfloat {zoom: 1;}
/*@清除浮动 */
.clearfloat:after{display: block;content: 'clear';clear: both;line-height: 0;visibility: hidden;}

.com_datum_w{ background:#fff; padding:15px 20px; margin-bottom:18px;}

.com_datum_item_w{ margin-bottom:20px;}
.com_datum_itemHead{ margin-bottom:15px; font-size:21px; font-style:italic; color:#f99f47; height:46px; padding-left:47px; line-height:46px;  font-weight:700; border-bottom:1px solid #ededed; position:relative;}
.com_datum_itemHead .ico_head{ display:inline-block; width:47px; height:46px; background:#fff url(../images/com_datum_item_head.png) no-repeat left top; position:absolute; bottom:-1px; left:0px;}

.com_datum_itemHead .turn_w{ width:64px; height:28px; position:absolute; bottom:4px; right:4px; cursor:pointer;}
.com_datum_itemHead .turn_w a{ display:inline-block; width:23px; height:23px; background-color:#f4f4f4; border:1px solid #e6e6e6;}
.com_datum_itemHead .turn_w .arrow_L{ margin-right:9px; background:#f4f4f4 url(../images/arrow_L.png) no-repeat center;}
.com_datum_itemHead .turn_w .arrow_R{ background:#f4f4f4 url(../images/arrow_R.png) no-repeat center;}
.com_datum_itemHead .turn_w a:hover{ background-color:#e1e1e1;}

.com_datum_itemContent .turn_w{ width:64px; height:28px; position:absolute; top:-22px; right:4px; cursor:pointer;}
.com_datum_itemContent .turn_w a{ display:inline-block; width:23px; height:23px; background-color:#f4f4f4; border:1px solid #e6e6e6;}
.com_datum_itemContent .turn_w .arrow_L{ margin-right:9px; background:#f4f4f4 url(../images/arrow_L.png) no-repeat center;}
.com_datum_itemContent .turn_w .arrow_R{ background:#f4f4f4 url(../images/arrow_R.png) no-repeat center;}
.com_datum_itemContent .turn_w a:hover{ background-color:#e1e1e1;}

.com_datum_itemContent{ width:936px; height:270px; position:relative;}
.com_datum_itemContent .wrap{ overflow:hidden; }
.com_datum_itemList ul{ width:2000px;}
.com_datum_itemList li{ float:left; width:214px; margin-right:25px;}
.com_datum_itemList li img{ width:214px; height:140px;}
.com_datum_itemList li h3{ padding:8px 0px;}
.com_datum_itemList li h3 a{ color:#343434; font-size:16px;}
.com_datum_itemList li .p1{ height:16px;}
.com_datum_itemList li .p1 a{ color:#808080; display:inline-block; padding-left:20px;}
.com_datum_itemList li .p1 .a_user{  background:url(../images/icon_user.png) no-repeat left top; margin-right:30px; }
