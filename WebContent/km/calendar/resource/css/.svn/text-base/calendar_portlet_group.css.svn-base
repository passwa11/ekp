@font-face {
  font-family: 'calendar_num';
  src: url('../font/font.eot?v=4.3.0');
  src: url('../font/font.eot?#iefix&v=4.3.0') format('embedded-opentype'), url('../font/font.woff?v=4.3.0') format('woff'), url('../font/font.ttf?v=4.3.0') format('truetype'), url('../font/font.svg?v=4.3.0#fontawesomeregular') format('svg');
  font-weight: normal;
  font-style: normal;
}
.lui_calendar_portlet_group{}
.lui_calendar_portlet_group table {
    width: 100%;
    border: 1px solid #ebebeb;
    table-layout: fixed;
}
.lui_calendar_portlet_group table tr th {
    vertical-align: bottom;
    background-color: #fcfcfc;
    border: 1px solid #D8D8D8;
    border-bottom: 0;
    height: 52px;
    padding: 10px;
    color: #84899c;
    font-size: 14px;
    position: relative;
}
.lui_calendar_portlet_group .lui_group_header th:first-child{
	width:120px;
}
.lui_calendar_portlet_group .lui_group_header .lui_pch_button{
	position: absolute;
    top: 10px;
}
.lui_calendar_portlet_group tr th .lui_group_month {
    padding-right: 10px;
    text-align: left;
    display: inline-block;
    position: relative;
    top: 4px;
}

.lui_calendar_portlet_group tr th .lui_group_month em {
    font-family: "xihei","微软雅黑";
    font-size: 36px;
    font-style: normal;
    color: #84899c;
    font-weight: normal;
    position: relative;
    top: 4px;
}
.lui_calendar_portlet_group .lui_group_date, .lui_calendar_portlet_group tr th .lui_group_month em,.lui_calendar_portlet_group .week_num_content .week em {
    color: #666;
}
.lui_calendar_portlet_group .lui_group_weekName {
    position: absolute;
    left: 10px;
    top: 10px;
    color: #999;
}
.lui_calendar_portlet_group .lui_group_date{
	/* font-family: "xihei","微软雅黑"; */
    position: absolute;
    right: 10px;
    bottom: 4px;
    font-size: 14px;
    font-weight: 100;
}
.lui_calendar_portlet_group tr th.today {
    color: #15a4fa;
    background-color: #4285F4;
}
.lui_calendar_portlet_group .today .lui_group_weekName,.lui_calendar_portlet_group .today .lui_group_date{
	color:#fff;
}
.lui_calendar_portlet_group table tr th.lui_group_weekNum {
    width: 109px;
}
.lui_calendar_portlet_group .week_num_content {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
}
.lui_calendar_portlet_group .lui_group_weekNum .prev,.lui_calendar_portlet_group .lui_group_weekNum .next {
    display: block;
    position: absolute;
    bottom: 5px;
    cursor: pointer;
    width: 0px;
    height: 0px;
    border: 6px solid transparent;
}
.lui_calendar_portlet_group .lui_group_weekNum .prev {
    border-right-color: #8b90a2;
    left: 0px;
}
.lui_calendar_portlet_group .week_num_content .week {
    position: absolute;
    width: 85px;
    text-align: center;
    left: 14px;
    bottom: 0px;
}
.lui_calendar_portlet_group .lui_group_weekNum .next {
    border-left-color: #8b90a2;
    right: -5px;
}
.lui_calendar_portlet_group .week_num_content .week em {
    font-family: "xihei","微软雅黑";
    font-size: 36px;
    position: relative;
    top: 6px;
    margin: 0px 3px;
    font-style: normal;
}
.lui_calendar_portlet_group table tr td {
    border: 0;
    border-left: 1px solid #d8d8d8;
    table-layout: fixed;
    width: 90px;
    height: 38px;
    position: relative;
    vertical-align: top;
}
.lui_calendar_portlet_group .lui_group_username {
    text-align: center;
}
.lui_calendar_portlet_group .lui_group_username span {
    font-size: 14px;
}
.lui_calendar_portlet_group table tr td:first-child {
    vertical-align: middle;
}
.lui_calendar_portlet_group > table > tbody > tr:nth-child(even){
    background-color: #FCFCFC;
}

.task_chartBar {
    display: block;
    width: 100%;
    height: 20px;
    line-height: 20px;
    background: #ffaa8a;
    cursor: pointer;
    position:absolute;
    z-index: 11;
    border: 1px solid #fff;
    left: 0px;
    top: 0px;
    color: #fff;
    font-size: 12px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-radius: 8px;
}
.lui_calendar_portlet_group .lui_group_personIcon{
	 background-color: #69A0FD;
}
.task_chartBar span {
    display: block;
    padding: 0px 8px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.lui_calendar_portlet_group .lui_pch_date_prevSelect {
    width: 8px;
    height: 8px;
    display: inline-block;
    background: url(../images/prev-arrow.png) no-repeat;
    background-size: contain;
    cursor: pointer;
    margin-right: 10px;
}
.lui_calendar_portlet_group .lui_pc_date_nextSelect {
    width: 8px;
    height: 8px;
    display: inline-block;
    background: url(../images/next-arrow.png) no-repeat;
    background-size: contain;
    cursor: pointer;
    margin-left: 10px;
}
.lui_calendar_portlet_group .lui_pch_date_switch em{
	color:#4285F4;
	font-size:14px;
	padding-right:5px;
	padding-left:5px;
}
.lui_calendar_portlet_group .lui_pch_button .lui_pch_currentDate {
    width: 20px;
    height: 12px;
    display: inline-block;
    border: 1px solid #aaa;
    border-radius: 4px;
    color: #C2C2C2;
    text-align: center;
    position: relative;
    bottom: 0px;
    vertical-align: text-bottom;
    line-height: 12px;
    font-size:12px;
}
.lui_calendar_portlet_group .lui_pch_renovate {
    width: 20px;
    height: 12px;
    display: inline-block;
    border: 1px solid #aaa;
    border-radius: 4px;
    background: url(../images/refresh.png) no-repeat center;
    background-size: contain;
    margin-left: 5px;
    vertical-align: text-bottom;
    font-size:12px;
}

.lui_calendar_portlet_group .lui_group_nodata{
	width: 100%;
    height: 120px;
    position: relative;
}
.lui_calendar_portlet_group .lui_group_nogroup{
	width: 100%;
    height: 120px;
    position: relative;
}
.lui_calendar_portlet_group .lui_pce_iconStyle{
	width: 100%;
    position: relative;
}
.lui_calendar_portlet_group .lui_pce_is_emptyImg{
	background: url(../images/empty.png) no-repeat center;
    background-size: contain;
    display: block;
    width: 64px;
    height: 64px;
    margin: 0 auto;
}
.lui_calendar_portlet_group .lui_pce_is_title{
	font-size: 12px;
    color: #333333;
    text-align: center;
}