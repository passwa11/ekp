@font-face {
  font-family: 'calendar_num';
  src: url('../font/font.eot?v=4.3.0');
  src: url('../font/font.eot?#iefix&v=4.3.0') format('embedded-opentype'), url('../font/font.woff?v=4.3.0') format('woff'), url('../font/font.ttf?v=4.3.0') format('truetype'), url('../font/font.svg?v=4.3.0#fontawesomeregular') format('svg');
  font-weight: normal;
  font-style: normal;
}
.lui_calendar_portlet_dirction {
  height: 100%;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,0.05);
  border-radius: 6px;
  color:#333;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}
.lui_calendar_portlet_dirction .lui_calendar_dirction_main thead td{
	font-size: 12px;
	color: #202124;
	line-height: 14px;
}

.lui_calendar_portlet_dirction_left {
  width: 40%;
  float: left;
  display: inline-block;
  vertical-align: top;
  height:373px;
}
.lui_calendar_portlet_dirction_right {
  width: 60%;
  float: right;
  /* min-width:325px; */
  display: inline-block;
}
.lui_calendar_portlet_direction {
    border-radius: 6px;
    color: #333;
    margin: 0 auto;
    overflow: hidden;
    position: relative;
}
.lui_calendar_portlet_dirction_right .lui_calendar_dirction_content {
	width:100%;
  	position: relative;
}
.lui_calendar_portlet_dirction_right .lui_calendar_direction_header {
	 height: 49px;
    position: relative;
}
.lui_calendar_portlet_dirction_right .lui_calendar_direction_header div {
    display: inline-block;
    color: #666;
    margin-right: 25px;
}
.lui_calendar_portlet_dirction_right .lui_tch_button {
    position: absolute;
    right: 0px;
    top: 50%;
    transform: translate(0,-50%);
}
.lui_calendar_portlet_dirction_right .lui_tch_button .lui_tch_currentDate {
    width: 27px;
    height: 17px;
    display: inline-block;
    border: 1px solid #aaa;
    border-radius: 4px;
    color: #C2C2C2;
    text-align: center;
    position: relative;
    bottom: 0px;
    vertical-align: text-bottom;
    line-height: 17px;
}
.lui_calendar_portlet_dirction_right .lui_tch_renovate {
    width: 27px;
    height: 17px;
    display: inline-block;
    border: 1px solid #aaa;
    border-radius: 4px;
    background: url(../images/refresh12.png) no-repeat center;
    background-size: contain;
    margin-left: 5px;
    vertical-align: text-bottom;
    background-size:14px;
}
.lui_calendar_dirction_main .lui_tcd_shadeBox {
  overflow: hidden;
  transition-duration: .5s;
  min-height:273px;
}
.lui_calendar_dirction_main table{
  width: 100%;
  text-align: center;
}
.lui_calendar_dirction_main table thead {
  opacity: 0.5;
  font-size: 12px;
  color: #999999;
}
.lui_calendar_dirction_main table td{
  padding: 4px 0px;
  position: relative;
  cursor: pointer;
}
.lui_calendar_dirction_main table td .lui_date_box{
  width: 42px;
  height: 42px;
  margin: 0 auto;
  position: relative;
  line-height: 22px;
  border: 1px solid transparent;
}
.lui_calendar_dirction_main .muiWeekRest .lui_number{
	color: #999999;
}
.lui_calendar_dirction_main .lui_tcd_table--tr .pre > div, .lui_calendar_dirction_main .lui_tcd_table--tr .next > div {
    color: #999;
}

.lui_calendar_dirction_main .lui_calendar_dot .point {
  width: 4px;
  height: 4px;
  background: rgb(66, 133, 244);
  display: inline-block;
  border-radius: 50%;
  position: absolute;
  bottom: 2px;
  left: 50%;
}
.lui_calendar_dirction_main .lui_calendar_dot.current .point {
    background: #fff;
}

.lui_calendar_dirction_main  .lui_calendar_work{
  position: absolute;
  top:7px;
  font-size: 8px;
  left: 34px;
  padding: 0 2px;
  border-radius: 4px;
  transform: translate(-50%,0%);
  text-align: center;
  color: #5795FD;
  line-height: 10px;
}
.lui_calendar_dirction_main .lui_calendar_holiday{
  color: #FF9680 !important;
}

.lui_calendar_dirction_main .today .lui_date_box {
  border-radius: 50%;
   border: 1px solid #5795FD;
}
.lui_calendar_dirction_main .current .lui_date_box {
  border: 1px solid rgba(66, 133, 244,.5);
  background: rgb(66, 133, 244);
  color: #fff;
  border-radius: 50%;
}

.lui_calendar_portlet_dirction_left .lui_tcp_dateShow {
  padding: 13px 19px;
}
.lui_calendar_portlet_dirction_left .lui_tcp_dateNumber {
  color: #fff;
  text-align: center;
}
.lui_calendar_portlet_dirction_left .lui_tcp_dateNumber_currentDay {
  font-size: 62px;
}
.lui_calendar_portlet_dirction_left .lui_tcp_dateNumber_box {
  margin-top: -4px;
} 
.lui_calendar_portlet_dirction_left .lui_calendar_portlet_dirction_data {
  padding: 10px 0px 0px 35px;
  position: relative;
  overflow: auto;
  height: 167px;
  margin-right: 2px;
}
.lui_calendar_portlet_dirction_data::-webkit-scrollbar {
  width: 6px !important;
}
.lui_calendar_portlet_dirction_data::-webkit-scrollbar-thumb {
  background: rgba(255,255,255,0.70);
  border-radius: 3px; 
}
.lui_calendar_portlet_dirction_data .lui_tct_ul{
	list-style: none;
}
.lui_calendar_portlet_dirction_data .lui_tct_ul_li {
  position: relative;
  margin-bottom: 8px;
}
.lui_calendar_portlet_dirction_data .lui_tct_ui_div {
  font-size: 14px;
  color: #fff;
}
.lui_calendar_portlet_dirction_data .lui_tct_ui_item a {
  border: 1px solid rgba(255,255,255,.6);
  border-radius: 2px;
  display: inline-block;
  font-size: 12px;
  color: rgba(255,255,255,.6);
  padding: 0px 7px;
  margin-left: 11px;
  height: 15px;
  line-height: 15px;
}

.lui_calendar_portlet_dirction_data .lui_tct_ul_li .lui_tct_ui_item{
  border-radius: 4px;
  width: 100%;
  /*height: 32px;*/
  line-height: 32px;
  box-sizing: border-box;
  color:rgba(255,255,255,.6);
  font-size:12px;
}
.lui_calendar_portlet_dirction_data .lui_tct_ul_li::before{
  content: ' ';
  height: 55px;
  width: 1px;
  display: inline-block;
  background: rgba(204, 204, 204,.2);
  position: absolute;
  left: -17px;
  top: 10px;
}
.lui_calendar_portlet_dirction_data .lui_tct_ui__i {
  position: absolute;
  left: -19px;
  background: #fff;
  width: 5px;
  height: 5px;
  border-radius: 50%;
  top: 6.5px;
}


.lui_tct_ul_li:last-child::before{
  content: normal;
}
.lui_pcd_dropdowm--button {
  text-align: center;
  margin-top: 4px;
}
.lui_pcd_dropdowm--button i{
  width: 17px;
  height: 15px;
  display: inline-block;
  background: url('../images/arrow_down.png') no-repeat;
  background-size: cover;
  cursor: pointer;
  transition-duration: .5s
}
.lui_pcd_dropdowm--button::after {
  content: ' ';
  width: 100%;
  height: 1px;
  background: #eee;
  display: block;
  position: absolute;
  left: 0;
}


.lui_transverse_calendar--footer {
  position: absolute;
  bottom:0px;
  background: #fff;
  width: 100%;
  /* background: linear-gradient(to bottom, transparent , #fff); */
}
.lui_tcf_button {
  width: 100%;
  text-align: right;
}
.lui_tcf_newCalendar,.lui_tcf_more {
  width: 30px;
  height: 22px;
  display: inline-block;
  border: 1px solid #DDDDDD;
  border-radius: 4px;
}
.lui_tcf_newCalendar {
  background:url('../images/add.png') no-repeat center;
  background-size: contain;
}
.lui_tcf_more {
  margin-left: 10px;
  margin-right: 17px;
  background:url('../images/more.png') no-repeat center;
  background-size: contain;
}
.lui_tce_iconStyle i{
  background:url('../images/empty.png') no-repeat center;
  background-size: contain;
  display: block;
 width: 64px;
 height: 64px;
 margin: 0 auto;
}
.lui_tce_iconStyle {
  width: 100%;
  top: 50%;
 left:50%;
 transform: translate(-50%,-50%);
 position: absolute;
}
.lui_calendar_portlet_dirction_nodata {
  width: 100%;
  position: relative;
}
.lui_calendar_portlet_dirction_nodata .lui_pce_is_title {
    font-size: 12px;
    color: #fff;
    text-align: center;
}
.lui_calendar_portlet_dirction_nodata .lui_pce_is_text {
    font-size: 12px;
    text-align: center;
    color: #fff;
    margin-top: 6px;
}
.lui_calendar_portlet_dirction_nodata .lui_pce_is_emptyImg{
	background: url(../images/empty.png) no-repeat center;
    background-size: contain;
    display: block;
    width: 64px;
    height: 64px;
    margin: 0 auto;
}
.lui_calendar_portlet_dirction_nodata .lui_tce_is_title {
  font-size: 12px;
color: #333333;
text-align: center;
}
.lui_calendar_portlet_dirction_nodata .lui_tce_is_text {
  font-size: 12px;
text-align: center;
color: #999999;
margin-top: 6px;
}


.lui_tcp_add--icon {
  display: inline-block;
  width: 17px;
  height: 17px;
  background: url('../images/newAdd.png') no-repeat center;
  background-size: contain;
  vertical-align: sub;
}
.lui_tcp_more--icon {
  display: inline-block;
  width: 17px;
  height: 17px;
  margin-left: 11px;
  background: url('../images/newMore.png') no-repeat center;
  background-size: contain;
  vertical-align: sub;
}


.lui_tcp_ds--prevButton,.lui_tcp_ds--nextButton {
    width: 10px;
    height: 10px;
  display: inline-block;
      vertical-align: inherit;
      cursor: pointer;
}
.lui_tcp_ds--prevButton {
  background: url('../images/prevDate.png') no-repeat center;
  background-size: contain;
}
.lui_tcp_ds--nextButton {
  background: url('../images/nextDate.png') no-repeat center;
  background-size: contain;
}



.lui_transverse_calendar--dateMain .lui_tcd_table--tr .pre > div, .lui_transverse_calendar--dateMain .lui_tcd_table--tr .next > div {
    color: #999;
}


.lui_tct_ui--div .label{
	color: rgb(193, 156, 83);
	border-color: rgb(193, 156, 83);
}
.lui_timeAixs_after {
}
.lui_timeAixs_after_i{
	background: #4285F4;
}
.lui_calendar_portlet_dirction .lui_solar{
	font-size: 10px;
	color: #CCCCCC;
	text-align: center;
	line-height: 10px;
	white-space: nowrap;
	transform: scale(.85);
	margin-top: 2px;
}
/* ===== IE8兼容 ===== */
@media \0screen {
	.lui_calendar_portlet_dirction .lui_solar{
		font-size:10px;
	}
}
.lui_calendar_portlet_dirction .lui_solar.lui_solar_term {
    color: #5795FD;
}
.lui_calendar_portlet_dirction .lui_solar.lui_solar_holiday{
	color: #FF9680;
}
.lui_calendar_portlet_dirction .current .lui_solar {
    color: #fff;
}
.lui_calendar_portlet_dirction .today .lui_solar {
    color: #5795FD;
}
.lui_calendar_portlet_dirction .lui_number{
	font-size: 16px;
    color: #333333;
    text-align: right;
}
.lui_calendar_portlet_dirction .current .lui_number{
    color: #fff;
}
.lui_calendar_portlet_dirction .today .lui_number{
    color: #5795FD;
}
.lui_calendar_portlet_dirction .pre .lui_number,.lui_calendar_portlet_dirction .next .lui_number{
    color: #C2C2C2;
}
