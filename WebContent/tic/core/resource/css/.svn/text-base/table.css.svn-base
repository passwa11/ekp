body {
  background: #fff;
  color: #000;
  font-family: Helvetica, Arial, sans-serif;
  font-size: .8em;
  line-height: 1.5;
}

pre.listing {
  background: #eee;
  border: 1px solid #ccc;
  margin: .3em auto;
  padding: .1em .3em;
  width: 90%;
}

pre.listing b {
  color: #f00;
}

/* TABLE
 * ========================================================================= */
table {
  border: 0px solid #888;
  border-collapse: collapse;
  line-height: 1;
}

/**
 * �Ժ���ģ����Ŀ�ȿ���
 */
#wsMapperTemplateTdId {
	width:90%;
}
#wsMapperTemplateTdIdOut {
	width:90%;
}

/* Caption
 * ------------------------------------------------------------------------- */
table caption {
  font-size: .9em;
  font-weight: bold;
}

/* Header
 * ------------------------------------------------------------------------- */
table thead {
  background: #aaa url(../images/bg-table-thead.png) repeat-x top left;
  font-size: .9em;
}

table thead tr th {
  border: 1px solid #888;
  font-weight: normal;
  padding: .3em 1.67em .1em 1.67em;
  text-align: left;
}

/**
 * �ı�Input��ʽ
 */
#docInfo {
	color: #0066FF;
	border-color: #999999;
	border-style: solid;
	width: 160px;
	border-width: 0px 0px 1px 0px;
}

#docOut {
	color: #0066FF;
	border-color: #999999;
	border-style: solid;
	width: 160px;
	border-width: 0px 0px 1px 0px;
}

#isBackSuccess {
	color: #0066FF;
	border-color: #999999;
	border-style: solid;
	width: 130px;
	border-width: 0px 0px 1px 0px;
}

#isBackFail {
	color: #0066FF;
	border-color: #999999;
	border-style: solid;
	width: 130px;
	border-width: 0px 0px 1px 0px;
}

#viewDocInfo {
	border-width: 0px 0px 0px 0px;
	width: 160px;
	oreadonly:expression(this.readOnly=true);
}

#viewIsBack {
	border-width: 0px 0px 0px 0px;
	oreadonly:expression(this.readOnly=true);
}

.treeTable tr td a.expander {
  background-position: left center;
  background-repeat: no-repeat;
  color: #000;
  text-decoration: none;
}

.treeTable tr.selected, .treeTable tr.accept {
  background-color: #3875d7;
}

.treeTable tr.selected a.expander, .treeTable tr.accept a.expander {
  color: #fff;
}

.treeTable tr.collapsed.selected td a.expander, .treeTable tr.collapsed.accept td a.expander {
  background-image: url(../images/toggle-expand-light.png);
}

.treeTable tr.expanded.selected td a.expander, .treeTable tr.expanded.accept td a.expander {
  background-image: url(../images/toggle-collapse-light.png);
}



table span {
  background-position: center left;
  background-repeat: no-repeat;
  padding: .2em 0 .2em 1.5em;
}

table span.file {
  background-image: url(../images/page_white_text.png);
}

table span.folder {
  background-image: url(../images/folder.png);
}
