<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.payment.model.ThirdPaymentOrder" 
        table="third_payment_order">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="500" 
            index="idx_thirdPaymentOrder_moName"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdPaymentOrder_moId"/>
        <property 
            name="fdKey" 
            column="fd_key" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdPaymentOrder_fdKey"/>
        <property 
            name="fdOrderDesc" 
            column="fd_order_desc" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdTotalMoney" 
            column="fd_total_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdOrderNo" 
            column="fd_order_no" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdPayType" 
            column="fd_pay_type" 
            update="true" 
            insert="true"
            length="100"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentOrder_CTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentOrder_ATime"/>
        <property 
            name="fdPaymentService" 
            column="fd_payment_service" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPaymentStatus" 
            column="fd_payment_status" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdCodeUrl" 
            column="fd_code_url" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdPayTime" 
            column="fd_pay_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentOrder_PTime"/>
        <property
                name="fdRelateOrderId"
                column="fd_relate_order_id"
                update="true"
                insert="true"
                length="36"
                index="idx_thirdPaymentOrder_RoId"/>
        <property
                name="fdPaymentStatusDesc"
                column="fd_payment_status_desc"
                update="true"
                insert="true"
                length="4000"/>
        <property
                name="fdTradeType"
                column="fd_trade_type"
                update="true"
                insert="true"
                length="200"/>
        <many-to-one 
            name="fdPayer" 
            column="fd_payer_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
