<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.landray.kmss.third.pda.model.PdaHomePagePortlet"
		table="pda_home_page_portlet">
		<id name="fdId" column="fd_id" length="36">
			<generator class="assigned" />
		</id>

		<property name="fdName" column="fd_name" update="true"
			insert="true" length="100" not-null="false" />

		<property name="fdOrder" column="fd_order" update="true"
			insert="true" length="10" not-null="false" />

		<property name="fdType" column="fd_type" update="true"
			insert="true" length="10" not-null="false" />

		<property name="fdIconUrl" column="fd_icon_url" update="true"
			insert="true" length="200" not-null="false" />

		<property name="fdFullRowFlag" column="fd_fullrow_flag"
			update="true" insert="true" length="2" not-null="false" />

		<property name="fdLabelId" column="fd_label_id" update="true"
			insert="true" length="36" not-null="false" />

		<property name="fdModuleId" column="fd_module_id" update="true"
			insert="true" length="36" not-null="false" />

		<property name="fdModuleName" column="fd_module_name" update="true"
			insert="true" length="200" not-null="false" />
			
		<property name="fdContent" column="fd_content" update="true"
			insert="true" length="300" not-null="false" />
			
		<property name="fdDataUrl" column="fd_data_url" update="true"
			insert="true" length="1500" not-null="false" />
	</class>
</hibernate-mapping>
