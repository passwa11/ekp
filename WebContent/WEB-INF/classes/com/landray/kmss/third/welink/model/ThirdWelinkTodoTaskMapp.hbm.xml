<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.welink.model.ThirdWelinkTodoTaskMapp" 
        table="third_welink_todo_task_mapp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWLTodoTaskMap_cTime"/>
        <property 
            name="fdTodoId" 
            column="fd_todo_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWLTodoTaskMap_todoId"/>
        <property 
            name="fdPersonId" 
            column="fd_person_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWLTodoTaskMap_perId"/>
        <property 
            name="fdTaskId" 
            column="fd_task_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWLTodoTaskMap_taskId"/>
        <property 
            name="fdWelinkUserId" 
            column="fd_welink_user_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWLTodoTaskMap_WLId"/>
    </class>
</hibernate-mapping>
