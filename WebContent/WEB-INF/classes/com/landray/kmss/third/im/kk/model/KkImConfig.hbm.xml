<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.im.kk.model.KkImConfig"
		table="kk_im_config">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdValue"
			column="fd_value"
			update="true"
			insert="true"
			length="500"
			not-null="false" />


	</class>
</hibernate-mapping>
