<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinChatDataTemp" 
        table="third_weixin_chat_data_temp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdSeq" 
            column="fd_seq" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMsgId" 
            column="fd_msg_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinChatDataTemp_fdMsgId"/>
        <property 
            name="fdPublickeyVer" 
            column="fd_publickey_ver" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEncryptRandomKey" 
            column="fd_encrypt_random_key" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdEncryptChatMsg" 
            column="fd_encrypt_chat_msg" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdHandleStatus" 
            column="fd_handle_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdErrTimes" 
            column="fd_err_times" 
            update="true" 
            insert="true"/>
        <property 
            name="fdErrMsg" 
            column="fd_err_msg" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinChatDataTemp_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinChatDataTemp_docAlterTime"/>
        <property 
            name="fdCorpId" 
            column="fd_corp_id" 
            update="true" 
            insert="true" 
            length="100"/>
    </class>
</hibernate-mapping>
