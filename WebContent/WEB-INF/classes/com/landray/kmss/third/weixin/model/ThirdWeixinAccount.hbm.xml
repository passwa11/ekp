<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinAccount" 
        table="third_weixin_account">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdAccountId" 
            column="fd_account_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinAcc_accId"/>
        <property 
            name="fdAccountType" 
            column="fd_account_type" 
            update="true" 
            insert="true"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="400"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true"
            lazy="true"
            index="idx_thirdWeixinAcc_cTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true"
            lazy="true"
            index="idx_thirdWeixinAcc_aTime"/>
        <property 
            name="fdEkpId" 
            column="fd_ekp_id" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdWeixinAcc_ekpId"/>
    </class>
</hibernate-mapping>
