<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.work.model.ThirdWeixinWorkLiving"
        table="third_weixin_work_living">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_wxLiving_docCreateTime"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdLivingId" 
            column="fd_living_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
