<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.weixin.mutil.model.ThirdWxworkMutilDynamicinfo"
		table="wxwork_mutil_dynamicinfo">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docTitle"
			column="doc_title"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="docDescription"
			column="doc_description"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />

		<property
			name="docUrl"
			column="doc_url"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />
		
		<property
			name="picUrl"
			column="pic_url"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />

		<property
			name="fdProcessId"
			column="fd_process_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
	</class>
</hibernate-mapping>
