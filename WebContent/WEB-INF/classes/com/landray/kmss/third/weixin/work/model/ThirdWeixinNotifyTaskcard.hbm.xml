<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.work.model.ThirdWeixinNotifyTaskcard" 
        table="third_weixin_notify_taskcard">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdNotifyId" 
            column="fd_notify_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_WXNotifyTaskcard_notifyId"/>
        <property 
            name="fdSubject" 
            column="fd_subject" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdTaskcardId" 
            column="fd_taskcard_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_WXNotifyTaskcard_taskcId"/>
        <property 
            name="fdTouser" 
            column="fd_touser" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_WXNotifyTaskcard_creTime"/>
        <property
                name="fdCorpId"
                column="fd_corp_id"
                update="true"
                insert="true"
                length="100"
                index="idx_WXNotifyTaskcard_CorpId"/>
    </class>
</hibernate-mapping>
