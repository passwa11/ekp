<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingDinstance" 
        table="third_ding_dinstance">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdInstanceId" 
            column="fd_instance_id" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdDingUserId" 
            column="fd_ding_user_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdEkpInstanceId" 
            column="fd_ekp_instance_id" 
            update="true" 
            insert="true" 
            length="50" 
            index="idx_thirdDingDins_ekpInsId"/>
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingDins_creTime"/>
        <many-to-one 
            name="fdTemplate" 
            column="fd_template_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCreator" 
            column="fd_ekp_user_id" 
            update="true" 
            insert="true"/>
        <list 
            name="fdDetail" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.third.ding.model.ThirdDingInstanceDetail"/>
        </list>
    </class>
</hibernate-mapping>
