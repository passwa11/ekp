<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingTodoTemplate" 
        table="third_ding_todo_template">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdIscustom" 
            column="fd_iscustom" 
            update="true" 
            insert="true" 
            length="4"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdDetail" 
            column="fd_detail" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdIsdefault" 
            column="fd_isdefault" 
            update="true" 
            insert="true" 
            length="4"/>
        <property 
            name="fdTemplateId" 
            column="fd_template_id" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingTodoTem_creTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingTodoTem_altTime"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdTemplateName" 
            column="fd_template_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdTemplateClass" 
            column="fd_template_class" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelNameText" 
            column="fd_model_name_text" 
            update="true" 
            insert="true" 
            length="100"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
