<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingCardLog" 
        table="third_ding_card_log">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_tdCardLog_createTime"/>
        <property 
            name="fdCardId" 
            column="fd_card_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdModelName"
            column="fd_model_name"
            update="true"
            insert="true"
            length="200"/>
        <property
            name="fdModelId"
            column="fd_model_id"
            update="true"
            insert="true"
            length="50"/>
        <property 
            name="fdRequest" 
            column="fd_request" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdResponse" 
            column="fd_response" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdRequestUrl" 
            column="fd_request_url" 
            update="true" 
            insert="true" 
            length="10000"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRequestTime" 
            column="fd_request_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdResponseTime" 
            column="fd_response_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdConsumingTime" 
            column="fd_consuming_time" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
