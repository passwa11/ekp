<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingCspace" 
        table="third_ding_cspace">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdMediaId" 
            column="fd_media_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAttId" 
            column="fd_att_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdFileId" 
            column="fd_file_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property
            name="fdDingFileId"
            column="fd_ding_file_id"
            update="true"
            insert="true"
            length="100"/>
        <property
            name="fdErrorMsg"
            column="fd_error_msg"
            update="true"
            insert="true"
            length="1000"/>
        <property
            name="fdType"
            column="fd_type"
            update="true"
            insert="true"
            length="10"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingCspa_creTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingCspa_altTime"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true" 
            length="10"/>
        <many-to-one 
            name="fdCreater" 
            column="fd_creater_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
