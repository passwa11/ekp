<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.ding.model.OmsRelationModel"
		table="oms_relation_model">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdEkpId"
			column="fd_ekp_id"
			update="true"
			insert="true"
			not-null="false"
			unique="true"
			length="50" />
		<property
			name="fdAppPkId"
			column="fd_app_pk_id"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdAppKey"
			column="fd_app_key"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			not-null="false"
			length="10" />
		<property
			name="fdAvatar"
			column="fd_avatar"
			update="true"
			insert="true"
			not-null="false"
			length="500" />
		<property
			name="fdUnionId"
			column="fd_union_id"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdAccountType"
			column="fd_account_type"
			update="true"
			insert="true"
			not-null="false"
			length="20" />
	</class>
</hibernate-mapping>
