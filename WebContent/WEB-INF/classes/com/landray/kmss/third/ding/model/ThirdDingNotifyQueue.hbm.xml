<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingNotifyQueue" 
        table="third_ding_notify_queue">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAppType" 
            column="fd_app_type" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdTodoId" 
            column="fd_todo_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdUserids" 
            column="fd_userids" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdEndTime" 
            column="fd_end_time" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
