<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysAttTmp"
		table="sys_att_tmp">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdFileId"
			column="fd_file_id"
			update="true"
			insert="true"
			length="36"
			not-null="false"
			index="idx_sysAttTmp_fdFileId"/>

		<property
			name="fdDeleteTime"
			column="fd_delete_time"
			update="true"
			insert="true"
			not-null="false" />

	</class>
</hibernate-mapping>
