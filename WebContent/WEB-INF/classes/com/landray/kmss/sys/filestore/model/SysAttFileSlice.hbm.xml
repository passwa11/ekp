<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysAttFileSlice"
		table="sys_att_file_slice">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdStartPoint"
			column="fd_start_point"
			update="true"
			insert="true"
			length="8"
			not-null="false" />

		<property
			name="fdEndPoint"
			column="fd_end_point"
			update="true"
			insert="true"
			length="8"
			not-null="false" />

		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdModifyTime"
			column="fd_modify_time"
			update="true"
			insert="true"
			length="8"
			not-null="false" />

		<many-to-one
			name="fdFile"
			column="fd_file_id"
			insert="true"
			update="true"
			not-null="false"/>

	</class>
</hibernate-mapping>
