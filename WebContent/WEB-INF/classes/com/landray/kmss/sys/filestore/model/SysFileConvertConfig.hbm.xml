<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.landray.kmss.sys.filestore.model.SysFileConvertConfig"
	table="sys_file_convert_config">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdFileExtName"
			column="fd_file_extname"
			update="true"
			insert="true"
			length="500"
			not-null="false" />
			
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />
			
		<property
			name="fdConverterKey"
			column="fd_converter_key"
			update="true"
			insert="true"
			length="100"
			not-null="false" />
		
		<property
			name="fdDispenser"
			column="fd_dispenser"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="2"
			not-null="false"/>
		
		<property
			name="fdConverterType"
			column="fd_converter_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdHighFidelity"
			column="fd_high_fidelity"
			update="true"
			insert="true"
			length="2"
			not-null="false"/>
		
		<property
			name="fdPicResolution"
			column="fd_pic_resolution"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdPicRectangle"
			column="fd_pic_rectangle"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

	</class>
</hibernate-mapping>
