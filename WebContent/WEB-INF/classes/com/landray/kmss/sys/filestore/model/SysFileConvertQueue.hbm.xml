<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysFileConvertQueue"
		table="sys_file_convert_queue">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdFileId"
			column="fd_file_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" 
			index="idx_sysFileCvtQ_fdFileId"/>
			
		<property
			name="fdConvertStatus"
			column="fd_convert_status"
			update="true"
			insert="true"
			not-null="false"
			index="idx_sysFileCvtQ_fdCvtJoin"/>
			
		<property
			name="fdConverterKey"
			column="fd_converter_key"
			update="true"
			insert="true"
			length="36"
			not-null="false"
			index="idx_sysFileCvtQ_fdCvtJoin"/>
			
		<property
			name="fdConverterType"
			column="fd_converter_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" 
			index="idx_sysFileCvtQ_fdCvtJoin"/>
			
		<property
			name="fdAttMainId"
			column="fd_attmain_id"
			update="true"
			insert="true"
			length="36"
			not-null="false"
			index="idx_sysFileCvtQ_fdAttMainId"/>
		
		<property
			name="fdAttMainModelName"
			column="fd_attmain_model_name"
			update="true"
			insert="true"
			not-null="false"
			length="450"
			index="idx_sysFileCvtQ_fdAttModNm"/>
			
		<property
			name="fdAttModelId"
			column="fd_attmodel_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" 
			index="idx_sysFileCvtQ_fdAttModelId"/>
			
		<property
			name="fdConverterParam"
			column="fd_converter_param"
			update="true"
			insert="true"
			length="255"
			not-null="false"/>
			
		<property
			name="fdDispenser"
			column="fd_dispenser"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdFileExtName"
			column="fd_file_extname"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdModelUrl"
			column="fd_model_url"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />
		<property
			name="fdFileDownUrl"
			column="fd_file_down_url"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />
			
		<property
			name="fdFileName"
			column="fd_file_name"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />
			
		<property
			name="fdConvertNumber"
			column="fd_convert_number"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdIsFinish"
			column="fd_is_finish"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdStatusTime"
			column="fd_status_time"
			update="true"
			insert="true"
			not-null="false"
			index="idx_sysFileCvtQ_fdStatusTime" />
			
		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdClientId"
			column="fd_client_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" 
			index="idx_sysFileCvtQ_fdClientId"/>
		<property
			name="fdIsLongQueue"
			column="fd_is_long_queue"
			update="true"
			insert="true"
			not-null="false" />
		<property
				name="fdIsPdfGenerated"
				column="fd_is_pdf_generated"
				update="true"
				insert="true"
				not-null="false" />
	</class>
</hibernate-mapping>
