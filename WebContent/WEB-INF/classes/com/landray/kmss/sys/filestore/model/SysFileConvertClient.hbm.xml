<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysFileConvertClient"
		table="sys_file_convert_client">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="version"
			column="fd_version"
			update="true"
			insert="true"
			length="16"
			not-null="false"/>
			
		<property
			name="processID"
			column="fd_process_id"
			update="true"
			insert="true"
			length="16"
			not-null="false" />
			
		<property
			name="clientIP"
			column="fd_client_ip"
			update="true"
			insert="true"
			length="32"
			not-null="false" />
			
		<property
			name="clientPort"
			column="fd_client_port"
			update="true"
			insert="true"
			length="8"
			not-null="false"/>
			
		<property
			name="clientMessageHandlerCode"
			column="fd_client_handler_code"
			update="true"
			insert="true"
			length="8"
			not-null="false"/>
		
		<property
			name="converterFullKey"
			column="fd_converter_fullkey"
			update="true"
			insert="true"
			length="36"
			not-null="false"/>
		
		<property
			name="converterVersion"
			column="fd_converter_version"
			update="true"
			insert="true"
			length="300"
			not-null="false"/>
			
		<property
			name="taskCapacity"
			column="fd_task_capacity"
			update="true"
			insert="true"
			length="3"
			not-null="false" />
			
		<property
			name="taskConvertingNum"
			column="fd_task_converting"
			update="true"
			insert="true"
			length="3"
			not-null="false" />
			
		<property
			name="avail"
			column="fd_is_avail"
			update="true"
			insert="true"
			length="2"
			not-null="false" />
		
		<property
			name="converterConfig"
			column="fd_converter_config"
			update="true"
			insert="true"
			length="2000"
			not-null="false"/>
		<property
			name="isLongTask"
			column="fd_is_long_task"
			update="true"
			insert="true"
			length="2"
			not-null="false" />		
		<property
			name="isUpgrade"
			column="fd_is_upgrade"
			update="true"
			insert="true"
			length="2"
			not-null="false" />		
	</class>
</hibernate-mapping>
