<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.portal.model.SysPortalMaterialMain" 
        table="sys_portal_material_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_portalMatMain_docCreaTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_portalMatMain_docAltTime"/>
        <property 
            name="fdSize" 
            column="fd_size" 
            update="true" 
            insert="true"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdWidth" 
            column="fd_width" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLength" 
            column="fd_length" 
            update="true" 
            insert="true"/>
        <property 
            name="fdAttId" 
            column="fd_att_id" 
            update="true" 
            insert="true"/>
        <property
            name="fdImgUrl"
            column="fd_img_url"
            update="true"
            insert="true"
            length="500"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
       <bag 
            name="fdTags" 
            table="sys_portal_material_main_tags" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.portal.model.SysPortalMaterialTag" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
