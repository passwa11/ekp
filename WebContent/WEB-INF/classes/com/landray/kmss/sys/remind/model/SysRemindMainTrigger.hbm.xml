<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.remind.model.SysRemindMainTrigger" 
        table="sys_remind_main_trigger">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36">
            <generator class="assigned" />
        </id>
		<property
			name="fdFieldId"
			column="fd_field_id"
			update="true"
			insert="true"
			not-null="false"
			length="100"/>
		<property
			name="fdFieldName"
			column="fd_field_name"
			update="true"
			insert="true"
			not-null="false"
			length="100"/>
		<property
			name="fdMode"
			column="fd_mode"
			update="true"
			insert="true"
			not-null="false"
			length="50"/>
		<property
			name="fdDay"
			column="fd_day"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdHour"
			column="fd_hour"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdMinute"
			column="fd_minute"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdTime"
			column="fd_time"
			update="true"
			insert="true"
			not-null="false"
			length="50"/>
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			not-null="false"/>
		
		<many-to-one
			name="fdRemind"
			column="fd_remind_id"
			insert="true"
			update="true"
			not-null="false" />
	</class>
</hibernate-mapping>
