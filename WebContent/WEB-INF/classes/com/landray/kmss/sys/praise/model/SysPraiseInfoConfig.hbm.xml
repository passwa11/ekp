<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.praise.model.SysPraiseInfoConfig" 
        table="sys_praise_config">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
            
        <property
			name="fdModuleUrlPrefix"
			column="fd_url_prefix"
			update="true"
			insert="true"
			not-null="false"
			length="50" />    
			
		<property
			name="fdModuleKey"
			column="fd_module_key"
			update="true"
			insert="true"
			not-null="false"
			length="100" />   
		
		<property
			name="fdIsUsed"
			column="fd_is_used"
			update="true"
			insert="true"
			not-null="false"
			length="10" /> 
            
    </class>
</hibernate-mapping>
