<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.praise.model.SysPraiseInfoPersonal" 
        table="sys_praise_i_personal">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        
        <many-to-one
			name="fdPerson"
			column="fd_person"
			insert="true"
			update="true"
			not-null="false" />
            
        <property
			name="fdUpdateTime"
			column="fd_update_time"
			update="true"
			insert="true"
			not-null="false" />
            
         <many-to-one
			name="fdWeek"
			column="fd_week"
			insert="true"
			update="true"
			not-null="false" />   
		
		 <many-to-one
			name="fdMonth"
			column="fd_month"
			insert="true"
			update="true"
			not-null="false" />   
		
		 <many-to-one
			name="fdYear"
			column="fd_year"
			insert="true"
			update="true"
			not-null="false" />   
		
		 <many-to-one
			name="fdTotal"
			column="fd_total"
			insert="true"
			update="true"
			not-null="false" />   
    </class>
</hibernate-mapping>
