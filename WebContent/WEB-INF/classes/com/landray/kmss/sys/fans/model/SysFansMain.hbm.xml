<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.fans.model.SysFansMain"
		table="sys_fans_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdFollowTime"
			column="fd_follow_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRelationType"
			column="fd_relation_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdUserType"
			column="fd_user_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdCanUnfollow"
			column="fd_can_unfollow"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="100"
			not-null="false" />
			
		<property
			name="fdUserId"
			column="fd_user_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdFansId"
			column="fd_fans_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
