<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.mportal.model.SysMportalCard"
		table="sys_mportal_card">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" 
			access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdEnabled"
			column="fd_enabled"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
		
		<property
			name="fdIsPushed"
			column="fd_is_pushed"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
		
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdPortletConfig"
			column="fd_portlet_config"
			update="true"
			insert="true"
			length="4000"
			not-null="false" />
			
		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
		
		<many-to-one
			name="fdModuleCate"
			column="fd_module_cate"
			not-null="false" 
			update="true"
			insert="true"/>
			
		<!-- 可编辑者 -->
		<bag
			name="authEditors"
			table="sys_mportal_card_editors">
			<key
				column="fd_card_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag> 
		
		<bag
			name="cards" 
			cascade="delete-orphan" 
			lazy="true" 
			inverse="true" >
			<key column="fd_card_id" not-null="true" />
			<one-to-many class="com.landray.kmss.sys.mportal.model.SysMportalPageCard" />
		</bag>
		
		<bag
			name="cpageCards" 
			cascade="delete-orphan" 
			lazy="true" 
			inverse="true" >
			<key column="fd_card_id" not-null="true" />
			<one-to-many class="com.landray.kmss.sys.mportal.model.SysMportalCpageCard" />
		</bag>
			
	</class>
</hibernate-mapping>
