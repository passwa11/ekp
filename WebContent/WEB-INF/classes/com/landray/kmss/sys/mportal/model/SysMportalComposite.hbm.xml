<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.mportal.model.SysMportalComposite" 
        table="sys_mportal_composite">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"
            access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />   
        <property 
            name="fdTitle" 
            column="fd_title" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdLogo" 
            column="fd_logo" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2" />
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_sysMpCom_fdOrder"/>
        <property 
            name="fdLang" 
            column="fd_lang" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdHeadType" 
            column="fd_head_type" 
            update="true" 
            insert="true"/>
        <property 
            name="fdNavLayout" 
            column="fd_nav_layout" 
            update="true" 
            insert="true"/>
        <property 
            name="fdHeadChangeEnabled" 
            column="fd_head_change_enabled" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMd5" 
            column="fd_md5" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_sysMpCom_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_sysMpCom_docAlterTime"/>
        <property 
            name="fdEnabled" 
            column="fd_enabled" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="sys_mportal_composite_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="sys_mportal_composite_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <!-- <bag 
            name="pages"
            table="sys_mportal_cpage_relation" 
            cascade="all-delete-orphan" 
            order-by="fd_order asc"
            inverse="true"
            lazy="true">
            <key 
                column="fd_composite_id"/>
            <one-to-many 
                class="com.landray.kmss.sys.mportal.model.SysMportalCpageRelation" />
        </bag> -->
        <bag 
            name="authOtherReaders" 
            table="sys_mportal_composite_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="sys_mportal_composite_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="sys_mportal_composite_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="sys_mportal_composite_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
