<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.zone.model.SysZoneAddressCateVo"
		table="sys_zone_address_cate_vo">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdCategoryId"
			length="36"
			column="fd_cate_id"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdItemType"
			column="fd_item_type"
			update="true"
			insert="true"
			not-null="false"
			length="36" />
	</class>
</hibernate-mapping>
