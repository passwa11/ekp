<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendDevice"
		table="sys_attend_device">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
	    <property
			name="fdDeviceIds"
			column="fd_device_ids"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />
		<property
			name="fdClientType"
			column="fd_client_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"/>
	</class>
</hibernate-mapping>
