<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendCategoryLocation"
		table="sys_attend_category_location">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<property
			name="fdDataType"
			column="fd_data_type"
			update="true"
			insert="true"
			length="10"
			not-null="false" />
		<property
			name="fdRow"
			column="fd_rowIndex"
			update="true"
			insert="true"
			length="4"
			not-null="false" />
		<property
			name="fdLimitIndex"
			column="fd_limitIndex"
			update="true"
			insert="true"
			length="4"
			not-null="false" />
			
		<property
			name="fdLimit"
			column="fd_limit"
			update="true"
			insert="true"
			length="4"
			not-null="false" />
		<property
			name="fdLocation"
			column="fd_location"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdLng"
			column="fd_lng"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdLat"
			column="fd_lat"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		<property
			name="fdLatLng"
			column="fd_lat_lng"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

	</class>
</hibernate-mapping>
