<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.attend.model.SysAttendSignBak" 
        table="sys_attend_sign_bak">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property
                name="fdBaseDate"
                column="fd_base_date"
                update="true"
                insert="true"
                index="idx_signLog_fdBaseDate"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_signBak_docCreateTime"/>
        <property 
            name="fdAddress" 
            column="fd_address" 
            update="true" 
            insert="true" 
            length="300"/>
        <property 
            name="fdWifiName" 
            column="fd_wifi_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property
                name="fdGroupId"
                column="fd_group_id"
                update="true"
                insert="true"
                length="36"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
