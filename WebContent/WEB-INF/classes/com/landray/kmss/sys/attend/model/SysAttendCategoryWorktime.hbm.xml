<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendCategoryWorktime"
		table="sys_attend_category_worktime">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdStartTime"
			column="fd_start_time"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdEndTime"
			column="fd_end_time"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdIsAvailable"
			column="fd_is_available"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="fdOverTimeType"
			column="fd_over_time_type"
			update="true"
			insert="true"
			not-null="false" />
		
		<many-to-one
			name="fdCategory"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
