<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendReport"
		table="sys_attend_report">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdMonth"
			column="fd_month"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdIsQuit"
			column="fd_is_quit"
			update="true"
			insert="true"
			length="4"
			not-null="false" />
			
		<property
			name="fdCategoryIds"
			column="fd_category_ids"
			update="true"
			insert="true"
			length="4000"
			not-null="false" />
			
		<property
			name="fdCategoryNames"
			column="fd_category_names"
			update="true"
			insert="true"
			length="2500"
			not-null="false" />
			
		<property
			name="fdTargetType"
			column="fd_target_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="authReaders"
			table="sys_attend_report_reader"
			lazy="true">
			<key
				column="fd_report_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="fdDepts"
			table="sys_attend_report_depts"
			lazy="true">
			<key
				column="fd_report_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			index="idx_sysAttendStat_authAreaId" />


	</class>
</hibernate-mapping>
