<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendSignStat"
		table="sys_attend_sign_stat">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdDate"
			column="fd_date"
			update="true"
			insert="true"
			index="idx_attendSignStat_fdDate" />
		<property
			name="fdSignCount"
			column="fd_sign_count"
			update="true"
			insert="true"
			length="4"
			not-null="false" />
		<property
			name="fdCategoryId"
			column="fd_category_id"
			update="true"
			insert="true"
			index="idx_attendSignStat_categoryId" 
			length="36"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
		<property
			name="docCreateTime"
			column="doc_create_time"
			index="idx_attendSignStat_createTime" 
			update="true"
			insert="true"
			not-null="false" />
	</class>
</hibernate-mapping>
