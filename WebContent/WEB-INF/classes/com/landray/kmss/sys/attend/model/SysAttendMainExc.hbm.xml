<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendMainExc"
		table="sys_attend_main_exc">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdDesc"
			column="fd_desc"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docHandleTime"
			column="doc_handle_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdAttendTime"
			column="fd_attend_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="fdAttendMain"
			column="fd_attend_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdHandler"
			column="fd_handler_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="fdManager"
			column="fd_manager_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="fdCateTempl"
			column="fd_cate_templ_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<!-- 所有人可阅读标记 -->
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<!-- 可阅读者 -->
		<bag
			name="authReaders"
			table="sys_attend_mainexc_readers">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		
		<!-- 其他可阅读者 -->
		<bag
			name="authOtherReaders"
			table="sys_attend_mainexc_oreader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<!-- 所有可阅读者 -->
		<bag
			name="authAllReaders"
			table="sys_attend_mainexc_areader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>
		
		<!-- 可编辑者 -->
		<bag
			name="authEditors"
			table="sys_attend_mainexc_editors">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		
		<!-- 其他可编辑者 -->
		<bag
			name="authOtherEditors"
			table="sys_attend_mainexc_oeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		
		<!-- 所有可编辑者 -->
		<bag
			name="authAllEditors"
			table="sys_attend_mainexc_aeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_editor_id" />
		</bag>
		


	</class>
</hibernate-mapping>
