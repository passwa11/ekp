<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysMonthCount"
		table="sys_month_count">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdDepartmentId"
			column="fd_department_id"
			update="true"
			insert="true"
			length="200" />
		<property
			name="fdDepartmentName"
			column="fd_department_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<property
			name="fdOfficial"
			column="fd_official"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdDispatch"
			column="fd_dispatch"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRehireAfterRetirement"
			column="fd_rehire_after_retirement"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdPractice"
			column="fd_practice"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdOfficialOut"
			column="fd_official_out"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdDispatchOut"
			column="fd_dispatch_out"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRehireAfterRetirementOut"
			column="fd_rehire_after_retirement_out"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdPracticeOut"
			column="fd_practice_out"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdOfficialIn"
			column="fd_official_in"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdDispatchIn"
			column="fd_dispatch_in"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRehireAfterRetirementIn"
			column="fd_rehire_after_retirement_in"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdPracticeIn"
			column="fd_practice_in"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdYearMonth"
			column="fd_year_month"
			update="true"
			insert="true"
			not-null="false" />
	</class>
</hibernate-mapping>