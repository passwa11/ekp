<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.attend.model.SysAttendRestatLog" 
        table="sys_attend_restat_log">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_sysAttendRestatLog_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_sysAttendRestatLog_docAlterTime"/>
        <property 
            name="fdBeginDate" 
            column="fd_begin_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndDate" 
            column="fd_end_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCategoryName" 
            column="fd_category_name" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdStatUserNames" 
            column="fd_stat_user_names" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCreateMiss" 
            column="fd_create_miss" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
