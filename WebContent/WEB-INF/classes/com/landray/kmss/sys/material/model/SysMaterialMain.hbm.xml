<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.material.model.SysMaterialMain"
		table="sys_material_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdModelTitle"
			column="fd_model_title"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdAttId"
			column="fd_att_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
