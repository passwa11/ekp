<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tic.soap.sync.model.TicSoapSyncTempFunc"
		table="tic_soap_sync_temp_func">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdInvokeType"
			column="fd_invoke_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdFuncMark"
			column="fd_func_mark"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdSoapXml"
			column="fd_soap_xml"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdUse"
			column="fd_use"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdQuartzTime"
			column="fd_quartz_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="fdQuartz"
			column="fd_quartz_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdSoapMain"
			column="fd_soap_main"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdCompDbcp"
			column="fd_comp_dbcp"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdSyncTableXpath"
			column="fd_sync_table_xpath"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />
			
		<property
			name="fdSyncType"
			column="fd_sync_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdTimeColumn"
			column="fd_time_column"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdDelCondition"
			column="fd_del_condition"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdLastDate"
			column="fd_last_date"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

	</class>
</hibernate-mapping>
