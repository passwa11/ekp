<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tic.rest.connector.model.TicRestSetting"
		table="tic_rest_setting">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdConnRequestTimeout"
			column="fd_conn_request_timeout"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdConnTimeout"
			column="fd_conn_timeout"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdSoTimeout"
			column="fd_so_timeout"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdHttpProxy"
			column="fd_http_proxy"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdHttpProxyHost"
			column="fd_http_proxy_host"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<property
			name="fdHttpProxyPort"
			column="fd_http_proxy_port"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdHttpProxyUsername"
			column="fd_http_proxy_username"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<property
			name="fdHttpProxyPassword"
			column="fd_http_proxy_password"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator"
			insert="true"
			update="true"
			not-null="false" />
		<property 
            name="fdAppType" 
            column="fd_app_type" 
            update="false" 
            insert="true" 
            length="200"
            not-null="false"/>
            
       <property
			name="fdEnviromentId"
			column="fd_enviroment_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
	  <property
			name="fdSourceId"
			column="fd_source_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			            
				
		 <many-to-one 
		        name="settCategory" 
		        column="sett_category_id"
				insert="true"
				update="true" 
				not-null="false" />

	</class>
</hibernate-mapping>
