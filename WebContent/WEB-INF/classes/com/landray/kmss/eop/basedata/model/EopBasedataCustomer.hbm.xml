<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataCustomer"
        table="eop_basedata_customer">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdTel"
            column="fd_tel"
            update="true"
            insert="true"
            length="200"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_supplier_createTime"/>
        <property 
            name="fdAbbreviation" 
            column="fd_abbreviation" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCreditCode" 
            column="fd_credit_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCodeValidityPeriod" 
            column="fd_code_validity_period" 
            update="true" 
            insert="true"/>
        <property 
            name="fdIndustry" 
            column="fd_industry" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdLegalPerson" 
            column="fd_legal_person" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRegistCapital" 
            column="fd_regist_capital" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdEstablishDate" 
            column="fd_establish_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEmail" 
            column="fd_email" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAddress" 
            column="fd_address" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBusinessScope" 
            column="fd_business_scope" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdBankAccountName" 
            column="fd_bank_account_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankName" 
            column="fd_bank_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankAccount" 
            column="fd_bank_account" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdTaxNo" 
            column="fd_tax_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdErpNo" 
            column="fd_erp_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one
                name="fdUser"
                column="fd_user_id"
                update="true"
                insert="true"/>
        <many-to-one
                name="fdCharge"
                column="fd_charge_id"
                update="true"
                insert="true"/>
        <list 
            name="fdAccountList" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCustomerAccount"/>
        </list>
        <list
                name="fdContactPerson"
                lazy="true"
                cascade="all-delete-orphan"
                inverse="false">
            <key
                    column="doc_main_id"/>
            <list-index
                    column="doc_index"/>
            <one-to-many
                    class="com.landray.kmss.eop.basedata.model.EopBasedataCustomerContact"/>
        </list>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_customer_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
