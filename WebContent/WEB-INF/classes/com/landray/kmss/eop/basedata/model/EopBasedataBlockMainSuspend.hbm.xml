<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.eop.basedata.model.EopBasedataBlockMainSuspend"
		table="eop_basedata_main_suspend">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			length="36"
			not-null="true" />
			
		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			length="36"
			not-null="true" />
			
		<property
			name="fdFeeMainId"
			column="fd_fee_main_id"
			update="true"
			insert="true"
			length="36"
			not-null="true" />
		
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />
	</class>
</hibernate-mapping>
