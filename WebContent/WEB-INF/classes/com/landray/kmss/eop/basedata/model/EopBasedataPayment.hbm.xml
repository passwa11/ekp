<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataPayment" 
        table="eop_basedata_payment">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdSubject" 
            column="fd_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelNumber" 
            column="fd_model_number" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdPaymentMoney" 
            column="fd_payment_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPaymentTime" 
            column="fd_payment_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="2000"/>
        <list 
            name="fdDetail" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataPaymentDetail"/>
        </list>
    </class>
</hibernate-mapping>
