<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataCostCenter" 
        table="eop_basedata_cost_center">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsGroup" 
            column="fd_is_group" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="50" 
            index="idx_costCenter_code"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property
            name="fdOrder"
            column="fd_order"
            update="true"
            insert="true"/>
         <property 
            name="fdJoinSystem" 
            column="fd_join_system" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdSystemParam" 
            column="fd_system_param" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_costCenter_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_costCenter_alterTime"/>
         <property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
         <many-to-one
			name="hbmParent"
			class="com.landray.kmss.eop.basedata.model.EopBasedataCostCenter"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />
        <many-to-one 
            name="fdType" 
            column="fd_type_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdFirstCharger" 
            table="eop_basedata_cost_center_fst" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdSecondCharger" 
            table="eop_basedata_cost_center_snd" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdManager" 
            table="eop_basedata_cost_center_mng" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdBudgetManager" 
            table="eop_basedata_cost_center_bmng" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdEkpOrg" 
            table="eop_basedata_cost_center_org" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_costcenter_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
