<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataItemField" 
        table="eop_basedata_item_field">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdFields" 
            column="fd_fields" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdFieldOne" 
            column="fd_field_one" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRequiredOne" 
            column="fd_required_one" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFieldTwo" 
            column="fd_field_two" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRequiredTwo" 
            column="fd_required_two" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFiledThree" 
            column="fd_filed_three" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRequiredThree" 
            column="fd_required_three" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFieldFour" 
            column="fd_field_four" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRequiredFour" 
            column="fd_required_four" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFieldFive" 
            column="fd_field_five" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRequiredFive" 
            column="fd_required_five" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_itemField_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_itemField_alterTime"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdItems" 
            table="eop_basedata_item_field_it" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataExpenseItem" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_itemfield_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
