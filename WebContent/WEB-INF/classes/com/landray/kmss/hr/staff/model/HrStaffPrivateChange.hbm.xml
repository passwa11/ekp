<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.hr.staff.model.HrStaffPrivateChange"
		table="hr_staff_private_change" >
		<!-- 主结构配置 -->
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator class="assigned" />
		</id>
		<property
			name="isBriefPrivate"
			column="is_brief_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="isProjectPrivate"
			column="is_project_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="isWorkPrivate"
			column="is_work_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="isEducationPrivate"
			column="is_edu_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="isTrainingPrivate"
			column="is_train_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="isBonusPrivate"
			column="is_bonus_private"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdPersonId"
			column="fd_person_id"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdPersonName"
			column="fd_person_name"
			update="true"
			insert="true"
			not-null="false"/>
	</class>
</hibernate-mapping>
