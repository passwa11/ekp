<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.ratify.model.HrRatifyTemplate" 
        table="hr_ratify_template">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_hrRatifyTemplate_fdOrder"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="false" 
            insert="true" 
            length="36"/>
        <property
			name="fdTempKey"
			column="fd_tempkey"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_hrRatifyTemplate_creTime"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="titleRegulation" 
            column="fd_title_regulation" 
            update="true" 
            insert="true" 
            length="3000"/>
        <property 
            name="titleRegulationName" 
            column="fd_title_regulation_name" 
            update="true" 
            insert="true" 
            length="3000"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeAtt" 
            column="auth_change_att" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNocopy" 
            column="auth_tmp_att_nocopy" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNodownload" 
            column="auth_tmp_att_nodownload" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNoprint" 
            column="auth_tmp_att_noprint" 
            update="true" 
            insert="true"/>
        <property 
            name="docXform" 
            column="doc_xform" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docUseXform" 
            column="doc_use_xform" 
            update="true" 
            insert="true"/>
        <property
			name="fdSignEnable"
			column="fd_sign_enable"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
        <many-to-one 
            name="docCategory" 
            column="doc_category_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="hr_ratify_template_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="hr_ratify_template_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpReaders" 
            table="hr_ratify_template_treader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttCopys" 
            table="hr_ratify_template_tattcopy" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttDownloads" 
            table="hr_ratify_template_tattdl" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttPrints" 
            table="hr_ratify_template_tattprint" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="hr_ratify_template_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="hr_ratify_template_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="docProperties" 
            table="hr_ratify_template_property" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.category.model.SysCategoryProperty" 
                column="fd_target_id"/>
        </bag>
        <bag
			name="docKeyword"
			cascade="all-delete-orphan"
			inverse="true">
			<key column="fd_object_id" />
			<one-to-many
				class="com.landray.kmss.hr.ratify.model.HrRatifyTKeyword" />
		</bag>
		<bag
			name="fdFeedback"
			table="hr_ratify_template_feedback"
			lazy="true">
			<key column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_feedback_id" />
		</bag>
		<!-- 修改人修改时间 -->
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdIsExternal"
			column="fd_is_external"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
	    <property 
			name="fdExternalUrl" 
    		column="fd_external_url" 
			update="true" 
			insert="true" 
			not-null="false" 
			length="1000" />
    </class>
</hibernate-mapping>
