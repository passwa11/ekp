<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.organization.model.HrOrganizationConPost" 
        table="hr_org_con_post">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdStartTime" 
            column="fd_start_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndTime" 
            column="fd_end_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="36"/>
        <many-to-one 
            name="fdPerson" 
            column="fd_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdPost" 
            column="fd_post_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdDept" 
            column="fd_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdStaffingLevel" 
            column="fd_staffing_level_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
