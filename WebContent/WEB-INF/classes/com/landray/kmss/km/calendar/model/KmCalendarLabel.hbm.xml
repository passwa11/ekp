<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.calendar.model.KmCalendarLabel"
		table="km_calendar_label">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdColor"
			column="fd_color"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" 
			index="idx_KmCalLabel_fdOrder"/>

		<property
				name="fdSelectedFlag"
				column="fd_selected_flag"
				update="true"
				insert="true"
				length="1"
				not-null="false" />

		<property
				name="fdCommonFlag"
				column="fd_common_flag"
				update="true"
				insert="true"
				length="20"
				not-null="false" />
			
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" 
			index="idx_KmCalLabel_fdModelName"/>

		<many-to-one
			name="fdCreator"
			column="fd_creator_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
