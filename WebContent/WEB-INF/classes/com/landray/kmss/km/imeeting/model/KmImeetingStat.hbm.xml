<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingStat"
		table="km_imeeting_stat">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />

		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="queryCondIds"
			column="query_cond_ids"
			update="true"
			insert="true"
			length="3000" />
		
		<property
			name="queryCondNames"
			column="query_cond_names"
			update="true"
			insert="true"
			length="3000" />
			
		<property
			name="fdTemplateId"
			column="fd_template_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
			
		<property
			name="fdTemplateName"
			column="fd_template_name"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="fdContiditionJson"
			column="fd_contidition_json"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdDateType"
			column="fd_date_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdStartDate"
			column="fd_start_date"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdEndDate"
			column="fd_end_date"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<bag
			name="authReaders"
			table="km_imeeting_stat_reader"
			lazy="true">
			<key
				column="fd_main_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		
		<bag
			name="authAllReaders"
			table="km_imeeting_stat_areader"
			lazy="true">
			<key
				column="fd_main_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />

	</class>
</hibernate-mapping>
