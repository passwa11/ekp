<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingRes"
		table="km_imeeting_res">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />
	
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdDetail"
			column="fd_detail"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="fdAddressFloor"
			column="fd_address_floor"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdSeats"
			column="fd_seats"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdUserTime"
			column="fd_user_time"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsAvailable"
			column="fd_is_available"
			update="true"
			insert="true"
			length="1" />

		<many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true" />

		<many-to-one
			name="docKeeper"
			column="doc_keeper_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdNeedExamFlag"
			column="fd_need_exam_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
		
		<property
			name="fdSeatDetail"
			column="fd_seat_detail"
			update="true"
			insert="true"
			not-null="false"
			length="1000000"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />
			
		<property
			name="fdSeatCount"
			column="fd_seat_count"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdCols"
			column="fd_cols"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdRows"
			column="fd_rows"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<bag
			name="authReaders"
			table="km_imeeting_res_reader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authEditors"
			table="km_imeeting_res_editor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		
		<bag
			name="authOtherReaders"
			table="km_imeeting_res_oreader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<bag
			name="authOtherEditors"
			table="km_imeeting_res_oeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		
		<bag
			name="authAllReaders"
			table="km_imeeting_res_areader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>
		
		<bag
			name="authAllEditors"
			table="km_imeeting_res_aeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_editor_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />
			
		<property
			name="fdInnerScreenEnable"
			column="fd_inner_screen_enable"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="fdOuterScreenEnable"
			column="fd_outter_screen_enable"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		
		<property
			name="fdSignInEnable"
			column="fd_sign_in_enable"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="fdSignInTypeCode"
			column="fd_sign_in_type_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdSignInIp"
			column="fd_sign_in_ip"
			update="true"
			insert="true"
			length="256"
			not-null="false" />
		
		<property
			name="fdSignInPort"
			column="fd_sign_in_port"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdSignInUserName"
			column="fd_sign_in_user_name"
			update="true"
			insert="true"
			length="256"
			not-null="false" />
			
		<property
			name="fdSignInPassword"
			column="fd_sign_in_password"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<list 
            name="fdInnerScreens" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="fd_res_id"/>
            <list-index 
                column="res_index"/>
            <one-to-many 
                class="com.landray.kmss.km.imeeting.model.KmImeetingInnerScreen"/>
        </list>
        
        <list 
            name="fdOuterScreens" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="fd_res_id"/>
            <list-index 
                column="res_index"/>
            <one-to-many 
                class="com.landray.kmss.km.imeeting.model.KmImeetingOuterScreen"/>
        </list>
	</class>
</hibernate-mapping>
