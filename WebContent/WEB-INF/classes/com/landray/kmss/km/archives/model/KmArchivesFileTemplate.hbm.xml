<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.archives.model.KmArchivesFileTemplate"
		table="km_archives_file_template">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200" />

		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			length="32"
			not-null="false" />

		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdTmpXml"
			column="fd_tmp_xml"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />
			
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="category"
			column="category_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="selectFilePersonType"
			column="select_file_person_type"
			update="true"
			insert="true" />
		
		<many-to-one
			name="fdFilePerson"
			column="fd_file_person_id"
			insert="true"
			update="true"
			not-null="false" />
		
		<property
			name="fdFilePersonFormula"
			column="fd_file_person_formula"
			update="true"
			insert="true" />
			
		<property
			name="fdFilePersonFormulaName"
			column="fd_file_person_formula_name"
			update="true"
			insert="true" />
		
		<property
			name="fdSaveApproval"
			column="fd_save_approval"
			update="true"
			insert="true" />
		<property
			name="fdPreFile"
			column="fd_pre_file"
			update="true"
			insert="true" />	
			
			
		<property
			name="fdSaveOldFile"
			column="fd_save_old_file"
			update="true"
			insert="true" />
			
		<property
			name="docSubjectMapping"
			column="doc_subject_mapping"
			update="true"
			insert="true" />
		<property
			name="fdLibraryMapping"
			column="fd_library_mapping"
			update="true"
			insert="true" />
		<property
			name="fdPeriodMapping"
			column="fd_period_mapping"
			update="true"
			insert="true" />
		<property
			name="fdVolumeYearMapping"
			column="fd_volume_year_mapping"
			update="true"
			insert="true" />
		<property
			name="fdUnitMapping"
			column="fd_unit_mapping"
			update="true"
			insert="true" />
		<property
			name="fdKeeperMapping"
			column="fd_keeper_mapping"
			update="true"
			insert="true" />
		<property
			name="fdValidityDateMapping"
			column="fd_validity_date_mapping"
			update="true"
			insert="true" />
		<property
			name="fdDenseLevelMapping"
			column="fd_dense_level_mapping"
			update="true"
			insert="true" />
		<property
			name="fdFileDateMapping"
			column="fd_file_date_mapping"
			update="true"
			insert="true" />
	</class>
</hibernate-mapping>
