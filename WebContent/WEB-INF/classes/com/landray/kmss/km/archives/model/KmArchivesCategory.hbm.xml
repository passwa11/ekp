<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.archives.model.KmArchivesCategory" 
        table="km_archives_category">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_archivesCate_order"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_archivesCate_createTime"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property
			name="fdIsinheritMaintainer"
			column="fd_isinherit_maintainer"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="fdIsinheritUser"
			column="fd_isinherit_user"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeAtt" 
            column="auth_change_att" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNocopy" 
            column="auth_tmp_att_nocopy" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNodownload" 
            column="auth_tmp_att_nodownload" 
            update="true" 
            insert="true"/>
        <property 
            name="authTmpAttNoprint" 
            column="auth_tmp_att_noprint" 
            update="true" 
            insert="true"/>
        <property 
            name="fdHierarchyId" 
            column="fd_hierarchy_id" 
            update="true" 
            insert="true" 
            length="450" 
            index="idx_archivesCate_fdHId"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <!-- 属性模板字段 -->
		<many-to-one
			name="sysPropertyTemplate"
			column="fd_prop_template_id"
			update="true"
			insert="true"
			not-null="false" />
        <many-to-one 
            name="hbmParent" 
            class="com.landray.kmss.km.archives.model.KmArchivesCategory" 
            column="fd_parent_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="km_archives_category_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="km_archives_category_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpReaders" 
            table="km_archives_category_treader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpEditors" 
            table="km_archives_category_teditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttCopys" 
            table="km_archives_category_tattcopy" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttDownloads" 
            table="km_archives_category_tattdl" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authTmpAttPrints" 
            table="km_archives_category_tattprint" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="km_archives_category_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="km_archives_category_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
