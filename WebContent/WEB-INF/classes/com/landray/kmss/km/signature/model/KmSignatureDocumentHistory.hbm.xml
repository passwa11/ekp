<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.signature.model.KmSignatureDocumentHistory"
		table="km_signature_doc_history">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<!-- 自动编号 -->
		<property
			name="fdHistoryId"
			column="fdHistoryId"
			update="true"
			insert="true"
			length="10" />
		<!-- 文档记录号 -->
		<property
			name="fdRecordId"
			column="fd_record_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		<!-- 字段名称 -->
		<property
			name="fdFieldName"
			column="fd_field_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 印章名称 -->
		<property
			name="fdMarkName"
			column="fd_mark_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 印章用户 -->
		<property
			name="fdUserName"
			column="fd_user_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 日期时间 -->
		<property
			name="fdDateTime"
			column="fd_date_time"
			update="true"
			insert="true"
			not-null="false" />
		<!-- 印章IP地址 -->
		<property
			name="fdHostName"
			column="fd_host_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		
		<property
			name="fdSigId"
			column="fd_sig_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 自动生成的印章唯一编号 -->
		<property
			name="fdMarkGuid"
			column="fd_mark_guid"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

	</class>
</hibernate-mapping>
