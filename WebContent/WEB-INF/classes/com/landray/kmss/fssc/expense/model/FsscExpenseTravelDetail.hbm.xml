<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseTravelDetail" 
        table="fssc_expense_travel_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdBeginDate" 
            column="fd_begin_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndDate" 
            column="fd_end_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdTravelDays" 
            column="fd_travel_days" 
            update="true" 
            insert="true"/>
        <property 
            name="fdStartPlace" 
            column="fd_start_place" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdArrivalId" 
            column="fd_arrival_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdArrivalPlace" 
            column="fd_arrival_place" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="fdVehicle" 
            column="fd_vehicle_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBerth" 
            column="fd_berth_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdPersonList" 
            table="fssc_expense_travel_detail_per" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgPerson" 
                column="fd_target_id"/>
        </bag>
        <property 
            name="fdSubject" 
            column="fd_subject" 
            update="true" 
            insert="true" 
            length="100"/>
    </class>
</hibernate-mapping>
