<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseAmortize" 
        table="fssc_expense_amortize">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdMonth" 
            column="fd_month" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPercent" 
            column="fd_percent" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
