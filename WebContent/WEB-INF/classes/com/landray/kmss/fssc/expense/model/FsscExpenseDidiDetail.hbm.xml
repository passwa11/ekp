            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdOrderId" 
            column="fd_order_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCarLevel" 
            column="fd_car_level" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
