<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseAccounts" 
        table="fssc_expense_accounts">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
      <property 
            name="fdBankAccountNo" 
            column="fd_bank_account_no" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankName" 
            column="fd_bank_name" 
            update="true" 
            insert="true" 
            length="400"/>
        <property 
            name="fdBankAccount" 
            column="fd_bank_account" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdExchangeRate" 
            column="fd_exchange_rate" 
            update="true" 
            insert="true"/>
        <property 
            name="fdAccountId" 
            column="fd_account_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <many-to-one 
            name="fdPayWay" 
            column="fd_pay_way_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBank" 
            column="fd_bank_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
         <property 
            name="fdAccountAreaCode" 
            column="fd_account_area_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdAccountAreaName" 
            column="fd_account_area_name" 
            update="true" 
            insert="true" 
            length="200"/>
    </class>
</hibernate-mapping>
