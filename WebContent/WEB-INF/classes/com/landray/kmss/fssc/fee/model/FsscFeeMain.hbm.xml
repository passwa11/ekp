<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.fee.model.FsscFeeMain" 
        table="fssc_fee_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdIsClosed" 
            column="fd_is_closed" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="docStatus" 
            column="doc_status" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="docNumber" 
            column="doc_number" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_feeMain_createTime"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdControlType" 
            column="fd_control_type" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="docXform" 
            column="doc_xform" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docUseXform" 
            column="doc_use_xform" 
            update="true" 
            insert="true"/>
        <property 
            name="extendDataXML" 
            column="extend_data_xml" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="extendFilePath" 
            column="extend_file_path" 
            update="true" 
            insert="true" 
            length="200"/>
        <!-- 回收站机制字段开始 -->
        <property
			name="docDeleteFlag"
			column="doc_delete_flag"
			update="true"
			insert="true"
			not-null="false"
			length="5" />
		<property
			name="docDeleteTime"
			column="doc_delete_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="docPublishTime"
			column="doc_publish_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docDeleteBy"
			column="doc_delete_by"
			insert="true"
			update="true"
			not-null="false" />
		<!-- 回收站机制字段结束 -->
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docTemplate" 
            column="doc_template_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="fssc_fee_main_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherReaders" 
            table="fssc_fee_main_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="fssc_fee_main_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="fssc_fee_main_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="fssc_fee_main_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
