<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.config.model.FsscConfigList" 
        table="fssc_config_list">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_fsscConfigList_docCreateTime"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdGoodsType" 
            column="fd_goods_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdGoodsProperty" 
            column="fd_goods_property" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMinNum" 
            column="fd_min_num" 
            update="true" 
            insert="true"/>
        <property 
            name="fdSpec" 
            column="fd_spec" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdUnit" 
            column="fd_unit" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPrice" 
            column="fd_price" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
