<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.budget.model.FsscBudgetMain" 
        table="fssc_budget_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdYear" 
            column="fd_year" 
            update="true" 
            insert="true" 
            length="12"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_budgetMain_createTime"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdEnableDate" 
            column="fd_enable_date" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBudgetScheme" 
            column="fd_budget__scheme_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompanyGroup" 
            column="fd_company_group_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
        <list 
            name="fdDetailList" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.fssc.budget.model.FsscBudgetDetail"/>
        </list>
    </class>
</hibernate-mapping>
