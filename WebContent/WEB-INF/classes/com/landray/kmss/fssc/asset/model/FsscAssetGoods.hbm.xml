<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.asset.model.FsscAssetGoods" 
        table="fssc_asset_goods">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_fsscAssetGoods_docCreateTime"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdNum" 
            column="fd_num" 
            update="true" 
            insert="true"/>
        <property 
            name="fdAssetName" 
            column="fd_asset_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
