<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.budgeting.model.FsscBudgetingApprovalLog" 
        table="fssc_budgeting_approval_log">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdMainId" 
            column="fd_main_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdDetailId" 
            column="fd_detail_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdApprovalType" 
            column="fd_approval_type" 
            update="true" 
            insert="true" 
            length="25"/>
        <property 
            name="fdApprovalTime" 
            column="fd_approval_time" 
            update="true" 
            insert="true"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="fdOperator" 
            column="fd_operator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
