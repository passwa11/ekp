<?xml version="1.0" encoding="UTF-8"?>
<beans 
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" 
    xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bean 
        id="fsscAssetGoodsDao" 
        class="com.landray.kmss.fssc.asset.dao.hibernate.FsscAssetGoodsDaoImp" 
        parent="KmssAuthBaseDao">
        <property 
            name="modelName">
            <value>com.landray.kmss.fssc.asset.model.FsscAssetGoods</value>
        </property>
    </bean>
    <bean 
        id="fsscAssetGoodsTarget" 
        class="com.landray.kmss.fssc.asset.service.spring.FsscAssetGoodsServiceImp" 
        parent="KmssExtendDataTarget">
        <property 
            name="baseDao">
            <ref 
                bean="fsscAssetGoodsDao"/>
        </property>
    </bean>
    <bean 
        id="fsscAssetGoodsService" 
        parent="KmssBaseService">
        <property 
            name="target">
            <ref 
                bean="fsscAssetGoodsTarget"/>
        </property>
    </bean>
</beans>
