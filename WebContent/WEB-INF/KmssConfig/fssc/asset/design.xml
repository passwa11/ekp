<?xml version="1.0" encoding="UTF-8"?>
<configs 
    xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd " 
    xmlns="http://www.example.org/design-config" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <module 
        urlPrefix="/fssc/asset/" 
        messageKey="fssc-asset:module.fssc.asset" 
        defaultValidator="roleValidator(role=ROLE_FSSCASSET_DEFAULT)">
        <roles>ROLE_FSSCASSET_DEFAULT;ROLE_FSSCASSET_SETTING</roles>
        <request 
            path="tree.jsp*" 
            defaultValidator="roleValidator(role=ROLE_FSSCASSET_SETTING)"/>
        <request 
            path="fssc_asset_goods/fsscAssetGoods.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.asset.model.FsscAssetGoods" 
            defaultValidator="roleValidator(role=ROLE_FSSCASSET_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCASSET_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCASSET_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCASSET_DEFAULT)"/>
        </request>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*"/>
        <request 
            path="sys_transport_export/SysTransportExport*"/>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*">
            <query 
                queryString="fdModelName=com.landray.kmss.fssc.asset.model.FsscAssetGoods" 
                validator="roleValidator(role=ROLE_FSSCASSET_SETTING)"/>
        </request>
        <request 
            path="sys_transport_export/SysTransportExport*">
            <query 
                queryString="fdModelName=com.landray.kmss.fssc.asset.model.FsscAssetGoods" 
                validator="roleValidator(role=ROLE_FSSCASSET_SETTING)"/>
        </request>
    </module>
    <homePage 
        messageKey="fssc-asset:module.fssc.asset" 
        urlPrefix="fssc/asset" 
        url="/fssc/asset/index.jsp" 
        config="/moduleindex_notopic.jsp?nav=/fssc/asset/tree.jsp"/>
    <profileConfig 
        key="fssc/asset" 
        messageKey="{fssc-asset:module.fssc.asset}" 
        url="/sys/profile/moduleindex.jsp?nav=/fssc/asset/tree.jsp" 
        icon="km_comminfo" 
        type="fs" 
        description="{fssc-asset:module.fssc.asset.description}"/>
</configs>
