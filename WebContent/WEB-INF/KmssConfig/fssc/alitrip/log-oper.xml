<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<!-- 阿里信息配置 -->
        <oper
        	id="fsscAlitripMessageOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripMessage"
        	action="/fssc/alitrip/fssc_alitrip_message/fsscAlitripMessage.do">
        </oper>
		<!-- 商旅订单信息 -->
        <oper
        	id="fsscAlitripOrderOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripOrder"
        	action="/fssc/alitrip/fssc_alitrip_order/fsscAlitripOrder.do">
        </oper>
		<!-- 保险信息 -->
        <oper
        	id="fsscAlitripInsureOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripInsure"
        	action="/fssc/alitrip/fssc_alitrip_order/fsscAlitripInsure.do">
        </oper>
		<!-- 阿里商旅字段映射表 -->
        <oper
        	id="fsscAlitripMappingOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripMapping"
        	action="/fssc/alitrip/fssc_alitrip_mapping/fsscAlitripMapping.do">
        </oper>
		<!-- 商旅模块配置 -->
        <oper
        	id="fsscAlitripModelOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripModel"
        	action="/fssc/alitrip/fssc_alitrip_model/fsscAlitripModel.do">
        </oper>
		<!-- 同步日志 -->
        <oper
        	id="fsscAlitripLogOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.alitrip.model.FsscAlitripLog"
        	action="/fssc/alitrip/fssc_alitrip_log/fsscAlitripLog.do">
        </oper>
	</logset>
</log-oper>
