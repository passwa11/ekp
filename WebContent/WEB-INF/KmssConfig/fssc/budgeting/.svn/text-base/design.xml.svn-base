<?xml version="1.0" encoding="UTF-8"?>
<configs 
    xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd " 
    xmlns="http://www.example.org/design-config" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <module 
        urlPrefix="/fssc/budgeting/" 
        messageKey="fssc-budgeting:module.fssc.budgeting" 
        defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)">
        <roles>
        	ROLE_FSSCBUDGETING_DEFAULT;  <!-- 预算编制默认权限 -->
        	ROLE_FSSCBUDGETING_SETTING;  <!-- 后台设置权限 -->
        	ROLE_FSSCBUDGETING_OPEN;  <!-- 预算编制-开启预算编制 -->
        	ROLE_FSSCBUDGETING_PERIOD;   <!-- 预算期间维护权限 -->
        	ROLE_FSSCBUDGETING_VIEW;   <!-- 预算编制查看权限 -->
        </roles>
        <request 
            path="tree.jsp*" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_OPEN)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)"/>
        <request 
            path="fssc_budgeting_period/fsscBudgetingPeriod.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingPeriod" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)">
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)"/>
            <query 
                queryString="method=add|save|saveadd" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)"/>
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)"/>
            <query 
                queryString="method=deteleall|detele" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_PERIOD)"/>
        </request>
        <request 
            path="fssc_budgeting_auth/fsscBudgetingAuth.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingAuth" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)">
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=add|save|saveadd" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=deteleall|detele" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
        </request>
        <request 
            path="fssc_budgeting_approval_auth/fsscBudgetingApprovalAuth.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingApprovalAuth" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)">
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=add|save|saveadd" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=deteleall|detele" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
        </request>
        <request 
            path="fssc_budgeting_effect_auth/fsscBudgetingEffectAuth.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingEffectAuth" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)">
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=add|save|saveadd" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=deteleall|detele" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
        </request>
        <request 
            path="fssc_budgeting_main/fsscBudgetingMain.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingMain" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|fieldValueValidator(fieldproperty=fdStatus,value=1)&amp;authFieldValidator(type=creator)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)"/>
        </request>
        <request 
            path="fssc_budgeting_approval_log/fsscBudgetingApprovalLog.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingApprovalLog" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)"/>
        </request>
        <request 
            path="fssc_budgeting_org/fsscBudgetingOrg.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.budgeting.model.FsscBudgetingOrg" 
            defaultValidator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_OPEN)"/>
            <query 
                queryString="method=add|save" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)|roleValidator(role=ROLE_FSSCBUDGETING_OPEN)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCBUDGETING_DEFAULT)"/>
        </request>
        <filters
			modelName="com.landray.kmss.fssc.budgeting.model.FsscBudgetingMain">
			<filter
				type="creator"
				expression="authFieldFilter(field=docCreator.fdId)" />
		</filters>
    </module>
    <homePage 
        messageKey="fssc-budgeting:module.fssc.budgeting" 
        urlPrefix="fssc/budgeting" 
        url="/fssc/budgeting/index.jsp" 
        config="/moduleindex_notopic.jsp?nav=/fssc/budgeting/tree.jsp"/>
    <profileConfig 
        key="fssc/budgeting" 
        messageKey="{fssc-budgeting:module.fssc.budgeting}" 
        url="/sys/profile/moduleindex.jsp?nav=/fssc/budgeting/tree.jsp" 
        icon="lui_icon_l_profile_icon_default" 
        type="fs" 
        description="{fssc-budgeting:module.fssc.budgeting.description}"/>
</configs>
