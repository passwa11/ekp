<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<!-- 借款类别 -->
        <oper
        	id="fsscLoanCategoryOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanCategory"
        	action="/fssc/loan/fssc_loan_category/fsscLoanCategory.do">
        </oper>
		<!-- 借款控制 -->
        <oper
        	id="fsscLoanControlOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanControl"
        	action="/fssc/loan/fssc_loan_control/fsscLoanControl.do">
        </oper>
		<!-- 借款主表 -->
        <oper
        	id="fsscLoanMainOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanMain"
        	action="/fssc/loan/fssc_loan_main/fsscLoanMain.do">
        </oper>
		<!-- 还款提醒 -->
        <oper
        	id="fsscLoanPayWarnOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanPayWarn"
        	action="/fssc/loan/fssc_loan_pay_warn/fsscLoanPayWarn.do">
        </oper>
		<!-- 还款分类 -->
        <oper
        	id="fsscLoanReCategoryOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanReCategory"
        	action="/fssc/loan/fssc_loan_re_category/fsscLoanReCategory.do">
        </oper>
		<!-- 还款主表 -->
        <oper
        	id="fsscLoanRepaymentOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanRepayment"
        	action="/fssc/loan/fssc_loan_repayment/fsscLoanRepayment.do">
        </oper>
		<!-- 借款对应会计科目 -->
        <oper
        	id="fsscLoanSubjectDetailOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanSubjectDetail"
        	action="/fssc/loan/fssc_loan_subject_detail/fsscLoanSubjectDetail.do">
        </oper>
		<!-- 借款转移主表 -->
        <oper
        	id="fsscLoanTransferOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.fssc.loan.model.FsscLoanTransfer"
        	action="/fssc/loan/fssc_loan_transfer/fsscLoanTransfer.do">
        </oper>
	</logset>
</log-oper>
