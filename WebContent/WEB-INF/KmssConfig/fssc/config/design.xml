<?xml version="1.0" encoding="UTF-8"?>
<configs 
    xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd " 
    xmlns="http://www.example.org/design-config" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <module 
        urlPrefix="/fssc/config/" 
        messageKey="fssc-config:module.fssc.config" 
        defaultValidator="roleValidator(role=ROLE_FSSCCONFIG_DEFAULT)">
        <roles>ROLE_FSSCCONFIG_DEFAULT;ROLE_FSSCCONFIG_SETTING</roles>
        <request 
            path="tree.jsp*" 
            defaultValidator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
        <request 
            path="fssc_config_score/fsscConfigScore.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.config.model.FsscConfigScore" 
            defaultValidator="roleValidator(role=ROLE_FSSCCONFIG_DEFAULT)">
            <query 
                queryString="method=view" 
                validator="authFieldValidator(type=SYS_READER)"/>
            <query 
                queryString="method=edit|update" 
                validator="authFieldValidator(type=SYS_EDITOR)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
        </request>
        <request 
            path="fssc_config_list/fsscConfigList.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.fssc.config.model.FsscConfigList" 
            defaultValidator="roleValidator(role=ROLE_FSSCCONFIG_DEFAULT)">
            <query 
                queryString="method=edit|update" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_DEFAULT)"/>
        </request>
        <filters 
            modelName="com.landray.kmss.fssc.config.model.FsscConfigScore">
            <filter 
                type="SYS_READER" 
                expression="authFieldFilter(field=authAllReaders.fdId)"/>
            <filter 
                type="SYS_READER" 
                expression="flagFieldFilter(field=authReaderFlag,value=1)"/>
            <filter 
                type="SYS_EDITOR" 
                expression="authFieldFilter(field=authAllEditors.fdId)"/>
        </filters>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*"/>
        <request 
            path="sys_transport_export/SysTransportExport*"/>
    </module>
    <module 
        urlPrefix="/sys/transport/">
        <request 
            path="sys_transport_import/SysTransportImport*">
            <query 
                queryString="fdModelName=com.landray.kmss.fssc.config.model.FsscConfigScore" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
        </request>
        <request 
            path="sys_transport_export/SysTransportExport*">
            <query 
                queryString="fdModelName=com.landray.kmss.fssc.config.model.FsscConfigScore" 
                validator="roleValidator(role=ROLE_FSSCCONFIG_SETTING)"/>
        </request>
    </module>
    <homePage 
        messageKey="fssc-config:module.fssc.config" 
        urlPrefix="fssc/config" 
        url="/fssc/config/index.jsp" 
        config="/moduleindex_notopic.jsp?nav=/fssc/config/tree.jsp"/>
    <profileConfig 
        key="fssc/config" 
        messageKey="{fssc-config:module.fssc.config}" 
        url="/sys/profile/moduleindex.jsp?nav=/fssc/config/tree.jsp" 
        icon="km_comminfo" 
        type="ekp" 
        description="{fssc-config:module.fssc.config.description}"/>
</configs>
