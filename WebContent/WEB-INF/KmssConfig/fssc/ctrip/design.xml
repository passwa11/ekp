    </module>
    <!-- 	同步组织架构到携程 -->
    <quartz
        messageKey="fssc-ctrip:fssc.ctrip.sysOrglInfo.to.ctrip"
        jobService="fsscCtripSynPersonService"
        jobMethod="addZlPersonToCtrip"
        cronExpression="0 0 1 * * ?"
        description="fssc-ctrip:fssc.ctrip.sysHotelInfo.to.ctrip.description" />
    <!-- 同步机票结算单信息 -->
	<quartz
		messageKey="fssc-ctrip:fssc.ctrip.syn.flight.settle.from.ctrip"
		jobService="fsscCtripSynFlightSettleService"
		cronExpression="0 0 1 * * ?" 
		jobMethod="saveFlightSettle" 
		description="fssc-ctrip:fssc.ctrip.syn.flight.settle.from.ctrip.description"/>
	<!-- 同步酒店结算单信息 -->
	<quartz
		messageKey="fssc-ctrip:fssc.ctrip.syn.hotel.settle.from.ctrip"
		jobService="fsscCtripSynHotelSettleService"
		cronExpression="0 0 1 * * ?" 
		jobMethod="saveHotelSettle" 
		description="fssc-ctrip:fssc.ctrip.syn.hotel.settle.from.ctrip.description"/>
	<!-- 同步火车票结算单信息 -->
	<quartz
		messageKey="fssc-ctrip:fssc.ctrip.syn.train.settle.from.ctrip"
		jobService="fsscCtripSynTrainSettleService"
		cronExpression="0 0 1 * * ?" 
		jobMethod="saveTrainSettle" 
		description="fssc-ctrip:fssc.ctrip.syn.train.settle.from.ctrip.description"/>
	<!-- 同步用车结算单信息 -->
	<quartz
		messageKey="fssc-ctrip:fssc.ctrip.syn.car.settle.from.ctrip"
		jobService="fsscCtripSynCarSettleService"
		cronExpression="0 0 1 * * ?" 
		jobMethod="saveCarSettle" 
		description="fssc-ctrip:fssc.ctrip.syn.car.settle.from.ctrip.description"/>
    <homePage 
        messageKey="fssc-ctrip:module.fssc.ctrip" 
        urlPrefix="fssc/ctrip" 
        url="/fssc/ctrip/index.jsp" 
        config="/moduleindex_notopic.jsp?nav=/fssc/ctrip/tree.jsp"/>
    <profileConfig 
        key="fssc/ctrip" 
        messageKey="{fssc-ctrip:module.fssc.ctrip}" 
        url="/sys/profile/moduleindex.jsp?nav=/fssc/ctrip/tree.jsp" 
        icon="fs_trip" 
        type="fs" 
        description="{fssc-ctrip:module.fssc.ctrip.description}"/>
    <profileConfig
        key="fssc/ctrip"
        messageKey="{fssc-ctrip:module.fssc.ctrip}"
        url="/sys/profile/moduleindex.jsp?nav=/fssc/ctrip/tree.jsp"
        icon="fs_trip"
        type="saas"
        description="{fssc-ctrip:module.fssc.ctrip.description}"/>
</configs>
