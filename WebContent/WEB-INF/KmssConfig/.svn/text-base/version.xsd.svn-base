<?xml version="1.0" encoding="UTF-8"?>
<schema
	xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/version-config"
	xmlns:tns="http://www.example.org/version-config"
	elementFormDefault="qualified">
	<element
		name="version">
		<complexType>
			<sequence>
				<element
					name="modifies"
					maxOccurs="unbounded"
					minOccurs="0">
					<annotation>
						<documentation
							xml:lang="zh">修改信息</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element
								name="modify"
								maxOccurs="unbounded"
								minOccurs="0">
								<annotation>
									<documentation
										xml:lang="zh">修改详细信息</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element
											name="description"
											type="tns:required"
											maxOccurs="1"
											minOccurs="1">
											<annotation>
												<documentation
													xml:lang="zh">修改描述</documentation>
											</annotation>
										</element>
										<element
											name="author"
											type="tns:required"
											maxOccurs="1"
											minOccurs="1">
											<annotation>
												<documentation
													xml:lang="zh">作者</documentation>
											</annotation>
										</element>
										<element
											name="revision-time"
											type="date"
											maxOccurs="1"
											minOccurs="1">
											<annotation>
												<documentation
													xml:lang="zh">修订时间</documentation>
											</annotation>
										</element>
										<element
											name="relation"
											maxOccurs="1"
											minOccurs="0">
											<annotation>
												<documentation
													xml:lang="zh">关联信息</documentation>
											</annotation>
											<complexType>
												<sequence>
													<element
														name="relation-module"
														type="tns:required"
														maxOccurs="unbounded"
														minOccurs="1">
														<annotation>
															<documentation
																xml:lang="zh">关联模块</documentation>
														</annotation>
													</element>
												</sequence>
											</complexType>
										</element>
										<element
											name="baseline"
											type="string"
											maxOccurs="1"
											minOccurs="0">
											<annotation>
												<documentation
													xml:lang="zh">关联模块的基线</documentation>
											</annotation>
										</element>
									</sequence>
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
	<simpleType
		name="required">
		<annotation>
			<documentation
				xml:lang="zh">必填约束</documentation>
		</annotation>
		<restriction
			base="string">
			<minLength
				value="1" />
		</restriction>
	</simpleType>
</schema>