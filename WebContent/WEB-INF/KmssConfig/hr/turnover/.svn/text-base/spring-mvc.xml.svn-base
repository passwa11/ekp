<?xml version="1.0" encoding="UTF-8"?>
<beans 
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" 
    xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bean 
        class="com.landray.kmss.hr.turnover.actions.HrTurnoverAnnualAction" 
        parent="KmssBaseAction" 
        name="/hr/turnover/hr_turnover_annual/hrTurnoverAnnual.do" 
        lazy-init="true">
        <property 
            name="formType" 
            value="com.landray.kmss.hr.turnover.forms.HrTurnoverAnnualForm"/>
        <property 
            name="forwards">
            <map>
                <entry 
                    key="data" 
                    value="/hr/turnover/hr_turnover_annual/hrTurnoverAnnual_data.jsp"/>
                <entry 
                    key="edit" 
                    value="/hr/turnover/hr_turnover_annual/hrTurnoverAnnual_edit.jsp"/>
                <entry 
                    key="view" 
                    value="/hr/turnover/hr_turnover_annual/hrTurnoverAnnual_view.jsp"/>
            </map>
        </property>
    </bean>
</beans>
