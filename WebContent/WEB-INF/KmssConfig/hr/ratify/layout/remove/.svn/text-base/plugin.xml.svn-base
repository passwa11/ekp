<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../plugin.xsd ">
	<!-- 基本属性布局扩展 -->
	<extension point="com.landray.kmss.sys.xform.fieldlayout" model="com.landray.kmss.hr.ratify.model.HrRatifyRemove">
		 <!--合同变更人员 -->
	    <item name="config">
			<param name="fieldIds" value="fdRemoveStaff"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovestaff/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovestaff/fdRemoveStaff"></param>
		</item>
		<!--所属部门 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveDept"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovedept/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovedept/fdRemoveDept"></param>
		</item>
		<!-- 解除的合同 -->
	    <item name="config">
			<param name="fieldIds" value="fdRemoveContract"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovecontract/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovecontract/fdRemoveContract"></param>
		</item>
		<!-- 解除的合同开始时间 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveBeginDate"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovebegindate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovebegindate/fdRemoveBeginDate"></param>
		</item>
		<!-- 解除的合同结束时间 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveEndDate"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremoveenddate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremoveenddate/fdRemoveEndDate"></param>
		</item>
		<!-- 解除的合同合同备注 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveRemark"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremoveremark/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremoveremark/fdRemoveRemark"></param>
		</item>
		<!-- 合同解除日期 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveCancelDate"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovecanceldate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovecanceldate/fdRemoveCancelDate"></param>
		</item>
		<!-- 合同解除原因 -->
		<item name="config">
			<param name="fieldIds" value="fdRemoveReason"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/remove/fdremovereason/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/remove/fdremovereason/fdRemoveReason"></param>
		</item>
	</extension>
</plugin>