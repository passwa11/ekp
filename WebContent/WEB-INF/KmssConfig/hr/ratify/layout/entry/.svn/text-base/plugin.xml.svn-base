<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../plugin.xsd ">
	<!-- 基本属性布局扩展 -->
	<extension point="com.landray.kmss.sys.xform.fieldlayout" model="com.landray.kmss.hr.ratify.model.HrRatifyEntry">
		 <!--入职员工 -->
	    <item name="config">
			<param name="fieldIds" value="fdEntryName"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentryname/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentryname/fdEntryName"></param>
		</item>
		<!--入职员工帐号登录名 -->
		<item name="config">
			<param name="fieldIds" value="fdLoginName"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdloginname/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdloginname/fdLoginName"></param>
		</item>
		<!--入职员工帐号初始密码 -->
		<item name="config">
			<param name="fieldIds" value="fdPassword"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdpassword/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdpassword/fdPassword"></param>
		</item>
		<!-- 入职部门 -->
	    <item name="config">
			<param name="fieldIds" value="fdEntryDept"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrydept/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrydept/fdEntryDept"></param>
		</item>
		<!-- 入职岗位 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryPosts"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrypost/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrypost/fdEntryPost"></param>
		</item>
		<!-- 入职职级 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryLevel"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrylevel/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrylevel/fdEntryLevel"></param>
		</item>
		<!-- 入职日期 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryDate"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrydate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrydate/fdEntryDate"></param>
		</item>
		<!-- 入职员工状态 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryStatus"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrystatus/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrystatus/fdEntryStatus"></param>
		</item>
		<!-- 试用期限（月） -->
		<item name="config">
			<param name="fieldIds" value="fdEntryTrialPeriod"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrytrialperiod/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrytrialperiod/fdEntryTrialPeriod"></param>
		</item>
		<!-- 预计转正日期 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryPeriodDate"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentryperioddate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentryperioddate/fdEntryPeriodDate"></param>
		</item>
		<!-- 试用期薪酬 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryPeriodSalary"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentryperiodsalary/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentryperiodsalary/fdEntryPeriodSalary"></param>
		</item>
		<!-- 转正薪酬 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryPositiveSalary"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrypositivesalary/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrypositivesalary/fdEntryPositiveSalary"></param>
		</item>
		<!-- 人员类别 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryStaffType"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentrystafftype/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentrystafftype/fdEntryStaffType"></param>
		</item>
		<!-- 工号 -->
		<item name="config">
			<param name="fieldIds" value="fdStaffNo"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdstaffno/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdstaffno/fdStaffNo"></param>
		</item>
		<!-- 入职指引人 -->
		<item name="config">
			<param name="fieldIds" value="fdTeacher"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdteacher/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdteacher/fdTeacher"></param>
		</item>
		<!-- 直接上级 -->
 		<item name="config">
			<param name="fieldIds" value="fdEntryLeader"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentryleader/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentryleader/fdEntryLeader"></param>
		</item>
		<!-- 工资账户名 -->
		<item name="config">
			<param name="fieldIds" value="fdSalaryAccountName"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdsalaryaccountname/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdsalaryaccountname/fdSalaryAccountName"></param>
		</item>
		<!-- 工资账号 -->
		<item name="config">
			<param name="fieldIds" value="fdSalaryAccountId"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/entry/fdsalaryaccountid/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdsalaryaccountid/fdSalaryAccountId"></param>
		</item>
		<!-- 办公地点,工资银行,公积金账号,社保账号-->
 		<item name="config">
			<param name="fieldIds" value="fdEntryWorkAddress,fdSalaryAccountBank,fdFundAccount,fdSocialSecurityAccount,fdNo"></param>
			<param name="required" value="false"></param>
		</item>
		<!-- 工作职责 -->
		<item name="config">
			<param name="fieldIds" value="fdEntryWorkDuty"></param>
			<param name="required" value="false"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/entry/fdentryworkduty/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/entry/fdentryworkduty/fdEntryWorkDuty"></param>
		</item>
	</extension>
</plugin>