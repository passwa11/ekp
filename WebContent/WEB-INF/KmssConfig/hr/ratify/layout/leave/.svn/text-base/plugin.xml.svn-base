		<item name="config">
			<param name="fieldIds" value="fdLeaveRealDate"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleaverealdate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleaverealdate/fdLeaveRealDate"></param>
		</item>
		<!-- 入职日期 -->
		<item name="config">
			<param name="fieldIds" value="fdLeaveEnterDate"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleaveenterdate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleaveenterdate/fdLeaveEnterDate"></param>
		</item>
		<!-- 离职原因 -->
		<item name="config">
			<param name="fieldIds" value="fdLeaveReason"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleavereason/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleavereason/fdLeaveReason"></param>
		</item>
		<!-- 部门领导意见-->
		<item name="config">
			<param name="fieldIds" value="fdLeaveLeaderView"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleaveleaderview/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleaveleaderview/fdLeaveLeaderView"></param>
		</item>
		<!-- 人力资源部门意见-->
 		<item name="config">
			<param name="fieldIds" value="fdLeaveHrView"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleavehrview/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleavehrview/fdLeaveHrView"></param>
		</item>
		<!-- 备注-->
 		<item name="config">
			<param name="fieldIds" value="fdLeaveRemark"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleaveremark/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleaveremark/fdLeaveRemark"></param>
		</item>
		<!-- 离职去向 -->
		<item name="config">
			<param name="fieldIds" value="fdNextCompany"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdnextcompany/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdnextcompany/fdNextCompany"></param>
		</item>
	</extension>
</plugin>