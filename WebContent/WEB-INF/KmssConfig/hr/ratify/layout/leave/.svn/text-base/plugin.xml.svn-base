<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../plugin.xsd ">
	<!-- 基本属性布局扩展 -->
	<extension point="com.landray.kmss.sys.xform.fieldlayout" model="com.landray.kmss.hr.ratify.model.HrRatifyLeave">
		 <!--离职人员 -->
	    <item name="config">
			<param name="fieldIds" value="fdLeaveStaff"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleavestaff/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleavestaff/fdLeaveStaff"></param>
		</item>
		<!--所属部门 -->
		<item name="config">
			<param name="fieldIds" value="fdLeaveDept"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleavedept/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleavedept/fdLeaveDept"></param>
		</item>
		<!-- 申请离职日期 -->
	    <item name="config">
			<param name="fieldIds" value="fdLeaveDate"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/leave/fdleavedate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/leave/fdleavedate/fdLeaveDate"></param>
		</item>
