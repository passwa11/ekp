<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../plugin.xsd ">
	<!-- 基本属性布局扩展 -->
	<extension point="com.landray.kmss.sys.xform.fieldlayout" model="com.landray.kmss.hr.ratify.model.HrRatifyRehire">
		 <!--返聘人员 -->
	    <item name="config">
			<param name="fieldIds" value="fdRehireStaff"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehirestaff/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehirestaff/fdRehireStaff"></param>
		</item>
		<!--返聘部门 -->
		<item name="config">
			<param name="fieldIds" value="fdRehireDept"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehiredept/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehiredept/fdRehireDept"></param>
		</item>
		<!--返聘岗位 -->
		<item name="config">
			<param name="fieldIds" value="fdRehirePosts"></param>
			<param name="required" value="true"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehirepost/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehirepost/fdRehirePost"></param>
		</item>
		<!-- 返聘日期 -->
	    <item name="config">
			<param name="fieldIds" value="fdRehireDate"></param>
			<param name="required" value="true"></param>
		    <param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehiredate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehiredate/fdRehireDate"></param>
		</item>
		<!-- 原入职日期 -->
		<item name="config">
			<param name="fieldIds" value="fdRehireEnterDate"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehireenterdate/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehireenterdate/fdRehireEnterDate"></param>
		</item>
		<!-- 返聘原因 -->
		<item name="config">
			<param name="fieldIds" value="fdRehireReason"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehirereason/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehirereason/fdRehireReason"></param>
		</item>
		<!-- 部门领导意见-->
		<item name="config">
			<param name="fieldIds" value="fdRehireLeaderView"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehireleaderview/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehireleaderview/fdRehireLeaderView"></param>
		</item>
		<!-- 人力资源部门意见-->
 		<item name="config">
			<param name="fieldIds" value="fdRehireHrView"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehirehrview/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehirehrview/fdRehireHrView"></param>
		</item>
		<!-- 备注-->
 		<item name="config">
			<param name="fieldIds" value="fdRehireRemark"></param>
			<param name="required" value="false"></param>
			<param name="jspParams" value="hr/ratify/fieldlayout/rehire/fdrehireremark/param.jsp"></param>
			<param name="jspRunProfix" value="hr/ratify/fieldlayout/rehire/fdrehireremark/fdRehireRemark"></param>
