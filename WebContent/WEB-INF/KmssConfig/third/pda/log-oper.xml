<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="pdaRowsPerPageConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/third/pda/pda_rows_per_page_config/pdaRowsPerPageConfig.do">
        </oper>
        <oper
        	id="pdaModuleLabelListOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.third.pda.model.PdaModuleLabelList"
        	action="/third/pda/pda_module_label_list/pdaModuleLabelList.do">
        	<query
				method="tainsit"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="button.view">
			</query>
        </oper>
        <oper
        	id="pdaModuleConfigMainOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/third/pda/pda_module_config_main/pdaModuleConfigMain.do">
        	<ignoreFields>authChangeEditorFlag</ignoreFields>
        	<query
        		method="selectIcon"
        		logPoint="Base_UrlParam"
        		messageKey="third-pda:pdaModuleConfigMain.oper.selectIcon">
        	</query>
        	<query
        		method="open"
        		logPoint="Base_UrlParam;Action_Find"
        		messageKey="button.view">
        	</query>
        	<query
        		method="updateIconVersion"
        		logPoint="Base_UrlParam"
        		messageKey="third-pda:pdaModuleConfigMain.oper.updateIconVersion">
        	</query>
        	<query
        		method="updateStatus"
        		logPoint="Base_UrlParam;updateStatus"
        		messageKey="third-pda:pdaModuleConfigMain.oper.updateStatus">
        	</query>
        </oper>
        <oper
        	id="pdaModuleCateOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.third.pda.model.PdaModuleCate"
        	action="/third/pda/pda_module_cate/pdaModuleCate.do">
        	<query
        		method="criteria"
        		logPoint="Base_UrlParam;Action_FindAll"
        		messageKey="button.list">
        	</query>
        </oper>
        <oper
        	id="pdaHomePageConfigOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.third.pda.model.PdaHomePageConfig"
        	action="/third/pda/pda_home_page_cfg/pdaHomePageConfig.do">
        	<query
        		method="updateStatus"
        		logPoint="Base_UrlParam;updateStatus"
        		messageKey="third-pda:pdaModuleConfigMain.oper.updateStatus">
        	</query>
        </oper>
        <oper
        	id="pdaLicenseConfigOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/third/pda/third_pda_config/pdaLicenseConfig.do">
        	<query
        		method="config"
        		logPoint="Base_UrlParam"
        		messageKey="third-pda:pdaLicenseConfig.oper.config">
        	</query>
        </oper>
        <oper
        	id="pdaMsgPushMemOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/third/pda/third_pda_config/pdaMsgPushMem.do">
        	<query
        		method="collectMsg|changeMsg"
        		logPoint="Base_UrlParam;collectMsg"
        		messageKey="third-pda:pdaMsgPushMem.oper.collectMsg">
        	</query>
        </oper>
        <oper
        	id="pdaMsgPushInfoOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/third/pda/third_pda_config/pdaMsgPushInfo.do">
        	<query
        		method="list"
        		logPoint="Base_UrlParam;getTodoList"
        		messageKey="third-pda:pdaMsgPushInfo.oper.list">
        	</query>
        </oper>
        <oper
            id="thirdPdaConfigOperLog"
            extends="sysAppConfigOperLog"
            model="*"
            action="/third/pda/third_pda_config/thirdPdaConfig.do">
            <query
            	method="configVersion"
            	logPoint="Base_UrlParam;configVersion"
            	messageKey="third-pda:thirdPdaConfig.oper.configVersion">
            </query>
        </oper>
        <oper
        	id="pdaSysNotifyTodoOperLog"
        	extends="sysNotifyTodoOperLog"
        	model="*"
        	action="/third/pda/sys_notify_todo/sysNotifyTodo.do">
        </oper>
        <oper
        	id="pdaTabViewConfigMainOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/third/pda/pda_tabview_config_main/pdaTabViewConfigMain.do">
        	<query
        		method="getTabJSON"
        		logPoint="Base_UrlParam;getTabJSON"
        		messageKey="third-pda:pdaTabViewConfigMain.oper.getTabJSON">
        	</query>
        	<query
        		method="getTabJSONFromDB"
        		logPoint="Base_UrlParam;getTabJSONFromDB"
        		messageKey="third-pda:pdaTabViewConfigMain.oper.getTabJSONFromDB">
        	</query>
        	<query
        		method="updateStatus"
        		logPoint="Base_UrlParam;updateStatus"
        		messageKey="third-pda:pdaTabViewConfigMain.oper.updateStatus">
        	</query>
        </oper>
	</logset>
</log-oper>