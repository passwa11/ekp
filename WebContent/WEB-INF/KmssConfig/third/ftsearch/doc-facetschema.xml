    <!-- 定义字段facet属性 -->
    <facets>
    	<!-- 类别  层次 单选-->
    	<facet>
    		<!-- 显示名称  -->
    		<dname>分类</dname>
    		<name>catePath</name>
    		<indexfield>
    			<name>catePath</name>
    		</indexfield>
    		<type>path</type>
    		<selectionOps>Single</selectionOps>
    		<rootFacet>rootCate</rootFacet>
    	</facet>
    	<facet>
    		<name>rootCate</name>
    		<indexfield>
    			<name>rootCate</name>
    		</indexfield>
    		<type>simple</type>
    		<selectionOps>Single</selectionOps>
    		<!-- 排序   可空 -->
    		<sort>OrderCountAsc</sort>
    		<maxSize>100</maxSize>
    	</facet>
    	<!-- 阅读数  range 单选  -->
    	<facet>
    		<dname>阅读次数</dname>
    		<name>readCount</name>
    		<indexfield>
    			<name>readCount</name>
    		</indexfield>
    		<type>range</type>
    		<selectionOps>Single</selectionOps>
    		<values>
    			<kv>
    				<key>5次以下</key>
    				<value>-,5</value>
    			</kv>
    			<kv>
    				<key>6~10次</key>
    				<value>6,10</value>
    			</kv>
    			<kv>
    				<key>11~15次</key>
    				<value>11,15</value>
    			</kv>
    			<kv>
    				<key>16~20次</key>
    				<value>16,20</value>
    			</kv>
    			<kv>
    				<key>21次以上</key>
    				<value>21,-</value>
    			</kv>
    		</values>
    	</facet> 
    	<!-- 评分  range 单选  -->
    	<facet>
    		<dname>评分</dname>
