    		<selectionOps>Single</selectionOps>
    		<!--  -->
    		<values>
    			<kv>
    				<key>5星</key>
    				<value>5,-</value>
    			</kv>
    			<kv>
    				<key>4星以上</key>
    				<value>4,-</value>
    			</kv>
    			<kv>
    				<key>3星以上</key>
    				<value>3,-</value>
    			</kv>
    			<kv>
    				<key>2星以上</key>
    				<value>2,-</value>
    			</kv>
    			<kv>
    				<key>1星以上</key>
    				<value>1,-</value>
    			</kv>
    		</values>
    	</facet> 
    	<!-- 发布日期 -->
    	<facet>
    		<dname>发布时间</dname>
    		<name>publishTime</name>
    		<indexfield>
