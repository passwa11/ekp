<?xml version="1.0" encoding="UTF-8"?>
<root xmlns="http://www.landray.com.cn/schema/lui" xmlns:degign="http://www.landray.com.cn/schema/design"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.landray.com.cn/schema/lui
	../../../sys/ui/lui.xsd
	http://www.landray.com.cn/schema/design
	../../../design-xml.xsd">

	<!-- 统一搜索 -->
	<render id="third.ftsearch.search.rd" name="搜索框模板" format="third.ftsearch.search" css="/third/ftsearch/third_ftsearch_portlet/style/third_ftsearch_portlet_back_search.css" type="Javascript">
		<code src="/third/ftsearch/third_ftsearch_portlet/js/third_ftsearch_portlet_back_search.js"/>
		<vars>
            <var name="{third-ftsearch:third.ftsearch.showHot.tip}" key="showHot" kind="enumValue" default="true">
		    	{"values":[{"text":"{third-ftsearch:third.ftsearch.showHot.yes}", "value":"true"},
		    				{"text":"{third-ftsearch:third.ftsearch.showHot.no}", "value":"false"},
   						  ],
		    	 "showType":"radio"}
			</var>
            <var key="img" kind="custom" name="{sys-ui:material.picture.back}" >
                {"file":"/sys/ui/extend/varkind/material.jsp"}
            </var>
        </vars>
