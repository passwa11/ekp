<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="cpEndpointOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/wx/cpEndpoint.do">
            <query
				method="service" 
				logPoint="Base_UrlParam;service"/>
        </oper>
        <oper
            id="wxMenuOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.spi.model.WxMenuModel"
            action="/third/wx/menu/wxMenu.do">
            <query
				method="publish" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
			<query
				method="save" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.save"/>
        </oper>
		<oper
            id="wxMenuDefineOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.spi.model.WxMenuModel"
            action="/third/wx/menu/wxMenuDefine.do">
            <query
				method="checkLen" 
				logPoint="Base_UrlParam;checkLen"
				messageKey="third-weixin:third.wxMenuDefine.oper.checkLen"/>
			<query
				method="appList" 
				logPoint="Base_UrlParam;appList"
				messageKey="button.list"/>
			<query
				method="publish" 
				logPoint="Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
        </oper>
		<oper
            id="wxAttachmentOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/weixin/attachment.do">
        	<query
				method="download" 
				logPoint="Base_UrlParam;download"
				messageKey="third-weixin:third.wxAttacment.oper.download"/>
			<query
				method="downloadWxPic" 
				logPoint="Base_UrlParam;downloadWxPic"
				messageKey="third-weixin:third.wxAttacment.oper.downloadWxPic"/>
        </oper>  
        
        <oper
            id="wxJsapiOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wx/jsapi/wxJsapi.do">
        	<query
				method="jsapiSignature" 
				logPoint="Base_UrlParam;jsapiSignature"
				messageKey="third-weixin:third.wxJsapi.oper.jsapiSignature"/>
        </oper>
        
        <oper
            id="weixinSynchroOrgCheckOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wx/weixinSynchroOrgCheck.do">
        	<query
				method="agentIdCheck" 
				logPoint="Base_UrlParam;agentIdCheck"/>
			<query
				method="checkData" 
				logPoint="Base_UrlParam;checkData"
				messageKey="third-weixin:third.weixinSynchroOrgCheck.oper.checkData"/>
			<query
				method="cleanTime" 
				logPoint="Base_UrlParam;cleanTime"
				messageKey="third-weixin:third.weixinSynchroOrgCheck.oper.cleanTime"/>
			<query
				method="urlCode" 
				logPoint="Base_UrlParam;urlCode"
				messageKey="third-weixin:third.weixin.oper.urlCode"/>
        </oper>
        
        <oper
            id="pcScanLoginOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wx/pcScanLogin.do">
        	<query
				method="service" 
				logPoint="Base_UrlParam;Action_Find"
				messageKey="third-weixin:third.wxPcScanLogin.oper.service"/>
        </oper> 
        
        <oper
            id="thirdWxOmsInitOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/weixin/third_wx_oms_init/thirdWxOmsInit.do">
        	<query
				method="ekpUpdate" 
				logPoint="Base_UrlParam;updateEKP"/>
			<query
				method="omsInit" 
				logPoint="Base_UrlParam;updateDept;updatePerson"/>
			<query
				method="wxDel" 
				logPoint="Base_UrlParam;Service_Delete;updateWx"
				messageKey="button.delete"/>
        </oper>
        
        <oper
            id="wxOmsRelationOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.spi.model.WxOmsRelationModel"
            action="/third/weixin/spi/wx_oms_relation/wxOmsRelation.do">
        	<query
				method="addOrUpdateCheck" 
				logPoint="Base_UrlParam;handle"
				messageKey="button.save"/>
			<query
				method="loadExcel" 
				logPoint="Base_UrlParam;addExcel"
				messageKey="button.import"/>
        </oper>     
        
        <oper
            id="userOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.spi.model.WxOmsRelationModel"
            action="/third/weixin/user.do">
        	<query
				method="getUserId" 
				logPoint="Base_UrlParam;Action_FindAll"/>
        </oper>
        
        <oper
            id="thirdeWixinDomainOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/weixin/domain.do">
        	<query
				method="upload" 
				logPoint="Base_UrlParam;upload"
				messageKey="common.fileUpLoad.upload"/>
        </oper> 
        
        <oper
            id="thirdWeixinOmsPostOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/weixin/third_weixin_oms_post/thirdWeixinOmsPost.do">
        </oper> 
	</logset>
</log-oper>