        	<query
				method="appList" 
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="button.list"/>
        </oper>
        
        <oper
            id="wxworkmutilpcScanLoginOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wxwork/mutil/pcScanLogin.do">
        	<query
				method="service" 
				logPoint="Base_UrlParam;Action_Find"
				messageKey="third-weixin:third.wxPcScanLogin.oper.service"/>
        </oper> 
        
        <oper
            id="thirdWxworkmutilOmsInitOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/weixin/mutil/third_wxwork_oms_init/thirdWxworkOmsInit.do">
