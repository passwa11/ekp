<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="wxworkmutilcpEndpointOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/wxwork/mutil/cpEndpoint.do">
            <query
				method="service" 
				logPoint="Base_UrlParam;service"/>
        </oper>
        <oper
            id="wxworkmutilMenuOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.mutil.spi.model.WxworkMutilMenuModel"
            action="/third/wxwork/mutil/menu/wxworkMenu.do">
            <query
				method="publish" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
			<query
				method="save" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.save"/>
        </oper>
		<oper
            id="wxworkmutilMenuDefineOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.mutil.spi.model.WxworkMutilMenuModel"
            action="/third/wxwork/mutil/menu/wxworkMenuDefine.do">
            <query
				method="checkLen" 
				logPoint="Base_UrlParam;checkLen"
				messageKey="third-weixin:third.wxMenuDefine.oper.checkLen"/>
			<query
				method="publish" 
				logPoint="Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
        </oper>
		<oper
            id="wxworkmutilAttachmentOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/wxwork/mutil/attachment.do">
