<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="wxworkcpEndpointOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/wxwork/cpEndpoint.do">
            <query
				method="service" 
				logPoint="Base_UrlParam;service"/>
        </oper>
        <oper
            id="wxworkMenuOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.work.spi.model.WxworkMenuModel"
            action="/third/wxwork/menu/wxworkMenu.do">
            <query
				method="publish" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
			<query
				method="save" 
				logPoint="Service_Add;Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.save"/>
        </oper>
		<oper
            id="wxworkMenuDefineOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.work.spi.model.WxworkMenuModel"
            action="/third/wxwork/menu/wxworkMenuDefine.do">
            <query
				method="checkLen" 
				logPoint="Base_UrlParam;checkLen"
				messageKey="third-weixin:third.wxMenuDefine.oper.checkLen"/>
			<query
				method="publish" 
				logPoint="Service_Update"
				messageKey="third-weixin:third.wx.menu.btn.publish"/>
        </oper>
		<oper
            id="wxworkAttachmentOperLog"
            extends="BaseOperLog"
            model="*"
            action="/resource/third/wxwork/attachment.do">
        	<query
				method="download" 
				logPoint="Base_UrlParam;download"
				messageKey="third-weixin:third.wxAttacment.oper.download"/>
			<query
				method="downloadWxPic" 
				logPoint="Base_UrlParam;downloadWxPic"
				messageKey="third-weixin:third.wxAttacment.oper.downloadWxPic"/>
        </oper>  
        
        <oper
            id="wxworkJsapiOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wxwork/jsapi/wxJsapi.do">
        	<query
				method="jsapiSignature" 
				logPoint="Base_UrlParam;jsapiSignature"
				messageKey="third-weixin:third.wxJsapi.oper.jsapiSignature"/>
        </oper>
        
        <oper
            id="wxworkweixinSynchroOrgCheckOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wxwork/weixinSynchroOrgCheck.do">
			<query
				method="checkData" 
				logPoint="Base_UrlParam;checkData"
				messageKey="third-weixin:third.weixinSynchroOrgCheck.oper.checkData"/>
			<query
				method="cleanTime" 
				logPoint="Base_UrlParam;cleanTime"
				messageKey="third-weixin:third.weixinSynchroOrgCheck.oper.cleanTime"/>
        </oper>
        
        <oper
            id="thirdWeixinWorkOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.work.model.ThirdWeixinWork"
            action="/third/weixin/work/third_weixin_work/thirdWeixinWork.do">
        	<query
				method="appList" 
				logPoint="Base_UrlParam;Action_FindAll"
				messageKey="button.list"/>
        </oper>
        
        <oper
            id="wxworkpcScanLoginOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/wxwork/pcScanLogin.do">
        	<query
				method="service" 
				logPoint="Base_UrlParam;Action_Find"
				messageKey="third-weixin:third.wxPcScanLogin.oper.service"/>
        </oper> 
        
        <oper
            id="thirdWxworkOmsInitOperLog"
            extends="BaseOperLog"
            model="*"
            action="/third/weixin/work/third_wxwork_oms_init/thirdWxworkOmsInit.do">
        	<query
				method="ekpUpdate" 
				logPoint="Base_UrlParam;updateEKP"/>
			<query
				method="omsInit" 
				logPoint="Base_UrlParam;updateDept;updatePerson"/>
			<query
				method="wxDel" 
				logPoint="Base_UrlParam;Service_Delete;updateWx"
				messageKey="button.delete"/>
        </oper>
        
        <oper
            id="wxworkOmsRelationOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.work.spi.model.WxworkOmsRelationModel"
            action="/third/weixin/work/spi/wxwork_oms_relation/wxworkOmsRelation.do">
        	<query
				method="addOrUpdateCheck" 
				logPoint="Base_UrlParam;handle"
				messageKey="button.save"/>
			<query
				method="loadExcel" 
				logPoint="Base_UrlParam;addExcel"
				messageKey="button.import"/>
        </oper>     
        
        <oper
            id="wxworkuserOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.third.weixin.work.spi.model.WxworkOmsRelationModel"
            action="/third/weixin/work/user.do">
        	<query
				method="getUserId" 
				logPoint="Base_UrlParam;Action_FindAll"/>
        </oper>
        
	</logset>
</log-oper>