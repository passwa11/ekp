<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

	<formset>
		<form name="kmSmissiveTemplateForm">
			<field
				property="fdName"
				depends="required,maxlength">
				<arg
					key="kmSmissiveTemplate.fdName"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			
			<field
				property="fdTmpTitle"
				depends="required,maxlength">
				<arg
					key="kmSmissiveTemplate.fdTmpTitle"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			<field
				property="fdOrder"
				depends="integer">
				<arg
					key="model.fdOrder"
					position="0" />
			</field>
		</form>

		<form name="kmSmissiveMainForm">
			<field
				property="fdTemplateId"
				depends="required,maxlength">
				<arg
					key="kmSmissiveMain.fdTemplateId"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>36</var-value>
				</var>
			</field>
			<field
				property="fdTitle"
				depends="required,maxlength">
				<arg
					key="kmSmissiveMain.fdTitle"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			<field
				property="docSubject"
				depends="required,maxlength">
				<arg
					key="kmSmissiveMain.docSubject"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			<field
				property="docAuthorId"
				depends="required,maxlength">
				<arg
					key="kmSmissiveMain.docAuthorId"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>36</var-value>
				</var>
			</field>
			<field
				property="fdMainDeptId"
				depends="required,maxlength">
				<arg
					key="kmSmissiveMain.fdMainDeptId"
					bundle="km-smissive"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>36</var-value>
				</var>
			</field>
