<?xml version="1.0" encoding="UTF-8"?>
<configs
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd ">

	<module
		messageKey="km-imeeting:module.km.imeeting"
		urlPrefix="/km/imeeting/"
		defaultValidator="roleValidator(role=ROLE_KMIMEETING_DEFAULT)">
		<roles>
			ROLE_KMIMEETING_DEFAULT;
			ROLE_KMIMEETING_SETTING;
			ROLE_KMIMEETING_SUMMARY_PRINT;
			ROLE_KMIMEETING_CIRCULATION_CREATE;
			ROLE_KMIMEETING_DATAMNG;
		</roles>
		<areaRoles>
			ROLE_KMIMEETING_CHANGE;
			ROLE_KMIMEETING_CANCEL;
			ROLE_KMIMEETING_OPTALL;
			ROLE_KMIMEETING_CHANGETEMPLATE;
			ROLE_KMIMEETING_CHANGRIGHT;
			ROLE_KMIMEETING_CREATE;
			ROLE_KMIMEETING_TOPIC_CREATE;
			ROLE_KMIMEETING_DELETE;
			ROLE_KMIMEETING_READER;
			ROLE_KMIMEETING_CATEGORY_MAINTAINER;
			ROLE_KMIMEETING_MANUALPUBLISH;
			<!-- ROLE_KMIMEETING_PRINT_NOTIFY;
			ROLE_KMIMEETING_PRINT_FEEDBACK; -->
			
		    ROLE_KMIMEETING_CIRCULATION_DELETE;
			ROLE_KMIMEETING_BOOKCREATE;
			ROLE_KMIMEETING_BOOKUPDATE;
			ROLE_KMIMEETING_BOOKDELETE;
			ROLE_KMIMEETING_SUMMARY_CREATE_WITHMEETING;
			ROLE_KMIMEETING_SUMMARY_CREATE;
			ROLE_KMIMEETING_SUMMARY_EDITOR;
			ROLE_KMIMEETING_SUMMARY_READER;
			
			ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER;
			ROLE_KMIMEETING_RES_CREATE;
			ROLE_KMIMEETING_RES_READER;
			ROLE_KMIMEETING_RES_EDITOR;
			ROLE_KMIMEETING_RES_DELETE;
			ROLE_KMIMEETING_STAT_MAINTAINER;
			ROLE_KMIMEETING_STAT_READER;
			ROLE_KMIMEETING_TRANSPORT_EXPORT;
			ROLE_KMIMEETING_BACKSTAGE_MANAGER;
			ROLE_KMIMEETING_FEEDBACK_TRANSPORT_EXPORT;
			ROLE_KMIMEETING_SUBSIDE_FILE;
			ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER;
			ROLE_KMIMEETING_TOPIC_EDITOR;
			ROLE_KMIMEETING_TOPIC_DELETE;
			ROLE_KMIMEETING_TOPIC_TRANSPORT_EXPORT		
		</areaRoles>
		<request
			path="tree.jsp*"
			defaultValidator="roleValidator(role=ROLE_KMIMEETING_BACKSTAGE_MANAGER)"/>
		<!-- 议题分类 -->
		<request
			path="km_imeeting_topic_category/kmImeetingTopicCategory.do*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingTopicCategory,adminRoleName=ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER">
			<!-- 保存 -->
			<query
				queryString="method=add"
				validator="sysSimpleCategoryEditorValidator(recid=parentId)" />
			<!-- 阅读 -->
			<query
				queryString="method=view"
				validator="sysSimpleCategoryEditorValidator()|sysSimpleCategoryReaderValidator()" />
			<!-- 编辑和删除 -->
			<query
				queryString="method=edit|update|delete"
				validator="sysSimpleCategoryEditorValidator()" />
			<query
				queryString="method=copy"
				validator="sysSimpleCategoryEditorValidator(recid=fdCopyId)" />
		</request>
		
		<!-- 会议室资源分类 -->
		<request
			path="km_imeeting_res_category/kmImeetingResCategory*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingResCategory,adminRoleName=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER">
			<!-- 新增-->
			<query
				queryString="method=add"
				validator="sysSimpleCategoryEditorValidator(recid=parentId)" />
			<!-- 阅读 -->
			<query
				queryString="method=view"
				validator="sysSimpleCategoryEditorValidator()|sysSimpleCategoryReaderValidator()" />
			<!-- 编辑和删除 -->
			<query
				queryString="method=edit|update|delete"
				validator="sysSimpleCategoryEditorValidator()" />
			<!-- 复制 -->
			<query
				queryString="method=copy"
				validator="sysSimpleCategoryEditorValidator(recid=fdCopyId)" />
		</request>
		
		<request
			path="km_imeeting_topic/kmImeetingTopic.do*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingTopic,cateModel=com.landray.kmss.km.imeeting.model.KmImeetingTopicCategory,adminRoleName=ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER">
			<query
				queryString="method=add&amp;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_CREATE)&amp;authSimpleCategoryReaderValidator(cateId=fdTemplateId)" />
			<!-- 新增 -->
			<query
				queryString="method=add"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_CREATE)" />
			<!-- 保存 -->
			<query
				queryString="method=save&amp;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_CREATE)&amp;authSimpleCategoryReaderValidator(cateId=fdTemplateId)" />
			<!-- 保存 -->
			<query
				queryString="method=save|saveadd"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_CREATE)&amp;authSimpleCategoryReaderValidator(cateId=fdTemplateId)" />
			<!-- 阅读 -->
			<query
				queryString="method=view"
				validator="authFieldValidator(type=SYS_READER)|authSimpleCategoryEditorValidator(property=fdTopicCategory)" />
			<!-- 编辑 -->
			<query
				queryString="method=edit"
				validator="authFieldValidator(type=SYS_EDITOR)|fieldValueValidator(fieldproperty=docStatus,value=30)&amp;areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_EDITOR)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(property=fdTopicCategory)]" />
			<query
				queryString="method=delete"
				validator="fieldValueValidator(fieldproperty=docStatus,value=10)&amp;authFieldValidator(type=creator)|areaRoleValidator(role=ROLE_KMIMEETING_TOPIC_DELETE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(property=fdTopicCategory)]" />
			<!-- 删除所有其他视图 -->
			<query
				queryString="method=deleteall"
				validator="roleValidator(role=ROLE_KMIMEETING_TOPIC_DELETE)&amp;[roleValidator(role=ROLE_KMIMEETING_TOPICCATEGORY_MAINTAINER)|roleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(cateId=categoryId)]" />
			<!-- 文档维护 -->
			<query
				queryString="method=manageList"
				validator="authSimpleCategoryEditorValidator(cateId=categoryId)" />
		</request>
		<!-- 会议室资源 -->
		<request
			path="km_imeeting_res/kmImeetingRes*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingRes,cateModel=com.landray.kmss.km.imeeting.model.KmImeetingResCategory">
			<query
				queryString="method=addSyncToBoen"
				validator="roleValidator(role=ROLE_KMIMEETING_SETTING)" />
			<!-- 指定模板下新增 -->
			<query
				queryString="method=add&amp;categoryId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CREATE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(cateId=categoryId)]" />
			<!-- 新增 -->
			<query
				queryString="method=add"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CREATE)" />
			<!-- 保存 -->
			<query
				queryString="method=save&amp;categoryId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CREATE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(cateId=categoryId)]" />
			<!-- 保存 -->
			<query
				queryString="method=save|saveadd"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CREATE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(cateId=categoryId)]" />
			<!-- 阅读 -->
			<query
				queryString="method=view"
				validator="authFieldValidator(type=SYS_READER,isArea=true)|areaRoleValidator(role=ROLE_KMIMEETING_RES_READER)" />
			<!-- 编辑 -->
			<query
				queryString="method=edit"
				validator="authFieldValidator(type=SYS_EDITOR)|areaRoleValidator(role=ROLE_KMIMEETING_RES_EDITOR)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(property=docCategory)]" />
			<!-- 文档中的删除 -->
			<query
				queryString="method=delete"
				validator="authFieldValidator(type=SYS_EDITOR)|areaRoleValidator(role=ROLE_KMIMEETING_RES_DELETE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(property=docCategory)]" />
			<!-- 删除所有其他视图 -->
			<query
				queryString="method=deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_DELETE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|authSimpleCategoryEditorValidator(cateId=categoryId)]" />
			<query
				queryString="method=listUse"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_READER)" />
		</request>
		
		<!-- 会议辅助服务 -->
		<request
			path="km_imeeting_device/kmImeetingDevice*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingDevice">
			<!-- 维护 -->
			<query
				queryString="method=add|deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)" />
			<query
				queryString="method=view"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|authFieldValidator(type=SYS_READER)" />
			<query
				queryString="method=save|saveadd|edit|delete"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|authFieldValidator(type=SYS_EDITOR)" />
		</request>
		
		<!-- 会议辅助设备 -->
		<request
			path="km_imeeting_equipment/kmImeetingEquipment*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingEquipment">
			<!-- 维护 -->
			<query
				queryString="method=add|deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)" />
			<query
				queryString="method=view"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|authFieldValidator(type=SYS_READER)" />
			<query
				queryString="method=save|saveadd|edit|delete"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_RES_CATEGORY_MAINTAINER)|authFieldValidator(type=SYS_EDITOR)" />
		</request>
		
		<!-- 会议模板 -->
		<request
			path="km_imeeting_template/kmImeetingTemplate*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingTemplate">
			<query
				queryString="method=addSyncToBoen"
				validator="roleValidator(role=ROLE_KMIMEETING_SETTING)" />
			<!-- 保存 -->
			
			
			<query
				queryString="method=save|saveadd"
				validator="authCategoryReaderValidator(cateid=docCategoryId)" />
			<!-- 阅读 -->
			<query
				queryString="method=view"
				validator="authFieldValidator(type=SYS_READER)|authCategoryEditorValidator(property=docCategory)" />
			<!-- 编辑和删除 -->
			<query
				queryString="method=edit|update|delete"
				validator="authFieldValidator(type=SYS_EDITOR)|authCategoryEditorValidator(property=docCategory)" />
			<!-- 删除所有 -->
			<query
				queryString="method=deleteall"
				validator="authCategoryEditorValidator(cateid=parentId)" />
		</request>
		
		<!-- 会议室预约 -->
		<request
			path="km_imeeting_book/kmImeetingBook*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingBook">
			<!-- 保存 -->
			<query
				queryString="method=add|save"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_BOOKCREATE)|authFieldValidator(type=creator)" />
			<!-- 更新 -->
			<query
				queryString="method=edit|update"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_BOOKUPDATE)|authFieldValidator(type=creator)" />
			<!-- 删除 -->
			<query
				queryString="method=delete"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_BOOKDELETE)|authFieldValidator(type=creator)" />
			<!-- 删除所有 -->
			<query
				queryString="method=deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_BOOKDELETE)" />
		</request>
		
		<request
			path="km_imeeting_main/kmImeetingMain*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingMain,templatemodel=com.landray.kmss.km.imeeting.model.KmImeetingTemplate">
			<!-- 发送会议通知单 -->
			<query
				queryString="method=sendNotify"
				validator="fieldValueValidator(fieldproperty=docStatus,value=30)&amp;[authFieldValidator(type=creator)|authFieldValidator(type=emcee)]" />
			<!-- 复制会议安排 -->
			<query
				queryString="method=add&amp;copyMeeting=*"
				validator="fieldValueValidator(recid=meetingId,fieldproperty=docStatus,value=30)&amp;authFieldValidator(recid=meetingId,type=creator)&amp;areaRoleValidator(role=ROLE_KMIMEETING_CREATE)" />
			<!-- 新建会议安排 -->
			<query
				queryString="method=add&amp;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_CREATE)&amp;authCategoryReaderValidator(templateid=fdTemplateId)" />
			<!-- 保存会议安排 -->
			<query
				queryString="method=save;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_CREATE)&amp;authCategoryReaderValidator(templateid=fdTemplateId)" />
			<query
				queryString="method=saveDraft;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_CREATE)&amp;authCategoryReaderValidator(templateid=fdTemplateId)" />	
			<!-- 阅读会议安排 -->
			<query
				queryString="method=view"
				validator="authFieldValidator(type=SYS_READER,isArea=true)|authCategoryEditorValidator(property=fdTemplate)" />
			<!-- 编辑、更新会议安排 -->
			<query
				queryString="method=edit"
				validator="fieldValueValidator(fieldproperty=docStatus,value=10;11)&amp;authFieldValidator(type=creator)|authFieldValidator(type=SYS_EDITOR)" />
			<!-- 删除会议安排 -->
			<query
				queryString="method=delete"
				validator="fieldValueValidator(fieldproperty=docStatus,value=10)&amp;authFieldValidator(type=creator)|areaRoleValidator(role=ROLE_KMIMEETING_DELETE)&amp;[authCategoryEditorValidator(property=fdTemplate,role=ROLE_KMIMEETING_OPTALL)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)]" />
			<!-- 删除所有会议安排草稿视图 -->
			<query
				queryString="method=deleteall&amp;status=10"
				validator="roleValidator(role=ROLE_KMIMEETING_DEFAULT)" />
			<!-- 删除所有会议安排其他视图 -->
			<query
				queryString="method=deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_DELETE)&amp;[fieldValueValidator(field=nodeType,value=CATEGORY)&amp;authCategoryEditorValidator(cateid=categoryId)|fieldValueValidator(field=nodeType,value=TEMPLATE)&amp;authCategoryEditorValidator(templateid=categoryId)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)]" />
			<!-- 催办会议 -->
			<query
				queryString="method=hastenMeeting"
				validator="[authFieldValidator(type=creator)|authFieldValidator(type=emcee)]&amp;fieldValueValidator(fieldproperty=docStatus,value=30)&amp;fieldValueValidator(fieldproperty=isNotify,value=true)" />
			<!-- 取消会议安排 -->
			<query
				queryString="method=cancelMeeting"
				validator="[authFieldValidator(type=creator)|authFieldValidator(type=emcee)|areaRoleValidator(role=ROLE_KMIMEETING_CANCEL)]&amp;fieldValueValidator(fieldproperty=docStatus,value=30)" />
			<!-- 会议变更 -->
			<query
				queryString="method=addSyncToKk"
				validator="[authFieldValidator(type=creator)|authFieldValidator(type=emcee)]&amp;fieldValueValidator(fieldproperty=docStatus,value=30)" />
			
			<!-- 会议变更 -->
			<query
				queryString="method=changeMeeting"
				validator="[authFieldValidator(type=creator)|authFieldValidator(type=emcee)|areaRoleValidator(role=ROLE_KMIMEETING_CHANGE)]&amp;fieldValueValidator(fieldproperty=docStatus,value=10;30)" />
			<!-- 会议提前结束 -->
			<query
				queryString="method=earlyEndMeeting|earlyEndMeetingMobile"
				validator="[authFieldValidator(type=creator)|authFieldValidator(type=emcee)|areaRoleValidator(role=ROLE_KMIMEETING_CANCEL)|areaRoleValidator(role=ROLE_KMIMEETING_CHANGE)]&amp;fieldValueValidator(fieldproperty=docStatus,value=30)" />
			<!-- 维护界面 -->
			<query
				queryString="method=manageList"
				validator="fieldValueValidator(field=nodeType,value=CATEGORY)&amp;authCategoryEditorValidator(cateid=categoryId)|fieldValueValidator(field=nodeType,value=TEMPLATE)&amp;authCategoryEditorValidator(templateid=categoryId)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)" />
			<!-- 转移模板按钮 -->
			<query
				queryString="method=changeTemplate&amp;nodeType=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_CHANGETEMPLATE)&amp;[fieldValueValidator(field=nodeType,value=CATEGORY)&amp;authCategoryEditorValidator(cateid=categoryId)|fieldValueValidator(field=nodeType,value=TEMPLATE)&amp;authCategoryEditorValidator(templateid=categoryId)|areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)]" />
			<!-- 转移模板提交 -->
			<query
				queryString="method=changeTemplate&amp;categoryId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_CHANGETEMPLATE)" />
			<query
				queryString="method=uploadTmpAttachment"
				validator="authFieldValidator(type=creator)|authFieldValidator(type=emcee)" />	
		</request>
		
		<!-- 会议纪要文档权限 -->
		<request
			path="km_imeeting_summary/kmImeetingSummary*"
			validatorParameter="recid=fdId,model=com.landray.kmss.km.imeeting.model.KmImeetingSummary,templatemodel=com.landray.kmss.km.imeeting.model.KmImeetingTemplate">
			<query
				queryString="method=operateSummary"
				validator="[authFieldValidator(type=summary,recid=meetingId,model=com.landray.kmss.km.imeeting.model.KmImeetingMain)|areaRoleValidator(role=ROLE_KMIMEETING_SUMMARY_CREATE_WITHMEETING)]&amp;fieldValueValidator(recid=meetingId,model=com.landray.kmss.km.imeeting.model.KmImeetingMain,fieldproperty=docStatus,value=30)" />
			<!-- 新建 -->
			<query
				queryString="method=add&amp;meetingId=*"
				validator="[authFieldValidator(type=summary,recid=meetingId,model=com.landray.kmss.km.imeeting.model.KmImeetingMain)|areaRoleValidator(role=ROLE_KMIMEETING_SUMMARY_CREATE_WITHMEETING)]&amp;fieldValueValidator(recid=meetingId,model=com.landray.kmss.km.imeeting.model.KmImeetingMain,fieldproperty=docStatus,value=30)" />
			<!-- 新建会议纪要 -->
			<query
				queryString="method=add&amp;fdTemplateId=*"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_SUMMARY_CREATE)&amp;authCategoryReaderValidator(templateid=fdTemplateId)" />
			<!-- 阅读会议纪要 -->
			<query
				queryString="method=view"
				validator="authFieldValidator(type=SYS_READER,isArea=true)|authCategoryEditorValidator(property=fdTemplate)" />
			<!-- 打印会议纪要 -->
			<query
				queryString="method=print"
				validator="roleValidator(role=ROLE_KMIMEETING_SUMMARY_PRINT)" />
			<!-- 编辑会议纪要 -->
			<query
				queryString="method=edit|update"
				validator="fieldValueValidator(fieldproperty=docStatus,value=10;11)&amp;authFieldValidator(type=creator)|authFieldValidator(type=SYS_EDITOR)|fieldValueValidator(fieldproperty=docStatus,value=30)&amp;areaRoleValidator(role=ROLE_KMIMEETING_SUMMARY_EDITOR)&amp;[authCategoryEditorValidator(property=fdTemplate,role=ROLE_KMIMEETING_OPTALL)|roleValidator(role=ROLE_MEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)]" />
			<!-- 删除会议纪要 -->
			<query
				queryString="method=delete"
				validator="fieldValueValidator(fieldproperty=docStatus,value=10)&amp;authFieldValidator(type=creator)|areaRoleValidator(role=ROLE_KMIMEETING_DELETE)&amp;[authCategoryEditorValidator(property=fdTemplate,role=ROLE_KMIMEETING_OPTALL)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)]" />
			<!-- 删除所有会议纪要草稿视图 -->
			<query
				queryString="method=deleteall&amp;status=10"
				validator="roleValidator(role=ROLE_KMIMEETING_DEFAULT)" />
			<!-- 删除所有会议纪要其他视图 -->
			<query
				queryString="method=deleteall"
				validator="areaRoleValidator(role=ROLE_KMIMEETING_DELETE)&amp;[areaRoleValidator(role=ROLE_KMIMEETING_OPTALL)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)|fieldValueValidator(field=nodeType,value=CATEGORY)&amp;authCategoryEditorValidator(cateid=categoryId)|fieldValueValidator(field=nodeType,value=TEMPLATE)&amp;authCategoryEditorValidator(templateid=categoryId)]" />
			<!-- 维护界面 -->
			<query
				queryString="method=manageList"
				validator="fieldValueValidator(field=nodeType,value=CATEGORY)&amp;authCategoryEditorValidator(cateid=categoryId)|fieldValueValidator(field=nodeType,value=TEMPLATE)&amp;authCategoryEditorValidator(templateid=categoryId)|areaRoleValidator(role=ROLE_KMIMEETING_CATEGORY_MAINTAINER)|roleValidator(role=ROLE_SYSCATEGORY_MAINTAINER)" />
			<!-- 转移模板按钮 -->
			<query
				queryString="method=changeTemplate&amp;nodeType=*"
