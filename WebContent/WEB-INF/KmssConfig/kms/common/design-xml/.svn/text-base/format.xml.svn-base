<?xml version="1.0" encoding="UTF-8"?>
<root xmlns="http://www.landray.com.cn/schema/lui" xmlns:degign="http://www.landray.com.cn/schema/design"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.landray.com.cn/schema/lui
	../../../sys/ui/lui.xsd
	http://www.landray.com.cn/schema/design
	../../../design-xml.xsd">
    
	<format id="kms.common.doc.zone" name="KMS部件-文档专区"
		defaultRender="kms.common.doc.zone.rt" help="" />
	<format id="kms.list.first.picture" name="首行封面列表"
      defaultRender="kms.list.first.picture.left" help="" />

	<!-- 简单列表(作者或者创建者->作者) -->
	<format id="sys.ui.classic.rename" name="{sys-ui:sys.ui.classic.name}" description="{sys-ui:sys.ui.classic.desc}"
			defaultRender="sys.ui.classic.default.rename" example="/sys/ui/extend/dataview/format/classic-example.jsp"
			help="/sys/ui/extend/dataview/format/classic-help.jsp" />
	<!-- 简单列表扩展 -->
	<format id="sys.ui.classic.rich.rename"
			name="{sys-ui:sys.ui.classic.rich.name}"
			description="{sys-ui:sys.ui.classic.rich.desc}"
			help="/sys/ui/extend/dataview/format/classicrich-help.jsp"
			defaultRender="sys.ui.classic.rich.rename" />
</root>
