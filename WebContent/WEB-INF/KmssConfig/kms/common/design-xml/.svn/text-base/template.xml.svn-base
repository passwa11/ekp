	../../../design-xml.xsd">
	
	<!-- 笔记的门户组件分开为防止组件放在一起channel和页码干扰 -->
	<render id="kms.courseNotes.hotSlide.listview.gridtable" name="{kms-common:kmsKnowledge.porlet.geizi.view}" for="listview.gridtable"
		type="Template">
		<code src="/kms/common/kms_course_notes/import/course_porlet_notes_hotSlide_tmpl.jsp" />
	</render>
	
	<render id="kms.courseNotes.mySlide.listview.gridtable" name="{kms-common:kmsKnowledge.porlet.geizi.view}" for="listview.gridtable"
