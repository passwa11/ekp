		<code src="/kms/common/kms_course_notes/import/course_porlet_notes_hotTile_tmpl.jsp" />
	</render>
	
	<render id="kms.courseNotes.myTile.listview.gridtable" name="{kms-common:kmsKnowledge.porlet.geizi.view}" for="listview.gridtable"
		type="Template">
		<code src="/kms/common/kms_course_notes/import/course_porlet_myTile_notes_tmpl.jsp" />
	</render>

	<template id="kms.common.profile.listview" name="后台配置中心列表视图模板"
