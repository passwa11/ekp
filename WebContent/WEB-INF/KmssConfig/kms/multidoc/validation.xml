<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="kmsMultidocKnowledgeForm">
			<field
				property="docSubject"
				depends="required,maxlength">
				<arg
					key="sysDocBaseInfo.docSubject"
					bundle="sys-doc"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			 
		</form>
		<form name="kmsMultidocTemplateForm">
			<field
				property="fdName"
				depends="required,maxlength">
				<arg
					key="kmsMultidocTemplate.fdName"
					bundle="kms-multidoc"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			<field
				property="docExpire"
				depends="integer">
				<arg
					key="kmsMultidocTemplate.fdStoretime"
					bundle="kms-multidoc"
					position="0" />
			</field>
			<field
				property="fdNumberPrefix"
				depends="required,maxlength">
				<arg
					key="kmsMultidocTemplate.fdNumberPrefix"
					bundle="kms-multidoc"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>
