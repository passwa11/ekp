<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">
<form-validation>
	<formset>
		<form name="kmsKnowledgeBaseDocForm">
			<field
				property="docSubject"
				depends="required,maxlength">
				<arg
					key="sysDocBaseInfo.docSubject"
					bundle="sys-doc"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			 
		</form>
		<form name="kmsKnowledgeCategoryForm">
			<field
				property="fdName"
				depends="required,maxlength">
				<arg
					key="kmsKnowledgeCategory.fdName"
					bundle="kms-knowledge"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				</var>
			</field>
			<field
				property="fdNumberPrefix"
				depends="maxlength">
				<arg
					key="kmsKnowledgeCategory.fdNumberPrefix"
					bundle="kms-knowledge"
					position="0" />
				<arg1
					name="maxlength"
					key="${var:maxlength}"
					resource="false" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>10</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>
