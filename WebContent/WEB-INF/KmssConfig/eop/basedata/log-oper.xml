        <oper
        	id="eopBasedataSwitchOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.eop.basedata.model.EopBasedataSwitch"
        	action="/eop/basedata/eop_basedata_switch/eopBasedataSwitch.do">
        	<query
				method="modifySwitch"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="button.view" />
        	<query
				method="updateSwitch"
				logPoint="updateSwitch"
				messageKey="button.submit" />
        </oper>
        <!-- 税率 -->
        <oper
        	id="eopBasedataTaxRateOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.eop.basedata.model.EopBasedataTaxRate"
