<?xml version="1.0" encoding="UTF-8"?>
<configs 
    xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd " 
    xmlns="http://www.example.org/design-config" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <module 
        urlPrefix="/sys/help/" 
        messageKey="sys-help:module.sys.help" 
        defaultValidator="roleValidator(role=ROLE_SYSHELP_DEFAULT)">
        <roles>ROLE_SYSHELP_DEFAULT;ROLE_SYSHELP_SETTING</roles>
        <request 
            path="sys_help_main/sysHelpMain.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.sys.help.model.SysHelpMain" 
            defaultValidator="roleValidator(role=ROLE_SYSHELP_DEFAULT)">
            <query 
                queryString="method=analyze|analyzePage" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=edit|update|add" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
        </request>
        <request 
            path="sys_help_catelog/sysHelpCatelog.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.sys.help.model.SysHelpCatelog" 
            defaultValidator="roleValidator(role=ROLE_SYSHELP_SETTING)">
            <query 
                queryString="method=editCatelogContent|openCatelogDialog|add" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=edit|update|add" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
        </request>
        <request 
            path="sys_help_config/sysHelpConfig.do*" 
            validatorParameter="recid=fdId,model=com.landray.kmss.sys.help.model.SysHelpConfig" 
            defaultValidator="roleValidator(role=ROLE_SYSHELP_DEFAULT)">
            <query 
                queryString="method=open|changeStatus|updateUrlData|getAllModule" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=edit|update|add" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=delete|deleteall" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
            <query 
                queryString="method=view" 
                validator="roleValidator(role=ROLE_SYSHELP_SETTING)"/>
        </request>
    </module>
    <homePage 
        messageKey="sys-help:module.sys.help" 
        urlPrefix="sys/help" 
        config="/moduleindex_notopic.jsp?nav=/sys/help/tree.jsp"/>
    <profileConfig 
        key="sys/help" 
        messageKey="{sys-help:module.sys.help}" 
        url="/sys/profile/moduleindex.jsp?nav=/sys/help/tree.jsp" 
        icon="km_comminfo" 
        type="mechanism" 
        description="{sys-help:module.sys.help.description}"/>
</configs>
