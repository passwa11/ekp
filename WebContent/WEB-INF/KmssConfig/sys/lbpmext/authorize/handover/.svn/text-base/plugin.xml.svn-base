<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../../../plugin.xsd ">
	<!-- 工作交接（事项交接） -->
	<extension point="com.landray.kmss.sys.handover.item">
		<item name="config">
			<!-- 主文档全名称 -->
			<param
				name="module"
				value="com.landray.kmss.sys.lbpmext.authorize.model.LbpmAuthorize" />
			<param
				name="messageKey"
				value="sys-lbpmext-authorize:module.sys.lbpmext.authorize" />
			<param
				name="handler"
				value="com.landray.kmss.sys.lbpmext.authorize.handover.LbpmAuthorizeItemHandler" />
			<param
				name="provider"
				value="com.landray.kmss.sys.lbpmext.authorize.handover.LbpmAuthorizeItemProvider" />
		</item>
	</extension>
</plugin>