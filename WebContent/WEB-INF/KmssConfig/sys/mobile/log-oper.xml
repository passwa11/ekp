<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="sysMobileModuleCategoryConfigOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.mobile.model.SysMobileModuleCategoryConfig"
            action="/sys/mobile/sys_mobile_module_category_config/sysMobileModuleCategoryConfig.do">
            <query 
                method="data"
                logPoint="Base_UrlParam"
                messageKey="button.list">
            </query>
        </oper>
        <oper
        	id="sysMobileCompressOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/sys/mobile/sys_mobile_compress/sysMobileCompress.do">
        	<query
        		method="listAll"
        		logPoint="Base_UrlParam;listAll"
        		messageKey="button.list">
        	</query>
        	<query
        		method="compress"
        		logPoint="BaseOperLog;compress"
        		messageKey="sys-mobile:sysMobileCompress.oper.compress">
        	</query>
        	<query
        		method="check"
        		logPoint="BaseOperLog;check"
        		messageKey="sys-mobile:sysMobileCompress.oper.check">
        	</query>
        </oper>
        <oper
        	id="bapiOperLog" 
        	extends="BaseOperLog" 
        	model="*" 
        	action="/sys/mobile/bapi.do">
        	<query
        		method="getLocation"
        		logPoint="Base_UrlParam;getLocation"
        		messageKey="sys-mobile:bapi.oper.getLocation">
        	</query>
        	<query
        		method="getPoisList"
        		logPoint="Base_UrlParam;getPoisList"
        		messageKey="sys-mobile:bapi.oper.getPoisList">
        	</query>
        </oper>
        <oper
        	id="sysMobileRetrievePasswordOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/sys/mobile/sys_mobile_retrieve_password/sysMobileRetrievePassword.do">
        	<query
        		method="validateAccount"
        		logPoint="Base_UrlParam;validateAccount"
        		messageKey="sys-mobile:sysMobileRetrievePassword.oper.validateAccount">
        	</query>
        	<query
        		method="sendMobileCode"
        		logPoint="Base_UrlParam;forwardResult"
        		messageKey="sys-mobile:sysMobileRetrievePassword.oper.sendMobileCode">
        	</query>
        	<query
        		method="validateMobileCode"
        		logPoint="Base_UrlParam;validateMobileCode"
        		messageKey="sys-mobile:sysMobileRetrievePassword.oper.validateMobileCode">
        	</query>
        	<query
        		method="savePwd"
        		logPoint="Base_UrlParam;forwardResult"
        		messageKey="sys-mobile:sysMobileRetrievePassword.oper.savePwd">
        	</query>
        </oper>
        <oper
        	id="sysMobilePackageOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/sys/mobile/sys_mobile_offline/sysMobilePackage.do">
        	<query
        		method="packageModule"
        		logPoint="Base_UrlParam;packageModule"
        		messageKey="sys-mobile:sysMobilePackage.oper.packageModule">
        	</query>
        </oper>
    </logset>
</log-oper>