<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
		<oper
            id="sysAppConfigOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/appconfig/sys_appconfig/sysAppConfig.do">
            <query 
                method="update"
                logPoint="sysAppConfigUpdate"
                messagekey="button.update">
            </query>
            <query 
                method="edit"
                logPoint="sysAppConfigEdit"
                messagekey="button.goEdit">
            </query>
        </oper>
	</logset>
</log-oper>