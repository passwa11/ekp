			<param
					name="module"
					value="com.landray.kmss.sys.modeling.base.profile.model.ModelingAppNav" />
			<param
					name="messageKey"
					value="sys-modeling-base:table.modelingAppNavHandover"/>
			<param
					name="provider"
					value="com.landray.kmss.sys.modeling.handover.SysModelingAppNavProvider"/>
			<param
					name="order"
					value="4"/>
		</item>
		<!-- 首頁設置 -->
		<item
				name="config">
			<param
					name="module"
					value="com.landray.kmss.sys.modeling.base.mobile.model.ModelingAppMobile" />
