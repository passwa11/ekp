			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgElement.hbmGroups" />
<!-- 		<collection-cache
			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgElement.hbmPosts" />
		<collection-cache
			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgElement.hbmPersons" /> -->
		<collection-cache
			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgElement.hbmThisLeaderChildren" />
		<collection-cache
			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgElement.hbmSuperLeaderChildren" />
		<collection-cache
			usage="nonstrict-read-write"
			collection="com.landray.kmss.sys.organization.model.SysOrgGroup.hbmMembers" />
