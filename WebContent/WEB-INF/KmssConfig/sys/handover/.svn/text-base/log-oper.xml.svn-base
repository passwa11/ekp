<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
			id="sysHandoverConfigMainOperLog"
			extends="BaseOperLog"
			model="*"
			action="/sys/handover/sys_handover_config_main/sysHandoverConfigMain.do">
			<query
				method="search"
				logPoint="Base_UrlParam;search"
				messageKey="sys-handover:sysHandoverConfigMain.oper.search">
			</query>
			<query
				method="getMainLogId"
				logPoint="getMainLogId"
				messageKey="sys-handover:sysHandoverConfigMain.oper.getMainLogId">
			</query>
			<query
				method="submit"
				logPoint="Base_UrlParam;submit"
				messageKey="sys-handover:sysHandoverConfigMain.doc">
			</query>
			<query
				method="perform"
				logPoint="Base_UrlParam;perform"
				messageKey="sys-handover:sysHandoverConfigMain.oper.perform">
			</query>
			<query
				method="viewAuth"
				logPoint="Base_UrlParam;viewAuth"
				messageKey="sys-handover:sysHandoverConfigMain.oper.viewAuth">
			</query>
			<query
				method="detail"
				logPoint="Base_UrlParam;detail"
				messageKey="sys-handover:sysHandoverConfigMain.oper.detail">
			</query>
			<query
				method="detailLog"
				logPoint="Base_UrlParam;detailLog;Action_FindAll"
				messageKey="sys-handover:sysHandoverConfigMain.oper.detailLog">
			</query>
			<query
				method="submitAuth"
				logPoint="Base_UrlParam;submitAuth"
				messageKey="sys-handover:sysHandoverConfigMain.oper.submitAuth">
			</query>
		</oper>
		<oper
			id="sysHandoverConfigAuthLogDetailOperLog"
			extends="BaseOperLog"
			model="*"
			action="/sys/handover/sys_handover_auth/sysHandoverConfigAuthLogDetail.do">
			<query
				method="getModules"
				logPoint="Base_UrlParam;getModules"
				messageKey="sys-handover:sysHandoverConfigAuthLogDetail.oper.getModules">
			</query>
		</oper>
		<oper
            id="sysHandoverTaskSetting"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/handover/sysHandoverTaskSetting.do">
        </oper>
	</logset>
</log-oper>
