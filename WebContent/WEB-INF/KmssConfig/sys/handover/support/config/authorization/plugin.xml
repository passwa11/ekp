<?xml version="1.0" encoding="UTF-8"?>
<plugin
	xmlns="http://www.example.org/plugin-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../../../../plugin.xsd ">
    <!-- 全系统流程模板 -->
	<extension
		point="com.landray.kmss.sys.handover">
		<item
			name="config">
			<param
				name="module"
				value="handOverSysAuthorization" />
			<param 
				name="messageKey" 
				value="sys-handover-support-config-authorization:sysHandoverConfigHandler.authorization"/>
			<param 
				name="handler" 
				value="com.landray.kmss.sys.handover.support.config.authorization.SysAuthHandler"/>
			<param 
				name="order" 
				value="2"/>
		</item>
	</extension>
</plugin>
