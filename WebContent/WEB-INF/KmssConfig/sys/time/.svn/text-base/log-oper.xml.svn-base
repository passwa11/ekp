<?xml version="1.0" encoding="UTF-8" ?>
<log-oper>
	<logset>
		<oper
			id="sysTimeAreaOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.time.model.SysTimeArea"
			action="/sys/time/sys_time_area/sysTimeArea.do">
			<query
				method="calendar|viewCalendar|viewMCalendar"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="sys-time:sysTimeArea.oper.calendar.view">
			</query>
			<query
				method="editCalendar|editMCalendar"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="sys-time:sysTimeArea.oper.calendar.edit">
			</query>
			<query
				method="calendarJson|mCalendarJson"
				logPoint="Base_UrlParam;Action_Find"
				messageKey="sys-time:sysTimeArea.oper.calendar.view">
			</query>
			<query
				method="editCalendarJson|saveOrgElementData"
				logPoint="Base_UrlParam;updateCalendar"
				messageKey="sys-time:sysTimeArea.oper.calendar.update">
			</query>
			<query
				method="listTime"
				logPoint="Base_UrlParam"
				messageKey="sys-time:sysTimeArea.oper.listTime">
			</query>
			<query
				method="importExcel"
				logPoint="Base_UrlParam;importExcel"
				messageKey="sys-time:sysTimeArea.btn.upload">
			</query>
			<query
				method="exportExcel"
				logPoint="Base_UrlParam;exportExcel">
			</query>
		</oper>
		<oper
			id="sysTimeLeaveRuleOperLog"
			extends="BaseOperLog"
			model="com.landray.kmss.sys.time.model.SysTimeLeaveRule"
			action="/sys/time/sys_time_leave_rule/sysTimeLeaveRule.do">
			<query
				method="enable"
				logPoint="Service_Update"
				messageKey="sys-time:sysTimeLeaveRule.fdIsAvailable.enable">
			</query>
			<query
				method="disable"
				logPoint="Service_Update"
				messageKey="sys-time:sysTimeLeaveRule.fdIsAvailable.disable">
			</query>
		</oper>
	</logset>
</log-oper>