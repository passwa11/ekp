<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="sysLbpmserviceImportController" class="com.landray.kmss.sys.lbpmservice.rest.controller.SysLbpmserviceImportController"/>
    <bean id="sysLbpmUsageController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmUsageController"/>
    <bean id="sysLbpmDataController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmDataController"/>
    <bean id="sysLbpmProcessController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmProcessController">
        <property name="lbpmProcessOperationService">
            <ref bean="lbpmProcessService" />
        </property>
    </bean>
    <bean id="sysLbpmNoticeHandlerController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmNoticeHandlerController"/>
    <bean id="sysLbpmProcessMainController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmProcessMainController"/>
    <bean id="lbpmHistoryWorkitemController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmHistoryWorkitemController"/>
    <bean id="MobileExtOperationController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmMobileExtOperationController"/>
    <bean id="checkNodeMsgDataController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.CheckNodeMsgDataController"/>
    <bean id="lbpmProcessLimitTimeOperationLogController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmProcessLimitTimeOperationLogController"/>
    <bean id="lbpmRouteDecisionRuleController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmRouteDecisionRuleController"/>
    <bean id="lbpmRefuseRuleController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmRefuseRuleController"/>
    <bean id="lbpmHandlerParseController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmHandlerParseController"/>
    <bean id="lbpmUsageContentController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmUsageContentController"/>
    <bean id="lbpmTempStorageController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmTempStorageController"/>
    <bean id="lbpmSettingInfoController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmSettingInfoController"/>
    <bean id="lbpmErrorQueueController"
          class="com.landray.kmss.sys.lbpmservice.rest.controller.LbpmErrorQueueController"/>
</beans>
