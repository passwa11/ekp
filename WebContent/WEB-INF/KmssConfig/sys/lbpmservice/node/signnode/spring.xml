<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean
		id="signNodeBehaviourService"
		parent="defaultManualNodeBehaviour"
		class="com.landray.kmss.sys.lbpmservice.node.support.DefaultManualNodeBehaviour" />
	<bean
		id="signNodeDesc"
		class="com.landray.kmss.sys.lbpmservice.node.signnode.SignNodeDesc" />

	<bean
		id="signNodeWorkitemBehaviourService"
		parent="abstractManualWorkitemBehaviour"
		class="com.landray.kmss.sys.lbpmservice.node.signnode.SignNodeWorkitemBehaviour" />
	<bean
		id="signNodeWorkitemDesc"
		class="com.landray.kmss.sys.lbpmservice.node.signnode.SignNodeWorkitemDesc" />
</beans>
