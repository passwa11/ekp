<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://www.example.org/plugin-config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/plugin-config ../../plugin.xsd ">
	<extension point="com.landray.kmss.sys.webservice2">
		<item name="registry">
			<param name="serviceName" value="{sys-fans:sysFansMain.user.follow.webserivce}" />
			<param name="serviceClass"
				value="com.landray.kmss.sys.fans.webservice.ISysFansWebService" />
			<param name="serviceBean" value="sysFansWebService" />
			<param name="serviceDoc" value="/sys/fans/webservice/sysFans_help.jsp" />
