<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

  <formset>
        <form name="sysBookmarkMainForm">
			<field property="docSubject" depends="required,maxlength">
			    <arg
					key="sysBookmarkMain.docSubject"
					bundle="sys-bookmark"
					position="0" />
			    <arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
			    <var><var-name>maxlength</var-name><var-value>200</var-value></var>
			</field>

			<field property="fdUrl" depends="required,maxlength">
			    <arg
					key="sysBookmarkMain.fdUrl"
					bundle="sys-bookmark"
					position="0" />
			    <arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
			    <var><var-name>maxlength</var-name><var-value>1000</var-value></var>
			</field>
        </form>
        
        <form name="sysBookmarkPersonCategoryForm">
			<field property="fdName" depends="required,maxlength">
			    <arg
					key="sysBookmarkPersonCategory.fdName"
					bundle="sys-bookmark"
					position="0" />
			    <arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
			    <var><var-name>maxlength</var-name><var-value>200</var-value></var>
			</field>
        </form>
        
        <form name="sysBookmarkPublicCategoryForm">
			<field property="fdName" depends="required,maxlength">
			    <arg
					key="sysBookmarkCategory.fdName"
					bundle="sys-bookmark"
					position="0" />
			    <arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
			    <var><var-name>maxlength</var-name><var-value>200</var-value></var>
			</field>
        </form>
  </formset>
</form-validation>
