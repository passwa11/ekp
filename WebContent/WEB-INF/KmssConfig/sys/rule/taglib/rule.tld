<?xml version="1.0" encoding="UTF-8"?>
<taglib
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>rule</short-name>
	<uri>http://www.landray.com.cn/tags/kmss/rule</uri>
	<tag>
		<name>text</name>
		<tag-class>com.landray.kmss.sys.rule.taglib.RuleTextTag</tag-class>
		<attribute>
			<name>property</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showStatus</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>		
		<attribute>
			<name>className</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>	
		<attribute>
			<name>langs</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[多语言值，格式要符合json数组格式]]></description>
		</attribute>
		<attribute>
			<name>onkeydown</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>alias</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[別名，对输入框提供别名，保证属性的唯一]]></description>
		</attribute>
		<attribute>
			<name>subject</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>validators</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[校验器集合，具体请参考$kmssValidation的相关描述]]></description>
		</attribute>
		<attribute>
			<name>onValueChange</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[字段值改变事件，请填写js代码。系统会调用相应的事件方法。]]></description>
		</attribute>
		<!-- 追加其他的HTML代码。若是值对（比如：id字段和显示字段），则追加的是显示字段的HTML。 -->
		<attribute>
			<name>htmlElementProperties</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[追加其他的HTML代码。若是值对（比如：id字段和显示字段），则追加的是显示字段的HTML。]]></description>
		</attribute>
	</tag>
</taglib>