<?xml version="1.0" encoding="UTF-8" ?>
<root xmlns="http://www.landray.com.cn/schema/lui" xmlns:degign="http://www.landray.com.cn/schema/design" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
	http://www.landray.com.cn/schema/lui
	../../../sys/ui/lui.xsd
	http://www.landray.com.cn/schema/design
	../../../design-xml.xsd">
	<!--标签云  -->
	<render id="sys.tagss.tagClound.rd" name="{sys-tag:sysTagTop.portlet}" format="sys.tagss.tagClound" css="/sys/tag/resource/css/tag_cloud.css" type="Javascript">
		<code src="/sys/tag/resource/js/tag_cloud.js" ellipsis="true" />
	</render>

	<!-- 标签呈现 -->
	<!-- 3d标签云 -->
	<render id="sys.tagss.tag.3d.rd" name="标签(3d)" format="sys.tagss.tag" css="/sys/tag/resource/css/tag_portlet_hot.css" type="Javascript">
		<code extend="3d" src="/sys/tag/resource/js/tag_portlet_hot.js"  ellipsis="true" />
	</render>
	<!-- 背景色 -->
	<render id="sys.tagss.tag.bg.rd" name="标签(背景色)" format="sys.tagss.tag" css="/sys/tag/resource/css/tag_portlet_hot.css" type="Javascript" thumb="/sys/tag/resource/thumb/sys.tagss.tag.bg.jpg">
		<code extend="bg" src="/sys/tag/resource/js/tag_portlet_hot.js"  ellipsis="true" />
	</render>
	<!-- 鼠标移入背景色 -->
	<render id="sys.tagss.tag.hoverBg.rd" name="标签(鼠标移入背景色)" format="sys.tagss.tag" css="/sys/tag/resource/css/tag_portlet_hot.css" type="Javascript" thumb="/sys/tag/resource/thumb/sys.tagss.tag.hoverBg.jpg">
		<code extend="hoverBg" src="/sys/tag/resource/js/tag_portlet_hot.js" ellipsis="true" />
	</render>
	<!-- 鼠标移入圆点 -->
	<render id="sys.tagss.tag.hoverPoint.rd" name="标签(鼠标移入圆点)" format="sys.tagss.tag" css="/sys/tag/resource/css/tag_portlet_hot.css" type="Javascript" thumb="/sys/tag/resource/thumb/sys.tagss.tag.hoverPoint.jpg">
		<code extend="hoverPoint" src="/sys/tag/resource/js/tag_portlet_hot.js" ellipsis="true" />
	</render>
	<!-- 边框sys-tag:sys.tagss.hot.name -->
	<render id="sys.tagss.tag.border.rd" name="标签(边框)" format="sys.tagss.tag" css="/sys/tag/resource/css/tag_portlet_hot.css" type="Javascript" thumb="/sys/tag/resource/thumb/sys.tagss.tag.border.jpg">
		<code extend="border" src="/sys/tag/resource/js/tag_portlet_hot.js" ellipsis="true" />
	</render>
	
</root>