<?xml version="1.0" encoding="UTF-8" ?>
<log-oper>
	<logset>
		<oper
        	id="sysRestserviceServerPolicyOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerPolicy"
        	action="/sys/restservice/server/sys_restservice_server_policy/sysRestserviceServerPolicy.do">
        	<query
        		method="editPassword"
        		logPoint="Service_Update"
        		messageKey="sys-restservice-server:button.password">
        	</query>
        </oper>
        <oper
        	id="sysRestserviceServerLogOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerLog"
        	action="/sys/restservice/server/sys_restservice_server_log/sysRestserviceServerLog.do">
        	<query
        		method="timeout"
        		logPoint="Base_UrlParam;Action_FindAll"
        		messageKey="button.list">
        	</query>
        </oper>
        <oper
        	id="sysRestserviceServerMainOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerMain"
        	action="/sys/restservice/server/sys_restservice_server_main/sysRestserviceServerMain.do">
        	<query
        		method="start|startall"
        		logPoint="Base_UrlParam;Action_Find"
        		messageKey="sys-restservice-server:button.list.startservice">
        	</query>
        	<query
        		method="stop|stopall"
        		logPoint="Base_UrlParam;Action_Find"
        		messageKey="sys-restservice-server:button.list.stopservice">
        	</query>
        	<query
        		method="download"
        		logPoint="Base_UrlParam;Action_Find"
        		messageKey="sys-restservice-server:button.download.client">
        	</query>
        </oper>
        <oper
        	id="sysRestserviceServerInitOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerMain"
        	action="/sys/restservice/server/sys_restservice_server_init/sysRestserviceServerInit.do">
        	<query
        		method="init"
        		logPoint="Base_UrlParam;Action_FindAll"
        		messageKey="sys-restservice-server:sysRestserviceServer.init">
        	</query>
        </oper>
        
        <oper
            id="sysRestserviceServerLogConfigOperLog"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/restservice/server/sys_restservice_server_log_config/sysRestserviceServerLogConfig.do">
        </oper>
	</logset>
</log-oper>
