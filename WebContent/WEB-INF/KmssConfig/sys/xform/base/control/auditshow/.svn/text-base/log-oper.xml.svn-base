<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
        <oper
            id="sysXformAuditshowLog"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.xform.base.model.controls.auditshow.SysXformAuditshow"
            action="/sys/xform/auditshow/sys_xform_auditshow/sysXformAuditshow.do">
            <query 
                method="disable"
                logPoint="updatedisable">
            </query>
            <query 
                method="disableall"
                logPoint="multiupdatedisable">
            </query>
            <query 
                method="copy"
                logPoint="Action_Find"
                messageKey="button.copy">
            </query>
        </oper>
	</logset>
</log-oper>