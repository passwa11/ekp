			<param name="tbClassName" value="tb_normal_solidborder_radius_bgcolor"></param>
			<param name="pathProfix" value="sys/xform/designer/standardtable/tablestyle/tb_normal_solidborder_radius_bgcolor"></param>
		</item>
		<item name="config">
			<param name="name" value="{sys-xform-base:sysForm.tableStyle.tb_normal_lightweight}"></param>
			<param name="tbClassName" value="tb_normal_lightweight"></param>
			<param name="pathProfix" value="sys/xform/designer/standardtable/tablestyle/tb_normal_lightweight"></param>
		</item>
