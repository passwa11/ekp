			<param name="name" value="{sys-xform-base:sysForm.tableStyle.tb_normal_noanyborder}"></param>
			<param name="tbClassName" value="tb_normal_noanyborder"></param>
			<param name="pathProfix" value="sys/xform/designer/standardtable/tablestyle/tb_normal_noanyborder"></param>
