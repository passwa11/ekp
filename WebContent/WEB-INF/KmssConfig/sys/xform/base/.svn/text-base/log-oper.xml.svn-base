<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
        <oper
            id="SysFormDbTableAction"
            extends="BaseOperLog"
            model="*"
            action="/sys/xform/base/sys_form_db_table/sysFormDbTable.do">
            <query 
                method="moveMappingHistoryData"
                messageKey="sys-xform:xform.button.mapping.historyDocData"
                logPoint="Base_UrlParam;moveMappingHistoryData"/>
            <query 
                method="update"
                messageKey="sys-xform:xform.button.mapping.historyDocData"
                model="com.landray.kmss.sys.xform.base.model.SysFormDbTable"/>
        </oper>
        <oper
            id="sysFormTemplateOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/xform/sys_form_template/sysFormTemplate.do">
            <query 
                method="reWriteAllTempates"
                logPoint="Base_UrlParam"
                messageKey="sys-xform:sysFormTemplate.config.tree.reUpdate.xform">
            </query>
        </oper>
        <oper
            id="sysFormRefCommonTemplate"
            extends="BaseOperLog"
            model="*"
            action="/sys/xform/sys_form_template/sysFormRefCommonTemplate.do">
            <query 
                method="getTemplateRefCommon"
                messageKey="button.list"
                logPoint="Base_UrlParam;Action_FindAll"/>
            <query 
                method="cleadOldData"
                messageKey="button.clear"
                logPoint="Base_UrlParam"/>
        </oper>
        <oper
            id="SysFormTemplateHistoryAction"
            extends="BaseOperLog"
            model="*"
            action="/sys/xform/base/sys_form_template_history/sysFormTemplateHistory.do">
            <query 
                method="getTemplateHistoryRefMain"
                messageKey="button.list"
                logPoint="Base_UrlParam;Action_FindAll;getTemplateHistoryRefMain"/>
        </oper>
        <oper
            id="formulaCalculationLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/xform/sys_form/formulaCalculation.do">
            <query 
                method="getDataJSON"
                messageKey="sys-xform:SysFormTemplateCalculationControl.calculation"
                logPoint="Base_UrlParam"/>
        </oper>
        <oper
            id="sysFormCommonTemplateOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.xform.base.model.SysFormCommonTemplate"
            action="/sys/xform/sys_form_common_template/sysFormCommonTemplate.do">
            <query 
                method="rebuildJsp"
                logPoint="Base_UrlParam"
                messageKey="sys-xform:sysFormTemplate.config.tree.reUpdate.xform">
            </query>
        </oper>
        <oper
            id="sysFormCommonTemplateHistory"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.xform.base.model.SysFormCommonTempHistory"
            action="/sys/xform/sys_form_common_template_history/sysFormCommonTemplateHistory.do">
            <query 
                method="viewHistory"
                logPoint="Base_UrlParam;Action_Find;viewHistory"
                messageKey="button.view">
            </query>
            <query 
                method="editHistory"
                logPoint="Base_UrlParam;Action_Find;editHistory"
                messageKey="button.edit">
            </query>
        </oper>
	</logset>
</log-oper>