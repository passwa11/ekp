<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration
    PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<!-- 片段集分类 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SysFormFragmentSetCategory.hbm.xml" />
		<!-- 片段集 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SysFormFragmentSet.hbm.xml" />
		<!-- 历史片段集 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SysFormFragmentSetHistory.hbm.xml" />
		<!-- 同步状态 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SynchronousStatus.hbm.xml" />
		<!-- 历史片段集和历史表单中间表 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SysFormFragmentSetTemplate.hbm.xml" />
		<!-- 范围表 -->
		<mapping
			resource="com/landray/kmss/sys/xform/fragmentSet/model/SysFormFragmentSetScope.hbm.xml" />
	</session-factory>
</hibernate-configuration> 