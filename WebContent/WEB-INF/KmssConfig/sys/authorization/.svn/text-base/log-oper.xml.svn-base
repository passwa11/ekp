<?xml version="1.0" encoding="UTF-8" ?>
<!-- 权限管理操作日志 -->
<log-oper>
    <logset>
        <oper id="sysAuthRoleOperLog" desc="" extends="BaseOperLog"
            model="com.landray.kmss.sys.authorization.model.SysAuthRole"
            action="/sys/authorization/sys_auth_role/sysAuthRole.do">
			<!-- ignoreFields忽略字段列表，多个字段使用';'分号分割，可继承自父类配置 -->
            <ignoreFields>authUraForms</ignoreFields>
        </oper>
        
        <oper id="sysAuthCategoryOperLog" desc="" extends="BaseOperLog"
            model="com.landray.kmss.sys.authorization.model.SysAuthCategory"
            action="/sys/authorization/sys_auth_category/sysAuthCategory.do">
        </oper>
    </logset>
</log-oper>