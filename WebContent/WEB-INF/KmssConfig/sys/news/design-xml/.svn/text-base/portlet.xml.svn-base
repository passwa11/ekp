				{"help":"{sys-news:sysNewsMain.portlet.newIcon.help}"}
			</var>
			<var key="target" kind="enumType" name="{sys-news:sysNewsMain.portlet.targetWindow}" default="_blank">
				{type:"sysUi_target", showType:"radio"}
			</var>
		</vars>
