			</var>
		</vars>
		<operations>
			<operation
				name="{operation.create}"
				type="create"
				href="javascript:(function(){seajs.use(['sys/news/sys_news_ui/js/create'], function(create) { 
							create.addDoc('!{cateid}');
					 });})();"
