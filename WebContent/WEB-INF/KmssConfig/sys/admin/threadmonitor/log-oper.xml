<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
		<oper
            id="threadmonitorOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/admin/threadmonitor.do">
            <query 
                method="monitor"
                logPoint="Base_UrlParam;monitor"
                messageKey="button.list">
            </query>
            <query 
                method="update"
                logPoint="update"
                messageKey="button.update">
            </query>
        </oper>
	</logset>
</log-oper>