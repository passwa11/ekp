<?xml version="1.0" encoding="UTF-8"?>

<log-oper>
	<logset>
		<oper
            id="passwordSecurityConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/profile/passwordSecurityConfig.do">
        </oper>
		<oper
            id="showConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/profile/showConfig.do">
        </oper>
        <oper
            id="sysCommonSensitiveConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/profile/sysCommonSensitiveConfig.do">
        </oper>
        <oper
            id="sysProfileOrder"
            model="*"
            action="/sys/profile/sysProfileOrder.do">
            <query 
                method="updateOrderNum"
                logPoint="updateOrderNum"
                messageKey="sys-organization:org.operation.quickSort">
            </query>
        </oper>
        <oper
            id="sysProfileI18nConfig"
            model="*"
            action="/sys/profile/i18n/sysProfileI18nConfig.do">
            <query 
                method="clearCache"
                logPoint="Base_UrlParam;clearCache"
                messageKey="sys-profile:sys.profile.i18n.tools1">
            </query>
            <query 
                method="clearFileKey"
                logPoint="Base_UrlParam;clearFileKey"
                messageKey="sys-profile:sys.profile.i18n.tools2">
            </query>
            <query 
                method="resetI18n"
                logPoint="Base_UrlParam;resetI18n"
                messageKey="sys-profile:sys.profile.i18n.tools3">
            </query>
            <query 
                method="viewDetailOriginal"
                logPoint="Base_UrlParam;viewDetailOriginal"
                messageKey="sys-profile:sys.profile.i18n.langtools.detailOriginal">
            </query>
            <query 
                method="getMessageByModule"
                logPoint="Base_UrlParam;getMessageByModule"
                messageKey="sys-profile:sys.profile.i18n.seach.module">
            </query>
            <query 
                method="getMessageByKeyWord"
                logPoint="Base_UrlParam;getMessageByKeyWord"
                messageKey="sys-profile:sys.profile.i18n.seach.keyWord">
            </query>
            <query 
                method="saveMessage"
                logPoint="saveMessage"
                messageKey="sys-profile:sys.profile.i18n.seach.submit">
            </query>
        </oper>
        <oper
            id="tripartiteAdminOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/profile/tripartiteAdminAction.do">
            <query 
                method="showVersion"
                logPoint="Base_UrlParam;showVersion"
                messageKey="sys-config:sys.sysAdminCommonTools.viewModuleInfo">
            </query>
        </oper>
         <oper
            id="sysListshowCategoryOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.profile.model.SysListshowCategory"
            action="/sys/profile/listShow/sys_listshow_category/sysListshowCategory.do">
        </oper>
        <oper
            id="orgImportOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/profile/org/orgImport.do">
            
            <query 
                method="importData"
                logPoint="Base_UrlParam;importData"
                messageKey="sys-profile:sys.profile.orgImport.title">
            </query>
        </oper>
        <oper
            id="orgExportOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/profile/org/orgExport.do">
            <query 
                method="exportData"
                logPoint="Base_UrlParam;exportData"
                messageKey="sys-profile:sys.profile.orgExport.title">
            </query>
        </oper>
        <oper
            id="sysLoginTemplateOperLog"
            extends="BaseOperLog"
            model="com.landray.kmss.sys.profile.model.SysLoginTemplate"
            action="/sys/profile/sys_login_template/sysLoginTemplate.do">
             <query 
                method="updateDefault"
                logPoint="Base_UrlParam;updateDefault"
                messageKey="sys-profile:sysLoginTemplate.logOper.updateDefault">
            </query>
        </oper>
        <oper
            id="sysListShowOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/profile/listShow/sys_listShow/sysListShow.do">
             <query 
                method="selectFields"
                logPoint="Base_UrlParam;Service_Update"
                model="com.landray.kmss.sys.profile.model.SysListshowCategory"
                messageKey="sys-profile:sys.profile.list.display.config">
            </query>
        </oper>
        <oper
            id="sysCfgProfileConfigOperLog"
            extends="BaseOperLog"
            model="*"
            action="/sys/profile/sys_profile_main/sysCfgProfileConfig.do">
             <query 
                method="data"
                logPoint="Base_UrlParam;Action_Find">
            </query>
             <query 
                method="customBrowser"
                logPoint="Base_UrlParam;Action_Find"
                messageKey="sys-profile:sys.profile.browserCheck.config.custom">
            </query>
        </oper>
	</logset>
</log-oper>