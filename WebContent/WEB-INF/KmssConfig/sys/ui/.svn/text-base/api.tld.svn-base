<?xml version="1.0" encoding="UTF-8"?>
<taglib
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<tlib-version>2.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>api</short-name>
	<uri>http://www.landray.com.cn/tags/api</uri>
	
	<tag>
		<name>response</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.ResponseTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>code</description>
			<name>code</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>msg</description>
			<name>msg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>data-object</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.DataObjectTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>name</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>prop-auto</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.AutoPropertyTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>ref</description>
			<name>ref</name>
			<!-- 非必须，默认值为form -->
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>include</description>
			<name>include</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>exclude</description>
			<name>exclude</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>prop</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>name</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>prop-attr</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyAttributeTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>name</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>value</description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
		
	
	<tag>
		<name>children</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyChildrenTag</tag-class>
		<body-content>JSP</body-content>
	</tag>
	
	<tag>
		<name>child</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyChildTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>key</description>
			<name>key</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>dataType</description>
			<name>dataType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>value</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>validators</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyValidatorsTag</tag-class>
		<body-content>JSP</body-content>
	</tag>
	
	<tag>
		<name>validator</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyValidatorTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>type</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[验证器的值，仅当验证类型为pattern该值表示表达式的regexp属性]]></description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为size的验证器，表示长度最小值]]></description>
			<name>min</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为size的验证器，表示长度最大值]]></description>
			<name>max</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为decimalMin DecimalMax的验证器，表示是否包含最大值最小值]]></description>
			<name>inclusive</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为digits验证器，表示整数精度]]></description>
			<name>integer</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为digits验证器，表示小数精度]]></description>
			<name>fraction</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[此属性仅适用于类型为pattern验证器，表示是否忽略大小写，值为"i"时忽略大小写]]></description>
			<name>flag</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
		
	<tag>
		<name>enum</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyEnumTag</tag-class>
		<body-content>JSP</body-content>
	</tag>
	
	<tag>
		<name>enum-item</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyEnumItemTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>value</description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>labelKey</description>
			<name>labelKey</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>bundle</description>
			<name>bundle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>optionValue</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyOptionValueTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>type</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>value</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>uri</description>
			<name>uri</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>render</name>
		<tag-class>com.landray.kmss.sys.ui.taglib.api.PropertyRenderTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>type</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>level</description>
			<name>level</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	
		
</taglib>