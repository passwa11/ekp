<?xml version="1.0" encoding="UTF-8"?>
<root xmlns="http://www.landray.com.cn/schema/lui"
	  xmlns:degign="http://www.landray.com.cn/schema/design"
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="
	http://www.landray.com.cn/schema/lui
	../../../sys/ui/lui.xsd
	http://www.landray.com.cn/schema/design
	../../../design-xml.xsd">
	<!-- 简单列表 -->
	<format id="sys.ui.classic" name="{sys-ui:sys.ui.classic.name}" description="{sys-ui:sys.ui.classic.desc}"
			defaultRender="sys.ui.classic.default" example="/sys/ui/extend/dataview/format/classic-example.jsp"
			help="/sys/ui/extend/dataview/format/classic-help.jsp" />

	<!-- 简单列表扩展 -->
	<format id="sys.ui.classic.rich"
			name="{sys-ui:sys.ui.classic.rich.name}"
			description="{sys-ui:sys.ui.classic.rich.desc}"
			help="/sys/ui/extend/dataview/format/classicrich-help.jsp"
			defaultRender="sys.ui.classic.rich" />

	<!-- 图文列表 -->
	<format id="sys.ui.imgtext" name="{sys-ui:sys.ui.imgtext.name}" description="{sys-ui:sys.ui.imgtext.desc}"
			defaultRender="sys.ui.imgtext.default" example="/sys/ui/extend/dataview/format/imgtext-example.jsp"
			help="/sys/ui/extend/dataview/format/imgtext-help.jsp" />

	<!-- 图文列表 -->
	<format id="sys.ui.imgtext.noreadcount" name="{sys-ui:sys.ui.imgtext.noreadcount.name}" description="{sys-ui:sys.ui.imgtext.noreadcount.desc}"
			defaultRender="sys.ui.imgtext.noreadcount" example="/sys/ui/extend/dataview/format/imgtext-example.jsp"
			help="/sys/ui/extend/dataview/format/imgtext-help.jsp" />

	<!-- 文字列表 -->
	<format id="sys.ui.text" name="{sys-ui:sys.ui.text.name}" description="{sys-ui:sys.ui.text.desc}"
			defaultRender="sys.ui.text.default" example="/sys/ui/extend/dataview/format/text-example.jsp"
			help="/sys/ui/extend/dataview/format/text-help.jsp" />

	<!-- 列表摘要 -->
	<format id="sys.ui.listdesc" name="{sys-ui:sys.ui.listdesc.name}" description="{sys-ui:sys.ui.listdesc.desc}"
			defaultRender="sys.ui.listdesc.default" example="/sys/ui/extend/dataview/format/listdesc-example.jsp"
			help="/sys/ui/extend/dataview/format/listdesc-help.jsp" />

	<!-- 幻灯片 -->
	<format id="sys.ui.slide" name="{sys-ui:sys.ui.slide.name}" description="{sys-ui:sys.ui.slide.desc}"
			defaultRender="sys.ui.slide.default" example="/sys/ui/extend/dataview/format/slide-example.jsp"
			help="/sys/ui/extend/dataview/format/slide-help.jsp" />

	<!-- 增强幻灯片 -->
	<format id="sys.ui.enhanced.slide" name="{sys-ui:sys.ui.enhanced.slide.name}" description="{sys-ui:sys.ui.enhanced.slide.desc}"
			defaultRender="sys.ui.enhanced.slide.default" example="/sys/ui/extend/dataview/format/slide-example.jsp"
			help="/sys/ui/extend/dataview/format/slide-help.jsp" />

	<!-- 图片菜单 -->
	<format id="sys.ui.picMenu" name="{sys-ui:sys.ui.picMenu.name}" description="{sys-ui:sys.ui.picMenu.desc}"
			defaultRender="sys.ui.picMenu.default" example="/sys/ui/extend/dataview/format/picmenu-example.jsp"
			help="/sys/ui/extend/dataview/format/picmenu-help.jsp" />

	<!-- 二级页面头部导航信息 -->
	<format id="sys.ui.navtitle.info" name="{sys-ui:sys.ui.navtitle.info.name}" description="{sys-ui:sys.ui.navtitle.info.desc}"
			defaultRender="sys.ui.navtitle.info" example="" help="" />

	<!-- 二级页面头部导航操作 -->
	<format id="sys.ui.navtitle.operation" name="{sys-ui:sys.ui.navtitle.operation.name}" description="{sys-ui:sys.ui.navtitle.operation.desc}"
			defaultRender="sys.ui.navtitle.operation" example="" help="" />

	<!-- 二级页面头部导航总数 -->
	<format id="sys.ui.navtitle.total" name="{sys-ui:sys.ui.navtitle.total.name}" description="{sys-ui:sys.ui.navtitle.total.desc}"
			defaultRender="sys.ui.navtitle.total" example="" help="" />

	<!-- 文本菜单 -->
	<format id="sys.ui.textMenu" name="{sys-ui:sys.ui.textMenu.name}" description="{sys-ui:sys.ui.textMenu.desc}"
			defaultRender="sys.ui.textMenu.default" example="/sys/ui/extend/dataview/format/textmenu-example.jsp"
			help="/sys/ui/extend/dataview/format/textmenu-help.jsp" />

	<!-- 系统导航 -->
	<format id="sys.ui.sysnav" name="{sys-ui:sys.ui.sysnav.name}" description="{sys-ui:sys.ui.sysnav.desc}"
			defaultRender="sys.ui.sysnav.default" example="/sys/ui/extend/dataview/format/sysnav-example.jsp"
			help="/sys/ui/extend/dataview/format/sysnav-help.jsp" />

	<!-- 二级树形菜单 -->
	<format id="sys.ui.treeMenu2" name="{sys-ui:sys.ui.treeMenu2.name}" description="{sys-ui:sys.ui.treeMenu2.desc}"
			defaultRender="sys.ui.treeMenu2.default" example="/sys/ui/extend/dataview/format/treemenu2-example.jsp"
			help="/sys/ui/extend/dataview/format/treemenu2-help.jsp" />

	<!-- 三级树形菜单 -->
	<format id="sys.ui.treeMenu3" name="{sys-ui:sys.ui.treeMenu3.name}" description="{sys-ui:sys.ui.treeMenu3.desc}"
			defaultRender="sys.ui.treeMenu3.default" example="/sys/ui/extend/dataview/format/treemenu3-example.jsp"
			help="/sys/ui/extend/dataview/format/treemenu3-help.jsp" />

	<!-- 自定义页面 -->
	<format id="sys.ui.html" name="{sys-ui:sys.ui.html.name}" description="{sys-ui:sys.ui.html.desc}"
			defaultRender="sys.ui.html.default" example="/sys/ui/extend/dataview/format/html-example.jsp"
			help="/sys/ui/extend/dataview/format/html-help.jsp" />

	<!-- 自定义页面 -->
	<format id="sys.ui.html.personinfo" name="{sys-ui:sys.ui.html.name}" description="{sys-ui:sys.ui.html.desc}"
			defaultRender="sys.ui.html.personinfo" example="/sys/ui/extend/dataview/format/html-example.jsp"
			help="/sys/ui/extend/dataview/format/html-help.jsp" />

	<!-- 图片摘要 -->
	<format id="sys.ui.classic.imageDesc" name="{sys-ui:sys.ui.classic.imageDesc.name}" description="{sys-ui:sys.ui.classic.imageDesc.desc}"
			defaultRender="sys.ui.classic.imageDesc.level"/>
	<format id="sys.ui.classic.imageFirst" name="{sys-ui:sys.ui.classic.imageFirst.name}" description="{sys-ui:sys.ui.classic.imageFirst.desc}"
			defaultRender="sys.ui.classic.imageFirst"/>

	<!-- iframe -->
	<format id="sys.ui.iframe" name="{sys-ui:sys.ui.iframe.name}" description="{sys-ui:sys.ui.iframe.desc}"
			defaultRender="sys.ui.iframe.default" example="/sys/ui/extend/dataview/format/iframe-example.jsp"
			help="/sys/ui/extend/dataview/format/iframe-help.jsp" />

	<!-- 列表视图 -->
	<format id="sys.ui.listtable" name="{sys-ui:sys.ui.listtable.name}" description="{sys-ui:sys.ui.listtable.desc}"
			defaultRender="sys.ui.listtable.default" example="/sys/ui/extend/dataview/format/listtable-example.jsp"
			help="/sys/ui/extend/dataview/format/listtable-help.jsp" />

	<!-- 后端部件 -->
	<format id="sys.ui.jsp" name="{sys-ui:sys.ui.jsp.name}" description="{sys-ui:sys.ui.jsp.desc}"
			defaultRender="sys.ui.jsp.default" example="/sys/ui/extend/dataview/format/jsp-example.jsp"
			help="/sys/ui/extend/dataview/format/jsp-help.jsp" />

	<!-- 分类导航 -->
	<format id="sys.ui.cate" name="{sys-ui:sys.ui.cate.name}" description="{sys-ui:sys.ui.cate.desc}"
			defaultRender="sys.ui.cate.default" example="/sys/ui/extend/dataview/format/cate-example.jsp"
			help="/sys/ui/extend/dataview/format/cate-help.jsp" />

	<!-- 图片视图 -->
	<format id="sys.ui.image" name="{sys-ui:sys.ui.image.name}" description="{sys-ui:sys.ui.image.desc}"
			defaultRender="sys.ui.image.default" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-help.jsp" />

	<!-- 富图片视图 -->
	<format id="sys.ui.image.rich" name="{sys-ui:sys.ui.image.rich.name}" description="{sys-ui:sys.ui.image.rich.desc}"
			defaultRender="sys.ui.image.rich.default" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-rich-help.jsp" />

	<!-- 简单列表表格(消息) -->
	<format id="sys.ui.notify" name="{sys-ui:sys.ui.notify.name}" description="{sys-ui:sys.ui.notify.desc}"
			defaultRender="sys.ui.notify"  />

	<!-- 数据统计视图 -->
	<format id="sys.ui.stat" name="{sys-ui:sys.ui.stat.name}" description="{sys-ui:sys.ui.stat.desc}"
			defaultRender="sys.ui.stat.simpleStatis" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-help.jsp" />

	<!-- 个人数据统计视图 -->
	<format id="sys.ui.mine.stat" name="{sys-ui:sys.ui.mine.stat.name}" description="{sys-ui:sys.ui.mine.stat.desc}"
			defaultRender="sys.ui.mine.stat.default" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-help.jsp" />

	<!-- 情感组件 -->
	<format id="sys.ui.emotional" name="{sys-ui:sys.ui.emotional.name}" description="{sys-ui:sys.ui.emotional.desc}"
			defaultRender="sys.ui.emotional.default" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-help.jsp" />

	<!-- 地图模板 -->
	<format id="sys.ui.mapTpl" name="{sys-ui:sys.ui.mapTpl.name}" description="{sys-ui:sys.ui.MapTpl.desc}"
			defaultRender="sys.ui.mapTpl.default" example="/sys/ui/extend/dataview/format/image-example.jsp"
			help="/sys/ui/extend/dataview/format/image-help.jsp" />

	<!--卡片列表  -->
	<format id="sys.ui.card" name="{sys-ui:sys.ui.card.name}" description="{sys-ui:sys.ui.card.desc}"
			defaultRender="sys.ui.card.default" example="/sys/ui/extend/dataview/format/classic-example.jsp"
			help="/sys/ui/extend/dataview/format/classic-help.jsp" />

	<!-- 图文摘要 -->
	<format id="sys.ui.image.desc" name="{sys-ui:sys.ui.image.desc.name}" description="{sys-ui:sys.ui.image.desc.desc}"
			defaultRender="sys.ui.image.desc.silder.default"/>

	<!--人员信息列表展示  -->
	<format id="sys.ui.personInfo" name="{sys-ui:sys.ui.personInfo.name}" help="/sys/ui/portal/jsp/personInfo-help.jsp"
			description="{sys-ui:sys.ui.personInfo.desc}" defaultRender="sys.ui.personInfo.default" />
</root>