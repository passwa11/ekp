	<theme id="default" name="{sys-ui:portlet.theme.default}" path="/sys/ui/extend/theme/default/"
		thumb="/sys/ui/extend/theme/default/thumb.jpg">
		<style>

			<!-- 通用级样式 -->
			<css type="common" path="style/common.css" />

			<!-- 图标样式 -->
			<css type="icon" path="style/icon.css" />
			<css type="iconfont" path="style/iconfont.css" />

			<!-- 员工黄页样式 -->
			<css type="zone" path="style/zone.css" />
			<!-- 社区样式 -->
			<css type="sns" path="style/sns.css" />
			<!-- 门户样式 -->
			<css type="portal" path="style/portal.css" />
