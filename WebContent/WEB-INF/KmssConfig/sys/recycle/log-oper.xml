<?xml version="1.0" encoding="UTF-8" ?>

<log-oper>
	<logset>
		<oper
            id="softDeleteConfig"
            extends="sysAppConfigOperLog"
            model="*"
            action="/sys/recycle/softDeleteConfig.do">
            <query
            	method="checkTransfer"
            	logPoint="checkTransfer"
            	messageKey="sys-recycle:config.setting.transfer.check">
            </query>
            <query
            	method="doTransfer"
            	logPoint="doTransfer"
            	messageKey="sys-recycle:config.setting.transfer.do">
            </query>
        </oper>
        <oper
        	id="sysRecycleOperLog"
        	extends="BaseOperLog"
        	model="*"
        	action="/sys/recycle/sys_recycle_doc/sysRecycle.do">
        	<query
        		method="listDoc|listDocOld"
        		logPoint="Base_UrlParam;Action_FindAll"
        		messageKey="button.list">
        	</query>
        	<query
        		method="redirect"
        		logPoint="Base_UrlParam;Action_Find"
        		messageKey="button.view">
        	</query>
        	<query
        		method="hardDeleteAll"
        		logPoint="Base_UrlParam;Service_DeleteAll"
        		messageKey="sys-recycle:config.setting.allDelete">
        	</query>
        	<query
        		method="recoverAll"
        		logPoint="Base_UrlParam;Service_Update"
        		messageKey="sys-recycle:config.setting.allRecover">
        	</query>
        	<query
        		method="hardDelete"
        		logPoint="Base_UrlParam;Service_Delete"
        		messageKey="sys-recycle:button.hardDelete">
        	</query>
        	<query
        		method="recover"
        		logPoint="Base_UrlParam;Service_Update"
        		messageKey="sys-recycle:button.recover">
        	</query>
        	<query
        		method="listModelDoc"
        		logPoint="Base_UrlParam;Action_FindAll"
        		messageKey="button.list">
        	</query>
        </oper>
        <oper
        	id="sysRecycleLogOperLog"
        	extends="BaseOperLog"
        	model="com.landray.kmss.sys.recycle.model.SysRecycleLog"
        	action="/sys/recycle/sys_recycle_log/sysRecycleLog.do">
        	<query
        		method="getModules"
        		logPoint="Base_UrlParam;getModules"
        		messageKey="sys-recycle:sysRecycleLog.oper.getModules">
        	</query>
        	<query
        		method="checkRecordExist"
        		logPoint="Base_UrlParam;checkRecordExist"
        		messageKey="sys-recycle:sysRecycleLog.oper.checkRecordExist">
        	</query>
        </oper>
	</logset>
</log-oper>