<?xml version="1.0" encoding="UTF-8"?>
<configs
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd ">

	<module
		messageKey="tic-jdbc:module.tic.jdbc"
		urlPrefix="/tic/jdbc/"
		defaultValidator="roleValidator(role=ROLE_TIC_BACKSTAGE_MANAGER)">
		<roles>
			<!-- 
			ROLE_TICJDBC_MANAGER
			 -->
		</roles>

		<request
			path="tic_jdbc_task_manage/ticJdbcTaskManage*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.jdbc.model.TicJdbcTaskManage">
		</request>

		<request
			path="tic_jdbc_relation/ticJdbcRelation*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.jdbc.model.TicJdbcRelation">
		</request>

		<request
			path="tic_jdbc_task_category/ticJdbcTaskCategory*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.jdbc.model.TicJdbcTaskCategory">
		</request>

		<request
			path="tic_jdbc_mapp_category/ticJdbcMappCategory*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.jdbc.model.TicJdbcMappCategory">
		</request>

		<request
			path="tic_jdbc_mapp_manage/ticJdbcMappManage*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.jdbc.model.TicJdbcMappManage">
		</request>

	</module>
