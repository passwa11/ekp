<?xml version="1.0" encoding="UTF-8"?>
<configs
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../design.xsd ">

	<module
		messageKey="tic-soap-sync:module.tic.soap.sync"
		urlPrefix="/tic/soap/sync/"
		defaultValidator="roleValidator(role=ROLE_TIC_SOAP_ADMIN)">

		<request
			path="tic_soap_sync_temp_func/ticSoapSyncTempFunc*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.soap.sync.model.TicSoapSyncTempFunc">
		</request>

		<request
			path="tic_soap_sync_category/ticSoapSyncCategory*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.soap.sync.model.TicSoapSyncCategory">
		</request>

		<request
			path="tic_soap_sync_job/ticSoapSyncJob*"
			validatorParameter="recid=fdId,model=com.landray.kmss.tic.soap.sync.model.TicSoapSyncJob">
		</request>

	</module>

	<homePage
		urlPrefix="tic/soap/sync"
		url="/moduleindex.jsp?nav=/tic/soap/sync/tree.jsp"
		messageKey="tic-soap-sync:module.tic.soap.sync" />

</configs>
