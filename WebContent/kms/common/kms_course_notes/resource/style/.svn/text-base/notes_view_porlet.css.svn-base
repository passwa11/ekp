pre{white-space:pre-wrap;white-space:-moz-pre-wrap;white-space:-pre-wrap;white-space:-o-pre-wrap;word-wrap:break-word;}

.notes_button{
	float: right;
	width: 80px;
	height: 30px;
	border: 1px solid rgb(133, 191, 239);
	font-size: 16px;
	font-weight: bold;
	cursor: pointer;
	background: url("images/btn_bg.jpg") repeat-x; 
	color:#ffffff;
	/*border-radius:4px; 
	-webkit-border-radius: 8px;*/
	Margin:10px
	
}
.lui_dialog_buttons .lui_dialog_buttons_container {
    margin-right:200px;
    padding: 4px 1px 8px 0;
}
.notes_editbutton{
	border: 1px solid rgb(133, 191, 239);
	background: url("images/btn_bg.jpg") repeat-x; 
	margin-right:150px;
	cursor: pointer;
	color:#ffffff;
	height: 25px;
	font-weight: bold;
}

.lui_widget_btn .lui_widget_btn_txt{
		font-size: 18px;
	
}
.notes_button:HOVER{
	border: 1px solid #307cc5;
	background: url("images/btn_on_bg.jpg") repeat-x; 
	cursor: default;
}

.notes_button_disable{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}
.notes_button_disable:HOVER{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}

.notes_sourse_title{
	
	line-height: 20px;
    color: #52a4c9;
    font-size: 15px;
    font-weight: bold;
}
.notes_sourse{
	font-size: 12px;
	text-align: right !important;
    margin: 0px 18px 5px !important;
    line-height: 20px;
    color: #9e9e9e;
}

.notes_sourse_sub{
	margin-left:5px;
	float:right;
	color: #333;
	font-size: 14px;
}
