/*doc_edit page*/
.notes_EditMain{
	height:  auto!important;
	text-align:left;
	min-height: 160px;
	width:100%;
	width: auto;
}



.notes_opt_table tr td{
	padding: 6px 0px;
}


.notes_Tips{
	float:left;
	Margin:10px
}

.notes_button{
background-color: #3796f8;
    font-size: 16px;
    height: 28px;
    width: 68px;
    border: none;
    border-radius: 3px;
    color: #fff;
    float: right;
    cursor: pointer;
    margin-top: -6px;
	
}
.lui_dialog_buttons .lui_dialog_buttons_container {
   /* margin-right:200px;*/
    padding: 4px 1px 8px 0;
}
.notes_editbutton{
	border: 1px solid rgb(133, 191, 239);
	background: url("images/btn_bg.jpg") repeat-x; 
	margin-right:150px;
	cursor: pointer;
	color:#ffffff;
	height: 25px;
	font-weight: bold;
}

.lui_widget_btn .lui_widget_btn_txt{
	
}
.notes_button:HOVER{
	border: 1px solid #307cc5;
	background: url("images/btn_on_bg.jpg") repeat-x; 
	cursor: default;
}

.notes_button_disable{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}
.notes_button_disable:HOVER{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}

.notes_content_iframe {
width: 93%;
    height: 128px;
    border: 1px solid #e2e2e2;
    border-radius: 3px;
    padding: 10px;
    background: white;
}
.notes_prompt{
	display:none;
	height: 30px;
	line-height:30px;
	vertical-align:middle;
	margin-right:14%;
	float: right;
	position: relative;
	top:-2px;
}

#share_notes_list{
	display:none;
}

.notes_label_title{
}




.notes_record_dl>dd {
    position: relative;
    padding-left: 10px;
    padding-top: 18px;
}

.notes_record_content{
	word-break: break-all;
    word-wrap: break-word;
    margin-bottom: 10px;
    line-height: 18px;
    color: black;
    width: 320px;
   	font-family: unset;
}

.notes_txt{
	text-align: right;
    height: 27px;
}
.notes_recorder{
	height: 16px;
	float:left;
	margin-right: 15px;
	color:#b5b50a;
}

.notes_time{
	height: 16px;
	float:left;
	color: #999;
}

.view_notes{

	/*margin-left: 250px;*/
	cursor: pointer;
    color: #3796f8;
    text-decoration: underline;
	margin-right: 5%;
	float:right;
}

.notes_cancel_share{
	float:left;
	margin-left: 10%;
	cursor: pointer;
	color: #3796f8;
    text-decoration: underline;
	
}
.edit_notes{
	float:left;
	cursor: pointer;
	color: #3796f8;
    text-decoration: underline;
}
.delete_notes{
	
	cursor: pointer;
	color: #3796f8;
    text-decoration: underline;
	float:right;
}

.button_notes{
	margin-right: 5%;
    float: right;
    width: 23%;
}

.km-note-share-textarea-wrap input[type=checkbox] {
    height: 0px;
    width: 0px;
    visibility: hidden;
}

.km-note-share-textarea-wrap  input:checked + label{
	background-color: #3796f8;
    border: 1px solid #3796f8;
    background-image: url(../images/tickIcon.png);
    background-repeat: no-repeat;
    background-position: center;
}

.km-note-share-textarea-wrap label{
  	width: 14px;
    height: 14px;
    border: 1px solid #d5d5d5;
    border-radius: 2px;
    background-color: #fff;
    display: inline-block;
    margin-bottom: -3px;

}
.km-note-share-textarea-wrap span{
  	margin-left:5px;
}

[data-lui-type="lui/panel!Content"] {
    padding: 8px 8px !important;
}