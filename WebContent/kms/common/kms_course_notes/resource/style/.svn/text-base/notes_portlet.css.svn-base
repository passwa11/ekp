.km-note-detail-wrap {
  padding: 30px 20px 0 20px;
}
.km-note-detail-person {
  padding: 8px 0 8px 65px;
  position: relative;
}
.km-note-detail-avatar-wrap {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 0;
}
.km-note-detail-person-msg>.name {
  color: #3796f8;
  margin-right: 2px;
}
.km-note-detail-time {
  color: #999;
  font-size: 12px;
  margin-top: 2px;
}
.km-note-detail-avatar-wrap>img {
  width: 100%;
  height: 100%;
}
.km-note-detail-content {
  border-top: 1px solid #f1f1f1;
  margin-top: 15px;
  padding: 10px 0 60px;  
  position: relative;
}
.km-note-detail-heading {
  padding-bottom: 10px;
}
.km-note-detail-heading .tag {
  width: 18px;
  height: 21px;
  background-color: #3796f8;
  display: inline-block;
  border-radius: 0 0 9px 9px;
  background-image: url(../images/tickIcon.png);
  background-repeat: no-repeat;
  background-position: center 45%;
  margin-right: 8px;
  position: relative;
  top: 8px;
}
.km-note-detail-heading .gray {
  color: #999;
}
.km-note-detail-heading .bold {
  font-weight: 600;
}
.km-note-detail-body {
  line-height: 24px;
}
.km-note-detail-content>.km-note-list-footer {
  right: 0;
}

pre {
    white-space: pre-wrap;
    white-space: -moz-pre-wrap;
    white-space: -pre-wrap;
    white-space: -o-pre-wrap;
    word-wrap: break-word;
}

.edit_notes .praise_count:after {
    content: '';
    width: 1px;
    height: 16px;
    position: absolute;
    top: 2px;
    right: -10px;
    background-color: white;
}

