.km-note-share-panel {
  width: 330px;
  height: 100%;
  background-color: #fbfdff;
  padding: 0 15px;
}
.km-note-share-list {
  list-style-type: none;
}
.km-note-share-textarea-wrap {
  padding-top: 15px;
}
.km-note-share-textarea {
  width: 93%;
  height: 128px;
  border: 1px solid #e2e2e2;
  border-radius: 3px;
  padding: 10px;
}
.km-note-share-textarea-footer {
  padding: 15px 0;
  border-bottom: 1px solid #e2e2e2;
}
.km-note-share-textarea-footer>label {
  cursor: pointer;
}
.active>.km-note-share-check {
  background-color: #3796f8;
  border: 1px solid #3796f8;
  background-image: url(../images/tickIcon.png);
  background-repeat: no-repeat;
  background-position: center;
}
.km-note-share-check {
  width: 14px;
  height: 14px;
  border: 1px solid #d5d5d5;
  border-radius: 2px;
  background-color: #fff;
  display: inline-block;
  margin-bottom: -3px;
}
.km-note-share-submit {
  /*background-color: #3796f8;*/
  font-size: 16px;
  height: 28px;
  width: 68px;
  border: none;
  border-radius: 3px;
  color: #fff;
  float: right;
  cursor: pointer;
  margin-top: -6px;
}
/*.km-note-share-submit:hover {
  background-color: #1986f6;
}*/
.km-note-share-list-heading {
  padding-top: 15px;
}
.km-note-share-list-heading>input {
  background-color: #fbfdff;
  height: 28px;
  width: 75px;
  border: 1px solid #999;
  border-radius: 15px;
  color: #999;
  margin-right: 5px;
  cursor: pointer;
}
.km-note-share-list-heading>input:hover,
.km-note-share-list-heading>input.active {
  color: #3796f8;
  border: 1px solid #3796f8;
}
.km-note-share-refresh {
  width: 18px;
  height: 19px;
  display: inline-block;
  background-image: url(../images/refresh.png);
  background-repeat: no-repeat;
  background-position: center;
  cursor: pointer;
  float: right;
  margin-top: 3px;
}
.km-note-share-list .content {
  color: #333;
}
.km-note-share-detail {
 padding: 15px 0;
}
.km-note-share-detail .date {
  color: #999;
  margin-left: 4px;
}
.km-note-share-detail a:hover,
.km-note-share-detail a {
  color: #3796f8;
  text-decoration: underline;
  float: right;
}
.km-note-share-list>li {
  border-bottom: 1px dashed #e2e2e2;
  padding-top: 15px;
}
.km-note-share-list>li:last-child {
  border-bottom: none;
}

/* ����ʼ� Ends
