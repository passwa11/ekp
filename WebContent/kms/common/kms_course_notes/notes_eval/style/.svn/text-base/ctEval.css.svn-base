	word-break:break-all;
	margin-left:10px;
}
.eval_quote_cont{
	background: #f8f8f8 url(images/quote_L.png) no-repeat 10px 10px;
	margin-bottom: 10px;
	line-height: 22px;
	color: #2f7daa;
	padding: 15px 20px 12px 45px;
}
.eval_quote_cont blockquote {
	background: #f8f8f8 url(images/quote_R.png) no-repeat right bottom;
