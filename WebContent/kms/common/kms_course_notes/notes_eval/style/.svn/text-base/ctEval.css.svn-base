/*doc_edit page*/
.eval_EditMain{
	height:  auto!important;
	text-align:left;
	min-height: 160px;
	width:100%;
	width: auto;
}

.eval_title_color{
	color:#646464;
}

.eval_opt_table tr td{
	padding: 3px 0px !important;
}

.eval_title{
	font-size: 14px;
	text-align: left;
	margin: 0px 6px 0px 0px;
	height: 20px;
	line-height: 20px;
	vertical-align: middle;
	font-weight: bold;
	display: inline-block;
}

.eval_star{
	height: 20px;
	display: inline-block;
	vertical-align: middle;
	margin-left:3px;
}

.eval_star li{
	list-style:none;
	display: inline-block;
	width: 16px;
	height: 20px;
	line-height: 20px;
	cursor: pointer;
	background-position: center center;
	background-repeat: no-repeat;
}
.eval_level{
	float:right;
	display:inline-block;
	height: 20px;
	line-height: 20px;
	vertical-align:middle;
	padding-left:19px;
	color:#646464;
	display: inline-block;
	background-position: left center;
	background-repeat: no-repeat;
	margin-right:7px;
}
.eval_content{
	height: 80px;
	width: 100%;
	border: 1px solid #DFDFDF; 
	overflow-y:auto;
}
.eval_button_disable{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}
.eval_button_disable:HOVER{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}

.eval_notify{
	height: 30px;
	line-height:30px;
	vertical-align: middle;
	display:inline-block;
}
.eval_prompt{
	display:none;
	height: 30px;
	vertical-align:middle;
	margin-right:4%;
	float: right;
}

.eval_hidden{
	display: none;
}

.eval_record{
	width:100%;
	margin-bottom:10px;
}


.eval_record_content{
	word-break: break-all;
	word-wrap: break-word;
	margin-bottom: 10px;
	line-height:18px;
	color:black;
}

.eval_summary{
	height: 16px;
	cursor: default;
}
.eval_summary span{
	height: 16px;
	float: left;
	margin-right: 8px;
}

.eval_evaler{
	color:#F09802;
}

.eval_summary_star{
	width: 120px;
	height: 16px;
	display: inline;
}

.eval_summary_star li{
	float:left;
	list-style:none;
	width: 16px;
	height: 16px;
}
.eval_delopt{
	display:none;
	color:#54a3cc;
	cursor: pointer;
	float:right!important;
}
.eval_doc_score{ 
	 position:relative;
	 height:26px; 
	 line-height:26px;
	 margin-bottom:10px;
}
.eval_doc_score span{
	display: inline-block;
}
.eval_score_mark{
	position:relative;
 	color:#f79301; 
 	font-size:32px; 
 	font-weight:bold; 
 	font-style:italic; 
 	height:26px; 
 	line-height:26px; 
 	margin-bottom:10px;
}
.eval_score,.eval_score_val{
	position:absolute;
	left: 60px;
	top: 8px;
	height:16px;
	width: 80px;
	background-position: left center;
	background-repeat: repeat-x;
}
.eval_record_dl>dd {
	position: relative;
	padding-left: 65px;
	padding-top: 18px;
}
.eval_record_dl dd .img {
	position: absolute;
	left: 5px;
	top: 18px;
}
.eval_record_dl dd .img img {
	width: 45px;
	height: 45px;
	border: 1px solid #e2e2e2;
}
.eval_record_dl dd .txt {
	/**padding-bottom: 18px;**/
}
.eval_record_dl dd .eval_info {
	height: 16px;
	line-height: 16px;
	color: #a3a3a3;
	margin-bottom: 10px;
}
.eval_record_dl dd .eval_author {
	color: #ffae00;
	font-size: 14px;
}
.eval_record_dl dd .eval_info .eval_delopt {
	color: #0086e4;
	cursor: pointer;
}
.eval_praise_reply{
	text-align: right;
	height: 27px;
}
.eval_praise {
	background: url(images/icon_thumbsUp.png) no-repeat left center;
	padding-left: 20px;
	cursor: pointer;
}
.eval_reply{
	background: url(images/icon_comment.png) no-repeat left center;
	padding-left: 17px;
	cursor: pointer;
	margin-left: 15px;
}
.eval_reply_info{
	padding: 8px 15px 1px;
}
.reply_content{
	/**
	background: #f8f8f8;
	padding: 10px 15px 10px;
	height:85px;**/
}
.replyIframe{
	padding: 10px 15px 30px;
	height:87px;
}
.eval_replys{
	position: relative;
	display:none;
}
.eval_replys .trig {
	display: inline-block;
	width: 18px;
	height: 9px;
	background: url(images/ico_trig.png) no-repeat left center;
	position: absolute;
	right: 12px;
	top: -9px;
}
.eval_reply_info .reply_li {
	position: relative;
	padding-left: 56px;
	margin-bottom: 15px;
	clear:both;
}
.lui_eval_biaoqing {
	cursor: pointer;
	text-align: center;
	float: left;
	list-style: none;
}
.eval_reply_info ul li .rtnPerson {
	position: absolute;
	left: 0px;
	top: 6px;
	width: 38px;
	height: 38px;
	border: 1px solid #e2e2e2;
}
.eval_reply_info ul li .eval_re_cont {
	margin-bottom: 0px;
	color: black;
	line-height: 24px;
	word-break: break-all;
}
.eval_re_cont .eval_author{
	margin: 0px 7px;
}
.eval_re_time{
	line-height: 22px;
	margin-bottom: 5px;
	color: #a3a3a3;
}
.eval_re_time>span{
	float:right;
}
.eval_re_time>span a{
	margin-left:10px;
}
.eval_re_reply{
	cursor: pointer;
	color: #a3a3a3;
	background: url(images/Comment_gray.png) no-repeat left center !important;
	display: inline-block;
	width:16px;
	height:17px;
}
.eval_re_dialogue{
	color: #a3a3a3;;
	cursor: pointer;
	background: url(images/ckdh.png) no-repeat left center !important;
	display: inline-block;
	width:16px;
	height:15px;
}
.eval_re_del{
	color: #a3a3a3;
	cursor: pointer;
	display: inline-block;
	width:16px;
	height:18px;
	background: url(images/delete.png) no-repeat left center !important;
}
.eval_reply_infos .eval_textarea,.eval_reply_infos .eval_textarea2{
	border: 0px solid #e6e6e6;
	background:white;
	width: 100%;
	height: 75px;
	overflow: hidden;
}
.eval_reply_submit{
	padding-top: 5px;
}
.eval_reply_tip{
	margin-right: 16px;
	line-height: 24px;
	color: #878787;
	display: block;
	float: right;
}
.reply_cont2{
	display:none;
	height: 115px;
	margin-left: -25%;
	margin-top: 14px;
	width: 125%;
}
