@CHARSET "utf-8";

.inner {
	margin: auto;
	width: 128px;
	height: 140px;
	overflow: hidden;
	background-image: url("../images/Ypage_honor_bg.png");
	background-repeat: no-repeat;
	background-position: center bottom ;
}

.inner img {
	width: 100px;
	height: 100px;
}

.txt {
	padding-left: 3px;
	font-size: 14px;
	line-height: 30px;
	text-overflow: ellipsis;
	width: 90px;
	overflow: hidden;
	white-space: nowrap;
	height: 30px;
	display:block;
	display:inline-block;
}
.txto{
	margin-top:2px;
}
.txto img {
	width: 30px;
	height: 30px;
	vertical-align: top;
	text-align: left;
}

.txtlist{
	font-size: 14px;
	line-height: 36px;
	text-overflow: ellipsis;
	width: 100%;
	overflow: hidden;
	white-space: nowrap;
	height: 36px;
}
.persontable{
	width: 100%;
}
.persontable tbody tr td{
	text-align:center;
	padding-top: 10px;
	padding-bottom: 10px;
	border-bottom: 1px dotted #8f8f8f;
}
.persontable tbody tr:last-child{
	border-bottom: 2px solid #bad4e5;
}
.lui_form_content_frame .min-sty td{
	padding-left:0;
	padding-right:0;
}
.personspan{
	display: block;
	text-align: center;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
	overflow:hidden;
	width:60px;
	margin:0 auto;
}
.personspancolor{
	color: #ff0000;
}

.file {
	position:absolute;
	margin-top:0px;
	margin-left:-53px;
	width:53px;
	height:17px;
	filter:alpha(opacity:0);
	opacity: 0;
	cursor: pointer;
}
.file:HOVER {
	cursor: pointer;
}
.tb_simple .inputsgl{height:24px;line-height:24px;font-size:12px;padding-left:3px;}
.s-btn{margin-left:10px;line-height:25px;padding: 4px 10px;min-width:50px;text-align:center;cursor: pointer;background:rgb(71, 181, 230);border:1px #35a1d0 solid;color:white;}
.s-btn:hover{background:#35a1d0;}
.lui_dialog_content,#info_table{padding:20px 10px;}
.info_table tr td{border:1px solid #e8e8e8;}

input[type=file]::-webkit-file-upload-button { 
    cursor: pointer; 
}
.luiMedalPersonNode {
	position: relative;
}
.luiMedalPersonNode:HOVER .luiRemoveIcon {
	display: block;
}
.luiRemoveIcon {
	cursor:pointer;
	display:none;
	position: absolute;
    top: -3px;
    right: 8px;
    width: 15px;
    height: 15px;
    border-radius: 50%;
    background: url(../images/member_remove.png) no-repeat center;
}