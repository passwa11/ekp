.lui_calendar_search_container .lui_calendar_header{
 	padding: 10px 20px;
}
.lui_calendar_search_container .listview{
	padding: 0 20px;
}
.lui_calendar_search_container .lui_calendar_search{
	float: left;
}
.lui_calendar_search_back{
	float: left;
    padding: 5px;
    background-color: #e5e5e5;
    margin-top: 5px;
    margin-left: 10px;
    cursor: pointer;
}

.train_calendar_botom{
	margin-top: 10px;
	width: 100%;
	text-align: center;
}
.train_calendar_label_unhold{
	background:#888888;
	width:18px;
	display: inline-block;
	height: 4px;
	vertical-align: middle;
}
.train_calendar_label_holding{
	background:#4285F4;
	width:18px;
	display: inline-block;
	height: 4px;
	vertical-align: middle;
}
.train_calendar_label_hold{
	background:#22B77D;
	width:18px;
	display: inline-block;
	height: 4px;
	vertical-align: middle;
}
.train_calendar_label_text{
	font-size: 12px;
	vertical-align: middle;
	margin: 0  8px;
}

/* 周div显示 */
.notHidden{
	width: 99%;
    overflow: hidden;
    display: block;
    /* white-space: nowrap; */
    text-overflow: ellipsis;
}

/* 培训目的 */
.viewPurpose{
	width: 230px;
	height: 60px;
}

/* 查看日程DIV */
.overDiv{
	text-align: left;
	width: 230px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow:ellipsis;
}
 