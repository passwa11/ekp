/* dialog Start */
.lui_dialog_head {
	border-bottom: 0 !important; 
    padding: 0 4px;
}

.lui_dialog_head_left {
    font-weight: bold;
}

/* dialog End */

/* input Start */
.td_normal_title {
	color: #333;
}

.inputnew {
    border: 1px solid #ddd;
    border-radius: 3px;
    height: 30px;
   	padding: 0 5px;
   	width: 100%;
}

.inputnew:focus {
	transition-duration: .3s;
    border: 1px solid #4285F4;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    font-size: 12px;  
}
input::-moz-placeholder, 
textarea::-moz-placeholder {  /* Mozilla Firefox 19+ */
    font-size: 12px;  
}
input:-moz-placeholder, 
textarea:-moz-placeholder {  /* Mozilla Firefox 4 to 18 */
    font-size: 12px;  
}
input:-ms-input-placeholder, 
textarea:-ms-input-placeholder {  /* Internet Explorer 10-11 */
    font-size: 12px;  
}
/* input End */

/* button Start */
.buttonClass {
	display: inline-block;
    width: 90px;
    height: 30px;
    line-height: 30px;
    border-radius: 3px;
    font-size: 14px;
	cursor: pointer;
}

.buttonDefault {
    background: #fff !important;
    color: #333 !important;
    border: 1px solid #ddd !important;
   	transition-duration: .3s;
}


.buttonDefault:hover {
	color: #4285F4 !important;
	border: 1px solid #4285F4 !important;
  	transition-duration: .3s;
}


.buttonBlue {
    font-size: 14px;
    color: #fff;
    text-align: center;
    border: 1px solid #4285f4;
    border-radius: 4px;
    background-color: #4285f4;
    transition-duration: .3s;
    margin-right: 5px;
}


.buttonBlue:hover {
    color: #fff;
    border-color: #346ac3;
    background-color: #346ac3;
   	transition-duration: .3s;
}

/* button End */

/* validation Start */
.validation-advice-msg {
	font-size: 12px !important;
}

.validation-advice, .lui_validate {
	padding: 0 !important;
	background-color: #fff !important;
	border: 1px solid #fff !important;
}
/* validation End */

/* index listview Start */
.knowledge_common thead tr {
	background: #FBFBFB;
	border-bottom: 1px solid transparent;
}

.knowledge_common thead tr th {
	background: #F8F8F8;
    font-weight: 400;
    height: 42px;
    color: #333333;
    text-align: left;
    padding-right:10px;
    padding-left: 10px;
}

.knowledge_common .lui_listview_columntable_table tbody tr:nth-child(even) {
    background-color: #fdfdfd;
}
.knowledge_common .lui_listview_columntable_table  tbody tr {
    border-bottom: 1px solid #eee;
}

.knowledge_common .lui_listview_columntable_table tbody tr td {
    font-size: 14px;
    color: #666666;
    text-align: left;
    padding-right:8px;
    padding: 7.5px 10px !important;
}



.knowledge_common .lui_paging_contentBox a {
    border: 1px solid #eeeeee !important;
}

.knowledge_common .lui_paging_contentBox li span input {
	border: 1px solid #eeeeee !important;
}

.knowledge_common .lui_list_operation {
    border-bottom: 1px solid #fff;
/*     margin-bottom: 10px; */
}

.knowledge_common .lui_list_sort {
    color: #000 !important;
}

.conf_show_more_w {
	width: 100% !important;
}

li {
	list-style: none;
}

.knowledge_common_listWithCheckbox thead tr th:first-child {
    padding-right: 0;
}
.knowledge_common_listWithCheckbox thead tr th:first-child + th{
    text-align: center;
}

.knowledge_common_listWithCheckbox .lui_listview_columntable_table tbody tr td:first-child + td{
    text-align: center;
}

.knowledge_common_listWithCheckbox .lui_listview_columntable_table tbody tr td:first-child{
    padding-right: 0;
}


.knowledge_common_listWithoutCheckbox .lui_listview_columntable_table tbody tr td:first-child{
    text-align: center;
}
.knowledge_common_listWithoutCheckbox thead tr th:first-child{
    text-align: center;
}

.knowledge_common_panelContent .lui-component[data-lui-type='lui/panel!Content'] {
    padding: 0!important;
}


.knowledge_common .lui_listview_columntable_table tbody tr:hover  {
    background: rgba(49,151,253,0.06) !important;
}

.knowledge_common .lui_listview_columntable_table .lui_multi_author_wrap {
    margin-left: 0 !important;
}

.knowledge_common .lui_listview_columntable_table input[type='checkbox'] {
    position: relative;
    top: 1px;
}

.lui_icon_s_new_know {
    height: 16px !important;
    min-width: 28px;
    width: auto !important;
    line-height: 16px !important;
    text-align: center;
    border-radius: 15px;
    font-size: 12px;
    padding: 0 6px;
    margin-right: 2px;
    position: relative;
    border: 1px solid transparent;
}

/* listview End */
.lui_listview_columntable_table{
    table-layout:fixed;
    width:100%;
}
body .lui_listview_columntable_table tbody tr td .com_subject {
    display:inline;
}