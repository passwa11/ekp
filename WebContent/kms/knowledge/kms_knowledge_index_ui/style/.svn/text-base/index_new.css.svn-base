@CHARSET "UTF-8";

#kms_knowledge_index_new_content_box {
    font-family: PingFangSC-Medium;
    background: #FFFFFF;
    box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.06);
    border-radius: 4px;
    /* height: 348px; */
    height: 360px;
    overflow: hidden;
    padding: 0 20px !important;
}

#kms_knowledge_index_new_content_box .no_data_box{
    text-align: center;
}

#kms_knowledge_index_new_content_box .no_data_box .no_data_img{
    background:url("../images/default1@2x.png") ;
    height:142px;
    width:142px;
    text-align: center;
    margin: auto;
    background-size: cover;
    margin-top: 77px;
}

#kms_knowledge_index_new_content_box .no_data_box .no_data_content{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #C3C3C3;
    line-height: 20px;
    font-weight: 400;
    margin-top:22px;
}

#kms_knowledge_index_new_content_box .new_list_title{
    padding: 12px 0 !important;
    /* border-bottom: solid 1px #EEEEEE; */
    height: 24px;
    padding-left: 20px;
}

#kms_knowledge_index_new_content_box .new_list_title .new_list_title_text_2{
    float: left;
    font-size: 18px;
    color: #333333;
    line-height: 24px;
    font-weight: 500;
}

#kms_knowledge_index_new_content_box .new_list_title .new_list_title_more{
    float: right;
    font-size: 14px;
    color: #333333;
    line-height: 22px;
    font-weight: 400;
    cursor: pointer;
}

#kms_knowledge_index_new_content_box .new_list_title .new_list_title_more i{
    background: url(../images/more.svg) no-repeat center center;
    vertical-align: middle;
}

#kms_knowledge_index_new_content_box .new_list_content ul{
    /* margin-left: 20px; */
    margin-right: 20px;
    list-style: none;
    padding-bottom: 20px;
}

#kms_knowledge_index_new_content_box .new_list_content ul li{
    padding:4px 0;
    font-size: 14px;
    color: #333333;
    line-height: 22px;
    font-weight: 400;
    /* border-bottom: 1px dashed #EEEEEE; */
    position: relative;
    padding-bottom: 12px;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_date_info{
    display:inline-block;
    border: 1px solid #F0F3FB;
    border-radius: 4px;
    width:58px;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_date_info .new_list_date_day{
    font-size: 16px;
    color: #333333;
    letter-spacing: 0;
    text-align: center;
    font-weight: 500;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_date_info .new_list_item_year{
    font-size: 10px;
    color: #999999;
    letter-spacing: 0;
    text-align: center;
    font-weight: 400;
    background: #F8FAFF;
    border-radius: 0 0 4px 4px;
    border-top:solid 1px  #F0F3FB;;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_subject_info{
    display:inline-block;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_item_subject{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_item_subject:hover{
    color: #4285F4;
    cursor:pointer;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_item_info{
    font-size: 14px;
    color: #999999;
    line-height: 22px;
    font-weight: 400;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_subject_info{padding-left: 8px;}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_subject_info .new_list_subject{
    font-size: 14px;
    color: #333333;
    font-weight: 400;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_item_info .new_list_item_author{
    display:inline-block;
    font-size: 12px;
    color: #999999;
    letter-spacing: 0;
    font-weight: 400;
    min-width: 36px;
}

#kms_knowledge_index_new_content_box .new_list_content ul li .new_list_item_info .new_list_item_cat{
    display:inline-block;
    font-size: 12px;
    color: #999999;
    letter-spacing: 0;
    font-weight: 400;
    padding-left: 20px;
}


@media screen and (min-width:600px) and (max-width:1367px){
    #kms_knowledge_index_new_content_box .new_list_content ul li .new_list_subject_info{
        width:75%;
    }
}

@media screen and (min-width:1px) and (max-width:600px){
    #kms_knowledge_index_new_content_box .new_list_content ul li .new_list_subject_info{
        width:50%;
    }
}
