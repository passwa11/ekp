@charset "utf-8";
ul{
    list-style: none;
}
.clearfix:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.clearfix {
zoom: 1;
}
/* 复选框 */
.d_lui_mix_Checkbox{
    display: inline-block;
    height:32px;
    line-height: 32px;
    position: relative;
    margin-right: 10px;
    cursor: pointer;
    vertical-align: middle;
}
.d_lui_mix_Checkbox input[type="checkbox"] {
    position: relative;
    top:3px;
    left:0px;
    width: 15px;
    height: 15px;
    background-color: #fff;
    -webkit-appearance: none;
    border: 1px solid #c9c9c9;
    border-radius: 2px;
    outline: none;
    margin-right: 4px;
}
input[type="checkbox"]:checked {
    background: url('../images/check_box.png')
      no-repeat center;
    background-size: 100% 100%;
    border: 0;
}
.d_lui_mix_Checkbox em{
    font-size: 14px;
    color: rgba(17,31,44,0.85);
    user-select: none;
    font-style: normal;
}
.d_lui_mix_Checkbox strong{
    font-size: 12px;
    color: #333333;
    user-select: none;
    font-weight: bold;
}
.d_lui_mix_Checkbox input[type="checkbox"]:checked+em{
    color:#4285f4;
}
.muiHrArchivesPreviewBoxLabel{
    float: left;
    margin-right: 10px;
    height:32px;
    line-height: 32px;
    font-size: 14px;
    color: #565656;
}
/* 单选框 */
.d_lui_mix_RadioBox {
    display: inline-block;
    height:32px;
    line-height: 32px;
    position: relative;
    margin-right: 10px;
    padding-left: 24px;
    cursor: pointer;
}
.d_lui_mix_RadioBox>input{
    display: none;
}
.d_lui_mix_RadioBox label {
    width: 15px;
    height: 15px;
    position: absolute;
    top: 8px;
    left: 0px;
    cursor: pointer;
    background: url(../images/radio_high_light.png) no-repeat center;
    background-size: 100% 100%;
}
/* 选中 */
.d_lui_mix_RadioBox input:checked+label{
    background: url(../images/radio_selected.png) no-repeat center;
    background-size: 100% 100%;
}
/* 禁用 */
.d_lui_mix_RadioBox input:disabled+label{
    background: url(../images/radio_disable.png) no-repeat center;
    background-size: 100% 100%;
}
/* 禁用，选中 */
.d_lui_mix_RadioBox input:checked:disabled+label{
    background: url(../images/radio_selected_disable.png) no-repeat center;
    background-size: 100% 100%;
}
.d_lui_mix_RadioBox em {
    font-size: 14px;
    color: rgba(17,31,44,0.85);
    font-style: normal;
}
/* 弹窗 */
.d_lui_mix_pop_mask {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 11;
    overflow: hidden;
    text-align: center;
    -webkit-transition: background-color .2s ease-in-out;
    transition: background-color .2s ease-in-out;
    background-color: rgba(26,26,26,.65);
}
.d_lui_mix_pop{
    display: inline-block;
    min-width: 360px;
    min-height: 125px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
    border: 1px solid rgba(17,31,44,0.12);
    box-shadow: 0 4px 8px 0 rgba(17,31,44,0.12);
    border-radius: 8px;
    z-index: 99;
}
.d_lui_mix_pop_header{
    height: 60px;
    line-height: 60px;
    padding: 0 25px;
}
.d_lui_mix_pop_header>p{
    font-weight: 600;
    font-size: 16px;
    color: #111F2C;
    float: left;
    user-select: none;
}
.d_lui_mix_pop_header>span{
    float: right;
    font-size: 16px;
    color: rgba(17, 31, 44, 0.85);
    margin-right: 10px;
}
.d_lui_mix_pop_footer{
    padding: 0px 25px;
    margin-bottom: 20px;
    position: fixed;
    bottom: 0px;
    right: 0px;
}
.d_lui_mix_pop_footer_btn{
    float: right;
    min-width: 20px;
    padding:0 15px;
    height: 32px;
    line-height: 32px;
    background-color: #fff;
    border: 1px solid rgba(17,31,44,0.12);
    border-radius: 100px;
    margin-left: 10px;
    text-align: center;
    cursor: pointer;
    color: rgba(17,31,44,0.85);
    font-size: 14px;
}
.d_lui_mix_pop_header_close{
    line-height: 20px;
    cursor: pointer;
    margin-top: 20px;
}
.d_lui_mix_pop_footer_btn.primary{
    color: #fff;
    background-color: #0089FF;
}
.d_lui_mix_pop_content{
    padding:0 25px 15px 25px;
}
.d_lui_mix_RadioBoxList>li{
    height:40px;
}