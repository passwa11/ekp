/*153251 end*/
body input[type=radio]:after {
	content: '';
	width: 18px;
	height: 18px;
	position: absolute;
	/* top: -4px;
	left: 0px; */
	top: -3px;
	left: -3px;
	border: 1px solid rgba(17,31,44,0.12);
	background-color: #fff;
	border-radius: 50%;
	cursor: pointer;
	transform: scale(.8);
	visibility: visible;
	box-sizing: border-box;
}
body input[type=radio]:checked:after {
	content: '';
	width: 18px;
	height: 18px;
	position: absolute;
	/* top: -4px;
	left: 0px; */
	top: -3px;
	left: -3px;
	border: 5px solid #0089FF;
	background-color: #fff;
	border-radius: 50%;
	visibility: visible;
	box-sizing: border-box;
}

