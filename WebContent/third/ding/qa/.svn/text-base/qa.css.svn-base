@CHARSET "UTF-8";
body,ul,li,h1,h2,h3,h4,h5,h6,p,div,dl,dt,dd{
	margin:0;
	padding:0; 
}
img{
	border:0;
}
ul{
	list-style-type:none;
}
a{
	font-size:16px!important;
	color: #3eb2e6!important;
	text-decoration: underline!important;
}
em,i{ 
	font-style:normal;
}
.red{
	color: #dc2127;
}
.tab1{
	margin-left: 30px;
}
.qaContainer{
	width:1050px;
	margin:50px auto;
}
.qaHeader{
	font-size: 18px;
    color: #8f8f8f;
    font-weight: 100;
    border-bottom: 1px solid #ededed;
    padding-bottom: 10px;
}
.qaContent dl{
	padding-bottom: 20px;
    margin-top: 20px;
    border-bottom: 1px solid #ededed;
}
.qaContent dt{
	font-size: 16px;
    color: #737373;
    cursor: pointer;
}
.qaContent dd{
	display:none;
	margin-top: 16px;
    overflow: hidden;
}
.qaContent dd p{
	line-height: 26px;
    font-size: 14px;
    color: #9a9a9a;
    margin-left: 25px;
}