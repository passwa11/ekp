/* ------------ 01-热门商城模板 Starts ------------ */

body {
  overflow-x: hidden;
}
.lui_print_body{
  background:#e9ecef;
}

.lui_print_body .lui_list_body_frame{
  background: #e9ecef;
}

/* 流程子选项卡样式覆盖 */
body .lui_profile_listview_card_page{
  float:none;
  padding-top:10px;
  padding-left:20px;
}
.prompt_container {
  padding-top: 1px !important;
}

.cm-hot-template {
  width: 920px;
}

.cm-hot-template .hot-template-main {
  content: '';
  display: block;
  clear: both;
}

.cm-hot-template .hot-template-main .cascade {
  padding: 0 10px;
  font-size: 12px;
  color: #333;
}

.cm-hot-template .hot-template-main .cascade .first-level {
  margin-bottom: 8px;
  width: 100%;
}

.cm-hot-template .hot-template-main .cascade .first-level span {
  margin-right: 20px;
  padding-bottom: 14px;
  line-height: 30px;
}

.cm-hot-template .hot-template-main .cascade .first-level .active {
  color: #4285F4;
  border-bottom: 2px solid #4285F4;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv {
  float: right;
  width: 207px;
  height: 28px;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  padding: 3px 8px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv input {
  border: 0;
  width: 165px;
  max-width: 165px;
  font-size: 12px;
  color: #333;
  line-height: 20px;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv input::-webkit-input-placeholder {
  font-size: 12px;
  color: #999999;
  line-height: 18px;
}

.cm-hot-template .hot-template-main .cascade .first-level .searchDiv::after {
  content: "";
  display: inline-block;
  width: 14px;
  height: 14px;
  background: url("../images/hypermarket/search.png") no-repeat;
  background-size: 14px 14px;
  margin: 3px 0 0 5px;
  vertical-align: top;
}

.cm-hot-template .hot-template-main .cascade p {
  height: 1px;
  background: #EEEEEE;
}

.cm-hot-template .hot-template-main .cascade .second-level {
  margin: 20px 0;
}

.cm-hot-template .hot-template-main .cascade .second-level .active {
  color: #4285F4;
}

.cm-hot-template .hot-template-main .cm-items {
  float: left;
  margin: 10px;
  width: 160px;
  height: 181px;
  overflow: hidden;
  /* -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16); */
  /* box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16); */
  border-radius: 4px;
  position: relative;
  border: 1px solid #eee;
}
.cm-hot-template .hot-template-main .cm-items .cm-items-inner{
  padding-top: 25px;
}
.portal.cm-hot-template .hot-template-main .cm-items{
  border:none;
}
/* 更多单独处理 */
.cm-hot-template .hot-template-main .cm-items[data-formid='more'] .cm-items-inner{
  padding-top:0;
}

.cm-hot-template .hot-template-main .cm-items .item-box {
  position: absolute;
  width: 100%;
  height: 100%;
}

.cm-hot-template .hot-template-main .cm-items .item-box img {
  width: 160px;
  height: 90px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content {
  padding: 6px 10px 0;
  /* -webkit-box-sizing: border-box; */
  /* box-sizing: border-box; */
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .title {
  font-size: 14px;
  color: #333333;
  line-height: 22px;
  margin-bottom: 16px;
  height: 40px;
  width: 140px;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews {
  margin-bottom: 10px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews img {
  width: 12px;
  vertical-align: middle;
  margin-right: 2px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .icon-pic {
  width: 160px;
  background: url("../images/pic_1@2x.png");
  display: inline-block;
  background-size: contain;
  height: 90px;
  background: url("../images/pic_1.png")\9;
}

.cm-hot-template .hot-template-main .cm-items .item-box .icon-view {
  width: 16px;
  vertical-align: middle;
  margin-right: 4px;
  background: url("../images/icon_see@2x.png");
  display: inline-block;
  background-size: 100% auto;
  height: 16px;
  background: url("../images/icon_see.png")\9;
}

.cm-hot-template .hot-template-main .cm-items .item-box .icon-download {
  width: 16px;
  vertical-align: middle;
  margin-right: 4px;
  background: url("../images/icon_copy@2x.png");
  display: inline-block;
  background-size: 100% auto;
  height: 16px;
  background: url("../images/icon_copy.png")\9;
}

.cm-hot-template .hot-template-main .cm-items .item-box .icon-person {
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-right: 2px;
  border-radius: 50%;
  background: url("../images/pic_3@2x.png");
  display: inline-block;
  background-size: 20px 20px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .icon-more {
  width: 16px;
  height: 16px;
  vertical-align: middle;
  background: url("../images/more@2x.png");
  background: url("../images/more.png") \9;
  display: inline-block;
  background-size: contain;
  transition: all .3s;
}
.cm-hot-template .hot-template-main .cm-items .item-box:hover .icon-more{
  background: url("../images/more-active@2x.png");
  background: url("../images/more-active.png") \9;
  background-size: contain;
}
.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews span {
  display: inline-block;
  max-width: 20px;
  font-size: 12px;
  line-height: 18px;
  color: #999999;
  margin-right: 3px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .pageviews p {
  float: right;
  width: 56px;
  height: 16px;
  line-height: 16px;
  background: #fff;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #4285F4;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  font-size: 12px;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account {
  padding-top: 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  border-top: 1px solid #ddd;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account span {
  font-size: 12px;
  color: #333333;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right {
  float: right;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right p {
  display: inline-block;
  width: 49px;
  height: 18px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .content .account .right img {
  width: 20px;
  height: 20px;
  -o-object-fit: full;
  object-fit: full;
  border-radius: 50%;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-box .more {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  margin-top: 24px;
}

.cm-hot-template .hot-template-main div[data-formid="more"].cm-items {
  cursor: pointer;
}

.cm-hot-template .hot-template-main .cm-items .item-box .more span {
  font-size: 14px;
  color: #333333;
}

.cm-hot-template .hot-template-main .cm-items .item-box .more img {
  width: 16px;
  vertical-align: middle;
}

.cm-hot-template .hot-template-main .cm-items .item-mask {
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  border-radius: 4px;
  /* top: 10px; */
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content {
  position: absolute;
  top: 50%;
  /* left: 10px; */
  margin-top: -20px;
  width: 100%;
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content a {
  display: block;
  width: 64px;
  height: 24px;
  line-height: 24px;
  color:#fff;
  background: #4285F4;
  border-radius: 2px;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  font-size: 12px;
  margin-left: auto;
  margin-right: auto;
}
.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content a:hover{
  background: #4285F4;
  color:#fff;
}

.cm-hot-template .hot-template-main .cm-items .item-mask .mask-content a:first-child {
  margin-bottom: 10px;
  /* margin-right: auto; */
}

.cm-hot-template .hot-template-main .cm-items:hover .item-mask {
  display: block;
  top: 24px;
  right: -20px;
  left: 0px;
  bottom: 0;
  height:auto;
  padding:0 1px 0 0;
}
.portal.cm-hot-template .hot-template-main .cm-items:hover .item-mask{
  top:1px;
}

.cm-items [data-formid="more"] {
  cursor: pointer;
}

.cm-form-box-item-title {
  overflow: hidden;
  margin-bottom: 4px;
}

.cm-form-box-item-title p {
  font-size: 24px;
  color: #4285F4;
  float: left;
  margin: 0;
}

.cm-form-box-item-data {
  margin-top: 10px;
  /* float: right; */
  overflow: hidden;
}

.cm-form-box-item-data div {
  float: left;
  margin-left: 6px;
}

.cm-form-box-item-data div span {
  font-size: 12px;
  color: #333;
}

.cm-form-box-item-desc {
  margin: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 12px;
  color: #333;
}

/* ------------ 01-热门商城模板 Ends ------------ */

/** 导航 **/

.nav_main {
  text-align: center;
  cursor: pointer;
}

.nav_main_title {
  max-width: 180px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 12px;
}

.nav_main_selected {
  background-color: transparent;
  color: #4285f4;
  font-weight: normal;
  border-top: 0;
}

.nav_operation_ul .lui_block_grid_item {
  background-color: #FFF;
  height: 50px;
  line-height: 50px;
  border-radius: 2px;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  margin-right: 20px;
}

/* ------------ 04-无法链接到云商城 Starts ------------ */
.cm-interlink {
  width: 920px;
  height: 300px;
}

.cm-interlink .interlink-main {
  width: 238px;
  margin: 40px auto 0;
}

.cm-interlink .interlink-main img {
  width: 100%;
}

.cm-interlink .interlink-main span {
  display: inline-block;
  width: 238px;
  opacity: 0.8;
  font-size: 14px;
  color: #333333;
  text-align: center;
  margin: 20px 0;
}

.cm-interlink .interlink-main p {
  margin: 0 auto;
  width: 90px;
  height: 30px;
  line-height: 30px;
  background: #4285F4;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #fff;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
}

/* ------------ 04-无法链接到云商城 Ends ------------ */

/* ------------ 03-预览 Starts ------------ */
.cm-preViews {
  width: 920px;
  height: 680px;
}

.cm-preViews .preViews-title {
  width: 100%;
  height: 59px;
  background: #EEEEEE;
}

.cm-preViews .preViews-title span {
  display: inline-block;
  width: 105px;
  font-size: 16px;
  color: #333333;
  text-align: center;
  line-height: 59px;
}

.cm-preViews .preViews-title .typeActive {
  background-color: #fff;
}

.cm-preViews .preViews-title .client {
  cursor: pointer;
}

.cm-preViews .preViews-content img {
  display: block;
  margin: 30px auto 0;
  text-align: center;
}

.cm-preViews .preViews-content .imgWrap {
  display: inline-block;
  text-align: center;
  width: 100%;
  overflow-y: scroll;
  height: 600px;
}

.cm-preViews .preViews-content .pc-img {
  width: 100%;
  box-shadow:0 0 10px 0 rgba(153,179,210,0.20);
}

.cm-preViews .preViews-content .mb-img {
  width: 272px;
  box-shadow:0 0 10px 0 rgba(153,179,210,0.20);
}

.cm-preViews .preViews-footer {
  position: absolute;
  right: 20px;
  bottom: 20px;
}

.cm-preViews .preViews-footer span {
  display: inline-block;
}

.cm-preViews .preViews-footer .useIt {
  width: 90px;
  height: 30px;
  line-height: 30px;
  background: #4285F4;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #fff;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  margin-right: 15px;
}

.cm-preViews .preViews-footer .cancel {
  width: 90px;
  height: 30px;
  line-height: 30px;
  background: #fff;
  border: 1px solid #ddd;
  border-radius: 2px;
  color: #333;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
}
/* ------------ 03-预览 Ends ------------ */

.preViews-tips {
  font-size: 13px;
  color: red;
  position: absolute;
  bottom: 30px;
  left: 150px;
  display:none;
  max-width:550px;
}

