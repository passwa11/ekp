/* 重置 */
.ld-mall-temeplate .lui_tabpanel_frame:hover,
.ld-mall-temeplate .lui_config_form .lui_tabpanel_simple_frame:hover,
.ld-mall-temeplate .lui_config_form .lui_tabpanel_light_frame:hover{
