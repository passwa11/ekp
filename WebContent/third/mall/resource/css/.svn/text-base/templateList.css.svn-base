/* 9 预览登录页 */
.cm-preViews .preViews-footer {
  right: 0;
  bottom: 15px;
  padding-top: 12px;
  border-top: 1px solid #eee;
  width: 100%;
  text-align: right;
}

.cm-preViews .preViews-footer .useIt {
