	border-radius: 6px 0 0 6px;
}

.ld-person-account-div .ld-toBeHired {
	background: #4285F4;
}

.ld-person-account-div .ld-toBeLeft {
	background: #59D15F;
	border-radius: 0 6px 6px 0;
}

.enableSwitch h3 {
	display: inline-block;
}

.enableSwitch img {
	height: 21px;
