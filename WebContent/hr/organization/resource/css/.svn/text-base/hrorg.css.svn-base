}

.ld-edit-unit-form .formItem p s {
	float: left;
	height: 12px;
	width: 12px;
	background: url('../images/error_close.png') no-repeat center center;
	margin-top: 4px;
	border-radius: 50%;
	margin-right: 5px;
}

.formItem.ld-switch div {
	display: inline-block;
}

.formItem.ld-switch div span {
	float: left;
