}

.formItem.ld-switch div:last-child span {
	width: 80px;
}

.formItem.ld-switch div img {
	height: 21px;
	width: 46px;
	float: left;
}

.desc.formItem {
	width: 100%;
}

.desc.formItem input {
	width: 605px;
}

