/* 条状图 */
.ld-person-account-content {
	padding: 0 35px 0 25px;
	box-sizing: border-box;
}

.ld-person-account-content h3 {
	font-family: HiraginoSansGB-W6;
	font-size: 14px;
	color: #333333;
	margin-bottom: 6px;
	margin-left: 16px;
	text-align:center;
}

.ld-person-account-total {
	text-align: center;
	margin-bottom: 6px;
}
.account-vertical .ld-person-account-progress{
	height:auto;
}
.account-vertical .ld-person-account-progress>div{
	float:none;
	width:100%;
}
.account-vertical .ld-person-account-progress>div>div{
	width:0.2%;
	margin-top:9px;
	border-radius:0!important;
    border-top-right-radius: 6px!important;
    border-bottom-right-radius: 6px!important;	
}

.account-vertical .ld-person-account-progress>div>P{
	margin-left:20px;
}
.account-vertical .ld-person-account-progress>div>div,.ld-person-account-progress>div>P{
	float:left;
}
.ld-person-account-progress{
	width:100%;
	height:40px;
}
.ld-person-account-progress>div{
	height:40px;
	width:33.3%;
	float:left;
}
.ld-person-account-progress-onpost>div{
	background-color:#F4C242;
	height:10px;
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;	
}
.ld-person-account-progress-waitentry>div{
	background-color:#fe7883;
	height:10px;
}
.ld-person-account-progress-waitleave>div{
	background-color:#4285F4;
	height:10px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;	
}
.ld-person-account-progress>div:first-child>div{
	    border-top-left-radius: 6px;
    	border-bottom-left-radius: 6px;
}
.ld-person-account-progress>div:last-child>div{
	    border-top-right-radius: 6px;
    	border-bottom-right-radius: 6px;
}
.ld-person-account-div {
	width: 520px;
	margin: 0 auto;
	height: 10px;
	margin-bottom: 48px;
}
