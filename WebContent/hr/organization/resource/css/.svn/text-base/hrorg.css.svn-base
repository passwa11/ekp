
/* 清除浮动 */
.clearfix:after {
	content: "";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both
}

.clearfix {
	zoom: 1
}

html, body {
	height: 100%;
	width: 100%;
	color: #333;
}
.weui_switch_bd>i,.weui_switch_bd dfn{
	font-style: normal;
	font-size:12px;
	position:relative;
}
.weui_switch_bd>i{
	z-index:1;
	left:24px;
	color:#4285F4;
}
.weui_switch_bd dfn{
	left:5px;
	color:#fff;
}
.weui_switch input ~small{
	z-index:2;
}
.ld-setting-compile-form-tips{
	font-size:12px;
	color:#999;	
}

.ld-modal-header {
	height: 44px;
	line-height: 44px;
	border-bottom: 1px solid #EEEEEE;
	box-sizing: border-box;
}

.ld-modal-header div {
	display: inline-block;
	font-size: 16px;
	color: #333333;
	margin-left: 22px;
}

.ld-modal-header i {
	height: 11px;
	width: 11px;
	background: url('../image/close.png') no-repeat center center;
	float: right;
	vertical-align: middle;
	margin-top: 15px;
	margin-right: 17px;
}

/* footer */
.ld-modal-footer {
	height: 50px;
	border-top: 1px solid #eee;
	box-sizing: border-box;
	line-height: 50px;
	margin-top: 36px;
}

.ld-modal-footer>div {
	float: right;
	margin-top: 10px;
	height: 30px;
	width: 90px;
	text-align: center;
	border-radius: 4px;
	line-height: 30px;
	border: 1px solid #DDDDDD;
	box-sizing: border-box;
	cursor: pointer;
	margin-right: 17px;
}

.ld-modal-footer>div.ld-confirm {
	background: #4285F4;
	color: white;
	border: none;
	font-size: 14px;
}

.ld-setting-compile {
	width: 750px;
