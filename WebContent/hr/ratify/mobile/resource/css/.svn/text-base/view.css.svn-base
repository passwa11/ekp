html,body{
	background-color:#fff!important;
}
.muiFlowBack{background-color: #F7F7F7; }
.muiFlowInfoW {margin-top:0.5rem; background: #fff; border-bottom:1px solid #ddd; padding-left:1.2rem;text-align: left;}
.muiFlowInfoW .muiFlowTable tr td{ border: none !important;}
.muiFlowInfoW .muiFlowTable .muiFlowCreator{color:#999;font-size: 1.3rem; padding-top:0.2rem; text-align: center;}
.muiFlowInfoW .muiFlowTable .muiFlowSummary{color:#999999;font-size: 1.3rem;}
.muiFlowInfoW .muiFlowTable .muiFlowIconTd{ width:4rem; padding-right:1rem; }
.muiFlowInfoW .muiFormSubject{padding-top:1.5rem; color: #2e2e2e;font-size: 2rem; overflow: hidden; line-height: 2.4rem;word-break: break-all;font-weight: bold;}
.muiFlowInfoW .muiNavbarContainer,.muiFlowFixedItem .muiNavbarContainer{ background: #fff; border-bottom: 1px solid #f7f7f7;}
.muiFlowInfoW .muiFlowHeader { display: table; width: 100%; padding: 0.8rem 0; position:relative; }
.newMui .muiFlowInfoW .muiFlowHeader{padding: 0.8rem 0 0 0;}
.muiFlowInfoW .muiFlowHeader > div { display: table-cell; vertical-align: middle; }
.muiFlowInfoW .muiFlowHeader .muiProcessIcon {  width: 4.2rem; padding:0 0.8rem; }
.muiFlowInfoW .muiFlowHeader .muiProcessIcon img{ width:4.2rem; height:4.2rem;}
.muiFlowInfoW .muiFlowHeader .muiProcessTitle { font-size: 1.4rem; }
.muiFlowInfoW .muiFlowHeader .muiProcessTitle .muiFormEleWrap {
  display: flex;
  align-items: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  padding-left: 0;
  
}
.muiFlowEditFixedItem{ font-size:0;}

.muiFlowEditFixedItem .muiNavbar {
  padding: 1rem 0rem;
  /* box-sizing: border-box; */
}
.muiFlowEditFixedItem .muiNavbarContainer{ padding:0 1rem;}

.muiFlowEditFixedItem .muiNavitem {
  background-color: #fff;
  font-size: 16px;
  color: #666;
  margin: 0!important;
  padding: 0;
  width: 50%;
  
}
.muiFlowEditFixedItem .muiNavitem:first-child{
  
}

.muiFlowEditFixedItem .muiNavitem:after{
  display:inline-block;
  content:"";
  width:1rem;
  content:"";
  color:#f00;
  height: 100%;
  background: #f7f7f7;
  transform: skewX(-10deg) translateX(0.5rem);
  transform-origin: center;
  z-index: 2;
  position:absolute;
  right: 0;
  top: 0;
  padding-top: 0.1rem;
  padding-bottom: 0.2rem;
}
.muiFlowEditFixedItem .muiNavitem:nth-child(2):after{
  left:0;
  right:auto;
  transform:skewX(-10deg) translateX(-0.5rem);
}
.muiFlowEditFixedItem .muiNavitem span {
  font-size: 16px;
  color: #666;
  margin: 0;
  width: 100%;
  max-width: 100%;
}

.muiFlowEditFixedItem .muiNavitemSelected {
  background-color: #4285f4;
}

.muiFlowEditFixedItem .muiNavitemSelected span {
  color: #fff
}
.muiFlowEditFixedItem .muiNavitem span em{ margin-left:0.1rem; margin-right:0.3rem;}


.muiProcessStatus { filter:alpha(opacity=0);-moz-opacity:0; opacity:0; width: 6rem; height: 3.8rem; padding-right: 1rem; position: absolute; top:1.5rem;right: 0.2rem;}
.muiProcessStatus .mui {
  vertical-align: top;
  font-size: 9rem;
  color: #6abea3;
  position: absolute;
  top: -1rem;
  right: 0;
}
.muiProcessStatus.stamp{ filter:alpha(opacity=100);-moz-opacity:1; opacity:1; animation:stamp 0.2s;-webkit-animation:stamp 0.2s; transition:all 0.1s ease 0s;}

.muiDiscardStatus { filter:alpha(opacity=0);-moz-opacity:0; opacity:0; width: 6rem; height: 3.8rem; padding-right: 1rem; }
.muiDiscardStatus .mui { font-size: 9rem; color: #ed726f;  }
.muiDiscardStatus .mui:before{ position:relative;} 

.muiDiscardStatus.stamp { }

.muiDiscardStatus.stamp{ filter:alpha(opacity=100);-moz-opacity:1; opacity:1; animation:stamp 0.2s;-webkit-animation:stamp 0.2s; transition:all 0.1s ease 0s;}
@-webkit-keyframes stamp {
    0% {-webkit-transform: scale(2);}
 100% {-webkit-transform: scale(1);}
}
@keyframes stamp {
     0% { transform:scale(2);}
  100% {transform:scale(1);}
}

.muiSubProcessList, .muiParentProcessList {
  margin: 0;
    padding: 0;
}
.muiSubProcessListItem, .muiParentProcessListItem {
  position: relative;
    display: block;
    list-style: none;
    border-bottom: 1px solid #e2e2e2;
}

.muiSubProcessList>.muiSubProcessListItem:last-child,
.muiParentProcessList>.muiParentProcessListItem:last-child
{
  border-bottom: 0;
}
.muiSubProcessLink, .muiParentProcessLink {
  display: block;
    height: 25px;
    line-height: 25px;
    padding: 12px 1rem;
    font-size: 1.4rem;
}
.muiSubProcessCreatorImg, .muiParentProcessCreatorImg {
  float: left;
    vertical-align: top!important;
    margin-right: 10px;
    border: 0;
    width: 25px;
    height: 25px;
    border-radius: 50%;
    background-color: #f8f8f8;
}
.muiSubProcessLabel, .muiParentProcessLabel {
  display: block;
    padding-right: 1rem;
    max-width: none;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    color: #000;
}
.oldMui .muiFlowInfoW  .muiSimple{
  margin-left:1rem;
}
.muiFormEleAuditShowWrap{
  padding-right:1rem;
}
