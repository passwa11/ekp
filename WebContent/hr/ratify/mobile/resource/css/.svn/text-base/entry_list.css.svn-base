html,body{
	background-color:#fff!important;
}

.entry_scanner,.entry_addenterh{
	height: 2rem;
    width: 2rem;
	display:inline-block;
	background-size:cover;
	position:relative;
	top:.4rem;
	margin-right:.24rem;
}
.entry_scanner{
	background-image:url('../images/item/scaner.png');
}
.entry_addenterh{
	background-image:url('../images/item/addreentry.png');
}


.entryItemNode{
	margin-top:2.7rem;
	margin-bottom:1rem;
}

.entryItemNode{
	display:flex;
}
.entryItemInfo{
	display:flex;
	flex-direction:column;
	margin-left:1.65rem;
}
.entryItemIcon{
	border-radius:.4rem;
	background-position:center;
	width:5.52rem;
	height:5.52rem;
	margin-left:1.65rem;
	position:relative;
}
.applicantIconMale {
    background: url(../images/male.png) no-repeat center;
}
.applicantIconFemale {
    background: url(../images/female.png) no-repeat center;
}
.entryItemIconSpan{
	display: inline-block;
    position: absolute;
    height: 3.5rem;
    width: 3.5rem;
    right: -1rem;
    bottom: -1rem;
    background-size: auto;
    background-color: #fff;
    transform: scale(0.5);
    border-top-left-radius: 1rem;
}

.entryItemName{
	font-size:1.76rem;
	display:flex;
}
.entryItemName span{
	color:#3E4665;
}
.entryItemstatus{
	background:url(../images/item/qrcode.png) no-repeat;
	width:2rem;
	height:2rem;
	background-size:cover;
	display:inline-block;
	position:relative;
	top: .4rem;
	margin-left:.2rem;
}
.entryItemTime{
	margin-top:1.54rem;
	display:flex;
}
.entryItemEnterTime:after{
	content:'|';
	color:#C9CAD1;
	padding-left:1.14rem;
}
.entryItemModifiedTime{
	margin-left:1.14rem;
}
.entryItemEnterTime,.entryItemModifiedTime{
	font-size:1.32rem;
	line-height:1.24rem;
	color:#999FB7;
}
.entryItemEnterTime span,.entryItemModifiedTime span{
	color:#999FB7;
	margin-right:1.2rem;
	background: #F7F7F7;
	border: 1px solid #CFD4E9;
	border-radius: 2px; 
	width: 40px;
    height: 16px;
    line-height: 16px; 
	display:inline-block;
	text-align:center;
	font-size:1.1rem;
}
.entrySearch{
    margin: .88rem 1.65rem;
    height: 3.75rem;
    position: relative;
}
.entrySearch input{
    background-color: #F7F7F7;
    border-radius: .5rem;
    height: 3.7rem;
    width: -webkit-fill-available;
    margin: 0 auto;
    padding-left: 3.86rem;
    border: 1px solid #fff;
    outline: none;
    background-repeat: no-repeat;
    background-position: -.1rem center;
    font-size: 1.32rem;
    position: absolute;
    color: #3E4665;
}
.entrySearch input::-webkit-input-placeholder{
	color:#C9CAD1;
}
.entrySearch .icon{
	background-image:url('../images/item/search_icon.png');
	display:inline-block;
	height:3.3rem;
	width:3.3rem;
	position:absolute;
	top:.4rem;
	left:.552rem;
	background-size:cover;
}

