
html,body{
	scroll-behavior:smooth;
	background-color:#fff!important;
}
.personal-files {
    width: 100%;
    height: 100%;
}

.personal-basic-info {
    display: flex;
    height: 75px;
    padding: 12px 20px 12px 15px;
    box-sizing: border-box;
    background: #F7F7F8;
}

.personal-photo {
    width: 50px;
}

.personal-photo img {
    width: 50px;
    height: 50px;
}

.personal-name-box {
    margin-left: 15px;
    margin-top: 3px;
    flex: 1;
}

.personal-name span {
    font-size: 16px;
    color: #3E4665;
}

.personal-name img {
    width: 20px;
    height: 20px;
    vertical-align: middle;
    margin-left: 10px;
}

.personal-name-box .edit-date {
    font-size: 12px;
    color: #999FB7;
    margin-top: 8px;
    position:absolute;
}

.personal-name-box .edit-date span:nth-child(3) {
    color: #4285F4;
    margin-left: 15px;
}

.right-btn-edit {
    width: 20px;
}

.right-btn-edit img {
    width: 20px;
    height: 20px;
    margin-top: 5px;
}

.personal-info-nav {
    height: 44px;
    line-height: 44px;
    display: flex;
    font-size: 14px;
}

.personal-info-nav li {
    height: 44px;
    flex: 1;
    text-align: center;
    color: #3E4665;
    position: relative;
}

.personal-info-nav .blue-line {
    position: absolute;
    background: #4285F4;
    border-radius: 2px;
    width: 2.53rem;
    height: .25rem;
    bottom: 5px;
    left: 50%;
    margin-left: -1.26rem;
}

.basic-info-title {
    width: 100%;
    height: 30px;
    background: #F7F7F8;
    padding-left: 15px;
    box-sizing: border-box;
    line-height: 30px;
    color: #999FB7;
    font-size: 12px;
}

.basic-info-item {
    height: 50px;
    width: 100%;
    line-height: 50px;
    padding: 0 15px;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    font-size: 14px;
}

.basic-info-item p:nth-child(1) {
    color: #999FB7;
}

.basic-info-item p:nth-child(2) {
    color: #3E4665;
}
.basic-info-item .none{
    color: #C9CAD1 !important;
}
.personal-item-bottom-btn{
    width: 100%;
    height: 64px;
    padding: 10px;
    box-sizing: border-box;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1000;
}
.personal-item-bottom-btn div{
    background: #4285F4;
    border-radius: 5px;
    text-align: center;
    color: #fff;
    font-size: 16px;
    height: 44px;
    line-height: 44px;
}
.work-experience{
    padding:0 15px;
    width: 100%;
    box-sizing: border-box;
    padding-bottom: 15px;
}
.work-experience-item{
    padding: 25px 15px 25px 15px;
    box-sizing: border-box;
    height: 93px;
    border: 1px solid #C9CAD1;
    border-radius: 5px;
    margin-top: 15px;
    display: flex;
    justify-content: space-between;
}
.company-name p:nth-child(1){
    color: #3E4665;
    font-size: 14px;
}
.company-name p:nth-child(2){
    color: #999FB7;
    font-size: 14px;
    margin-top: 8px;
}
.work-experience-item .date{
    color:#999FB7;
    font-size: 12px;
}
.education-info{
    display: flex;
    padding: 15px 15px 0 15px;
    box-sizing: border-box;
    justify-content: space-between;
}
.education-info .education-info-title span{
    font-size: 14px;
    color: #999FB7;
}
.education-info .education-info-title img{
    width: 17px;
    height: 17px;
    margin-left: 7px;
    vertical-align: middle;
}
.education-info .add{
    font-size: 12px;
    color: #4285F4;
}
.add-education-experience{
    padding: 15px 15px 0 15px;
    width: 100%;
    box-sizing: border-box;
    font-size: 14px;
    color: #999FB7;
}
.add-education-experience-btn{
    height: 40px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #C9CAD1;
    border-radius: 5px;
}
.add-education-experience-btn .add-icon{
    width: 20px;
    height: 20px;
    vertical-align: text-top;
}
.entry_others_empty{
	background:url('../images/empty.png') no-repeat;
	height: 4.5rem;
    background-size: cover;
    width: 3.5rem;
    background-position: center;
    margin:0 auto;
}
.entry_empty{
	margin:2rem 0;
}
.entry_empty p{
	color:#C9CAD1;
	text-align:center;
	font-size:1.32rem;
	margin-top: 1.1rem;
}