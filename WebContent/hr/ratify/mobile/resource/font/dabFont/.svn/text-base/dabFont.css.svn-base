@font-face {
    font-family: "dabFont";
    src: url("dabFont.eot"); /* IE9 */
    src: url("dabFont.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url("dabFont.woff") format("woff"), /* chrome、firefox */
    url("dabFont.ttf") format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("dabFont.svg#dabFont") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}


