/*** 新地址本样式 ***/
div.mf_container {
  border: 0px;
  cursor: text;
  display: inline-block;
  /* padding: 2px; */
  width: 100%;
  white-space: normal;
}

div.mf_container ol.mf_list {
  display: inline;
  padding-left:0px;
}

div.mf_container ol.mf_list li.mf_item {
  border: 1px solid transparent;
  border-radius:10px;
  cursor: pointer;
  display: inline-block;
  margin: 2px 0px;
  padding-left: 2px;
  padding-right: 0px;
  color: #1b83d8;
  transition-duration: .3s;
}

div.mf_container ol.mf_list li.mf_item.mf_highlighted {
  color: #f56b6b;
  background-color: transparent;
}
div.mf_container ol.mf_list li.mf_item.mf_selected a.mf_remove,
div.mf_container ol.mf_list li.mf_item.mf_highlighted a.mf_remove{
  visibility: visible;
  color: #f56b6b!important;
}

div.mf_container ol.mf_list li.mf_item.mf_selected {
  color: #f56b6b;
  background-color: transparent;
}
div.mf_container ol.mf_list li.mf_item a.mf_remove {
  color: #e0e0e0;
  font-size: 12px;
  margin-left: 2px;
  text-decoration: none;
  transition-duration: .3s;
  visibility: hidden;
}

div.mf_container ol.mf_list li.mf_item.mf_highlighted a.mf_remove {
  color: #FFFFFF;
}

div.mf_container ol.mf_list li.mf_item.mf_selected a.mf_remove {
  color: #FFFFFF;
}

div.mf_container input.mf_input {
  border: 0;
  font: inherit;
  font-size: 100%;
  /* margin: 2px; */
  outline: none;
  /* padding: 4px; */
  min-height:20px;
}

ol.mp_list {
  min-width: 200px;
  width: 100%!important;
  max-width: 550px;
  margin: 3px 0 0;
  padding-left:0;
  max-height: 400px;
  border: 1px solid #e0e0e0;
  background-color: #fff;
  -webkit-box-shadow: 0 1px 8px rgba(0,0,0,.125);
     -moz-box-shadow: 0 1px 8px rgba(0,0,0,.125);
      -ms-box-shadow: 0 1px 8px rgba(0,0,0,.125);
       -o-box-shadow: 0 1px 8px rgba(0,0,0,.125);
          box-shadow: 0 1px 8px rgba(0,0,0,.125);
  overflow: hidden;
  overflow-y:auto;
  position: absolute;
  z-index: 1030;
}

ol.mp_list li {
  border-bottom: 1px solid #e0e0e0;
  padding: 8px 10px;
  font-size:14px;
  font-weight: normal;
  line-height:20px;
  color:#333;
}

ol.mp_list li>span {
	color:#999;
}

ol.mp_list li.mp_item {
	overflow:visible;
}

ol.mp_list li.mp_selectable {
  cursor: pointer;
  overflow:visible;
}

ol.mp_list li.mp_highlighted {
  background-color: #f7f7f7;
  overflow:visible;
}
/* 新地址本样式结束 */



