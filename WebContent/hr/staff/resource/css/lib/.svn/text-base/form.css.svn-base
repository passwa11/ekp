/**页面背景**/
.lui_form_body {
  background-color: #f7f6f6;
}

/**三级页面主体内容**/
.lui_form_content {
  background-color: #fff;
}
.lui_form_content_td .lui_form_content .lui_ask_view_content {
  width: auto;
  float: none;
}
/** 标题**/
.lui_form_title_frame {
}
.lui_form_subject {
  text-align: center;
  font-size: 18px;
  line-height: 30px;
  padding: 12px 5px 5px;
}
.lui_form_title_frame .lui_form_baseinfo {
  font-size: 12px;
  text-align: center;
  line-height: 20px;
  margin: 0px 0px 5px;
}
.lui_form_title_frame .lui_form_baseinfo span,
.lui_form_title_frame .lui_form_baseinfo .com_author {
  margin-right: 8px;
}

/** 摘要**/
.lui_form_summary_frame {
  border: #ebebeb 1px solid;
  background: #f8f8f8;
  font-size: 12px;
  margin: 0px 14px 10px;
  padding: 10px 10px 6px 10px;
  line-height: 20px;
  text-indent: 2em;
  word-break: break-word;
}

/** 正文和附件**/
.lui_form_content_frame {
  padding: 0 6px 10px;
}
.lui_form_content_frame .groupDivHead,
.lui_form_content_frame .grougDiv{
  margin-right: 10px;
}

.lui_form_content_frame .groupDivHead{
  margin-bottom: 10px;
}
.lui_form_content_frame .groupDivHead:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.lui_form_content_frame .groupDivHead .groupDivHead_left{
  padding-left: 5px;
  float: left;
  max-width: 40%;
}
.lui_form_content_frame .groupDivHead .groupDivHead_right{
  float: right;
  max-width: 50%;
  text-align: right;
}
.lui_form_content_frame .groupDivHead .groupDivHead_right a{
  margin-left: 10px;
  color: #4285f4;
  font-size: 12px;
}
.lui_form_content_frame .groupDivHead .groupDivHead_right a:hover{
  text-decoration: underline;
}
.lui_form_content_frame .grougDiv{
  margin-bottom: 10px;
}
.lui_form_content_frame .grougDiv > table{
  width: 100%;
}
.lui_form_content_frame .grougDiv > table > tbody > tr{
  background-color: #f9f9f9;
}
.lui_form_content_frame .grougDiv > table > tbody > tr > td{
  padding: 5px 0;
}
.lui_form_content_frame .grougDiv .tongleHead{
  background-color: transparent;
}
.lui_form_content_frame .grougDiv .upload_opt_td{
  position: relative;
  cursor: pointer;
}
.lui_form_content_frame .grougDiv .upload_opt_td:before{
  content: '';
  display: block;
  border-bottom: 1px solid #d9d9d9;
  position: absolute;
  left: 15px;
  right: 25px;
  top: 50%;
}
.lui_form_content_frame .grougDiv .upload_opt_td:after{
  content: '';
  display: block;
  width: 16px;
  height: 16px;
  background: url(/ekp/sys/ui/extend/theme/default/images/panel/toggle_light_1.gif) no-repeat 50%;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -8px;
}
.lui_form_content_frame .grougDiv .unexpanded .upload_opt_td:after{
  background-image: url(/ekp/sys/ui/extend/theme/default/images/panel/toggle_light_2.gif);
}

.lui_form_content_frame .grougDiv .upload_opt_td > div{
  background-color: #fff;
  position: relative;
  z-index: 1;
}
.lui_form_content_frame .grougDiv .upload_opt_td input[type=checkbox], 
.lui_form_content_frame .grougDiv .upload_opt_td input[type=radio]{
  margin-left: 5px;
}
.lui_form_content_frame .grougDiv .upload_list_icon{
  padding-left: 5px;
}
.lui_form_content_frame .grougDiv .upload_opt_batchdown{
  padding: 0 5px;
}
.lui_form_content_frame .grougDiv .upload_list_status{
  text-align: right;
}


/*.lui_form_content_frame div p{ line-height:22px;}*/
.lui_form_spacing {
  border-top: 1px dashed #bbb;
  margin-top: 30px;
  margin-bottom: 5px;
}
.lui_form_subhead {
  padding: 0px 5px;
  line-height: 30px;
  font-size: 14px;
  font-weight: bold;
}

/*文档信息内容*/
.lui_form_info {
  line-height: 25px;
  list-style: none;
}
.lui_form_info li {
  word-break: break-all;
  word-wrap: break-word;
}

/******表单控件******/

/*按钮*/
.lui_form_button {
  border: none;
  color: #fff;
  padding: 5px 10px;
  cursor: pointer;
  text-decoration: none;
  background-color: #51B5EB;
}

/*输入控件颜色、大小、边距*/
textarea,
.input input,
.inputsgl,
select {
  color: #1b83d8;
  padding-left: 4px;
}

/*输入控件只读状态颜色、大小、边距*/
textarea[readonly],
.input input[readonly],
.inputsgl[readonly],
select[readonly] {
  color: #333;
}
textarea[readonly]{
  resize: none;
  border-color: #eee;
}

/*下拉框*/
select {
  border: 1px #b4b4b4 solid;
  height: 22px;
  line-height: 22px;
  box-sizing: content-box;
}

xformflag select{
	height: 22px;
}

/*多行文本垂直拖拽*/
textarea {
  resize: vertical;
}

/*单选，多选*/
input[type=checkbox], input[type=radio] {
  margin-right: 2px;
}
input[name=DocList_Selected]:HOVER {
  cursor: auto;
}

/*单行文本*/
.inputsgl,
.tb_normal .inputsgl {
  /* height: 20px; */
  border: 0px;
  border-bottom: 1px solid #b4b4b4;
  white-space: nowrap;
  padding-bottom: 1px;
}

/*默认、有边框*/
.tb_simple .inputsgl {
  height: 25px;
  border: 0px;
  border: 1px solid #b4b4b4;
}

/*无边框*/

/*多行文本*/
.inputmul, textarea {
  height: 80px;
  border: 1px solid #b4b4b4;
  overflow-y: auto;
  vertical-align: top;
}

/*单行带选择的输入框，结构：.inputselectsgl>.input>input*/

/*外层*/
.inputselectsgl,
.tb_normal .inputselectsgl {
  min-height: 20px;
  line-height: 20px;
  border-bottom: 1px solid #b4b4b4;
  position: relative;
  cursor: pointer;
  display: inline-block;
  *display: inline;
  vertical-align: text-bottom;
}

/*默认、有边框*/
.tb_simple .inputselectsgl {
  height: 30px;
  border: 1px solid #b4b4b4;
  position: relative;
  cursor: pointer;
  display: inline-block;
  *display: inline;
  line-height: 25px;
}
.inputselectsglreadonly {
  border: none;
}
.inputselectsglreadonly input {
  border: none;
  color: #868686;
  width: 100%;
}

/*中间层*/
.inputselectsgl .input {
  display: block;
  border: 0px;
  position: absolute;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 23px;
  cursor: pointer;
  line-height: 20px;
  overflow: hidden;
}

/*里层*/
.inputselectsgl input {
  width: 100%;
  height: 100%;
  border: 0px;
  resize: none;
  overflow: hidden;
}

/*多行带选择的输入框，结构：.inputselectmul>.textarea>textarea*/

/*外层*/
.inputselectmul {
  height: 80px;
  border: 1px solid #b4b4b4;
  position: relative;
  cursor: pointer;
  display: inline-block;
}

/*中间层*/
.inputselectmul .textarea {
  padding-left: 4px;
  display: block;
  border: 0px;
  position: absolute;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 23px;
  cursor: text;
  line-height: 20px;
}

/*里层*/
.inputselectmul textarea {
  padding-left: 0px;
  width: 100%;
  height: 100%;
  border: 0px;
  resize: none;
  overflow: auto;
  line-height:26px;
}

/*选择框右边小图标*/
.inputselectsgl .orgelement, .inputselectmul .orgelement {
  background: url(/ekp/sys/ui/extend/theme/default/images/form/orgelement.png);
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  width: 23px;
  height: 100%;
  right: 0px;
}

/*组织*/
.inputselectsgl .selectitem, .inputselectmul .selectitem {
  background: url(/ekp/sys/ui/extend/theme/default/images/form/selectitem.png);
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  width: 23px;
  height: 100%;
  right: 0px;
}

/*其它*/
.inputselectsgl .inputdatetime {
  background: url(/ekp/sys/ui/extend/theme/default/images/form/datetime.png);
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  width: 23px;
  height: 100%;
  right: 0px;
}

/*时间*/
.inputselectsgl .inputtime {
  background: url(/ekp/sys/ui/extend/theme/default/images/form/time.png);
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  width: 23px;
  height: 100%;
  right: 0px;
}

/*时间*/
.inputselectsgl .search {
  background: url(/ekp/sys/ui/extend/theme/default/images/form/search.png);
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  width: 23px;
  height: 100%;
  right: 0px;
}

/*搜索*/

/*多行选择框图标左边加上边线*/
.inputselectmul .orgelement, .inputselectmul .selectitem {
  border-left: 1px #b4b4b4 solid;
}

