/*---------------------- 通讯录 ---------------------*/
ul, li {
	list-style: none;
}

.lui_zone_address_containter {
	margin: 0px auto;
	background: #fff;
	padding: 10px 0px;
}

.lui_zone_address_content {
	padding: 0 10px;
}

.lui_zone_address_frame {
	border: 1px solid #e0e0e0;
	display: table;
	width: 100%;
	table-layout: fixed;
	min-height: 500px;
}

.lui_zone_address_frame:after {
	content: "";
	clear: both;
	display: table;
}

/*---------- 选项卡菜单（左侧）----------*/
.lui_zone_address_menu {
	display: table-cell;
	vertical-align: top;
	width: 300px;
	padding: 10px 0;
	border-right: 1px solid #e0e0e0;
	position: relative;
}

/*----- 搜索框 -----*/
.lui_zone_address_search {
	margin-left: 15px;
	margin-right: 15px;
	font-size: 0;
	position: relative;
}

.lui_zone_address_search > input {
	padding: 5px 12% 5px 5%;
	width: 82.5%;
	height: 20px;
	line-height: 20px;
	vertical-align: top;
	border: 1px solid #e0e0e0;
	border-radius: 30px;
	background-color: #fff;

}

.lui_zone_address_searchBtn {
	width: 32px;
	padding: 0;
	vertical-align: top;
	border-top-right-radius: 30px;
	border-bottom-right-radius: 30px;
	background: transparent url(../images/icon_search.png) no-repeat center;
	-webkit-appearance:none;
	border: 0;
	transition-duration: .3s;
	cursor: pointer;
	position: absolute;
	right: 0;
	top: 1px;
	bottom: 1px;
}
.lui_zone_address_searchBtn:hover {
	background-color: #f8f8f8;
	background-image: url(../images/icon_search_hover.png);
}
.lui_zone_address_searchBtn:active{
	outline: 0;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
  				box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
}
/*---- 选项卡 -----*/
.lui_zone_address_menus {
	margin-bottom: 5px;
	padding-left: 15px;
	padding-right: 15px;
}

.lui_zone_address_menus_ul {
	border-bottom: 1px solid #e0e0e0;
}

.lui_zone_address_menus_ul:after {
	content: '';
	display: table;
	visibility: hidden;
	clear: both;
}

.lui_zone_address_menus_ul > li {
	float: left;
	width: 33.3%;
	position: relative;
	top: 1px;
	text-align: center;
}
.lui_zone_address_menus_ul > li > span {
	margin: auto 1px;
	padding: 10px 0;
	border-bottom: 2px solid transparent;
	transition-duration: .3s;
	display: block;
	cursor: pointer;
}
.lui_zone_address_menus_ul > li > span:hover{ color: #15a4fa;}
.lui_zone_address_menus_ul > li.active > span,
.lui_zone_address_menus_ul > li.active > span:hover{
	color: #15a4fa;
	border-color: #15a4fa;
}

/*----- 目录树 -----*/
.lui_zone_address_tree { /* padding:10px; */
	min-height: 300px;
}
/* 超链接 */
.lui_zone_address_tree a {
	color: #222;
	text-decoration: none;
}
/* 鼠标经过的超链接 */
.lui_zone_address_tree a:hover {
	color: #15a4fa;
}

.lui_zone_address_tree li.active>a {
	color: #15a4fa;
}
/* 菜单 */
.lui_zone_address_tree ul {
	padding: 0 0 0 18px;
	margin: 0;
}

.lui_zone_address_tree ul:first-child {
	padding-left: 0;
}
/* 菜单项 */
.lui_zone_address_tree ul li {
	font-size: 13px;
	color: #222;
	line-height: 20px;
	cursor: pointer;
	list-style: none;
	background: url(../images/st_node.png) no-repeat left center;
	padding: 3px 0 3px 16px;
}
/* 子菜单 */
.lui_zone_address_tree ul li ul {
	
}
/* 子菜单项 */
.lui_zone_address_tree ul li ul li {
	
}
/* 子菜单的父节点 */
.lui_zone_address_tree .folder {
	background: url(../images/st_folder.png) no-repeat left center;
	padding: 3px 0 3px 16px;
}
/* 展开的父节点 */
.lui_zone_address_tree .open {
	background: url(../images/st_folder_open.png) no-repeat left center;
	padding: 3px 0 3px 16px;
}
/* 当前节点 */
.lui_zone_address_tree .current a {
	color: #15a4fa !important;
}

/*---------- 部门详细信息 （中间） ----------*/
.lui_zone_address_child_middle {
	padding: 10px 0;
	width: 300px;
	border-right: 1px solid #e0e0e0;
	display: table-cell;
	vertical-align: top;
}

.department_name {
	margin: 0;
	padding: 6px 15px;
	line-height: 20px;
	color: #333;
	font-size: 16px;
	font-weight: normal;
	background-color: #fff;
	position: relative;
	z-index: 1;
}

.department_name > em {
	color: #ff6f0f;
	margin: 0 3px;
	font-style: normal;
}
.lui_zone_address_org{
	height: 536px;
	overflow: auto;
	overflow-x: hidden;
}
.lui_zone_address_org > dt {
	color: #7a7a7a;
	font-size: 14px;
	margin-top: 20px;
	margin-bottom: 8px;
	padding: 0 15px;
}
.lui_zone_address_org > dt:empty{ height: 0;margin-bottom: 0;margin-top: 0;}
.lui_zone_address_org > dt:first-child{ margin-top: 12px; }
.lui_zone_address_org dd {
	margin: 3px 0;
	cursor: pointer;
}

.lui_zone_address_org dd:after {
	display: table;
	content: "";
	clear: both;
}

/*--- 人员信息 ---*/
.lui_zone_address_personItem {
	padding: 5px 15px;
	transition-duration: .3s;
}

.lui_zone_address_personItem:after {
	display: table;
	content: "";
	clear: both;
}

.lui_zone_address_personItem .personImg {
	width: 40px;
	height: 40px;
	border-radius: 50%;
	overflow: hidden;
	float: left;
	border:1px solid #e0e0e0;
	background-color: #f8f8f8;
}

.lui_zone_address_personItem .personImg img {
	width: 100%;
	height: 100%;
}

.lui_zone_address_personItem .personInfo {
	margin-left: 55px;
}

.lui_zone_address_personItem .personInfo .person_name {
	font-size: 14px;
	color: #333;
	margin-top:2px;
	margin-bottom: 2px;
}

.lui_zone_address_personItem .personInfo .person_dep {
	font-size: 12px;
	color: #999;
}

/*--- 部门信息 ---*/
.lui_zone_address_depItem {
	padding: 5px 15px;
}

.lui_zone_address_depItem .depImg {
	width: 40px;
	height: 40px;
  border: 1px solid #e0e0e0;
	background-color: #e6e6e6;
	border-radius: 50%;
	overflow: hidden;
	transition-duration: .3s;
	float: left;
}

.lui_zone_address_depItem .icon_depart {
	background: url(../images/icon_org.png) no-repeat center;
	display: inline-block;
	width: 40px;
	height: 40px;
}
.lui_zone_address_depItem .icon_depart_external {
	background: url(../images/icon_depart_external.png) no-repeat center;
	display: inline-block;
	width: 40px;
	height: 40px;
	background-size: 100% 100%;
}

.lui_zone_address_depItem .depInfo {
	margin-left: 55px;
}

.lui_zone_address_depItem .depInfo .dep_name {
	font-size: 14px;
	color: #333;
	margin-top:2px;
	margin-bottom: 2px;
	transition-duration: .3s;
}

.lui_zone_address_depItem .depInfo .dep_num {
	font-size: 12px;
	color: #999;
}

/* 选中效果 */
.lui_zone_address_org dd.active .lui_zone_address_depItem .depImg,
.lui_zone_address_org dd:hover .lui_zone_address_depItem .depImg {
	background-color: #15a4fa;
}
.lui_zone_address_org dd.active .lui_zone_address_depItem .dep_name,
.lui_zone_address_org dd:hover .lui_zone_address_depItem .dep_name {
	color: #15a4fa;
}
.lui_zone_address_org dd.active .lui_zone_address_personItem .personInfo .person_name,
.lui_zone_address_org dd:hover .lui_zone_address_personItem .personInfo .person_name {
	color: #15a4fa;
}
.lui_zone_address_org dd.active .lui_zone_address_personItem,
.lui_zone_address_org dd:hover .lui_zone_address_personItem {
	background-color: #f8f8f8;
}

/*---------- 详细信息（右侧） ----------*/
