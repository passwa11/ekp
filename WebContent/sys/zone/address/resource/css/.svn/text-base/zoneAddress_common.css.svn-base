/* 二维码 */
.lui_personal_code {
	/*margin: 15px auto 0;*/
	padding: 15px;
	width: 200px;
	border: 1px solid #e0e0e0;
	text-align: center;
	/*#159237 因为二维码在员工描述同一列会当到员工的信息，故修改为相对父元素固定到右下角 update by zhouwen 20220127*/
	position: absolute;
	right: 0;
	bottom: 0;
}

.lui_personal_code:after {
	display: table;
	content: "";
	clear: both;
}

.lui_personal_code>.icon_qrcode {
	display: inline-block;
	width: 168px;
	height:168px;
}

.lui_personal_code>.icon_qrcode img {
	width: 100%;
	height: 100%;
}

.lui_personal_code p {
	margin-top: 5px;
	line-height: 1;
	text-align: center;
	font-size: 12px;
}

/* 二维码浮层 */
.lui_personal_code_float {
	display: none!important;
	position: absolute;
	left: 24px;
	bottom: 93px;
	background: #fff;
	width: 160px;
	height: 160px;
	border: 1px solid #e0e0e0;
	box-shadow: 0 0 5px #e0e0e0;
	padding: 10px;
}

.lui_personal_code_float .icon_trg {
	position: relative;
}

.lui_personal_code_float .icon_trg:before, .lui_personal_code_float .icon_trg:after
	{
	display: inline-block;
	content: "";
	width: 0;
	height: 0;
	border: 10px solid transparent;
	position: absolute;
	left: 80px;
	bottom: -25px;
}

.lui_personal_code_float .icon_trg:before {
	border-top-color: #e0e0e0;
}

.lui_personal_code_float .icon_trg:after {
	border-top-color: #fff;
	bottom: -24px;
}

