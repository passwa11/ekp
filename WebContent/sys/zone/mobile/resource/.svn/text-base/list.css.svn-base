

.muiHeader{ margin-bottom:1px;}
/**list**/
.mui_zone_detail_show {
	display: none!important;
}

.muiZoneDetail {
	display: table;
	position: relative;
	padding: 10px 0px;
	width: 100%;
	background: #399be8;
	color: #ffffff; 
}
.muiZoneDetail .trig {
	display: block;
	width: 0px;
	height: 0px;
	border: 6px solid transparent;
	border-bottom-color: #399be8;
	position: absolute;
	top: -11px;
	left: 2.7rem;
}
.muiZoneDetail .mui_zone_detail_item_container {
	display:table-cell;
	width:20%;
	text-align: center; vertical-align:middle;
}
.muiZoneDetail .mui_zone_detail_item {
	display: inline-block;
	border: .1rem solid #fff;
	border-radius: 50%;
	height: 3.2rem;
	width: 3.2rem;
	line-height: 3.2rem;
	text-align: center;
	
}
.mui_zone_detail_item .mui {
	line-height:3.2rem;
	color:#fff;
	cursor: pointer; font-size:2.5rem;
}
.mui_zone_cate_icon {
	vertical-align: top!important;
	width:5rem;
}
.mui_zone_cate_phone, .mui_zone_cate_post {
	color: #989898;
	font-size: 1.2rem;
	line-height: 1;
}
.mui_zone_cate_post {
	margin-bottom: .3rem;
}
.mui_zone_cate_person_name {
	font-size: 1.7rem;
	font-weight: normal;
	color:#52B1F6;
	padding-top: .2rem;
	padding-bottom: .5rem;
}
.mui_zone_cate_phone .mui-tel,
.mui_zone_cate_post .mui-post{
	margin-right:.3rem;
	vertical-align: top;
}

.muiCateInfo {
	padding-left: 1.2rem;
}

.mui_zone_content_border {
	border-bottom: .1rem solid #399be8;
}

.muiCateHeaderContent {
	min-height: 4rem;
}

.muiZoneContactUl {
	min-height: 10rem !important;
	margin-top: -25%;
	position: absolute;
	top: 50%;
	left: 50%;
	width: 90%;
	margin-left: -45%;
	-webkit-appearance: none;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	min-width: 20rem;
	min-height: 20rem;
	/* margin: 0;*/
	padding: 0;
}
.muiZoneContact {
	float: left;
	/* margin: 1.5rem 1.2rem; */
	width: 33%;
	text-align: center;
	margin:1rem 0;
	-webkit-transition: all 0.5s ease-out;
	-webkit-transform: translate3d(0px,1000px, 0px);
}
.muiZoneDetailItem {
	display: inline-block;
	border-radius: 50%;
    height: 6.6rem;
    width: 6.6rem;
    line-height: 6.6rem;
	text-align: center;
	color: #fff;
	font-size: 28px;
}
.muiZoneDetailItemText {
	display: block;
	color: #FFFFFF;
	padding: 0.4rem 0;
}
.muiZoneDetailItem .mui {
	font-size: 2.4rem;
	vertical-align: middle;
}
.muiZoneDetailItemBgColorGray{
	background-color: #BBBBBB;
}
.muiZoneDetailItemBgColor0 {
	background-color: #FFB900;
}
.muiZoneDetailItemBgColor1 {
	background-color: #2BCF8F;   
}
.muiZoneDetailItemBgColor2{
	background-color: #6E93FF;
}
.muiZoneDetailItemBgColor3{
	background-color: #6E93FF;
}
.muiZoneDetailItemBgColor4{
	background-color: #2BCF8F;
}
.muiZoneDetailItemBgColor5 {
	background-color: #EECC00;
}
.muiZoneContactMask {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: rgba(0, 0, 0, 0.7);
	z-index: 2;
	overflow: hidden;
	-webkit-transition: opacity 0.2s linear;
	opacity: 0;
}
/*关注按钮样式*/
.muiCateMore {
	padding-right: .5rem;
	text-align: right;
	cursor: pointer;
}


/*** 地址本 ***/
.muiGroupItem{ background:#f4f4f4; border-bottom:1px solid #dfdfdf;}
.muiCateTitle{ padding:.3rem .5rem;}



.muiSearchInput{ -webkit-appearance: none; -webkit-tap-highlight-color:rgba(255,255,255,0);  height:3.4rem; line-height:2rem;}
.muiCateHeaderReturn .mui-back{ font-size:1.8rem;}
.muiCateHeaderReturn{ -webkit-tap-highlight-color:rgba(255,255,255,0); }

.muiCateMore .mui-forward{ margin-left:2rem;}
.muiCateNav{ text-align:center; right:0.1rem;}
.muiCateNav .muiCateNavItem{ font-size:10px!important; }
.muiCateNav .muiCateNavItem .mui-search{  font-weight:bold;}

.muiListNoData .muiListNoDataArea{
    vertical-align: top;
}


/**----------------------员工黄页包屑导航（Start）----------------------**/
.mui_zone_list_breadcrumb_vav_sv{
	background-color: #F7F7F8;
    margin-top: 1rem;
    line-height: 3rem;
}

.mui_zone_list_breadcrumb_nav_container{
}

.mui_zone_list_breadcrumb_nav{
	white-space: nowrap;
}

.mui_zone_list_breadcrumb_nav .mui_zone_list_breadcrumb_nav_item{
	display: inline-block;
	color: #3E4665;
	height: 3rem;
    line-height: 3rem;
}

.mui_zone_list_breadcrumb_nav .mui_zone_list_breadcrumb_nav_item:first-child{
	margin-left: 1.2rem;
}
.mui_zone_list_breadcrumb_nav .mui_zone_list_breadcrumb_nav_item:last-child{
	margin-right: 1.2rem;
}

.mui_zone_list_breadcrumb_nav_item_text{
	display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 30rem;	
}
.mui_zone_list_breadcrumb_nav_item_separator{
	display: inline-block;
	vertical-align: top;
	margin-left: .4rem;
	margin-right: .4rem;
	font-size: 1rem;
}
/**----------------------员工黄页包屑导航（End）----------------------**/

/**----------------------员工黄页列表（Start）----------------------**/
.mui_zone_list_item{

}

.mui_zone_list_item_divider_line{
	height: 1rem;
    background-color: #F7F7F8;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container{
    position: relative;
    display: table;
    width: 100%;
    padding-top: 1.2rem;
    padding-left: 1.2rem;    
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container>div{
	display: table-cell;
	padding-bottom: 1.2rem;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon{
    vertical-align: middle;
    width: 5.6rem;
    border-radius: 50%;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon .mui_zone_list_item_icon_org{
    width: 4rem;
    height: 4rem;
    line-height: 4rem;
    text-align: center;
    font-size: 2rem;
    background-color: #4285F4;
    color: #FFFFFF;
    overflow: hidden;
    border-radius: 50%;   
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon .mui_zone_list_item_icon_org.external{
    background-color: #fe8a2c;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon .mui_zone_list_item_icon_org>.fontmuis{
	color: #FFF;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon .mui_zone_list_item_icon_org>i.fontmuis.muis-group{
	font-size: 2rem;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_icon .mui_zone_list_item_icon_person{
    width: 4rem;
    height: 4rem;
    border: .1rem solid #c9dfff;
    overflow: hidden;
    border-radius: 50%;    
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info{
	vertical-align: middle;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info,
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_more,
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_btn{
	position: relative;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info:after,
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_more:after,
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_btn:after{
    content: " ";
    position: absolute;
    z-index: 2;
    bottom: 0;
    left: 0;
    right: 0;
    border-top: .1rem solid #EEEEEE;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);	
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_name{
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    -webkit-line-clamp: 2;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    box-orient: vertical;
    -webkit-box-pack: start;
    box-pack: start;
    -webkit-box-align: start;
    box-align: start;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_person_name{
	display: inline-block;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_follow_icon{
	position: relative;
    display: inline-block;
    vertical-align: top;
    margin-left: .6rem;
    margin-top: .4rem;
    color: #FFB900;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_follow_icon>i{
	position: absolute;
    top: 0;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_follow_icon>i.fontmuis.muis-follow{
   font-size: 1rem;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_follow_icon>i.fontmuis.muis-mutual-follow{
   font-size: 1rem;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_post{
    margin-top: .4rem;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_phone{
	margin-top: .2rem;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_post span:first-child,
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_info>div.mui_zone_list_item_phone span:first-child{
	margin-right: 0.6rem;
}



.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_more{
    vertical-align: middle;
    width: 3.2rem;
    padding-left: 1rem;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_more>i.fontmuis.muis-to-right{
    font-size: 1rem;
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_btn{
    vertical-align: middle;
    width: 5rem;
    
}
.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_btn .mui_zone_list_item_opt{
    display: block;
    height: 3rem;
    line-height: 3rem;
    padding-left: 1rem;
}

.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_container .mui_zone_list_item_btn .mui_zone_list_item_opt>i.fontmuis.muis-more{
   font-size: 1.6rem;	
}


.mui_zone_list_item .mui_zone_list_item_content .mui_zone_list_item_header{
	background-color: #F7F7F8;
    color: #3E4665;
    margin-top: 1.2rem;
    padding: 0 1.2rem;
    height: 3rem;
    line-height: 3rem;
}
/**----------------------员工黄页列表（End）----------------------**/





