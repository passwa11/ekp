.muiPersonSearchItem {
	list-style: none;
	overflow: hidden;
	display: block;
}

.muiPersonSearchIContent {
	display: block;
	padding: 0.8rem 0.5rem 0.8rem 1rem;
	border-bottom: 1px solid #dfdfdf;
}

.muiPersonSearchItemContariner {
	clear: both;
	width: 100%;
	overflow: hidden;
	display: table;
	border: none;
	table-layout: fixed;
}

.muiPersonSearchItemContariner>div {
	vertical-align: middle;
	height: 100%;
	border: none;
	display: table-cell;
}

.muiPersonSearchIcon {
	vertical-align: top !important;
	width: 5rem;
}

.muiPersonSearchIcon span {
	width: 5rem;
	height: 5rem !important;
	border-radius: 50%;
	overflow: hidden;
	text-align: center;
	line-height: 2.5rem;
	vertical-align: middle;
}

.muiPersonSearchItemInfo {
	padding-left: 1.2rem;
}

.muiPersonSeachItemDep {
	padding: 0.6rem 0 0;
	color: #9d9d9d;
	font-size: 1.5rem;
}

.muiFilterSearchBar {
	border-bottom: none !important;
}

.muiFilterSearchSecHeader {
	box-shadow: none;
	border-top: 1px solid #d9d9d9;
	margin-bottom: 0;
}

.muiTabfilterDiaItem {
	border-left: 1px solid rgba(209, 209, 209, 0.1);
}

.muiTabfilterAddressItem {
	color: #3564aa;
	width: 4rem;
	height: 100%;
	cursor: pointer;
}



.muiTabfilterResultBar {
    clear: both;
    display: block;
    height: 3rem;
    line-height: 3rem;
    padding: 0 1.4rem;
    background: #dedede;
    border-top: 0.1rem #d6d5da solid;
    border-bottom: 0.1rem #d6d5da solid;
    color: #979797;
    font-size: 1.4rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.muiTabfilterResultBar em {
	color:#d02300 ;
}

