#filterSettingMain{
	text-align: center;
	width:480px;
	height:80px;
}

#filterSettingMain #selectList{
	float:left;
	width:160px;
	height:400px;
	border: #D1DAD9 1px solid;
	overflow:scroll;
}
#filterSettingMain #selectList #customProperty{
	text-align: left;
}
#filterSettingMain #selectList #customProperty a div,#filterSettingMain #selectList #regularFilter a div{
	margin-left:15px;
	line-height:22px;
	text-align: left;
	white-space: nowrap; 
}

#filterSettingMain #selectList .header{
	padding-left:5px;
	width:160px;
	height:25px;
	padding-top:6px;
	font-size: 12px;
	font-weight: bold;
	text-align: left;
	background-color: #f5f5f5;
}
#filterSettingMain #property .header{
	padding-left:5px;
	width:100%;
	height:25px;
	padding-top:6px;
	font-size: 12px;
	font-weight: bold;
	text-align: left;
	background-color: #f5f5f5;
}

#filterSettingMain #property{
	text-align:left;
	float:right;
	width:295px;
	height:400px;
	border: #D1DAD9 1px solid;
	overflow-y:auto;
}
.filterSettingMainBottom{
	padding-top:20px;
	height:60px;
	
}

a.Selected{
	height:22px;
	width:160px;
	text-align: left;
	font-weight: bold;
}
UnSelect a{
	height:22px;
	width:160px;
	text-align: left;
}

A:link {
	text-decoration: none;
	color: #1B75A8;
	font-size:12px;
}
	
A:visited {
	text-decoration: none;
	color: #1B75A8;
	font-size:12px;
}
	
A:active {
	text-decoration: none;
	color: #4F9CDE;
	font-size:12px;
}
	
A:hover {
	text-decoration: none;
	color: #4F9CDE;
	font-size:12px;
}