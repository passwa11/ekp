}
.lui_eval_tooltip_share .lui_eval_mtrig {
	width: 0;
	height: 0;
	line-height: 0;
	border-width: 0 5px 5px 5px;
	border-style: dashed dashed solid dashed;
	position: absolute;
	right: 30px;
}
.lui_eval_tooltip_share .lui_eval_cover {
	border-color: transparent transparent #eefaff transparent;
