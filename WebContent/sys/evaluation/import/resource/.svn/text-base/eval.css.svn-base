.lui_eval_eva_title {
	padding: 10px 11px 6px 14px;
	clear: both;
}
.lui_form_summary_frame {
	border: #ebebeb 1px solid;
	background: #f8f8f8;
	font-size: 12px;
	margin: 0px 14px 10px;
	padding: 10px 10px 6px 10px;
	line-height: 20px;
	text-indent: 2em;
}
.lui_eval_eva_object {
	height: 60px;
	overflow-y: auto;
}
.lui_eval_eva_title {
	padding: 10px 11px 6px 14px;
	clear: both;
}
.lui_eval_eva_content {
	padding: 4px 11px 6px 14px;
	clear: both;
}
.eval_info .com_author {
	margin-right: 18px;
	float: left;
}
.eval_icons{
	display: inline-block;
	height: 24px;
	line-height: 24px;
	padding-left: 20px;
	background: url(images/icon_biaoqing.png) no-repeat 0 50%;
	cursor: pointer;
