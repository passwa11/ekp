	top: -9px;
	left: 20px;
	background-position: 0 0;
}
.eval_bq_arrow_down {
	position: absolute;
	overflow: hidden;
	display: block;
	background: url(images/layer_arrow_down.png) no-repeat;
	width: 14px;
	height: 9px;
	top: 169px;
	left: 20px;
	background-position: 0 0;
}
.eval_reply_author{
	margin-right:5px;
}
.inputText{
	width: 96%;
	height: 32px;
	line-height: 32px;
	border: 1px solid #E7E7E7;
