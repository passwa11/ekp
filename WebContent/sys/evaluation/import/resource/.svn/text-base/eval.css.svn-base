/*---------------------*/
.qtip-contentWrapper {
	border: 2px solid #56b7ec;
	background:#fff;
}

.evaluation_div {
	padding:10px;
}

.qtip-contentWrapper em {
	font-style: normal;
}

.qtip-contentWrapper .qtip-info {
	line-height: 22px;
	font-size: 14px;
}

.qtip-contentWrapper .qtip-info span {
	word-wrap: break-word;
}

.qtip-contentWrapper .qtip-user {
	margin-top: 5px;
	text-align: right;
	font-size: 14px;
}

.qtip-contentWrapper  strong {
	font-weight: normal;
}

.lui_form_content_frame img[e_id] {
	height: auto;
	vertical-align: text-top;
	margin: 0 5px;
}
.eval_level.lui_icon_s_recommend{ color:#ffaf00; padding-left:20px;}
.eval_level.lui_icon_s_poor{ color:#333; padding-left:20px;}
.eval_record_msg .tmpl_cur{ position: relative;}
.eval_record_msg .tmpl_cur .sys_unpraise{ background:url(images/icon_unpraise.png) no-repeat left center;  width: 16px; height:16px; vertical-align: middle;}
.eval_record_msg .tmpl_cur .sys_praise{ background:url(images/icon_praise.png) no-repeat left center;  width: 16px; height:16px; vertical-align: middle;}
.praise_count { vertical-align: middle;}
.eval_record_msg .tmpl_cur .sys_unpraise+.praise_count{ color:#ffaf00;}
/* 编辑器 */
.evel_edit_body{ padding:5px; min-height: 60px;}
.evel_edit_body img{ vertical-align: text-bottom;}

/* MAC系统高清屏特殊处理 */
@media screen and (-webkit-min-device-pixel-ratio:2) {
	.eval_record_msg .tmpl_cur .sys_unpraise{ background:url(images/icon_unpraise@2x.png) no-repeat left center; background-size:16px; }
	.eval_record_msg .tmpl_cur .sys_praise{ background:url(images/icon_praise@2x.png) no-repeat left center; background-size: 16px;}
	.eval_reply{background: url(images/icon_comment@2x.png) no-repeat left 3px; background-size: 16px;}
	.reply_off{background: url(images/icon_comment_off@2x.png) no-repeat left 3px; background-size: 16px;}
}



.lui_eval_addition_info {
	height: 16px;
    line-height: 16px;
    color: #a3a3a3;
    margin-bottom: 10px;
}
.lui_eval_addition_ul {
	list-style: none;
}
.lui_eval_addition_ul li {
	border-top: 1px dashed #e7e7e7;
	padding: 10px 0 5px 0;
}
.lui_evaluation_title {
	padding: 8px 0;
	color: #a3a3a3;
	display: none;
}

/*弹窗内容居左样式重置 Starts*/
.lui_eval_eva_title,
.lui_form_summary_frame,
.lui_eval_eva_content{
	text-align: left;
}
/*弹窗内容居左样式重置 Ends*/

#my_view{
	position: relative;
	z-index: 1;
}

/*点评内容换行显示*/
.linefeed_desc {
	white-space: pre-line;
	padding-top: 0 !important;
}