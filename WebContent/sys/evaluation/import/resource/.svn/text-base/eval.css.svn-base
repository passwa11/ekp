}

.lui_eval_tooltip_share a.lui_eval_share_icon2 {
	background: url(images/tooltip_share_icon_2.png) no-repeat 0 50%;
}
.lui_eval_tooltip_share a {
	padding-left: 22px;
	font-size: 14px;
	color: #3e9ece;
}
