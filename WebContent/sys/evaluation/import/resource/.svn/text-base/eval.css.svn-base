/*doc_edit page*/
.eval_EditMain{
	height:  auto!important;
	text-align:left;
	min-height: 160px;
	width:100%;
	width: auto;
}

.eval_title_color{
	color:#646464; 
}

.eval_opt_table,.lui_paging_box{
	background-color: transparent!important;
}

.eval_opt_table tr td{
	padding: 6px 0px;
}
.tb_simple.eval_opt_table > tbody > tr > td{
	padding: 6px 0px;
}

.eval_title{
	font-size: 14px;
	text-align: left;
	margin: 0px 6px 0px 0px;
	height: 20px;
	line-height: 20px;
	vertical-align: middle;
	font-weight: bold;
	display: inline-block;
}
#eval_main .eval_title{ margin-bottom:10px;}
.eval_star{
	height: 20px;
	display: inline-block;
	vertical-align: middle;
}

.eval_star li{
	list-style:none;
	display: inline-block;
	width: 16px;
	height: 20px;
	line-height: 20px;
	cursor: pointer;
	background-position: center center;
	background-repeat: no-repeat;
}
.eval_level{
	display:inline-block;
	height: 20px;
	line-height: 20px;
	vertical-align:middle;
	padding-left:16px;
	color:#e07e27;
	display: inline-block;
	background-position: left center;
	background-repeat: no-repeat;
}
.eval_content{
	height: 80px;
	width: 100%;
	border: 1px solid #DFDFDF; 
	overflow-y:auto;
}

.eval_button{
	width: 80px;
	height: 80px;
	border: 1px solid #307cc5;
	font-size: 16px;
	font-weight: bold;
	cursor: pointer;
	background: url("images/btn_bg.jpg") repeat-x; 
	color:#ffffff;
}

.eval_button:HOVER{
	background: url("images/btn_on_bg.jpg") repeat-x; 
}

.eval_button_disable{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x!important; 
	cursor: default;
}
.eval_button_disable:HOVER{
	border: 1px solid gray;
	background: url("images/btn_bg1.jpg") repeat-x; 
	cursor: default;
}

.eval_notify{
	height: 30px;
	line-height:30px;
	vertical-align: middle;
	display:inline-block;
}
.eval_prompt{
	display:none;
	height: 30px;
	line-height:30px;
	vertical-align:middle;
	margin-right:14%;
	float: right;
	position: relative;
	top:-2px;
}

.eval_hidden{
	display: none;
}

.eval_record{
	width:100%;
	margin-bottom:10px;
}


.eval_record_content{
	word-break: break-all;
	word-wrap: break-word;
	margin-bottom: 5px;
	line-height:18px;
	color:#646464;
}

.eval_record_content img{ vertical-align: text-bottom;}

.eval_summary{
	height: 16px;
	cursor: default;
}
.eval_summary span{
	height: 16px;
	float: left;
	margin-right: 8px;
}

.eval_evaler{
	color:#F09802;
}

.eval_summary_star{
	width: 120px;
	height: 16px;
	display: inline;
}

.eval_summary_star li{
	float:left;
	list-style: none;
	display: inline-block;
	width: 16px;
	height: 20px;
	line-height: 20px;
	cursor: pointer;
	background-position: center center;
	background-repeat: no-repeat;
}
.eval_delopt{
	display:none;
	color:#54a3cc;
	cursor: pointer;
	float:right!important;
}
.eval_doc_score{ 
	 position:relative;
	 height:26px; 
	 line-height:26px;
	 padding:10px 0;
}
.eval_doc_score span{
	display: inline-block;
}
.eval_score_mark{
	position:relative;
 	color:#ffb644; 
 	font-size:24px; 
 	font-weight:bold; 
 	/*font-style:italic;*/ 
 	height:26px; 
 	line-height:26px; 
 	margin-bottom:10px;
 	margin-left: 90px;
 	top:-4px;
}
.eval_score,.eval_score_val{
	position:absolute;
	left: 0px;
	top: 10px;
	height:22px;
	width: 80px;
	background-position: left center;
	background-repeat: repeat-x;
} 
.eval_record_dl>dd {
	position: relative;
	padding-left: 70px;
	padding-top: 18px;
}
.eval_record_dl dd .img {
	position: absolute;
	left: 5px;
	top: 18px;
}
.eval_record_dl dd .img img {
	width: 48px;
	height: 48px;  
	border: 1px solid #e2e2e2;
	border-radius: 50%;
}
.eval_record_dl dd .txt {
	border-bottom: 1px solid #e7e7e7;
	/**padding-bottom: 18px;**/
}
.eval_record_dl dd .eval_info {
	line-height: 16px;
	color: #a3a3a3;
	margin-bottom: 10px;
}
.eval_record_dl dd .eval_author {
	color: #ffae00;
	font-size: 14px;
}
.eval_record_dl  .eval_delopt {
	color: #a3a3a3;
	text-align: right;
	width: 30px;
	cursor: pointer;
	margin-right:0px!important;
	font-size: 12px;
	position: absolute;
	right: 5px;
}
.eval_record_dl .eval_delopt:hover { 
	color:#ffaf00;
}
.eval_praise_reply{
	text-align: right;
	padding-bottom: 3px;
}
.eval_praise {
	background: url(images/icon_thumbsUp.png) no-repeat left center;
	padding-left: 20px;
	cursor: pointer;
}
.eval_reply{
	background: url(images/icon_comment.png) no-repeat left 1px;
	padding-left: 17px;
	cursor: pointer;
	margin-left:15px;
	position: relative;
	vertical-align: middle;
}
.reply_off{
	background: url(images/icon_comment_off.png) no-repeat left 1px;
}
.eval_reply_info{
	/*background: #f8f8f8;*/
	padding: 8px 0px 10px;
}
.eval_reply_info ul {
	list-style: none;
}
.reply_content{
	/**
	background: #f8f8f8;
	padding: 10px 15px 10px;
	height:85px;**/
}
.reply_content .inputText{ width:99%; padding-left: 1%; margin-left: 0; margin-right: 0;}
.replyIframe{
	/*background: #f8f8f8;*/
	padding: 5px 0px 35px;
	height:87px;
}
.eval_replys{
	/*background:#f8f8f8;*/
	position: relative;
	display:none;
}
.eval_replys .trig {
	display: inline-block;
	width: 18px;
	height: 9px;
	/*background: url(images/ico_trig.png) no-repeat left center;*/
	position: absolute;
	right: 12px;
	top: -9px;
}
.eval_reply_info .reply_li {
	position: relative;
	padding-left: 48px;
	margin-bottom: 10px;
	padding-top:10px; 
	border-top:1px dashed #e7e7e7;
}
.eval_reply_info ul:first-child .reply_li{border-top:none; padding-top:0}
.eval_reply_info ul:first-child .reply_li .rtnPerson{ top:0}
.eval_reply_info ul li .rtnPerson {
	position: absolute;
	left: 0px;
	top: 10px;
	width: 36px;
	height: 36px;
	border: 1px solid #e2e2e2;
}
.eval_reply_info ul li .eval_re_cont {
	margin-bottom: 0px;
	color: #6b6b6b;
	line-height: 24px;
	word-break: break-all;
}
.eval_reply_info ul li .eval_re_cont img{ vertical-align: text-bottom; margn:0 3px;}
.eval_re_cont .eval_author{
	margin: 0px 7px;
}
.eval_re_time{
	line-height: 22px;
	margin-bottom: 5px;
	color: #a3a3a3;
	text-align: left;
	font-size:12px;
}
.eval_re_time:after{
	display: table;
	content:"";
	clear: both;
}
.eval_re_time span{ float: right;}
.eval_re_time span a{ font-size:12px;}
.eval_re_reply{
	/**background: url(images/icon_comment.png) no-repeat left center;**/
	/**margin-left: 22px;**/
	padding-left: 20px;
	cursor: pointer;
	color: #a3a3a3;;
}
.eval_re_del,.eval_re_dialogue{
	float: none;
	margin-left: 22px;
	width: 50%;
	color: #a3a3a3;;
	cursor: pointer;
}
.eval_re_del:hover,.eval_re_dialogue:hover{ color:#ffaf00;}
.eval_reply_infos .eval_textarea,.eval_reply_infos .eval_textarea2{
	border: 1px solid #e6e6e6;
	background:white;
	width: 100%;
	height: 75px;
	overflow: hidden;
}
.eval_reply_submit{
	padding-top: 5px;
}
.eval_reply_tip{
	margin-right: 16px;
	line-height: 24px;
	color: #878787;
	display: block;
	float: right;
}
.reply_cont2{
	/*background: #f8f8f8;*/
	display:none;
	padding: 0 0 25px;
	height: 87px;
}
/*.eval_textarea2{
	border: 1px solid #e6e6e6;
	background:white;
	height: 55px;
	overflow: hidden;
}*/
.eval_reply_words{
	word-break:break-all;
	margin-left:10px;
}
.eval_quote_cont{
	background: #f8f8f8 url(images/quote_L.png) no-repeat 10px 10px;
	margin-bottom: 10px;
	line-height: 22px;
	color: #2f7daa;
	padding: 15px 20px 12px 45px;
}
.eval_quote_cont blockquote {
	background: #f8f8f8 url(images/quote_R.png) no-repeat right bottom;
	padding-right: 36px;
}
.eval_record_dl dd .eval_info .eval_author{
	color: #ffae00;
	font-size: 14px;
	cursor: pointer;
	margin-right: 18px;
	float:left;
}
.eval_star_display{
	height: 16px;
	cursor: default;
}
.eval_star_display .eval_summary_star li{ cursor: default;}
.eval_info span{
	float:left;
	margin-right: 18px;
}

