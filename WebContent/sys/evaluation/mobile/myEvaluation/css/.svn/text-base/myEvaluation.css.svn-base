
.muiRatingArea i, .muiRatingValue i {
    font-size: 1.2rem;
}

.muiMyEvaluationListItem {
	padding: 15px;
}

.muiMyEvaluationListItem .modelDocSubject {
	font-weight:bold;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size: 16px;
	color: #2A304A;
	letter-spacing: 0;
	line-height: 18px;
}

.muiMyEvaluationListItem .topBottomContainer {
	display:flex;
	margin-top:10px;
}

.muiMyEvaluationListItem .modelModuleName {
	padding:0.4rem;
	font-size: 1rem;
	color: #4285F4;
	letter-spacing: 0;
	line-height: 1rem;
	background: #E4EEFF;
	border-radius: 0.2rem;
	border-radius: 0.2rem;
	align-items: center; 
	display: flex;;
}


.muiMyEvaluationListItem .evalContainer {
	margin-top:20px;
	background: #FFFFFF;
	box-shadow: 0 0 15px 0 rgba(153,179,210,0.20);
	border-radius: 5px;
}

.muiMyEvaluationListItem .showHideButton {
	font-size: 12px;
	color: #999FB7;
	line-height: 12px;
	letter-spacing: 0;
	height:4.3rem;
	display:flex;
	align-items:center;
	justify-content:center;
}

.muiMyEvaluationListItem .arrrow-up {
	transform: rotate(180deg);
	height:1.2rem;
	width:1.2rem;
	margin-right:0.5rem;	
}

.muiMyEvaluationListItem .arrrow-down {
	height:1.2rem;
	width:1.2rem;
	margin-right:0.5rem;
}

.muiMyEvaluationListChildItem {
	
}

.muiMyEvaluationListChildItem .evalAddition {
	font-size: 14px;
	color: #999FB7;
	letter-spacing: 0;
	padding: 12px 12px 0 12px;
}

.muiMyEvaluationListChildItem .evalContent {
	font-size: 14px;
	color: #2A304A;
	letter-spacing: 0;
	line-height: 22px;
	padding: 12px 12px 0 12px;
}

.muiMyEvaluationListChildItem .evalBottom {
	display: flex;
	height: 50px; 
	align-items: center;
	padding: 0 12px;
}

.muiMyEvaluationListChildItem .evalTime {
	display:flex;
	flex:1;
	font-size: 12px;
	color: #999FB7;
	letter-spacing: 0;
	line-height: 18px;
}

.muiMyEvaluationListChildItem .deleteDiv {
	width: 50px;
	height: 100%; 
	display: flex;
	justify-content: center;
	align-items: center;
}

.muiMyEvaluationListChildItem .deleteImg {
	width:13.2px;
	height:14.2px;
}

.muiMyEvaluationListChildItem .hr {
	height:1px;
	background: #F4F4F4;
	box-shadow: 0 0 15px 0 rgba(153,179,210,0.20);
	border-radius: 5px;
}