.left
{
    float: none;
}

.right
{
    float: none;
}

body
{
    font-family: 微软雅黑;
    width: 100%;
    height: 480px;
}
a
{
    color: #69A2CD;
    cursor: pointer;
    text-decoration: none;
}
h1
{
    font-size: 1.3em;
    padding-bottom: 10px;
}
h2
{
    font-size: 1em;
    margin-top: 0px;
    margin-bottom: 8px;
}
p
{
    margin: 0px;
    padding: 0px;
    line-height: 1.5em;
}
div#content
{
    position: relative;
}
div.text
{
    padding: 10px;
}
div#left a
{
    color: #0000a0;
}
div.orgChart a
{
    color: black;
    text-decoration: none;
}
div.orgChart a:hover
{
    color: black;
    text-decoration: underline;
}
img.star
{
    width: 16px;
    height: 16px;
}



div.orgChart
{
    border: 0px solid #cccccc;
    background-color: #ffffff;
    margin: 10px;
    padding: 10px;
    display: inline-block;
}
div.orgChart h2
{
    margin: 0px;
    font-size: 1em;
    border-bottom: none;
}
div.orgChart ul
{
    list-style: none;
    margin: 4px;
    padding: 0px;
    font-size: 0.8em;
    text-align: left;
}
div.orgChart ul.stack, div.orgChart ul.stack ul
{
    text-align: center;
}
div.orgChart table
{
    width: 100%;
}
div.orgChart tr.lines td.line
{
    width: 1px;
    height: 20px;
}
div.orgChart tr.lines td.top
{
    border-top: 2px solid #E3E3E3;
}
div.orgChart tr.lines td.left
{
    border-right: 2px solid #E3E3E3;
}
div.orgChart tr.lines td.right
{
    border-left: 2px solid #E3E3E3;
}
div.orgChart td
{
    text-align: center;
    vertical-align: top;
    padding: 0px 2px;
}
div.orgChart div.node
{
    cursor: default;
    display: inline-block;
    padding: 0 0px;
    width: 150px;
    height: 80px;
    background-color: #FFFFFF;
    line-height: 1.3em;
    border: 1px #d5d5d5;
    border-radius: 4px;
    /**box-shadow: 0px 0px 4px 0px rgba(0,0,0,0.3);**/
    -moz-border-radius: 4px;
    /**-moz-box-shadow: 0px 0px 4px 0px rgba(0,0,0,0.3);**/
    -webkit-border-radius: 4px;
    /**-webkit-box-shadow: 0px 0px 4px 0px rgba(0,0,0,0.3);**/
}
div.orgChart div.hasChildren
{
    background-color: #ffffff;
}
div.orgChart.interactive div.hasChildren
{
    /**cursor: pointer;**/
}
div.orgChart div.node.hover
{
    background-color: white;
}
div.orgChart div.adjunct.node
{
    position: absolute;
    margin-top: 8px;
    margin-left: -110px;
    width: 80px;
    height: 40px;
    background-color: #efefef;
}
div.orgChart div.adjunct-link
{
    display: inline-block;
    position: absolute;
    margin-left: -20px;
    margin-top: 25px;
    width: 20px;
    border: 1px dashed black;
}

.divShowD
{
    background-color: #FFFFFF;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px; /*IE 7 AND 8 DO NOT SUPPORT BORDER RADIUS*/
    -moz-box-shadow: 0px 0px 4px rgba(0,0,0,0.3);
    -webkit-box-shadow: 0px 0px 4px rgba(0,0,0,0.3);
    box-shadow: 0px 0px 4px rgba(0,0,0,0.3); /*IE 7 AND 8 DO NOT SUPPORT BLUR PROPERTY OF SHADOWS*/
}

.headrow-bradius
{
    -moz-border-top-left-radius: 4px;
    -moz-border-top-right-radius: 4px;
    -webkit-border-top-left-radius: 4px;
    -webkit-border-top-right-radius: 4px;
    border-top-left-radius: 4px; /*IE 7 AND 8 DO NOT SUPPORT BORDER RADIUS*/
    border-top-right-radius: 4px; /*IE 7 AND 8 DO NOT SUPPORT BORDER RADIUS*/
}

.modal-body
{
    background-color: #f7f7f7;
}
.topNoTdLeft
{
    font-family: 微软雅黑;
    font-size: 14px;
    font-weight: bold;
    color: #333;
}
.topNoTdRight
{
    font-family: 微软雅黑;
    font-size: 14px;
    color: #555;
    word-wrap: break-word;
}
#orgChartFieldset
{
    width: 80% !important;
   }
div.content
{
    margin: 0px 0px 0px 0px !important;
}
/*列表尾*/
.fatherNode-footer{
	position: relative;
	background-color: #dddddd;
	border-radius: 2px;
}
.fatherNode-footer-icon{
	position: relative;
	width: 100%;
	height: 10px;
	cursor: pointer;
}
.fatherNode-footer-icon-arr{
	position: absolute;
	bottom: 3px;
	left: 50%;
	margin-left: -3px;
	width: 0;
	height: 0;
	border: 5px solid transparent;
	border-bottom-color: #fff;
	cursor: pointer;
}
.fatherNode-footer-icon-arrClose{
	top: 3px;
	border-bottom-color: transparent;
	border-top-color: #fff;
	z-index: 1;
}
.sibling-director{
	border-top: 1px solid rgb(227, 227, 227);
	display: none;
}