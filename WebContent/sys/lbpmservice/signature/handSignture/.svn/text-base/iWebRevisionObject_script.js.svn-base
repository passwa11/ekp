function iWebRevision_OpenSignture(dom){
	var $tb = $(dom).closest('table');
	if($tb){
		var webRevision = $tb.find("[name='iWebRevisionObject']");
		if(webRevision.length > 0){
			try{
				webRevision[0].OpenSignature();
			}catch(e){
				
			}		
		}
	}
}

