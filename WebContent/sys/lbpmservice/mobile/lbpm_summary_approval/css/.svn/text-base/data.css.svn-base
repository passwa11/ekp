html,body{
	overflow: hidden;
}
#content{
	background: #FFFFFF;
	overflow: hidden;
}
.scrollView{
}
.lbpmSummaryNodeName{
	font-size: 1.4rem;
    color: #2A304A;
    text-align: center;
    height: 4.4rem;
    line-height: 4.4rem;
    border-bottom: 1px solid #EEEEEE;
}
#lbpmSummaryList{
	margin-bottom: 6.7rem;
}
#lbpmSummaryList .muiLbpmSummaryItem{
	box-shadow: 0 0 30px 0 rgba(153,179,210,0.20);
}
.muiLbpmSummaryItem .muiItemContent{
	margin-top: 1rem;
    background: #FFFFFF;
    box-shadow: 0 0 30px 0 rgba(153,179,210,0.20);
    padding-top: 1.5rem;
    padding-left: 1.5rem;
    padding-right: 1.5rem;
}
.muiLbpmSummaryItem .muiItemInfos{
	margin-bottom: 1.5rem;
	position: relative;
}
.muiItemInfos .muiItemSelArea{
	position: absolute;
    left: 0;
}
.muiItemSelArea .muiItemSel{
	width: 1.8rem;
    height: 1.8rem;
    line-height: 1.8rem;
    border: 0.1rem solid #dfe1e0;
    text-align: center;
    border-radius: 2px;
}
.muiItemSelArea .muiItemSel.muiItemSeled{
	background-color: #4285F4;
    color: #fff;
    border: 0;
    border-radius: 2px;
}
.muiItemSelArea .muiItemSel .muiItemSelected{
	font-size: 1rem;
	font-weight: bold;
    position: relative;
    top: -0.1rem;
}
.muiItemInfos .muiItemInfo{
	padding-left: 3rem;
}
.muiLbpmSummaryItem .muiItemBaseInfo{
	position: relative;
    width: 100%;
}
.muiItemBaseInfo .muiItemTitle{
	font-size: 1.4rem;
    color: #2A304A;
    max-width: 480px;
    overflow: hidden;
    word-break: break-all;
    width: 50%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}
.muiItemBaseInfo .muiItemCreator{
	position: absolute;
    right: 0px;
    top: 0;
}
.muiItemBaseInfo .muiItemCreatorImg{
	position: absolute;
    top: 0;
    left: 0;
}
.muiItemBaseInfo .muiItemCreatorImg img{
	width: 2.1rem;
    height: 2.1rem;
    border-radius: 50%;
}
.muiItemBaseInfo .muiItemCreatorName{
	height: 2.1rem;
    line-height: 2.1rem;
    width: 100%;
    padding-left: 2.9rem;
    font-size: 1.2rem;
}
.muiItemBaseInfo .muiItemCreateTime{
	font-size: 1.2rem;
    color: #999FB7;
    margin-top: .4rem;
}
.muiItemSummaryProperty{
	/* position: relative; */
	margin-bottom: 1rem;
	height: 1.2rem;
}
.muiItemSummaryProperty .label{
	position: absolute;
	font-size: 1.2rem;
	color: #979DB1;
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
    font-size: 1.2rem;
    max-width: 20%;
}
.muiItemSummaryProperty .value{
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
    font-size: 1.2rem;
	color: #2A304A;
}
.muiItemSummaryProperty .prompt{
	margin-left:25%!important;
	top:-2px;
	max-width:75%;
}
.muiItemSummaryProperty .prompt .lui_dropdown_tooltip_menu_down:before{
	bottom: auto;
	top:-10px;
}
.muiItemSummaryTable table{
	border: none!important;
	width: 100%;
}
.muiItemSummaryTable tr{
	border-top: none!important;
	border-bottom: 1px solid #EEEEEE!important;
}
.muiItemSummaryTable tr.tr_normal_title{
	background-color:#F5F6FB!important;
    border: none!important;
}
.muiItemSummaryTable td{
	border: none!important;
}
.muiItemSummaryTable tr.tr_normal_title td{
	font-size: 1rem!important;
    color: #979DB1!important;
}
.muiItemSummaryTable tr:not(.tr_normal_title) td{
	font-size: 1rem!important;
    color: #2A304A!important;
}
.muiItemSummaryTable tr:not(.tr_normal_title) td div.value{
	display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.muiItemSummaryTable tr:not(.tr_normal_title) td>div.prompt .lui_dropdown_tooltip_menu_down:before{
	bottom:auto;
	top:-10px;
}
.muiItemSummaryInfo{
	margin-top: 1.5rem;
}
.muiItemBtns{
	border-top: 1px solid #EEEEEE;
}
.muiItemBtns .muiItemBtn{
	display: inline-block;
    width: 50%;
    height: 4.2rem;
    line-height: 4.2rem;
    text-align: center;
    font-size: 1.2rem;
	color: #4285F4;
}
.muiItemBtns .muiItemPassBtn.over{
	width: 100%;
}
.muiItemBtns .muiItemRejectBtn{
	position: relative;
}
.muiItemBtns .muiItemSplitBtn{
	position: absolute;
    left: 0;
    font-size: 2rem;
    color: #EEEEEE;
}

/*底部*/
.muiLbpmSummarySelection{
	position: fixed;
    bottom: 0;
    z-index: 10;
    width: 100%;
    height: 6.7rem;
    line-height: 6.7rem;
    border-top: 1px solid #EEEEEE;
    background-color: #FFFFFF;
}
.muiLbpmSummarySelection .muiLbpmSummarySecContainer{
	position: relative;
	height: 100%;
	margin-left: 1.5rem;
    margin-right: 1.5rem;
}
.muiLbpmSummarySelection .muiLbpmSummarySecRight{
	position: absolute;
    right: 0;
}
.muiLbpmSummarySecRight .muiLbpmSummaryBatchBtn{
	border: 1px solid #4285F4;
    border-radius: .5rem;
    width: 8rem;
    height: 4.4rem;
    display: inline-block;
    text-align: center;
    line-height: 4.4rem;
    font-size: 1.6rem;
}
.muiLbpmSummarySecRight .muiLbpmSummaryBatchRejectBtn{
    color: #4285F4;
}
.muiLbpmSummarySecRight .muiLbpmSummaryBatchPassBtn{
    background: #4285F4;
    color: #FFFFFF;
    margin-left: 1rem;
}
.muiLbpmSummarySelection .muiLbpmSummarySecLeft{
	float: left;
}
.muiLbpmSummarySecLeft .muiLbpmSummarySelArea{
    font-size: 1.6rem;
}
.muiLbpmSummarySelArea .muiLbpmSummarySel{
	width: 1.8rem;
    height: 1.8rem;
    line-height: 1.8rem;
    border: 0.1rem solid #dfe1e0;
    text-align: center;
    border-radius: 2px;
    position: absolute;
    top: 50%;
    margin-top: -1rem;
}
.muiLbpmSummarySelArea .muiLbpmSummarySel.muiLbpmSummarySeled{
	background-color: #4285F4;
    color: #fff;
    border: 0;
    border-radius: 2px;
}
.muiLbpmSummarySelArea .muiLbpmSummarySel .muiLbpmSummarySelected{
	font-size: 1rem;
	font-weight: bold;
    position: relative;
    top: -0.1rem;
}
.muiLbpmSummarySelArea .muiLbpmSummarySelText{
    font-size: 1.3rem;
    color: #999FB7;
    position: relative;
    margin-left: 3rem;
}