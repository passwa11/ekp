.muiAuditLog {
	width: 100%;
}

.muiAuditList {
	margin: 0;
	padding: 0;
}

.muiAuditLogItem {
	list-style: none;
	padding: 8px 0;
	line-height: 2rem;
	vertical-align: middle;
}
/*.muiAuditLogItem:last-child {
	border-bottom: 0;
}*/
.muiAuditLogItem .muiAuditLogItemInfo {
	position: relative;
}

.muiAuditLogItem .muiAuditLogItemPlay {
	margin-right: 4px;
	color: #3fa9f5;
	line-height: 1;
	top: -.7rem;
}
@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
	.muiAuditLogItem .muiAuditLogItemPlay {
		top: -.1rem;
	}
}
@media (device-width:414px) and (-webkit-min-device-pixel-ratio:3.0){
	.muiAuditLogItem .muiAuditLogItemPlay {
		top: -.1rem;
	}
}
.muiAuditLogItem .muiAuditLogItemName {
	max-width: 80%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow:ellipsis;
	display: inline-block;
}

.muiAuditLogItem .muiAuditLogItemMore {
	color: #999;
	font-size: 1.6rem;
	line-height: 2rem;
	position: absolute;
	right: 0;
}