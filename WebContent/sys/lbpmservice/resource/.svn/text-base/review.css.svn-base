	.lbpm_freeflow_draglist i.lbpm_freeflow_split_status{
	    background: url(./images/fold_ie.png) no-repeat center center;
	}
	.lbpm_freeflow_draglist li.fold i.lbpm_freeflow_split_status{
	    background: url(./images/unfold_ie.png) no-repeat center center;
	}
	.lbpm_freeflow_draglist i.lbpm_freeflow_status_normal{
	    background-image: url(./images/freeflow_status_normal.png);
	}
	.lbpm_freeflow_draglist i.lbpm_freeflow_status_fixed{
	    background-image: url(./images/freeflow_status_fixed.png);
	}
	.lbpm_freeflow_draglist i.lbpm_freeflow_status_passed{
	    background-image: url(./images/freeflow_status_passed.png);
	}
	.lbpm_freeflow_draglist i.lbpm_freeflow_status_running{
	    background-image: url(./images/freeflow_status_running.png);
	    width: 17px;
    	height: 17px;
    	margin-left: 3px;
	}
