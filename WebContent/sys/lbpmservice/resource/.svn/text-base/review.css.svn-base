#viewFreeFlowDIV{
	text-align: right;
}
.lbpm_freeflow_appendNode{
	display: inline-block;
	cursor: pointer;
	border-radius: 2px;
    height: 24px;
    text-align: center;
    line-height: 24px;
    font-size: 12px;
    margin-right: 12px;
    padding: 0px 5px;
}
.lbpm_freeflow_appendNode i{
	width: 10px;
    height: 10px;
    background-size: 10px;
    background-repeat: no-repeat;
    display: inline-block;
    padding-right: 3px;
}
.lbpm_freeflow_reviewNode{
	background: rgba(66,133,244,0.10);
    border: 1px solid #70A3F7;
    color: #4285F4;
}
.lbpm_freeflow_reviewNode i{
	 background-image: url(./images/freeflow_add_reviewNode@2x.png);
}
.lbpm_freeflow_signNode{
	background: rgba(255,185,0,0.10);
	border: 1px solid #FFD159;
	color: #FFB900;
}
.lbpm_freeflow_signNode i{
	background-image: url(./images/freeflow_add_signNode@2x.png);
}
.lbpm_freeflow_sendNode{
	background: rgba(34,176,123,0.10);
	border: 1px solid rgba(75,183,142,0.80);
	color: #22B07B;
}
.lbpm_freeflow_sendNode i{
	background-image: url(./images/freeflow_add_sendNode@2x.png);
}
.lbpm_freeflow_draglist{
	max-height: 300px;
    overflow-y: auto;
}
.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li_passed,.lbpm_freeflow_draglist .lbpm_freeflow_placeholder{
	/* height: 50px; */
	line-height: 49px;
	cursor: pointer;
	/* width: 100%; */
}
.lbpm_freeflow_draglist .ui-sortable-helper{
	width: 95%;
    height: 50px;
}

.lbpm_freeflow_draglist .lbpm_freeflow_draglist_sub li,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_split li{
	list-style: none;
}

.lbpm_freeflow_draglist .lbpm_freeflow_draglist_sub >li:first-child:before{
	content: '';
    display: block;
    border-top: 1px dashed #4285F4;
    position: relative;
    width: 9px;
    top: 25px;
    left: -10px;
}

.lbpm_freeflow_draglist .lbpm_freeflow_draglist_sub >li:first-child >.lbpm_freeflow_item:before{
	content: '';
    display: inline-block;
    position: relative;
    top: -2px;
    left: 0;
    border-left: 4px solid #4285f4;
    width: 0;
    height: 4px;
    border-radius: 50%;
}

.lbpm_freeflow_draglist li[data-node-type='splitNode']:after{
	content: '';
    display: node;
    border-top: 1px dashed #4285F4;
    position: relative;
    transform: rotate(90deg);
}

.lbpm_freeflow_draglist.lbpm_freeflow_sortable li[data-node-type='splitNode']:after,
.lbpm_freeflow_draglist.lbpm_freeflow_sortable .lbpm_freeflow_draglist_sub >li:first-child:before,
.lbpm_freeflow_draglist.lbpm_freeflow_sortable .lbpm_freeflow_draglist_sub >li:first-child >.lbpm_freeflow_item:before{
	display: none !important;
}

.lbpm_freeflow_draglist li.lbpm_freeflow_draglist_li.fold[data-node-type='splitNode']:after{
	display: none;
}

.lbpm_freeflow_draglist .lbpm_freeflow_placeholder{
	width: 95%;
    height: 50px;
	background-size: 100%;
    background-repeat: no-repeat;
    background-image: url(./images/move_line_row.png);
    background-position:bottom;
    margin-left:24px;
}
.right_model .lbpm_freeflow_draglist .lbpm_freeflow_placeholder{
    width: 93%;
    background-image: url(./images/move_line.png);
}
.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.temp,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li:hover{
	background: rgba(66,133,244,0.05);
}
.lbpm_freeflow_draglist i.lbpm_freeflow_split_status{
	display: inline-block;
    height: 16px;
    width: 12px;
    background: url(./images/fold_big.png) no-repeat center center;
    background-size: cover;
    vertical-align: middle;
    cursor: pointer;
}
.lbpm_freeflow_draglist li.lbpm_freeflow_draglist_li.fold i.lbpm_freeflow_split_status{
    background: url(./images/unfold_big.png) no-repeat center center;
    background-size: cover;
}
.lbpm_freeflow_draglist i.lbpm_freeflow_status{
	display:inline-block;
	width: 14px;
    height: 14px;
    background-image: url(./images/freeflow_status_normal@2x.png);
    background-size: 14px;
    background-repeat: no-repeat;
    vertical-align: middle;
    margin: 0 5px;
}
.lbpm_freeflow_draglist i.lbpm_freeflow_status_normal{
    background-image: url(./images/freeflow_status_normal@2x.png);
}
.lbpm_freeflow_draglist i.lbpm_freeflow_status_fixed{
    background-image: url(./images/freeflow_status_fixed@2x.png);
    height: 20px;
}
.lbpm_freeflow_draglist i.lbpm_freeflow_status_passed{
    background-image: url(./images/freeflow_status_passed@2x.png);
}
.lbpm_freeflow_draglist i.lbpm_freeflow_status_running{
    background-image: url(./images/freeflow_status_running@2x.png);
}
.lbpm_freeflow_draglist div{
	display:inline-block;
}
.lbpm_freeflow_draglist .lbpm_freeflow_item{
    width: 98%;
}
.lbpm_freeflow_draglist .lbpm_freeflow_split{
	border-bottom: 1px solid #F8F8F8;
    display: block;
    margin-left: 24px;
}
.lbpm_freeflow_draglist >.lbpm_freeflow_draglist_li:last-child .lbpm_freeflow_split{
     border-bottom: none;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName{
	height: 17px;
    line-height: 17px;
    font-size: 12px;
    margin-right: 5px;
	border-radius: 2px;
	max-width: 60px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    vertical-align: middle;
    padding: 0 3px;
    background: #F8F8F8;
    border: 1px solid #DDDDDD;
    color: #B8B8B8;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_draftNode{
	background: #F8F8F8;
	border: 1px solid #DDDDDD;
	color: #B8B8B8;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_reviewNode{
	background: #ECF2FD;
	border: 1px solid #70A3F7;
	color: #4285F4;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_signNode{
	background: #FFF8E5;
	border: 1px solid #FFD159;
	color: #FFB900;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_sendNode{
	background: #E8F7F1;
	border: 1px solid #6AC3A2;
	color: #22B07B;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_joinNode{
	max-width: 100px;
}
.lbpm_freeflow_draglist .lbpm_freeflow_sortName_splitNode{
	max-width: 100px;
}
.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li_passed .lbpm_freeflow_sortName{
	background: #F8F8F8;
	border: 1px solid #DDDDDD;
    color: #B8B8B8;
}
.lbpm_freeflow_draglist .lbpm_freeflow_handlerName{
	height: 17px;
    line-height: 17px;
    vertical-align: middle;
    margin-top: 2px;
    max-width: 150px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active .lbpm_freeflow_handlerName{
	color: #4285F4;
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation{
	float: right;
	display: none;
}
.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active .lbpm_freeflow_operation,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.temp .lbpm_freeflow_operation,.lbpm_freeflow_draglist_li:hover .lbpm_freeflow_operation{
	display: inline-block;
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation i{
	display: inline-block;
	width: 17px;
    height: 17px;
    background-size: 17px;
    background-repeat: no-repeat;
    margin-left: 7px;
    vertical-align: middle;
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation .lbpm_freeflow_add i{
	background-image: url(./images/freeflow_add@2x.png);
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation .lbpm_freeflow_edit i{
	background-image: url(./images/freeflow_edit@2x.png);
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation .lbpm_freeflow_delete i{
	background-image: url(./images/freeflow_delete@2x.png);
	height: 18px;
} 
.lbpm_freeflow_draglist .lbpm_freeflow_operation a.lbpm_freeflow_add:hover i,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active .lbpm_freeflow_add i,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.temp .lbpm_freeflow_add i{
	background-image: url(./images/freeflow_add_selected@2x.png);
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation a.lbpm_freeflow_edit:hover i,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active .lbpm_freeflow_edit i{
	background-image: url(./images/freeflow_edit_selected@2x.png);
}
.lbpm_freeflow_draglist .lbpm_freeflow_operation a.lbpm_freeflow_delete:hover i,.lbpm_freeflow_draglist .lbpm_freeflow_draglist_li.active .lbpm_freeflow_delete i{
	background-image: url(./images/freeflow_delete_selected@2x.png);
	height: 18px;
}

.lbpm_freeflow_add_moreList:before,
.lbpm_freeflow_add_moreList:after{
	content:"";
	display:inline-block;
	width:0;
	height:0;
	border: 6px solid transparent;
	border-bottom-color: #EAEDF3;
	position:absolute;
	top: -13px;
	z-index: 1;
	left: 50%;
	transform: translateX(-50%);
}

.lbpm_freeflow_add_moreList:before{}
.lbpm_freeflow_add_moreList:after{
	border-bottom-color: #fff;
	margin-top: 1px;
}

.lbpm_freeflow_add_moreList{
	display: none;
	list-style: none;
	background: #FFFFFF;
	border: 1px solid #EAEDF3;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.04);
	border-radius: 4px;
	min-width: 85px;
	text-align: center;
	position: fixed;
	z-index: 999;
}

.lbpm_freeflow_add_moreList li{
	height:35px;
	line-height:35px;
	color:#666;
	position: relative;
	z-index: 2;
	cursor: pointer;
}
.lbpm_freeflow_add_moreList li:hover{
	background: #ecf3fe;
	color:#4285F4;
}

.lbpm_freeflow_sortName_nameList li{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	max-width: 200px;
}

.lbpm_freeflow_defaultTemp_btnList:before,
.lbpm_freeflow_defaultTemp_btnList:after,
.lbpm_freeflow_sortName_nameList:before,
.lbpm_freeflow_sortName_nameList:after,
.lbpm_freeflow_process_typeList:before,
.lbpm_freeflow_process_typeList:after{
	content:"";
	display:inline-block;
	width:0;
	height:0;
	border: 6px solid transparent;
	border-bottom-color: #EAEDF3;
	position:absolute;
	top: -13px;
	z-index: 1;
	left: 50%;
	transform: translateX(-50%);
}

.lbpm_freeflow_defaultTemp_btnList:before,.lbpm_freeflow_sortName_nameList:before,.lbpm_freeflow_process_typeList:before{}
.lbpm_freeflow_defaultTemp_btnList:after,.lbpm_freeflow_sortName_nameList:after,.lbpm_freeflow_process_typeList:after{
	border-bottom-color: #fff;
	margin-top: 1px;
}

.lbpm_freeflow_defaultTemp_btnList,.lbpm_freeflow_sortName_nameList,.lbpm_freeflow_process_typeList{
	display: none;
	list-style: none;
	background: #FFFFFF;
	border: 1px solid #EAEDF3;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.04);
	border-radius: 4px;
	min-width: 85px;
	text-align: center;
	position: fixed;
	z-index: 999;
}

.lbpm_freeflow_defaultTemp_btnList li,.lbpm_freeflow_sortName_nameList li,.lbpm_freeflow_process_typeList li{
	height:35px;
	line-height:35px;
	color:#666;
	position: relative;
	z-index: 2;
	cursor: pointer;
	padding: 0 5px;
}
.lbpm_freeflow_defaultTemp_btnList li:hover,.lbpm_freeflow_sortName_nameList li:hover,.lbpm_freeflow_process_typeList li:hover,.lbpm_freeflow_process_typeList li.checked{
	background: #ecf3fe;
	color:#4285F4;
}
.lbpm_freeflow_defaultTemp_btn{
	display: none;
}

