<c:choose>
	<c:when test="${showConfig.pagingSetting eq '2' }">
		<list:paging layout="sys.ui.paging.simple2" ></list:paging>
	</c:when>
	<c:when test="${showConfig.pagingSetting eq '3' }">
		<list:paging layout="sys.ui.paging.can.change" ></list:paging>
	</c:when>
	<c:otherwise>
		<list:paging></list:paging>
	</c:otherwise>
</c:choose>