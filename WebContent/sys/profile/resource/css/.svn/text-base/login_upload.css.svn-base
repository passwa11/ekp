.profile_loginSetting_bg .line {
    float: left;
    padding: 16px 0px 2px;
    margin-top: 11px;
    border-left: 1px solid;
    font-size: 0px;
    margin-left: -1px;
}

.profile_loginSetting_bg:after {
    display: table;
    content: "";
    clear: both;
}

.loginTemplete_title {
    text-align: center;
    padding: 10px;
    font-size: 14px;
}
.profile_loginSetting_portlet_item:hover .loginTemplete_title{
    box-shadow: 0 2px 8px 0 #ddd;
}
.profile_icon_default,.profile_icon_view,.profile_icon_custom,.profile_icon_del {
    background: url(../images/profile_loginSetting_icon.png) no-repeat left top;
    width: 16px;
    height: 16px;
    display: inline-block;
    vertical-align: middle;
    margin-right: 5px;
}
/*鼠标悬浮starts*/
.profile_loginSetting_bg a:hover{
    color:#4285f4;
}
.profile_loginSetting_bg a:hover .profile_icon_default,
.profile_loginSetting_bg a:hover .profile_icon_view,
.profile_loginSetting_bg a:hover .profile_icon_custom,
.profile_loginSetting_bg a:hover .profile_icon_del{
    background-image: url(../images/profile_loginSetting_icon_on.png);
}
.profile_loginSetting_bg a:hover .profile_icon_download{
    background-image: url(../images/profile_icon_download_on.png);
}
/*鼠标悬浮ends*/
.profile_icon_view {
    background-position: -16px 0;
}

.profile_icon_custom {
    background-position: -32px 0;
}

.profile_icon_del {
    background-position: -48px 0;
}


/* 登录页上传扩展页面*/
.profile_loginSetting_split {
    border-top: 1px solid #e5e5e5;
    padding-top: 5px;
    padding-left: 10px;
    margin-top: 15px;
    margin-bottom: 15px;
    margin-left: 10px;
    margin-right: 10px;
    line-height: 20px;
}

.profile_loginSetting_uploadBtn {
    text-align: center;
    cursor: pointer;
}

.profile_loginSetting_uploadBtn:hover .btn-upload {
    background: #346ac3;
    cursor: pointer;
}

.profile_loginSetting_uploadBtn .btn-upload {
    display: inline-block;
    width: 70px;
    height: 70px;
    background: #4285f5;
    border-radius: 50%;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
    color: #fff;
}

.profile_loginSetting_uploadBtn .btn-upload i {
    position: absolute;
    top: 5px;
    left: 33px;
    top: 18px;
}

.profile_loginSetting_uploadBtn .btn-upload i:before, .profile_loginSetting_uploadBtn .btn-upload i:after {
    display: inline-block;
    content: "";
    background: #fff;
    position: absolute
}

.profile_loginSetting_uploadBtn .btn-upload i:before {
    width: 3px;
    height: 33px;
}

.profile_loginSetting_uploadBtn .btn-upload i:after {
    width: 33px;
    height: 3px;
    left: -15px;
    top: 15px;
}

/* 删除按钮 */
.profile_custom_del {
    display: none;
    width: 32px;
    height: 32px;
    line-height: 37px;
    text-align: center;
    position: absolute;
    right: 17px;
    top: 23px;
    text-align: center;
    position: absolute;
    border-radius: 50%;
    background: #000;
    background: rgba(0,0,0,.7);
    cursor: pointer;
    z-index: 2;
}

.profile_custom_del i {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../images/profile_icon_del@2x.png) no-repeat center;
    background-size: cover;
}

/*部件管理删除starts*/
.profile_loginSetting_portlet_item .profile_custom_del{
    right: 8px;
    top: 8px;
}
/*部件管理删除ends*/

.profile_loginSetting_list li:hover .profile_custom_del {
    display: block;
}

.profile_icon_download {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../images/profile_icon_download.png);
    vertical-align: text-top;
    margin-right: 5px;
}

/* 清除浮动 */
body, input, textarea, select, div, a, table, tr, td, th {
    font-family: "PingFang SC", "Lantinghei SC", "Helvetica Neue", Arial, "Microsoft YaHei", "WenQuanYi Micro Hei", "Heiti SC", "Segoe UI", sans-serif;
    font-size: 14px;
}

body {
    color: #333;
    background-color: transparent;
    -webkit-text-size-adjust: none;
}

body, div, dl, dt, dd, ul, pre, form, fieldset, input, textarea, p, blockquote, th, td,h4,h3 {
    margin: 0;
    padding: 0;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

fieldset, img {
    border: 0;
}

.clearfix:after {
    content: "";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both
}

.clearfix {
    zoom: 1 }

ul,li {
    list-style: none;
}

a {
    color: #333;
    text-decoration: none;
}

