/* 预览部件start */
.ld-mall-preview-part {
    width: 900px;
    background: white;
    border-right: 4px;
    height: 700px;
}

.ld-mall-preview-part-list {
    padding: 28px 4px 28px 14px;
    width: 840px;
    background: #F8F8F8;
    border: 1px solid #EEEEEE;
    margin: 64px auto 44px ;
    box-sizing: border-box;
}

.ld-mall-preview-part-list ul {
    max-height: 470px;
    width: 100%;
    overflow-y: auto;
}

.ld-mall-preview-part-list ul li {
    height: 200px;
    width: 260px;
    float: left;
    margin-right: 16px;
    position: relative;
    margin-bottom: 16px;
}

.ld-mall-preview-part-list ul li:nth-child(3n) {
    margin-right: 0;
}

.ld-mall-preview-part-list ul li p {
    position: absolute;
    bottom: 0;
    height: 30px;
    line-height: 30px;
    text-align: center;
    width: 100%;
    background: white;
    color: #666;
    font-size: 12px;
}

.ld-mall-preview-part-list-item .ld-mall-preview-part-list-item-img {
    width: 100%;
    height: 100%;
    overflow-y: auto;
}

.ld-mall-preview-part-list-item .ld-mall-preview-part-list-item-img img {
    width: 100%;
    object-fit: cover;
}

/* 修改滚动条 */
.ld-mall-preview-part-list ul::-webkit-scrollbar, .ld-mall-preview-part-list-item-img::-webkit-scrollbar {
    /*滚动条整体样式*/
    width: 5px;
    /*高宽分别对应横竖滚动条的尺寸*/
    height: 1px;
}

.ld-mall-preview-part-list ul::-webkit-scrollbar-thumb, .ld-mall-preview-part-list-item-img::-webkit-scrollbar-thumb {
    /*滚动条里面小方块*/
    border-radius: 5px;
    background: #c0c0c0;
}

.ld-mall-preview-part-list ul::-webkit-scrollbar-track, .ld-mall-preview-part-list-item-img::-webkit-scrollbar-trac {
    background: transparent;
}

/* 全局修改滚动条start */
::-webkit-scrollbar {
    width: 5px;
    /*高宽分别对应横竖滚动条的尺寸*/
    height: 1px;
}

