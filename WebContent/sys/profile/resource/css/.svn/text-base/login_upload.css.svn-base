/* 登录页模板页 */
.profile_loginSetting_frame .profile_loginsetting_title {
    text-align: center;
    font-size: 16px;
    padding: 10px;
}

.profile_loginSetting_list {
    width: 100%;
    font-size: 0;
}

.profile_loginSetting_list li {
    display: inline-block;
    width: 33.333333%;
    vertical-align: top;
}

.profile_loginSetting_content {
    padding: 10px 10px 0px;
    line-height: 0;
    position: relative;
    overflow: hidden;
}

.profile_loginSetting_frame .default_label {
    padding: 2px 3px;
    border-radius: 4px;
    margin-right: 5px;
    font-style: normal;
    line-height: 16px;
    background: #4285F4;
    color: #fff;
    font-size: 10px;
}

.profile_loginSetting_content .thumb_img {
    display: none;
}

.profile_loginSetting_content .thumb_img_bg {
    width: 100%;
    /* height: 230px; */
    background-size: cover;
    border: 1px solid #eee;
    background-color: #f8f8f8;
    padding-bottom: 56%;
    position: relative;
    border: 1px solid #eee;
}

/*--- ie8 兼容 ---*/
@media \0screen {
    .profile_loginSetting_content .thumb_img {
        display: block;
        position: absolute;
        top: 0;
        width: 100%;
        height: 100%;
    }

    .profile_loginSetting_content .thumb_img_bg {
        /*         display: bock; */
        background-image: none!important;
    }

    .profile_loginSetting_content .thumb_img_bg {
    }
}

.profile_loginSetting_list li img {
    width: 100%;
    border: 1px solid #eee;
}

.profile_loginSetting_bg {
    background: rgba(0,0,0,0.7);
    background: url(../images/profile_loginSetting_bg.png) repeat\9;
    color: #fff;
    height: 40px;
    line-height: 40px;
    position: absolute;
    bottom: -40px;
    left: 11px;
    right: 11px;
    transition: bottom .1s ease-in-out .3s;
}

.profile_loginSetting_list li:hover .profile_loginSetting_bg {
    bottom: 0px;
}

.profile_loginSetting_bg a {
    color: #fff;
    font-size: 14px;
    float: left;
    width: 33.3%;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
/**/
.profile_componentSet_bg a {
    color: #fff;
    font-size: 14px;
    float: left;
    width: 100%;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
