/*----- 日历选择框 -----*/
.ui-datepicker.ui-widget-content{ }
.ui-datepicker .ui-datepicker-header { background:#f0f0f0; border-radius: 0; border:none;}
.ui-datepicker .ui-datepicker-header .ui-icon{ background-image: url(../images/calendar/date_arrow.png);}
.ui-datepicker .ui-icon-circle-triangle-w{ background-position: 0 0;}
.ui-datepicker .ui-state-hover .ui-icon-circle-triangle-w{ background-position: 0 -16px;}
.ui-datepicker .ui-icon-circle-triangle-e{ background-position: -16px 0;}
.ui-datepicker .ui-state-hover .ui-icon-circle-triangle-e{ background-position: -16px -16px;}
.ui-datepicker .ui-datepicker-header .ui-state-hover{ /*background:none; border:none;*/}
.ui-datepicker .ui-datepicker-header .ui-state-default{ /*border:none;*/}

.ui-datepicker .ui-datepicker-calendar th{ border-bottom:1px solid #d5d5d5;}
.ui-datepicker .ui-datepicker-calendar td{ text-align: center; position: relative; padding: 2px 1px;}
.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-sign{
	position: absolute;
	width: 0;
	height: 0;
	border-right: 7px solid transparent;
	border-top: 7px solid transparent;
	left: 10px;
    top: 1px;
    font-size: 0;
}
.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-sign[data-type="1"]{
	border-top-color: #E60000;
}
.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-sign[data-type="2"]{
	border-top-color: #2B9AE0;
}

.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-signlabel{
	position: absolute;
	color: white;
	background-color: rgba(0,0,0,0.7);
	z-index: 1;
	left: 0;
	display: none;
	right: 0;
	text-align: center;
	top: -30px;
	border-radius: 2px;
	font-size: 12px;
}

.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-signlabel::after{
	content: '';
	width: 0;
	height: 0;
	display: inline-block;
	position: absolute;
	bottom: -6px;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-top: 6px solid rgba(0,0,0,0.7);
	left: -4px;
	margin-left: 50%;
}

.ui-datepicker .ui-datepicker-calendar td:hover .ui-datepicker-signlabel{
	display: inline-block;
}

.ui-datepicker .ui-datepicker-calendar .ui-state-default{
  display: inline-block;
	border:none; background: none; text-align: center; color:#333; border:1px solid #fff; border-radius: 50%;
	padding: 0;
	width: 24px;
	height: 24px;
	line-height: 24px;
}
.ui-datepicker .ui-datepicker-calendar td:hover .ui-state-default{ border-color:#4285f4;}
.ui-datepicker .ui-datepicker-calendar .ui-state-highlight{ background:#4285f4; color:#fff; border-color:#4285f4;}
.ui-datepicker-buttonpane .ui-state-default{ border:1px solid #e5e5e5; background:#f8f8f8; border-radius: 0; background: #fff;}
.ui-datepicker .ui-datepicker-title select.ui-datepicker-year{ margin-right: 5px;}

.ui-datepicker .ui-datepicker-calendar .ui-state-default.ui-state-active{ border-color:#4285f4;}
