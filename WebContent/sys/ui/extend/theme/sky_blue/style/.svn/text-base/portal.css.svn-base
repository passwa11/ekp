/*-- 多功能页眉 Starts  --*/
.lui_portal_header_complex_frame {
	width: auto;
	padding-left: 15px;
	padding-right: 15px;
	top: 0px;
	z-index: 50;
	background: #4285f4 url(../images/portal/header_complex_frame_bg.png) no-repeat 100% 100%;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, .15)
		 -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, .15)
			-ms-box-shadow: 0 1px 1px rgba(0, 0, 0, .15)
			 -o-box-shadow: 0 1px 1px rgba(0, 0, 0, .15)
					box-shadow: 0 1px 1px rgba(0, 0, 0, .15)
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_c{
	margin-top: 5px;
	padding-left: 10px;
	padding-right: 10px;
	height: 30px;
	line-height: 30px;
	color: #fff;
	border-radius: 40px;
	transition-duration: .3s;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_c:hover{
	color: #4285f4;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_div,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_current {
	padding-right: 10px;
	padding-left: 10px;
	position: relative;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_frame .lui_portal_header_menu_item_div:after,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_frame .lui_portal_header_menu_item_current:after{
	content: '';
	display: block;
	height: 16px;
	border-right: 1px solid #fff;
	border-right: 1px solid rgba(255,255,255,.4);
	position: absolute;
	right: 0;
	top: 50%;
	margin-top: -8px;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_current {
	height: 40px;
	border-bottom: 0;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_line{
	background:none;
	width: 0;
	height: auto;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_l{
	margin-left: 0;
	margin-right: 0;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_hover .lui_portal_header_menu_item_c,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_current .lui_portal_header_menu_item_c {
	padding-left: 10px;
	padding-right: 10px;
	color: #fff;
	background-color: #6bb3fb;
	background-color: rgba(255,255,255,.3);
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_left,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_right,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_left:hover,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_right:hover{
	background-color: transparent;
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_left:hover,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_right:hover{
	opacity: .75;
	filter: alpha(opacity=75);
}
.lui_portal_header_complex_frame .lui_portal_header_menu_item_left:before,
.lui_portal_header_complex_frame .lui_portal_header_menu_item_right:before{
	background-image: url(../images/portal/nav_arrow_blue_sprite.png);
}


.lui_portal_header_text{
	cursor: pointer;
}
.lui_portal_header_complex_person {
	position: static;
	right: 0;
	top: 0;
	float: right;
}
.lui_portal_header_complex_logo{
	position: static;
	float: left;
}
.lui_portal_header_complex_logo .lui_portal_header_logo_div{
	margin-top: 0;
	height: 40px;
	line-height: 40px;
}
.lui_portal_header_complex_logo .lui_portal_header_logo_div img{ max-height: 100% }
.lui_portal_header_complex_portal{
	position: static;
	float: left;
}
.lui_portal_header_complex_search{
	margin-top: 5px;
	position: static;
	text-align: center;
	left: 0;
	/*min-width: 350px;*/
	float: left;
	margin-left: 20px;
}
.lui_portal_header_complex_search .lui_portal_header_search_include{
	margin-top: 0;
	font-size: 0;
	*white-space: -1px;
}
.lui_portal_header_complex_search .lui_portal_header_search_input{
	margin-right: -2px;
	width: 220px;
	display: inline-block;
	vertical-align: middle;
	border-top-left-radius: 40px;
	border-bottom-left-radius: 40px;
	border: 1px solid transparent;
	background-color: #1e68b1;
	background-color: rgba(30,104,177,.4);
}
.lui_portal_header_complex_search .lui_portal_header_search_input input {
	border: 0;
	width: 92%;
	color: #fff;
	font-size: 14px;
	height: 20px;
	line-height: 20px;
	border: 0;
	padding: 4px 4%;
	background-color: transparent;
}
.lui_portal_header_complex_search .lui_portal_header_search_input input::-moz-placeholder {
  color: #fff!important;
  opacity: .5;
}
.lui_portal_header_complex_search .lui_portal_header_search_input input:-ms-input-placeholder {
  color: #fff!important;
  opacity: .5;
}
.lui_portal_header_complex_search .lui_portal_header_search_input input::-webkit-input-placeholder {
  color: #fff!important;
  opacity: .5;
}

.lui_portal_header_complex_search .lui_portal_header_search_btn {
	width: auto;
	height: auto;
	line-height: 20px;
	padding: 0;
	cursor: pointer;
	margin-right: 4px;
	display: inline-block;
	vertical-align: top;
	background-color: transparent;
	border: 0;
}
.lui_portal_header_complex_search .lui_portal_header_search_btn input{
	margin-left: 2px;
	padding: 0;
	min-width: 40px;
	height: 28px;
	line-height: 28px;
	color: #fff;
	font-size: 14px;
	border: 1px solid transparent;
	background-color: #1e68b1;
	background-color: rgba(30,104,177,.4);	
	border-radius: 40px;
	cursor: pointer;
	transition-duration: .3s;
	box-sizing: content-box;
}
.lui_portal_header_complex_search .lui_portal_header_search_btn.search_btn_icon input{
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}
.lui_portal_header_complex_search .lui_portal_header_search_btn input:hover{
	color: #fff;
	border-color: #346ac3;
	background-color: #346ac3;
}
.lui_portal_header_complex_search .lui_portal_header_search_btn .lui_portal_header_search_full_l input{
	font-size: 0;
	*white-space: -1px;
	background-image: url(../images/portal/portal_header_search.png);
	background-repeat: no-repeat;
	background-position: 50%;
}

.lui_portal_header_complex_frame .lui_icon_s_daiban { background-position:-32px 0; }
.lui_portal_header_complex_frame .lui_icon_s_daiyue	{ background-position:-48px 0; }
.lui_portal_header_complex_frame .lui_icon_s_person { background-position:0px 0; }
.lui_portal_header_complex_frame .lui_icon_s_collect {background-position: -16px 0;}
.lui_portal_header_complex_frame .lui_icon_s_code { background-position:-80px 0;}
.lui_portal_header_complex_frame .lui_portal_header_hitch .lui_icon_hitch{ background-position:-64px 0; }

.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_daiban,
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_daiyue,
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_person,
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_collect,
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_code,
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_portal_header_hitch .lui_icon_hitch {
	background-image: url(../images/portal/head_sprite_default.png);
}
.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_collect { background-position: -16px -32px; }
@media screen and (-webkit-min-device-pixel-ratio: 2){
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_daiban,
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_daiyue,
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_person,
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_collect,
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_icon_s_code,
	.lui_portal_header_complex_frame .lui_portal_header_text_over .lui_portal_header_hitch .lui_icon_hitch {
		background-image: url(../images/portal/head_sprite_default@2x.png);
	}
}

/*-- 多功能页眉 Ends  --*/


.lui_portal_body{ background-color: #f7f7f7;}
.lui_portal_body > div[style*='height']:nth-of-type(2){
	margin-top: 15px;
	height: 0!important;
}

.lui_portal_header_menu_item_left,
.lui_portal_header_menu_item_right {
	background: none;
	line-height: 38px;
	z-index: 1;
}
.lui_portal_header_menu_item_right{ text-align: right; }
.lui_portal_header_menu_item_left:hover,
.lui_portal_header_menu_item_right:hover {
	background: #4285f4;
	opacity: .75;
	filter: alpha(opacity=75);
}
.lui_portal_header_menu_item_left:before,
.lui_portal_header_menu_item_right:before {
	display: inline-block;
	content: "";
	width: 16px;
	height: 16px;
	background: url(../images/portal/nav_arrow_blue_sprite.png) no-repeat left top;
	vertical-align: middle;
}
.lui_portal_header_menu_item_right:before{ background-position:-16px 0 ;}
.lui_portal_header_menu_item_left:hover:before{ background-position: -32px 0;}
.lui_portal_header_menu_item_right:hover:before{ background-position: -48px 0;}

