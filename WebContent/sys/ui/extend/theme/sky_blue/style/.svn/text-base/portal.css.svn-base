/* MAC系统高清屏特殊处理 */
@media screen and (-webkit-min-device-pixel-ratio:2) {
	/*----- 默认页眉 -----*/
	.lui_portal_header_zone_frame .lui_icon_s_daiban,
	.lui_portal_header_zone_frame .lui_icon_s_daiyue,
	.lui_portal_header_zone_frame .lui_icon_s_person,
	.lui_portal_header_zone_frame .lui_icon_s_collect,
	.lui_portal_header_zone_frame .lui_icon_s_code,
	.lui_portal_header_zone_frame .lui_portal_header_hitch .lui_icon_hitch{ background-image:url(../icon/s/head_sprite_iceblue@2x.png); background-size:96px auto; }

	.lui_icon_s_default_filter,
	.lui_icon_s_up_filter,
	.lui_icon_s_on_filter{
	  background-image: url(../icon/s/filter@2x.png); background-size: 15px auto;
	}
	
	.lui_icon_s_liebiao,
	.lui_icon_s_zaiyao,
	.lui_icon_s_tuwen,
	.lui_icon_s_fujian {
	  background-image:url(../icon/s/selection@2x.png); background-size: 16px auto;
	}
	.lui_icon_on .lui_icon_s_liebiao,
	.lui_icon_on .lui_icon_s_zaiyao,
	.lui_icon_on .lui_icon_s_tuwen,
	.lui_icon_on .lui_icon_s_fujian  {
	  background-image:url(../icon/s-on/selection@2x.png); background-size: 16px auto;
	}
	
}
