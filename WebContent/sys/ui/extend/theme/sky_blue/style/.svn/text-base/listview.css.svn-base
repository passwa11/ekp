  .lui_paging_t_notpre,
  .lui_paging_t_notnext,
  .lui_paging_t_hasnext,
  .lui_paging_t_haspre { background-image: url(../images/paging/view_icon_list_Dnor@2x.png)!important; background-size:32px auto!important;}
}







