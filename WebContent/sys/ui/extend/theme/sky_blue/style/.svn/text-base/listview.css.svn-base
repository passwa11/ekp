@-webkit-keyframes refresh-rotate {
  0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
  }
  100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
  }
}

@keyframes refresh-rotate {
  0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
  }
  100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
  }
}

.refresh-rotate {
  -webkit-animation-name: refresh-rotate;
          animation-name: refresh-rotate;
}

.lui_paging_t_refresh_l:hover .lui_paging_t_refresh,
.lui_paging_t_refresh:hover{
  -webkit-animation: refresh-rotate .8s linear 0s 2 both;
     -moz-animation: refresh-rotate .8s linear 0s 2 both;
      -ms-animation: refresh-rotate .8s linear 0s 2 both;
       -o-animation: refresh-rotate .8s linear 0s 2 both;
          animation: refresh-rotate .8s linear 0s 2 both;
  background: url(../images/paging/view_icon_list_refresh_hover.png) no-repeat 50%;
}

/* MAC系统高清屏特殊处理 */
@media screen and (-webkit-min-device-pixel-ratio:2) {
