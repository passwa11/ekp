body .lui-search-panel-heading-title {
  color: #6081F8;
}

body .lui-search-panel-heading {
  border-bottom-color: #6081F8;
}

body .lui-cate-queryEmpty .queryEmpty-icon {
  background-image: url(../images/icon-primary/query-empty-icon.png);
}

