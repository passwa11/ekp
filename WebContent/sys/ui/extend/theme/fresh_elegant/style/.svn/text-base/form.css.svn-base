  color: #999;
  font-weight: normal;
}

/* 路径导航 Ends */
.lui_form_title_frame {
  margin-bottom: 20px;
}

.lui_form_subject {
  margin-bottom: 10px;
  padding: 0;
  line-height: 30px;
  text-align: center;
  color: #333;
  font-size: 24px;
}

.lui_form_title_frame .lui_form_baseinfo {
  margin: 0 0 5px;
  line-height: 20px;
  text-align: center;
  color: #999;
  font-size: 12px;
}

.lui_form_title_frame .lui_form_baseinfo span,
.lui_form_title_frame .lui_form_baseinfo .com_author {
  margin-right: 8px;
  font-size: 12px;
}

body .lui_form_title_frame .lui_form_baseinfo .com_author {
  color: #6081F8 !important;
}

body .lui_form_title_frame .lui_form_baseinfo .com_author:hover {
  color: #4B71FA !important;
}

.lui_form_body .tempTB {
  background: #fff;
}

.lui_form_content .txttitle {
  margin-bottom: 15px;
  color: #333333;
}

.lui_form_content {
  padding: 10px 15px;
  background-color: #fff;
}

.lui_form_content_frame {
  padding: 0;
}

.lui_form_content .lui_form_summary_frame {
  margin-left: 0;
  margin-right: 0;
  border: 0;
  color: #333;
  font-size: 14px;
