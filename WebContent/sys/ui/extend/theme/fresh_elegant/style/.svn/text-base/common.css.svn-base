:root {
  /* 主色 */
  --primary-color: $primary-color;
  /* 交互色 */
  --alter-color: $alter-color;
  --thirdray-color: #1d1b3c;
  --primary-color-rgb: 215, 0, 15;
  --alter-color-rgb: 223, 51, 63;
  --thirdray-color-rgb: 29, 27, 60;
}

html {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

body,
input,
textarea,
div,
a,
table,
tr,
td,
th {
  font-size: 14px;
}

body {
  line-height: 1.42857143;
  color: #333;
  background-color: transparent;
}

div {
  color: inherit;
}

a {
  color: inherit;
  text-decoration: none;
  background-color: transparent;
  transition-duration: 0.3s;
  transition: none;
  cursor: pointer;
}

a:hover {
