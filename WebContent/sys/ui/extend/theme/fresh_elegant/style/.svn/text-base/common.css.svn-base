}

.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-signlabel {
  position: absolute;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 1;
  left: 0;
  display: none;
  right: 0;
  text-align: center;
  top: -30px;
  border-radius: 2px;
  font-size: 12px;
}

.ui-datepicker .ui-datepicker-calendar td .ui-datepicker-signlabel::after {
  content: '';
  width: 0;
  height: 0;
  display: inline-block;
  position: absolute;
  bottom: -6px;
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
  border-top: 6px solid rgba(0, 0, 0, 0.7);
  left: -4px;
  margin-left: 50%;
}

