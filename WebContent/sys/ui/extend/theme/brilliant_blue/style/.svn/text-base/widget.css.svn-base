.lui_accordionpanel_nav_text{
	padding: 9px 20px 9px 15px;
	width: auto;
	height: 20px;
	line-height: 20px;
	color: #fff;
	font-size: 16px;
	font-weight: normal;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.lui_accordionpanel_toggle_up,
.lui_accordionpanel_toggle_down{
	width: 16px;
	height: 16px;
	background-image: url(../images/icons/icon-arrow-s.png);
	background-repeat: no-repeat;
	background-size:  auto 16px;
	float: none;
	position: absolute;
	top: 50%;
	right: 4px;
	margin-top: -8px;
	cursor: pointer;
}
.lui_accordionpanel_toggle_up{
	background-position: -32px 0;
}
.lui_accordionpanel_toggle_down{
	background-position: -48px 0;
}
@media screen and (-webkit-min-device-pixel-ratio: 2){
	.lui_accordionpanel_toggle_up,
	.lui_accordionpanel_toggle_down{
		background-image: url(../images/icons/icon-arrow-s@2x.png);
	}
}
.lui_accordionpanel_content_c{
	margin: -8px;
	padding: 8px;
	background-color: transparent;
}

.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_nav_text{
	padding: 7px 20px 7px 18px;
}
.lui_accordionpanel_simpletitle_nav_c:before {
	content: '';
	display: block;
	width: 4px;
	height: 16px;
	border-radius: 2px;
	background-color: #101B48;
	position: absolute;
	left: 5px;
	top: 50%;
	margin-top: -10px;
}
.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_up,
.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_down{
	background-image: url(../images/icons/icon-arrow-on-s.png);
	background-size: auto 16px;
}
.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_up{
	background-position: -32px 0;
}
.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_down{
	background-position: -48px 0;
}
@media screen and (-webkit-min-device-pixel-ratio: 2){
	.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_up,
	.lui_accordionpanel_simpletitle_nav_c .lui_accordionpanel_toggle_down{
		background-image: url(../images/icons/icon-arrow-on-s@2x.png);
	}
}

