.lui_portlet_operations {
	padding: 0 8px;
}
.lui_portlet_operation {
	margin: 5px 2px 10px;
	padding: 0;
	display: inline-block;
	width: 32px;
	height: 22px;
	border: 1px solid #d5d5d5;
	border-radius: 3px;
	background-color: #fff;
	background-repeat: no-repeat;
	background-position: 50%;
	background-size: auto 16px;
	font-size: 0;
