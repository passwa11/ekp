@charset "utf-8";

html,body{
	height: 100%;
}

iframe{
	border:0;
	width:100%;
	height: 100%;
}
/*::-webkit-scrollbar {
	width:0;
}*/
ul,
li {
  margin: 0px;
  padding: 0px;
  list-style: none;
}
/* header */
.lui_profile_header_frame {
  background: #37a8f5;
  height: 90px;
  /*padding: 0 25px;*/
  border-bottom: 1px #dbdbdb solid;
}
.lui_porfile_header_content{
	height: 90px;
    margin: 0 auto;
    position: relative;
}
.lui_porfile_header_logo,
.lui_porfile_header_title,
.lui_profile_header_navTop{
	display: inline-block;
}
.lui_porfile_header_logo{
	padding:0px 15px;
  width: 165px;
	line-height: 90px;
  overflow: hidden;
  float: left;
}
.lui_porfile_header_logo_img{
	max-height: 90px;
	cursor: pointer;
}
.lui_porfile_header_title{
	display: none;
	font-size: 14px;
	color: #949494;
}
.lui_porfile_header_title:before{
	content: '|';
	margin: 0 10px;
}
.lui_porfile_header_logo_div img{
  width: 100%;
  max-height: 100%;
	vertical-align: middle;
}
.lui_profile_header_navTop {
  position: absolute;
  top: 0;
  left: 195px;
  right: 240px;
  height: 90px;
  overflow: hidden;
}

.lui_profile_header .lui_portal_header_userinfo{
	position: absolute;
	top : 0;
	right: 0px;
	cursor: pointer;
	/*margin-top: -15px;*/
}
.lui_popup_border_content {
	margin-left: 1px;
}
.lui_portal_header_text,
.lui_portal_header_hitch{
	line-height: 30px; 
	font-size: 14px;
	padding: 0 5px;
}
.lui_portal_header_text_over { 
	background: white; 
	color:#333;
	cursor: pointer; 
}
.lui_portal_header_icon_arrow {
	vertical-align: text-bottom; 
	background:url(/ekp/sys/ui/extend/theme/default/images/profile/arrow.png) no-repeat center center;
}
.lui_portal_header_text:hover{ 
	color: #333;
}
.lui_profile_header_navTop_root{
	position: relative;
    height: 100%;
	overflow: hidden;
}
.lui_profile_header_navTop_rootInner{
	/*display: inline-block;
	float: right;*/
}
.lui_profile_header_navTop_frame{
	position: absolute;
	overflow: hidden;
}
.lui_profile_header_navTop_body{
	display: inline-block;
}
.lui_profile_header_navTop_item{
	display: inline-block;
	vertical-align: middle;
	margin-right: 2px;
	padding:18px 0px;
	color: #fff;
	font-size: 16px;
	cursor: pointer;
	text-align: center;
	height:54px;
	width:110px;
  transition-duration: .3s;
}
.lui_profile_header_navTop_item:hover{
  color:#fff;
  background-color: #2596e4;
}
.lui_profile_header_navTop_item.current{
	background-color: #2596e4;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
     -moz-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
      -ms-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
       -o-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
          box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
}
.lui_profile_header_navTop_itemIcon {
	display: inline-block;
	width: 26px;
	height: 26px;
	margin-bottom: 10px;
	background-size: cover;
}
.lui_profile_header_navTop_itemText {
	font-size: 14px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;	
}
.lui_profile_header_navTop_item.more{
	text-indent: -999em;
	background:url(../icon/m/profile_navTop_more.png) no-repeat center;
	/*width:110px;*/
	height: 90px;
	padding:0px;
	margin-right: 0px;
	vertical-align: top;
}
.lui_profile_header_navTop_item.more.current,
.lui_profile_header_navTop_item.more:hover,
.lui_profile_header_navTop_item.lui_profile_navTop_over{
	background-color:#2596e4;
}
.lui_popup_border_content.lui_profile_header_navTop_popup{
	/*display:none;*/
	position: absolute;
	/*margin-top: -7px;*/
	background-color: #fff;
	margin-left: -2px;
	border:none;
    /*box-shadow: 1px 1px 1px #ECECEC,-1px -1px 1px #ECECEC;*/
}
.lui_profile_header_navTop_popup .lui_profile_header_triangle_border,
.lui_profile_header_navTop_popup .lui_profile_header_triangle_bg{
	position:absolute;
	width: 0;
    height: 0;
    border: 12px solid transparent;
    left: 85px;
    display: none;
}
.lui_profile_header_navTop_popup .lui_profile_header_triangle_border{
	border-bottom-color:#ECECEC;
	top: -24px;
}
.lui_profile_header_navTop_popup .lui_profile_header_triangle_bg{
	border-bottom-color:#fff;
	top : -22px;
}
.lui_profile_header_navTop_popup .lui_profile_header_navTop_item{
	/*padding-left:15px;
	padding-right: 15px;*/
	margin:0px;
	background: #37a8f5;
	display: block;
	border:1px solid #fff;
	border-bottom:none;
}
.lui_profile_header_navTop_popup .lui_profile_header_navTop_item:hover{
	background:#2596e4;
}
.lui_profile_header_navTop_popup .lui_profile_header_navTop_item.current{
	height: auto;
}
.lui_profile_header_navTop_popup{
	z-index:50;
}

/** userInfo **/
.lui_profile_userinfo_img{
	display: inline-block;
    width: 30px;
    height: 30px;
    border-radius: 50%;
    line-height: 0px;
    overflow: hidden;
}

.lui_profile_userinfo{
	background: #37a8f5;
    width: 140px;
    height: 90px;
    line-height: 90px;
    text-align: center;
    /*border-left:1px solid #fff;*/
	vertical-align: top;
}
.lui_profile_userinfo.lui_profile_userinfo_over{
	background:#2596e4;
}
.lui_profile_userinfo .lui_profile_userinfo_img {
  display: inline-block;
  height: 90px;
  line-height: 90px;
  vertical-align: top;
  margin-right: 3px;
  vertical-align: top;
}

.lui_profile_userinfo_img img {
    width: 30px;
    height: 30px;
    border-radius: 50%;
}
.lui_portal_header_userinfo .lui_profile_userinfo_img img{ vertical-align: middle;}
.lui_profile_userinfo_name{
	 display:inline-block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis;width:80px; color:#fff;
	 line-height: 90px; text-align: left;
}

.lui_profile_userinfo_popup{
	width: 280px;
    height: 300px;
    border: 1px solid #DBDBDB;
    background-color: #fff;
}

.lui_profile_userinfo_header{
	height: 160px;
    background: #2596e4;
    color: #fff;
    text-align: center;
}

.lui_profile_userinfo_header .lui_profile_userinfo_img {
    width: 70px;
    height: 70px;
    margin-top: 15px;
    margin-bottom: 5px;
    border: 4px solid #92cbf2;
}
.lui_profile_userinfo_header .lui_profile_userinfo_img  img{
	width:100%;
	height: 100%;
}

.lui_profile_userinfo_header .lui_profile_userinfo_headername {
	margin:0;
    font-size: 16px;
    margin-bottom: 5px;
}

.lui_profile_userinfo_link {
    margin-top: 15px;
    /*margin-bottom: 15px;*/
}

.lui_profile_userinfo_link ul {
    display: table;
    border-collapse: collapse;
    width: 100%;
}

.lui_profile_userinfo_link ul li {
    display: table-cell;
    border: 1px solid #DBDBDB;
    text-align: center;
    padding: 10px 0px;
    border-right: none;
    height: 48px;
}

.lui_profile_userinfo_link ul li:first-child{ border-left:none;}

.lui_profile_userinfo_link ul li:hover {
	background: #F3F3F3;
}

.lui_profile_userinfo_link ul li a {
    display: block;
}

.lui_profile_userinfo_popup .lui_profile_userinfo_icon {
    display: inline-block;
    margin-bottom: 5px;
    width: 16px;
    height: 16px;
}

.lui_profile_userinfo_link ul li .lui_profile_userinfo_link_text {
    font-size: 14px;
    color: #949494;
}

.lui_profile_userinfo_btnGroup {
    padding: 15px 5px 5px;
    background: #f9f9f9;
}

.lui_profile_userinfo_btnGroup:after {
    display: table;
    content: "";
    clear: both;
}

.lui_profile_userinfo_btn {
    cursor: pointer;
    display: inline-block;
    height: 32px;
    line-height: 32px;
    border: 1px solid #DBDBDB;
    padding: 0px 25px;
    color: #949494;
}

.lui_profile_userinfo_btn:hover {
	background-color: #F3F3F3;
}

.lui_profile_userinfo_btn.userinfo_quit{
	float: right;
}

/* navLeft */
.lui_profile_navLeft_container {
  width: 200px;
  background-color: #242f3a;
  /*padding: 15px 0;*/
  position: absolute;
  bottom: 0;
  top: 90px;
  overflow: auto;
  overflow-x: hidden;
  z-index: 40;
}
.lui_profile_link{
	display: block;
	padding-left: 15px;
  height: 55px;
	line-height: 55px;
  color: #fff;
  border-left:3px solid transparent;
  transition-duration: .3s;
}
.lui_profile_navLeft_item_icon {
  display: inline-block;
  width: 16px;
  height: 16px;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  vertical-align: middle;
}
.lui_profile_navLeft{ width: 180px; }
.lui_profile_navLeft_item{
	cursor: pointer;
	margin-bottom: 2px;
  transition-duration: .3s;
}
.lui_profile_navLeft_item.current .lui_profile_link,
.lui_profile_navLeft_item.current .lui_profile_link:hover{
  color: #fff;
	background-color: #18212a;
	border-left-color:#2596e4;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
     -moz-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
      -ms-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
       -o-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
          box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
}
.lui_profile_navLeft_item .lui_profile_link:hover{
  color: #fff;
	background-color: #18212a;
}
.lui_profile_navLeft_item > div{
	margin: 0 auto;
}
.lui_profile_navLeft_item .lui_profile_navLeft_item_text{
	font-size:14px;
	display: inline-block;
	vertical-align: middle;
	padding:0px 10px;
	width:120px;
	text-align:left;
	white-space:nowrap;
	text-overflow:ellipsis;
	overflow:hidden;
}

/* moduleMain & moduleIndex */
.lui_profile_moduleMain{
	position: absolute;
	top : 90px;
	bottom:0;
	left: 180px;
	right:0;
	background-color: #fff;
	z-index:49;
	overflow-y : hidden;
}
.lui_profile_moduleMain iframe{
	border: 0;
	width: 100%;
	height: 100%;
}
.lui_profile_moduleIndex_down,
.lui_profile_moduleIndex_down > div{
	background-color: #fff;
}
.lui_profile_moduleIndex_down > div{
	position:absolute;
	top : 0;
	bottom : 0;
  z-index: 50;
	-o-transition: all 0.2s ease;
    -ms-transition: all 0.2s ease;
    -moz-transition: all 0.2s ease;
    -webkit-transition: all 0.2s ease;
}
.lui_profile_list_body{ background-color: #fff; }
.lui_profile_moduleIndex_down .lui_profile_moduleIndex_tree{
	width: 180px;
	background-color:#fbfbfb;
	left:0;
	z-index : 39;
	overflow-y: hidden;
  overflow-x: hidden;
}
.lui_profile_moduleIndex_tree{
	overflow-x:hidden;
}
.lui_profile_moduleIndex_tree > iframe{
	width: 10000%;
}
.lui_profile_moduleIndex_ctrlborder{
	height : 100%;
	border-left: 1px #dbdbdb solid;
}
.lui_profile_moduleIndex_ctrl1,
.lui_profile_moduleIndex_ctrl2{
	width: 10px;
}
.lui_profile_moduleIndex_ctrl1{
	left: 180px;
	z-index : 38;
}
.lui_profile_moduleIndex_ctrlbtn {
  position: absolute;
  width: 10px;
  height: 60px;
  top: 50%;
  /*background-image: "../images/profile/ctrl.png";*/
  background-color: #bdbdbd;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  transition-duration: .3s;
  cursor: pointer;
}
.lui_profile_moduleIndex_ctrlbtn.toLeft{
	margin-top: -40px;
}
.lui_profile_moduleIndex_ctrlbtn.toRight{
	margin-top: -120px;
}
.lui_profile_moduleIndex_ctrlbtn:hover{ background-color:#18212a;}
.lui_profile_moduleIndex_ctrlbtn.toLeft:before{
	/*content: '<';*/
	content:'';
	color: #fff;
	width: 100%;
	height:100%;
	display: inline-block;
	background:url(../images/profile/profile_ctrbtn.png) no-repeat -10px center;
}
.lui_profile_moduleIndex_ctrlbtn.crtl_btn_unable{ cursor: default; cursor: not-allowed;}
.lui_profile_moduleIndex_ctrlbtn.crtl_btn_unable:hover{background:#bdbdbd;}

.lui_profile_moduleIndex_ctrlbtn.toRight:before{
	/*content: '>';*/
	content:'';
	color: #fff;
	width: 100%;
	height: 100%;
	display: inline-block;
	background:url(../images/profile/profile_ctrbtn.png) no-repeat left center;
}
.lui_profile_moduleIndex_down .lui_profile_moduleIndex_org{
	/* width:180px; */
	background-color:#fbfbfb;
	left : 190px;
	z-index : 40;
	overflow-y: hidden;
  overflow-x: hidden;
}
.lui_profile_moduleIndex_org > iframe{
	width: 10000%;
}
.lui_profile_moduleIndex_ctrl2{
	left:370px;
	z-index : 36;
}
.lui_profile_moduleIndex_org,
.lui_profile_moduleIndex_ctrl2{
	width:0;
	overflow-x: hidden;
}
.lui_profile_moduleIndex_right{
  margin-left: -2px;  /* 针对edge的兼容，处理【后台管理】滚动条异常 */
  position: absolute;
  left: 200px;
  right: 0;
  overflow-y : hidden;
}
.lui_profile_moduleIndex_viewStroke{
	display: none;
	position: absolute;
	top:0;
	right: 0;
	height:100%;
	width:17px;
	/*width:17px\0;*/
	background-color: #fff;
}
/*.lui_profile_moduleIndex_viewStroke:not(*:root) { width:0px;}*/
.lui_profile_moduleIndex_view{
	margin-right:0px;
	/*margin-right:0px\0;*/
	height: 100%;
  /*min-width: 800px;*/
}
/*.lui_profile_moduleIndex_viewStroke:not(*:root) { margin-right:0px;}*/
.lui_profile_moduleIndex_viewframe{
	height: 100%!important;
}
.lui_profile_moduleIndex_doc{
	height: 0;
}

/* listView */
.lui_profile_listview_body{
	overflow: auto;
}
.lui_profile_listview_container{
}
.lui_profile_listview_content{
	padding: 0px 20px;
}
.lui_profile_listview_icon{
	display: inline-block;
    width: 16px;
    height: 16px;
}
.lui_profile_listview_l_icon {
    display: inline-block;
    width: 50px;
    height: 50px;
    background-repeat: no-repeat;
    background-position: center;
}
.lui_profile_listview_search {
  background: #f3f3f3;
  padding-top: 10px;
  padding-bottom: 10px;
  z-index: 10;
}
.lui_profile_listview_search .lui_profile_listview_content{ }
.lui_profile_listview_search .lui_profile_listview_content:after{
	display: table;
    content: "";
    clear: both;
}
.lui_profile_listview_switchView{
	font-size: 0px;
    float: right;
}
.lui_profile_listview_switchView > a {
	display: inline-block;
    border: 1px solid #dbdbdb;
    height: 38px;
    line-height: 38px;
    padding-top: 0px;
    padding-bottom: 0px;
    background: #f3f3f3;
    margin-left: -1px;
	width:36px;
	position: relative;
}
.lui_profile_listview_switchView > a > i{
	background-repeat: no-repeat;
    background-position: center;
    vertical-align: middle;
	position: absolute;
	top:0px;
	right: 0px;
	bottom: 0px;
	left:0px;
	margin:auto;
}
.lui_profile_listview_searchWrap {
  width: 300px;
  background: #fff;
  margin-right: 28px;
  position: relative;
  float: right;
  vertical-align: middle;
  position: relative;
}
.lui_profile_listview_searchWrap .tips,
.lui_profile_listview_searchWrap .tipsNull{
  margin-top: 5px;
  padding: 5px 10px;
  position: absolute;
  top: 100%;
  left: 0;
  right: 0;
  z-index: 10;
  color: #fff;
  border: 1px solid #242f3a;
  background-color: #242f3a;
  -webkit-box-shadow: 0 1px 8px rgba(0,0,0,.35);
     -moz-box-shadow: 0 1px 8px rgba(0,0,0,.35);
      -ms-box-shadow: 0 1px 8px rgba(0,0,0,.35);
       -o-box-shadow: 0 1px 8px rgba(0,0,0,.35);
          box-shadow: 0 1px 8px rgba(0,0,0,.35);
}
.lui_profile_listview_searchWrap .tips:after,
.lui_profile_listview_searchWrap .tips:before,
.lui_profile_listview_searchWrap .tipsNull:after,
.lui_profile_listview_searchWrap .tipsNull:before {
  content: '';
  display: block;
  border-bottom: 6px dashed;
  border-bottom: 6px solid \9;
  border-right: 6px solid transparent;
  border-left: 6px solid transparent;
  position: absolute;
  left: 25px;
}
.lui_profile_listview_searchWrap .tips:before,
.lui_profile_listview_searchWrap .tipsNull:before{
  color: #242f3a;
  top: -6px;
}
.lui_profile_listview_searchWrap .tips:after,
.lui_profile_listview_searchWrap .tipsNull:after{
  color: #242f3a;
  top: -5px;
}
.lui_profile_listview_searchWrap .close{
  width: 16px;
  height: 16px;
  background: url(../icon/s/icon_close_red.png) no-repeat 50%;
  cursor: pointer;
  transition-duration: .3s;
  position: absolute;
  right: 5px;
  top: 50%;
  margin-top: -8px;
}
.lui_profile_listview_searchWrap .close:hover{ opacity: .75; }

.lui_icon_s_icon_search {
  position: absolute;
  top: 0px;
  bottom: 0px;
  margin: auto 10px;
}
.lui_profile_search_input {
  display: inline-block;
  border: 1px solid #dbdbdb;
  font-size: 14px;
  padding: 9px 10px;
  padding-left: 30px;
  padding-right: 25px;
  width: 245px;
  line-height: 20px;
  -webkit-appearance: textfield;
  outline: none;
}
.lui_profile_listview_switchTo{
	cursor: pointer;
}
.lui_profile_listview_switchTo.lui_icon_on{
	background-color: #fff;
}
.lui_profile_listview_grid{
	overflow: hidden;
    position: relative;
    margin: 0 auto;
}
.lui_profile_listview_grid_bd{
	left: 0px;
    position: relative;
    overflow: hidden;
    padding: 0px;
    margin: 0px;
}
.lui_profile_listview_card_page{
    font-size: 0;
	float: left;
}
.lui_profile_block_grid_item {
  position: relative;
  width: 243px;
  height: 178px;
  border: 1px solid #dbdbdb;
  overflow: hidden;
  /*text-align: justify;*/
  display: inline-block;
  margin: 30px 15px 0px;
}
.lui_profile_block_grid_item:hover{
	border-color: #46b1fc;
}
.lui_profile_block_grid_iconWrapper{
	width: 100%;
    height: 164px;
    padding-top: 44px;
    text-align: center;
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 6;
}
.lui_profile_block_grid_titleWrapper {
  position: absolute;
  left: 0px;
  top: 138px;
  width: 100%;
  height: 100%;
  z-index: 7;
  background: #fff;
  -webkit-transition: height .3s, top .3s;
          transition: height .3s, top .3s;
}
.lui_profile_block_grid_item:hover .lui_profile_block_grid_titleWrapper{
	top: 0px;
	height: 208px;
	opacity: 0.9;
	filter: alpha(opacity=90);
	cursor: pointer;
}
.lui_profile_block_grid_itemTitle {
  display: block;
  padding: 10px 5px 15px;
  text-align: center;
  font-size: 16px;
  color: #323941;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lui_profile_block_grid_itemDes{
	padding: 10px 10px 0px;
    line-height: 20px;
    height: 100px;
    overflow: hidden;
    font-size: 12px;
    color: #707070;
}
.lui_profile_block_grid_item:hover .lui_profile_block_grid_itemDes{
	position: relative;
	top: -10px;
}
.lui_profile_listview_grid_hd{
	padding: 30px 0px;
}
.lui_profile_listview_grid_hd ul {
  font-size: 0;
  text-align: center;
}
.lui_profile_listview_grid_hd ul li {
  margin: 0 5px;
  padding: 0;
  min-width: 20px;
  text-align: center;
  display: inline-block;
  cursor: pointer;
  -webkit-transition: padding .15s;
          transition: padding .15s;
}
.lui_profile_listview_grid_hd ul li span {
    border-radius: 8px;
    display: inline-block;
    width: 12px;
    height: 10px;
    transition: width .15s;
    -webkit-transition: width .15s;
    background: #dbdbdb;
}
.lui_profile_listview_grid_hd ul li.active span, .lui_profile_listview_grid_hd ul li:hover span {
  width: 20px;
