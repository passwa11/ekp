	cursor: default;
}

.location_arrow:before,
.location_arrow:after {
  content: "";
  display: inline-block;
  width: 0;
  height: 0;
  border-width: 4px 8px;
  border-style: solid;
  border-color: transparent;
  border-bottom-color: #dbdbdb;
  position: absolute;
  top: -8px;
  left: 50%;
  margin-left: 25px;
}

.location_arrow:after {
	border-bottom-color: #fff;
	top: -7px;
}

