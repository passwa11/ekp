	border-color: #15a4fa;
}

.location_input input {
	border: none;
	width: 336px;
	height: 28px;
	line-height: 28px;
}

.location_input_selection {
	border: 1px solid #dbdbdb;
	position: absolute;
	top: 38px;
	left: -1px;
	right: -1px;
	background: #fff
}

.location_input_selection ul li {
	height: 38px;
	line-height: 38px;
	padding-left: 10px;
	cursor: pointer;
}

