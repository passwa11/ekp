/* 个人空间发布动态，图册弹出框 */
.lui_sns_dialog_headC .trig {
	display: block;
	position: absolute;
	left: 20px;
	top: -10px;
}
.lui_sns_dialog_headC .trig i.mtr1 {
	border-bottom-color: #dadada;
	top: -1px;
}
.lui_sns_dialog_headC .trig i {
	display: block;
	position: absolute;
	left: 0px;
	width: 0px;
	height: 0px;
	border: 5px solid transparent;
	border-width: 5px 7px;
	border-bottom-color: #f8fcfd;
}
.lui_sns_dialog_headC .trig i.mtr2 {
	position: relative;
	z-index: 101;
	top: 0px;
}
