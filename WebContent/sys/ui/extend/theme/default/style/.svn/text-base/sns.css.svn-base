.alterEvent_approve_list ul li .ck{ position:absolute; left:13px; top:35px;}
.alterEvent_approve_list ul li .cover{ position:absolute; left:46px; top:15px; width:52px; height:52px; z-index:10; background:url(../images/sns/user_border_L_1.png) no-repeat center;}
.alterEvent_approve_list ul li .user_img{ width:50px; height:50px; position:absolute; left:47px; top:16px;}
.alterEvent_approve_list ul li p.basicinfo{ padding-top:8px; color:#898989;}
.alterEvent_approve_list ul li .com_author{ margin-right:12px;}
.alterEvent_approve_list ul li p.basicinfo em,.alterEvent_approve_list ul li p.content em{ margin:0px 15px;}
.alterEvent_approve_list ul li p.content{ font-size:14px; color:#000; line-height:22px; padding-top:4px;}
 /* 活动详情切换 */
.lui_tabpanel_eventDetail_frame {padding-top: 20px;position:relative}
.lui_tabpanel_eventDetail_navs_l{width:100%;}
.lui_tabpanel_eventDetail_navs_c {font-size: 12px;background-color: #FFFFFF;height: 33px;color: #FFFFFF;line-height: 35px;border-bottom: 1px solid #FFFFFF;vertical-align: middle;}
.lui_tabpanel_eventDetail_navs_item_l {float: left;overflow: hidden;}
.lui_tabpanel_eventDetail_navs_item_c {padding: 0 18px;height: 22px;line-height: 22px;text-align: center;color: #383838;font-size: 14px;cursor: pointer;float: left;border-right: 3px solid #ececec;}
.lui_tabpanel_eventDetail_navs_item_l .lui_tabpanel_eventDetail_navs_item_c:hover{font-size:16px;color:#ffaa00;}
.lui_tabpanel_eventDetail_navs_item_selected .lui_tabpanel_eventDetail_navs_item_c{font-size:16px;color:#ffaa00}
/* 发起活动 */
.lui_sheet_dialog_table tr td.diaR .num{display:inline-block;font-size:14px;padding-left:4px;color:#acacac}
.lui_sheet_dialog_table tr td.diaR .num  em{color:#fa0;padding:0}
.publish_to_topic{padding-left:30px}
/** 活动报名 */
.createEvent_wrapper{ background:#fff; padding:12px;}
.createEvent_wrapper table tr td{ vertical-align:top;}
.createEvent_wrapper table tr td input{ float:left;}
.createEvent_wrapper table tr td textarea{ float:left;}
.createEvent_wrapper table tr td .num{ float:left; color:#acacac; font-size:14px; margin-left:10px;}
.createEvent_wrapper table tr td .num em{ color:#fa0;}

/*** 活动日志列表 ***/
.lui_sns_event_recordContent{padding-top:12px}
.lui_sns_event_recordContent>ul li{ zoom:1; color:#7d7d7d;  padding:5px;position:relative}
.lui_sns_event_recordContent>ul li:after{ content:"clear"; clear:both; display:block; visibility:hidden; line-height:0px;}
.lui_sns_event_recordContent>ul li:hover{ background:#f8f8f8;}
.lui_sns_event_recordContent>ul li .date{ color:#000; position:absolute;right:0}
.lui_sns_event_recordContent>ul li .com_author{ margin-right:10px;width:100px;text-align:left;position:absolute;left:0;white-space: nowrap;overflow: hidden;text-overflow:ellipsis;}
.lui_sns_event_recordContent>ul li .lui_event_record{display:inline-block;padding:0 120px 0 100px}
.lui_sns_event_recordContent>ul li .lui_event_record .remark{width:500px;display:inline-block;vertical-align:bottom}
.lui_sns_event_recordContent>ul li .status{ color:#ccc; position:absolute;right:130px}

