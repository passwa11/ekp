/*** reset ***/
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,iieldset,input,textarea,p,blockquote,th,td{padding:0;margin:0;font-weight:normal}
ol,ul{ list-style:none;}
em{ font-style:normal;}
.hand{cursor:pointer}
.default-cur{cursor:default}
.nor-lh{line-height:normal !important;}
a{ text-decoration:none; transition:background-color 0.3s ease 0s, border 0.3s ease 0s, color 0.3s ease 0s, opacity 0.3s ease-in-out 0s;}
a:visited{background:none}

