/*** 邀请同事加入 ***/
.lui_sns_btn_invite{ text-align:center; cursor:pointer; }
.lui_sns_btn_invite .sns_btn_inviteC{ background:#fff;  height:48px; line-height:48px; border:1px solid #15a4fa;  transition:background-color 0.3s ease 0s, border 0.3s ease 0s, color 0.3s ease 0s, opacity 0.3s ease-in-out 0s; }
.lui_sns_btn_invite.bg_2 .sns_btn_inviteC{ background:#fff;}
.lui_sns_btn_invite.bg_1 .sns_btn_inviteC{ /*background:#f7f7f7;*/}
.lui_sns_btn_invite .sns_btn_invite_icon:before {
  content: "";
  display: inline-block;
  width: 24px;
  height: 20px;
  vertical-align: middle;
  background: url(../images/sns/btn_invite_icon.png) no-repeat left center;
  margin-right: 10px;
}
