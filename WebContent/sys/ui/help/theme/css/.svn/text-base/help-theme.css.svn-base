/* 主题包帮助文档 */
.lux-portal-layout-table {
    vertical-align: top;
    width:100%;
}

.lux-portal-layout-table td {
    vertical-align: top;
}

#map_btn {
    text-align: center;
    background: transparent;
    border: 1px solid #4285F4;
    border-radius: 2px;
    width: 164px;
    height: 30px;
    margin-left: 15px;
    margin-top: 5px;
    margin-bottom: 15px;
    line-height: 30px;
}

#map_btn img {
    position: relative;
    top: 3px;
}

#map_btn:hover {
    background: rgba(66, 133, 244, 0.60) !important;
}

#map_btn_span {
    padding-top: 1px;
    font-family: MicrosoftYaHei;
    font-size: 14px;
    color: #FFFFFF;
    letter-spacing: 0;
    cursor: pointer;
}

.lui-jsmind-innerToolBar {
    position: absolute;
    bottom: 50%;
    right: 3px;
    width: 25px;
    z-index: 10;
    transform: none;
}

#kmseditors-sidebar ul,
.lui-jsmind-innerToolBar ul {
    border-width: 1px 1px 1px;
    border-style: solid none solid solid;
    border-color: #d9d9d9 #d9d9d9 #d9d9d9;
    border-right: none;
}

#kmseditors-sidebar ul li,
.lui-jsmind-innerToolBar li {
    cursor: pointer;
    padding: 5px;
    border-bottom: 1px solid #d9d9d9;
    display: inline-block;
    vertical-align: middle;
    width: 16px;
    height: 16px;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 16px;
}

.lui-jsmind-innerToolBar li:LAST-CHILD {
    border-bottom: none;
}

.lui_map_icon_zoom_out {
    background-image: url(./images/zoomout.png)
}

.lui_map_icon_zoom_in {
    background-image: url(./images/zoomin.png)
}

.lui_map_icon_zoom_reset {
    background-image: url(./images/zoomreset.png)
}

#top_frame canvas {
    position: static;
}

#adjust_box {
    width: 100px;
    border: 1px solid #ddd;
    border-radius: 3px;
    position: absolute;
    text-align: center;
    height: 60px;
    background: #fff;
}

#adjust_box_head {
    height: 20px;
    margin-top: -8px;
    border-bottom: 1px solid #ddd;
}

#adjust_box_head h1 {
    font-size: 14px;
}

#adjust_box_cont span {
    display: block;
    width: 32px;
    float: left;
    height: 36px;
    border-right: 1px solid #ddd;
    position: relative;
    left: 1px;
}

#adjust_box_cont span:first-child+span+span {
    border-right: 0;
}

#adjust_box_cont label {
    display: block;
    width: 32px;
    background-size: 100% 100%;
    float: left;
    height: 36px;
    cursor: pointer;
}

#adjust_box_cont span:hover {
    background: #c9eeff;
}

#adjust_box_cont span:active {
    background: #c9eeff;
}

.pc_label {
    background-image: url(./images/pc_platf.png);
}

.mobile_label {
    background-image: url(./images/mobile_platf.png);
    height: 28px !important;
    margin-top: 5px;
}

.all_label {
    background-image: url(./images/all_platf.png);
}

.pc_label_light {
    background-image: url(./images/pc_platf_light.png);
}

.mobile_label_light {
    background-image: url(./images/mobile_platf_light.png);
    height: 28px !important;
    margin-top: 5px;
}

.all_label_light {
    background-image: url(./images/all_platf_light.png);
}

.lui_listview_columntable_table {
    position: relative;
}

