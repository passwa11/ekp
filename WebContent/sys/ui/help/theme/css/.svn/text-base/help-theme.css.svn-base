/* create 200803 */
/* 新开按钮 */
.lux-link-btn {
    display: inline-block;
    border: 1px solid #4285f4;
    color: #4285f4;
    padding: 5px;
    text-align: center;
    min-width: 100px;
    margin: 10px 0;
    border-radius: 2px;
    cursor: pointer;
}

/* 菜单 */
.lux-theme-menu {
    position: fixed;
    bottom: 0px;
    left: 0;
    background-color: #FFEEEE;
    border: 1px #FF9999 solid;
    z-index: 9999;
    color:#333;
    cursor: pointer;
}
.lux-template-menu,
.lux-list-menu,
.lux-form-menu{
    left: 46px;
}
#theme_help_menu_pop,
#template_help_menu_pop{
    background: #fff;
}
.lux-theme-innner{
    background:#fff;
    padding:10px;
}
.lux-theme-menu-title{
    text-align: center;
    padding:8px;
}

/* 主题选择列表 */
.lux-theme-list {
    list-style: none;
    overflow: hidden;
}

.lux-theme-item {
    width: 25%;
    position: relative;
    float: left;
}

.lux-theme-item-inner {
    padding: 10px;
}

.lux-theme-no {
    position: absolute;
    left: 10px;
    top: 10px;
    font-style: normal;
    padding: 10px;
}

.lux-theme-thumb {
    text-align: center;
    background: #f0f0f0;
    width: 100%;
    height: 100px;
    line-height: 100px;
}

.lux-theme-thumb img {
    max-width: 100%;
    max-height: 100%;
    height: auto;
    width: auto;
}

.lux-theme-item:hover {
    background: #f8f8f8;
}

.lux-theme-item p {
    padding: 5px;
}

.lux-theme-item p span:first-child {
    /* padding-top: 5px; */
}

.lux-theme-item p span {
    display: block;
    font-size: 12px;
    text-align: center;
    padding: 5px 0 0;
}

