@charset "utf-8";
/* 后台图标选择弹出框 */
.lui_iconList {
    padding-top: 42px;
    overflow: hidden;
}

.lui_profile_iconSelect_header {
    padding: 10px 0;
    position: fixed;
    top: 0;
    background: #fff;
    width: 100%;
    z-index: 2;
}

.lui_profile_iconSelect_header:after {
    display: table;
    content: "";
    clear: both;
}

.lui_profile_iconSelect_category {
    width: 50%;
    float: left;
}

.lui_profile_iconSelect_category select {
    width: 200px;
    height: 24px;
    padding: 2px 5px;
    color: #999;
    vertical-align: middle;
    border-radius: 4px;
    outline: none;
    -webkit-appearance: none;
    margin-left: 10px;
}

.lui_profile_iconSelect_search {
    width: 50%;
    float: left;
}

.lui_profile_iconSelect_searchbox {
    position: relative;
    display: inline-block;
    margin-right: 1px;
    padding-left: 0;
    padding-right: 24px;
    width: 200px;
    height: 24px;
    border: 1px solid #d5d5d5;
    border-radius: 4px;
    float: right;
    margin-right: 10px;
}

.lui_profile_iconSelect_searchbox input {
    width: 96%;
    height: auto;
    line-height: 20px;
    padding: 2px 2%;
    color: #7d7d7d;
    border: none;
    outline: none;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}

.lui_profile_iconSelect_searchbtn {
    margin-left: 0;
    position: absolute;
    right: 0px;
    top: 0px;
    display: inline-block;
    border: none;
    text-indent: -9999em;
    width: 24px;
    height: 24px;
    background: url(../images/search_btn.png) no-repeat 50%;
}
.lui_profile_iconSelect_searchbtn:hover{
    background: url(../images/search_btn_on.png) no-repeat 50%;
}
.lui_profile_iconSelect_content{
    position:absolute;
    top: 46px;
    bottom:0;
    overflow:auto;
    width: 100%;
}

.lui_profile_iconSelect_list ul {
    list-style: none;
    padding: 0 10px 10px;
}

.lui_profile_iconSelect_list ul:after {
    display: table;
    content: "";
    clear: both;
}

.lui_profile_iconSelect_list ul li {
    width: 80px;
    margin: 8px 10px;
    float: left;
    text-align: center;
    cursor: pointer;
}
.lui_profile_iconSelect_list .lui_paging_box ul li {
    width: auto;
}
.lui_profile_iconSelect_list ul.lui_paging_contentBox li span{
     width: auto;
     height: auto;
     color: #999;
     font-size: 12px;
}
.lui_profile_iconSelect_list ul li:hover,
.lui_profile_iconSelect_list ul li:hover span {
    color: #4285f4;
}

.lui_profile_iconSelect_list ul li p {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 14px;
}

.lui_profile_iconSelect_list ul li span {
    display: inline-block;
    width: 40px;
    height: 40px;
    color: #999;
    font-size: 40px;
    margin-bottom: 5px;
}

/* 图标选择 选项卡 */
.lui_profile_iconSelect_tab {
    border-bottom: 1px solid #d5d5d5;
}

.lui_profile_iconSelect_tab ul {
    overflow: hidden;
}

.lui_profile_iconSelect_tab ul li {
    list-style: none;
    display: inline-block;
    padding: 8px 10px;
    float: left;
    margin-right: 5px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom:2px solid #fff;
    cursor:pointer;
}

.lui_profile_iconSelect_tab ul li.status_current
 {
    border-bottom-color:#4285f4;
    color:#4285f4;
}
.lui_profile_iconSelect_tab ul li:hover{
    color:#4285f4;
}
.lui_profile_iconSelect_search{
    position:absolute;
    right:0; 
    top:10px;
}
.tb_normal.material_table,
.tb_normal.material_table > tbody > tr > td,
.tb_normal.material_table > thead > tr > td,
.tb_normal.material_table > thead > tr > th{
    border:0;
}