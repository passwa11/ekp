/* 复制成功 */
.profile-iconList-item .suc-copy {
	position: absolute;
	top: 20%;
	left: 50%;
	transform: translate(-50%);
	width: 80px;
	height: 30px;
	line-height: 30px;
	color: #fff;
	text-align: center;
	margin: 0 auto;
	background-color: #071e44;
	border-radius: 5px;
	box-shadow: 0 0 10px 0 rgba(153, 179, 210, 0.4);
	display: none;
	transition: all 1s ease;
}
