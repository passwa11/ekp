/*部件*/
.profile_loginsetting_box_curstom .profile_loginSetting_bg a{
	width: 100%;
}

/* ------------ 03-预览 Starts ------------ */
.cm-preViews {
	width: 920px;
	height: 680px;
}

.cm-preViews .preViews-title {
	width: 100%;
	height: 59px;
	background: #EEEEEE;
}

.cm-preViews .preViews-title span {
	display: inline-block;
	width: 105px;
	font-size: 16px;
	color: #333333;
	text-align: center;
	line-height: 59px;
}

.cm-preViews .preViews-title .typeActive {
	background-color: #fff;
}

.cm-preViews .preViews-title .client {
	cursor: pointer;
}

.cm-preViews .preViews-content img {
	display: block;
	margin: 30px auto 0;
	text-align: center;
}

.cm-preViews .preViews-content .imgWrap {
	display: inline-block;
	text-align: center;
	width: 100%;
	overflow-y: scroll;
	height: 600px;
}

.cm-preViews .preViews-content .pc-img {
	width: 100%;
	box-shadow:0 0 10px 0 rgba(153,179,210,0.20);
}

.cm-preViews .preViews-content .mb-img {
	width: 272px;
	box-shadow:0 0 10px 0 rgba(153,179,210,0.20);
}

.cm-preViews .preViews-footer {
	position: absolute;
	right: 20px;
	bottom: 20px;
}

.cm-preViews .preViews-footer span {
	display: inline-block;
}

.cm-preViews .preViews-footer .useIt {
	width: 90px;
	height: 30px;
	line-height: 30px;
	background: #4285F4;
	border: 1px solid #4285F4;
	border-radius: 2px;
	color: #fff;
	font-size: 14px;
	text-align: center;
	cursor: pointer;
	-webkit-transition: all .3s;
	transition: all .3s;
	margin-right: 15px;
}

.cm-preViews .preViews-footer .cancel {
	width: 90px;
	height: 30px;
	line-height: 30px;
	background: #fff;
	border: 1px solid #ddd;
	border-radius: 2px;
	color: #333;
	font-size: 14px;
	text-align: center;
	cursor: pointer;
	-webkit-transition: all .3s;
	transition: all .3s;
}
/* ------------ 03-预览 Ends ------------ */

.profile_loginSetting_list .profile_loginSetting_item img {
	width: 100%;
	border: 1px solid #eee;
}

.profile_loginSetting_list .profile_loginSetting_item:hover .profile_loginSetting_bg {
	bottom: 0px;
}

.profile_loginSetting_item:hover .profile_custom_del {
	display: block;
}
.lui_listview_gridtable_table {
	table-layout: fixed;
}