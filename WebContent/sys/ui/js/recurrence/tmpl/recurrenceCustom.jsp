<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="/WEB-INF/kmss-bean.tld" prefix="bean"%>
{$
<div class="rTypeContainer">
	<table width="100%" class="tb_simple">
		<tr>
			<td width="15%" class="td_normal_title">
				<bean:message bundle="sys-ui" key="ui.recurrence.rtype"/>
			</td>
			<td>
				<select class="rTypeSelect">
					<option value="DAILY" selected="selected"> 
						<bean:message bundle="sys-ui" key="ui.recurrence.daily"/>
					</option>
					<option value="WEEKLY">
						<bean:message bundle="sys-ui" key="ui.recurrence.weekly"/>
					</option>
					<option value="MONTHLY">
						<bean:message bundle="sys-ui" key="ui.recurrence.monthly"/>
					</option>
					<option value="YEARLY">
						<bean:message bundle="sys-ui" key="ui.recurrence.yearly"/>
					</option>
				</select>
			</td>
		</tr>
	</table>
	<div class="rTypeContent"></div>
</div>
$}