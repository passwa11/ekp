<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="/WEB-INF/kmss-bean.tld" prefix="bean"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
{$
	<tr>
		<td width="15%" class="td_normal_title">
	  		<bean:message bundle="sys-ui" key="ui.recurrence.time"/>
	  	</td> 
	  	<td>
	  		<input class="monthlyType" name="{% param.cid + '_monthlyType' %}" type="radio" value="month" checked="checked"/>
	  		<span><bean:message bundle="sys-ui" key="ui.recurrence.time.month.oneday"/></span>
	  		<input class="monthlyType" name="{% param.cid + '_monthlyType' %}" type="radio" value="week"/>
	  		<span><bean:message bundle="sys-ui" key="ui.recurrence.time.month.oneweek"/> </span>
	  	</td>
	</tr>
$}
