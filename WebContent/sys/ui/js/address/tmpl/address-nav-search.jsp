<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib uri="/WEB-INF/KmssConfig/sys/ui/lfn.tld" prefix="lfn"%>
{$
<div class="lui-address-nav-search" data-lui-mark="lui-nav-search">
    <div class="lui-address-nav-searchbar">
        <input class="lui-form-control" type="text" data-lui-mark="lui-nav-search-input"  placeholder="${lfn:message('sys-organization:sysOrg.address.search.inputkeyword') }" />
        <div class="lui-address-nav-searchcancel" data-lui-mark="lui-nav-search-cancel"></div>
    	<button type="button" data-lui-mark="lui-nav-search-btn"></button>
    </div>
    <div class="lui-address-nav-range-searchbar" data-lui-mark="lui-nav-range-search-btn">
    	<button type="button" class="lui-address-nav-range-search-word" data-lui-mark="lui-nav-range-search-word">${lfn:message('sys-organization:sysOrg.address.search.rangeSearch')}</button>
    	<span class="lui-address-nav-range-search-img" data-lui-mark="lui-nav-range-search-img"></span>
    </div>
    <div class="lui-address-nav-range-search-div" data-lui-mark="lui-nav-range-search-div">
   		<div class="lui-address-nav-range-search-row">
 			<span class="lui-address-nav-range-search-row-name">${lfn:message('sys-organization:sysOrgRoleConf.fdName')}</span>
 			<span class="lui-address-nav-range-search-row-input"><input type="text" data-lui-mark="lui-nav-range-search-fdName" /></span>
   		</div>
		<div class="lui-address-nav-range-search-row">
			<span class="lui-address-nav-range-search-row-name">${lfn:message('sys-organization:sysOrg.address.search.range')}</span>
			<span class="lui-address-nav-range-search-row-input"><input type="text" data-lui-mark="lui-nav-range-search-fdDeptName" /></span>
		</div>
		<div class="lui-address-nav-range-search-row">
			<span class="lui-address-nav-range-search-row-checkbox"><input type="checkbox" class="chkbox" data-lui-mark="lui-nav-range-search-fdHasChild" /></span>
			<span class="lui-address-nav-range-search-row-name">${lfn:message('sys-organization:sysOrg.address.search.fdHasChild')}</span>
		</div>
   		<div class="lui-address-nav-range-search-row lui-ding-address-nav-range-search-row" style="width: 100%;text-align: center">
   			<span class="lui-address-nav-range-search-query"><button class="lui-component lui_widget_btn lui_toolbar_btn_def" data-lui-mark="lui-nav-range-search-query">${lfn:message('button.list')}</button></span>
   			<span class="lui-address-nav-range-search-cancel"><button class="lui-component lui_widget_btn lui_toolbar_btn_gray" data-lui-mark="lui-nav-range-search-cancel">${lfn:message('button.cancel')}</button></span>
   		</div>
    </div>
</div>
$}