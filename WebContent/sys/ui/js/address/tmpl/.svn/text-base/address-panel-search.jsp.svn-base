<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib uri="/WEB-INF/KmssConfig/sys/ui/lfn.tld" prefix="lfn"%>
<%@ taglib uri="/WEB-INF/kmss-bean.tld" prefix="bean"%>
{$
<div class="lui-address-search-panel">
	<div class="lui-address-search-group" data-lui-mark="lui-address-panel-nav">
		<div class="lui-address-searchbar" data-lui-mark="lui-address-searchbar">
			<input class="lui-address-searchbar-input" data-lui-mark="lui-address-searchbar-input" type="text" placeholder="${lfn:message('sys-organization:sysOrg.address.search.inputkeyword') }">
		</div>
		<ul class="lui-address-searchbar-ul">
			<li class="group1 firstchild lui-address-searchbar-li selectall" >
				<span class="checkbox"  data-lui-mark="lui-address-search-selectall"></span>
				<span class="txt">
					<bean:message bundle="sys-ui" key="ui.listview.selectall"/>
				</span>
			</li>
			<li class="group1 lui-address-searchbar-li" >
				<span class="checkbox" data-lui-mark="lui-address-search-type" data-lui-value="1"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.org') }
				</span>
			</li>
			<li class="group1 lui-address-searchbar-li" >
				<span class="checkbox" data-lui-mark="lui-address-search-type" data-lui-value="2"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.dept') }
				</span>
			</li>
			<li class="group1 lui-address-searchbar-li">
				<span class="checkbox"  data-lui-mark="lui-address-search-type" data-lui-value="4"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.post') }
				</span>
			</li>
			<li class="group1 lui-address-searchbar-li">
				<span class="checkbox"  data-lui-mark="lui-address-search-type" data-lui-value="8"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.person') }
				</span>
			</li>
			<li class="group2 lui-address-searchbar-li" >
				<span class="checkbox"  data-lui-mark="lui-address-search-type" data-lui-value="16"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.group') }
				</span>
			</li>
			<li class="group3 lui-address-searchbar-li" >
				<span class="checkbox"  data-lui-mark="lui-address-search-type" data-lui-value="32"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.filterOption.role') }
				</span>
			</li>
			<li class=" lui-address-searchbar-li last" >
				<span class="checkbox"  data-lui-mark="lui-address-search-accurate"></span>
				<span class="txt">
					${lfn:message('sys-organization:sysOrg.address.search.accurate') }
				</span>
			</li>
		</ul>
	</div>
	
	<div class="lui-address-search-list" data-lui-mark="lui-address-search-list">
		
	
	</div>
	
	
</div>


$}