<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>甘特图</title>
  <link rel="stylesheet" href="../css/gantt/index.css">
</head>

<body>
  <div class="model-gantt">
    <div class="model-gantt-wrap">

      <!-- 抽屉层 Starts -->
      <div class="model-gantt-drawer">
        <div class="model-gantt-drawer-wrap">
        </div>
        <ul class="model-gantt-drawer-setting">
          <li>
            <div>项目</div>
          </li>
          <li>
            <div>开始时间</div>
          </li>
          <li>
            <div>结束时间</div>
          </li>
          <li>
            <div>进度</div>
          </li>
        </ul>
        <div class="model-gantt-drawer-add">添加里程碑</div>
        <div class="model-gantt-drawer-opt"></div>
      </div>
      <!-- 抽屉层 Ends -->
      <!-- 内容层 Starts -->
      <div class="model-gantt-table">
        <div class="model-gantt-table-wrap">
          <div class="model-gantt-table-content">
          </div>
        </div>
      </div>
      <!-- 内容层 Ends -->
    </div>
  </div>
  <script src="../js/jquery-1.11.3.min.js"></script>
  <script src="../data/gantt.js"></script>
  <script src="../js/gantt.js"></script>
  <script>
  </script>
</body>

</html>