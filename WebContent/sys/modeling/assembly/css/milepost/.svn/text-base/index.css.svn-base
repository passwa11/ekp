.model-gantt-milepost {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}

.model-gantt-milepost .milepost-dialog {
  width: 540px;
  background: #FFFFFF;
  border-radius: 4px;
  position: fixed;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}

.model-gantt-milepost .milepost-dialog-head {
  position: relative;
  height: 50px;
  border-bottom: 1px solid #EEEEEE;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 20px;
}

.model-gantt-milepost .milepost-dialog-head p {
  font-size: 16px;
  color: #333333;
  line-height: 50px;
}

.model-gantt-milepost .milepost-dialog-head i {
  position: absolute;
  right: 20px;
  top: 18px;
  width: 14px;
  height: 14px;
  background: url(../../img/milepost/close@2x.png);
  background-size: 14px 14px;
  cursor: pointer;
}

.model-gantt-milepost .milepost-dialog-container {
  margin-top: -50px;
  padding: 50px 0 70px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-gantt-milepost .milepost-dialog-container-box {
  padding: 5px 20px;
}

.model-gantt-milepost .milepost-dialog-container-item {
  padding: 15px 0;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-title {
  width: 61px;
  font-size: 14px;
  color: #333333;
  line-height: 30px;
  text-align: right;
  display: inline-block;
  vertical-align: top;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content {
  display: inline-block;
  vertical-align: top;
  margin-left: 16px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.name,
.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position {
  width: 394px;
  height: 30px;
  background: #FFFFFF;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.name:after,
.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position:after {
  content: '*';
  color: #FF4444;
  position: absolute;
  top: 5px;
  right: -14px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.name #titleVal {
  width: 100%;
  padding: 0 8px;
  line-height: 28px;
  font-size: 12px;
  color: #333;
  border: 0px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position {
  position: relative;
  margin-bottom: 20px;
  font-size: 0;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select {
  display: inline-block;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select .select-box {
  position: relative;
  width: 80px;
  background: #FAFAFA;
  border-right: 1px solid #D9D9D9;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select #select-txt {
  padding-left: 8px;
  padding-right: 24px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  width: 100%;
  line-height: 28px;
  cursor: pointer;
  overflow: hidden;
  border: 0;
  font-size: 12px;
  color: #333;
  background: transparent;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select .select-arrow {
  position: absolute;
  right: 8px;
  top: 13px;
  width: 8px;
  height: 3px;
  background: url(../../img/colorSet/select@2x.png);
  background-size: 8px 3px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select .option-list {
  background: #FFFFFF;
  border-width: 0;
  border-style: solid;
  border-color: #ddd;
  -webkit-box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.15);
          box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.15);
  border-radius: 0 0 2px 2px;
  width: 100%;
  position: absolute;
  top: 28px;
  left: -1px;
  z-index: 2;
  overflow: hidden;
  height: 0px;
  -webkit-transition: all 0.3s ease;
  transition: all 0.3s ease;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select .option-list .option-list-item {
  width: 100%;
  height: 30px;
  font-size: 12px;
  color: #666666;
  line-height: 30px;
  text-align: left;
  padding: 0 7px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  cursor: default;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .type-select .option-list .option-list-item:hover {
  background: #ECF2FD;
  color: #4285f4;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .position-vlaue {
  position: relative;
  display: inline-block;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .position-vlaue #positionVal {
  border: 0;
  width: 310px;
  font-size: 12px;
  color: #333333;
  line-height: 28px;
  padding-left: 8px;
  padding-right: 30px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .position-vlaue .positionTab {
  position: absolute;
  width: 14px;
  height: 14px;
  right: 8px;
  top: 8px;
  font-size: 12px;
  color: #999999;
  line-height: 14px;
  background-size: 14px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .project-time {
  position: absolute;
  bottom: -20px;
  left: 0;
  font-size: 12px;
  color: #999999;
  line-height: 12px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.position .project-time i {
  display: inline-block;
  width: 14px;
  height: 14px;
  margin-bottom: -3px;
  margin-right: 6px;
  background: url(../../img/milepost/tips@2x.png);
  background-size: 14px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon {
  font-size: 0;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon > div {
  display: inline-block;
  vertical-align: middle;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .showIcon {
  width: 52px;
  height: 52px;
  background: #FFFFFF;
  border: 1px solid #DFE3E9;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 8px;
  margin-right: 28px;
  position: relative;
  display: inline-block;
  text-align: center;
  line-height: 52px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .showIcon i {
  font-size: 30px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .showIcon .flag {
  display: inline-block;
  width: 100%;
  height: 100%;
  background: url(../../img/milepost/milepost.svg) center no-repeat;
  background-size: 30px,30px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .showIcon:after {
  content: '*';
  color: #FF4444;
  position: absolute;
  /*top: 18px;*/
  right: -14px;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .changeIcon, .model-gantt-milepost .milepost-dialog-container-item .container-item-content.icon .changeColor {
  width: 90px;
  height: 30px;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  font-size: 14px;
  line-height: 30px;
  color: #333333;
  text-align: center;
  margin-right: 8px;
  cursor: pointer;
}

.model-gantt-milepost .milepost-dialog-container-item .container-item-content.desc textarea {
  width: 400px;
  min-height: 60px;
  max-height: 160px;
  resize: vertical;
  font-size: 12px;
  color: #333;
  line-height: 16px;
  padding: 8px;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-gantt-milepost .milepost-dialog-footer {
  position: absolute;
  bottom: 0;
  font-size: 0;
  width: 100%;
  height: 70px;
  padding-top: 20px;
  padding-right: 5px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-top: 1px solid #eee;
}

.model-gantt-milepost .milepost-dialog-footer > div {
  float: right;
  margin-right: 15px;
  width: 90px;
  height: 30px;
  font-size: 14px;
  line-height: 30px;
  text-align: center;
  border-radius: 2px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  cursor: pointer;
}

.model-gantt-milepost .milepost-dialog-footer .sure {
  background-color: #4285f4;
  color: #fff;
}

.model-gantt-milepost .milepost-dialog-footer .cancel {
  border: 1px solid #ddd;
  color: #333;
}

.model-gantt-milepost input::-webkit-input-placeholder,
.model-gantt-milepost textarea::-webkit-input-placeholder {
  /* WebKit, Blink, Edge */
  color: #999 !important;
  font-size: 12px;
}

.model-gantt-milepost input:-moz-placeholder,
.model-gantt-milepost textarea:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: #999 !important;
  font-size: 12px;
}

.model-gantt-milepost input::-moz-placeholder,
.model-gantt-milepost textarea::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: #999 !important;
  font-size: 12px;
}

.model-gantt-milepost input:-ms-input-placeholder,
.model-gantt-milepost textarea:-ms-input-placeholder {
  /* Internet Explorer 10-11 */
  color: #999 !important;
  font-size: 12px;
}

