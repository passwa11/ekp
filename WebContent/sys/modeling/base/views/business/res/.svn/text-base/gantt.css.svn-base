.model-source {
    width: 100%;
    height: 550px;
    background: #FFFFFF;
    margin: auto;
    padding: 16px 0 0 20px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

table {
    border-spacing: 0;
    border-collapse: separate;
}

table td {
    /*padding: 0 5px;*/
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.noEllipsis{
    text-overflow: clip;
}
ul, li {
    padding: 0;
    margin: 0;
    list-style: none;
}

p {
    margin: 0;
}

.model-source-wrap {
    border-top: 1px ;
    border-left: 1px;
    border-bottom: 1px;
    border-right: 1px;
    border-color: #DDDDDD;
    border-style:dashed solid solid solid;
    height: 450px;
    overflow: hidden;
}

.model-source-wrap .model-source-title {
    font-size: 14px;
    color: #4285F4;
    line-height: 20px;
    vertical-align: middle;
    margin-bottom: 16px;
}

.model-source-wrap .model-source-title:after {
    content: '';
    display: inline-block;
    width: 18px;
    height: 18px;
    vertical-align: middle;
    margin-left: 6px;
    background: url(img/sourcePanel/edit_hover@2x.png) no-repeat;
    background: url(img/sourcePanel/edit_hover.png) no-repeat \9;
    background-size: 18px;
}

.model-source-wrap .model-source-msg {
    height: 32px;
    width: 100%;
    margin-bottom: 10px;
    position: relative;
}

.model-source-wrap .model-source-msg .model-source-data,
.model-source-wrap .model-source-msg .model-source-legend {
    padding: 3px 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.model-source-wrap .model-source-msg .model-source-data p,
.model-source-wrap .model-source-msg .model-source-legend li {
    display: inline-block;
    height: 26px;
    border-radius: 2px;
    margin-right: 10px !important;
    padding: 4px 10px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 12px;
    color: #999999;
    letter-spacing: 0;
    line-height: 18px;
    background: #f7f7f7;
    cursor: pointer;
}

.model-source-wrap .model-source-msg .model-source-legend li {
    position: relative;
}

.model-source-wrap .model-source-msg .model-source-data {
    float: left;
    position: absolute;
}

.model-source-wrap .model-source-msg .model-source-data p:first-child {
    background: #F0F0F0;
}

.model-source-wrap .model-source-msg .model-source-headline {
    position: absolute;
    left: 45%;
    height: 32px;
    line-height: 32px;
    width: 82px;
    border-radius: 2px;
    background-color: #F0F0F0;
    font-size: 12px;
    color: #666666;
    letter-spacing: 0;
    text-align: center;
    cursor: pointer;
    z-index: 2;
}

.model-source-wrap .model-source-msg .model-source-legend ul {
    position: absolute;
    right: 20px;
    vertical-align: middle;
}

.model-source-wrap .model-source-left {
    float: left;
    width: 20%;
    margin-left: 16px;
}

.model-source-wrap .model-source-right {
    height: 100%;
    width: 75%;
    float: left;
}

.model-source-wrap .model-source-time-dimension {
    width: 100%;
    height: 116px;
    /*padding: 0 20px 0 2px;*/
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;

    /* position: relative;
    z-index: 2;
    cursor: pointer; */
}

.model-source-wrap .model-source-time-dimension .model-source-time-dimension-time{
    width: 100%;
    text-align: center;
    border-right: 1px solid #DDDDDD;
    border-top: 1px solid #DDDDDD;
    border-bottom: 1px solid #DDDDDD;
    border-spacing: 0;
}

.model-source-wrap .model-source-time-dimension .model-source-time-dimension-dimension{
    float: left;
    width: 100%;
}

.model-source-wrap .model-source-time-dimension .model-source-time-dimension-dimension .model-source-time-dimension-dimension-cell{
    float: left;
    width: 5%;
    text-align: center;
    /*border-bottom: 1px solid #f0f0f0;*/
    border-right: 1px solid #f0f0f0;
}

.model-source-wrap .model-source-time-dimension .model-source-time-dimension-dimension .model-source-time-dimension-dimension-cell p{
    padding: 20px 0;
}


.model-source-wrap .model-source-table-main {

}

.model-source-wrap .model-source-table-main .model-source-table-box {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.model-source-wrap .model-source-table-main .table-fd-display {
    width: 100%;
    table-layout: fixed;
    cursor: pointer;
    position: relative;
    border-left: 1px solid #DDDDDD;
    border-top: 1px solid #DDDDDD;
}

.model-source-wrap .model-source-table-main .table-fd-display td {
    /*border-radius: 2px;*/
    height: 58px;
    /* border: 5px solid #ffffff;*/
    border-right: 1px solid #DDDDDD ;
    border-bottom: 1px solid #DDDDDD ;
    /* padding: 0;*/
}

.model-source-wrap .model-source-table-main .table-fd-display .top-row {
    height: 116px;
    text-align: center;
}

.model-source-wrap .model-source-table-main .table-fd-display .base-row {
    /* background: #F0F0F0;*/
}


.model-source-wrap .model-source-table-main .table-fd-display .base-row .row-div{
    background: #F0F0F0;
    height: 60%;
    margin: 5px 10px 5px 10px;
}

.model-source-wrap .gantt-view-grid {
    width: 100%;
    float: left;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
}

.model-source-wrap .gantt-view-grid .gantt-view-grid-row{
    width: 100%;
    float: left;
}

.model-source-wrap .gantt-view-grid .gantt-view-grid-row .gantt-view-grid-row-cell{
    float: left;
    width: 5%;
    height: 58px;
    /*    border-bottom: 1px solid #f0f0f0;*/
    border-right: 1px dashed #f0f0f0;
}

.model-source-wrap .gantt-view-grid-blocks {
    /* margin-top: -25px; */
    /*padding-top: 30px;*/
}

.model-source-wrap .gantt-view-grid-blocks.active {
    border: 1px dashed #4285f4;
    position: relative;
}

.model-source-wrap .gantt-view-grid-blocks.active:before {
    content: '';
    width: 100%;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: url(../../../resources/images/edit-mask.png) repeat center;
}

.model-source-wrap .gantt-view-grid-blocks .gantt-view-block-container {
    /* height: 28px;
    padding-top: 40px; */
    position: relative;
}

.model-source-wrap .gantt-view-grid-blocks .gantt-view-block-container .gantt-view-block {
    position: absolute;
    top: 50%;
    transform: translate(0, -50%);
    height: 30px;
    background-color: #f0f0f0;
    /* border: 1px solid #c0c0c0; */
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
}

.model-source-wrap .gantt-view-grid-blocks .gantt-view-block-container .gantt-view-block-text {
    position: absolute;
    height: 12px;
    font-size: 0.8em;
    color: #999;
    line-height: 30px;
    padding-left: 10px;
    white-space: nowrap;
    width: 100%;
    box-sizing: border-box;
}



.model-source-wrap .model-source-msg .model-source-data.active:before,
.model-source-wrap .model-source-msg .model-source-headline.active:before,
.model-source-wrap .model-source-msg .model-source-legend.active li:before,
.model-source-wrap .model-source-left .table-ord.active:before,
.model-source-wrap .model-source-table-title.active:before,
.model-source-wrap .model-source-time-dimension.active:before,
.model-source-wrap .model-source-time-dimension-time.active:before,
.model-source-wrap .model-source-time-dimension-dimension.active:before,
.model-source-wrap .gantt-view-grid.active:before,
.model-edit-left-wrap .model-edit-view .model-edit-view-title .table-name.active:before,
.model-source-wrap .model-source-table-main.active table:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    width: 100%;
    height: 100%;
    background: url(../../../mobile/design/images/edit-mask.png) repeat center;
}

