.model-view-panel-table-td .select-box {
    display: inline-block;
    vertical-align: middle;
    position: relative;
    width: 330px;
    font-size: 12px;
    line-height: 30px;
}

.model-view-panel-table-td #select-txt {
    padding-left: 8px;
    padding-right: 24px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
    line-height: 30px;
    height: 30px;
    cursor: pointer;
    overflow: hidden;
    border: 1px solid #DDDDDD;
    border-radius: 2px;
    font-size: 12px;
    color: #333;
}
.model-view-panel-table-td .select-arrow {
    position: absolute;
    right: 8px;
    top: 13px;
    width: 8px;
    height: 3px;
    background: url(../../../../assembly/img/colorSet/select@2x.png);
    background-size: 8px 3px;
}
.model-view-panel-table-td .option-list {
    background: #FFFFFF;
    border: 1px solid #DDDDDD;
    -webkit-box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.15);
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.15);
    border-radius: 0 0 2px 2px;
    width: 100%;
    position: absolute;
    top: 30px;
    left: -1px;
    z-index: 2;
    overflow: hidden;
    display: none;
}
.model-view-panel-table-td .option-list .option-list-item {
    display: block;
    width: 100%;
    height: 30px;
    line-height: 30px;
    text-align: left;
    padding: 0 7px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 0;
}
.color-list {
    display: inline-block;
    vertical-align: middle;
    margin-right: 14px;
    font-size: 0;
}

.color-list li {
    width: 24px;
    height: 16px;
    display: inline-block;
    vertical-align: middle;
}

.model-view-panel-table-td .option-list .option-list-item .color-schemes-name {
    display: inline-block;
    vertical-align: middle;
    font-size: 12px;
    color: #333;
}
