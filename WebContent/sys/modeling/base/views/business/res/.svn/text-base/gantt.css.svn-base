@keyframes fade {
    from {
        background-color:rgba(70,135,244,0.4) ;
    }
    50% {
        background-color:rgba(70,135,244,1) ;
    }
    to {
        background-color:rgba(70,135,244,0.4) ;
    }
}

@-webkit-keyframes fade {
    from {
        background-color:rgba(70,135,244,0.4) ;
    }
    50% {
        background-color:rgba(70,135,244,1) ;
    }
    to {
        background-color:rgba(70,135,244,0.4) ;
    }
}
