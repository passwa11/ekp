.calendar-title{
    font-size: 18px;
    color: #333333;
    font-weight: bold;
    width: 200px;
    margin: 0 auto;
    text-align: center;
    position: relative;
    top: 10px;
    left: 0;
}
.lui_calendar_header_tab div.lui_calendar_header_right{
    display: inline-block;
    width: 200px;
}
.lui_calendar_header_tab div.lui_calendar_header_left{
    width: 100%;
    float: left;
    text-align: center;
    height: 35px;
    line-height: 35px;
    border-top: 1px solid #EEEEEE !important;
}
.lui_calendar_header_tab .lui_calendar_header_left > div, .lui_calendar_header_tab .lui_calendar_header_right > div{
    color: #333333!important;
    background-color: #FFFFFF!important;
    border: 1px solid #DDDDDD;
    border-radius: 4px;
}

.lui_calendar_header_tab .lui_calendar_header_left > div:hover, .lui_calendar_header_tab .lui_calendar_header_right > div:hover, div.lui_calendar_header_tab>div>div.lui_calendar_header_right>div.lui_calendar_header_button_on{
    color: #fff!important;
    background-color: #4285F4!important;
}
div.lui_calendar_header_tab .lui_calendar_header_left>div,div.lui_calendar_header_tab .lui_calendar_header_left{
    border:0;
}
div.lui_calendar_header_tab .lui_calendar_header_left span.lui_calendar_header_prev,div.lui_calendar_header_tab .lui_calendar_header_left span.lui_calendar_header_next{
    background-color: #fff;
    transform: scale(0.5);
}
.lui_calendar_header_tab .lui_calendar_header_right > div.lui_calendar_header_month,.lui_calendar_header_tab .lui_calendar_header_right > div.lui_calendar_header_today{
    box-sizing: border-box;
    width: 44px;
    font-size: 12px;
    text-align: center;
}
.lui_calendar_header_tab .lui_calendar_header_right > div.lui_calendar_header_refresh{
    box-sizing: border-box;
    font-size: 12px;
    text-align: center;
    height: 30px;
    line-height: 30px;
}
.lui_calendar_header_tab .lui_calendar_header_center div.lui_calendar_header_title{
    font-size: 14px;
    color: #333333;
}
div.lui_calendar_header_tab>div>div.lui_calendar_header_left>div[cal-opt]:hover,
div.lui_calendar_header_tab>div>div.lui_calendar_header_left>div[cal-opt]:hover>span
{
    background-color: #FFF!important;
}
div.lui_calendar_header_tab>div>div.lui_calendar_header_left>div.lui_calendar_header_center:hover{
    background-color: #FFF!important;
}
div.lui_calendar_frame .lui_calendar_header{
    background-color: #FFF;
}
