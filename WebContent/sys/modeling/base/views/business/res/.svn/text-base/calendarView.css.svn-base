#calendar{
    margin-top: -30px;
}

ul, li {
    padding: 0;
    margin: 0;
    list-style: none;
}

.lui_calendar_header_right_month li{
    float: left;
    width: 42px;
    height: 28px;
    text-align: center;
    vertical-align: middle;
    font-size: 12px;
    color: #333333;
    line-height: 28px;
    font-weight: 400;
}
.lui_calendar_header_right_month li.lui_calendar_header_button_on{
    background: #4285F4;
    color: #FFFFFF;
}
.lui_calendar_header_right_month li:first-child{
    border-radius: 4px 0px 0px 4px;
    border: 1px solid #DDDDDD;
    cursor: pointer;
}
.lui_calendar_header_right_month li:last-child{
    border-radius: 0px 4px 4px 0px;
    border: 1px solid #DDDDDD;
    cursor: pointer;
}

.lui_calendar_header_today{
    margin-left: 15px;
}

.fc-view-basicWeek .model-fc-header.model-fc-today{
    width: 60px;
    height: 36px;
    background: #4285F4;
    border-radius: 185px;
    display: inline-block;
    line-height: 36px;
    margin-top: 12px;
}
.fc-view-basicWeek .model-fc-header.model-fc-today .model-fc-week{
    height: 18px;
    font-size: 14px;
    color: #FFFFFF;
    letter-spacing: 0;
    text-align: center;
    line-height: 18px;
    font-weight: 700;
}

.fc-view-basicWeek .model-fc-header.model-fc-today .model-fc-day{
    height: 18px;
    font-size: 12px;
    color: #FFFFFF;
    letter-spacing: 0;
    text-align: center;
    line-height: 18px;
    font-weight: 700;
}
.fc-view-basicWeek .model-fc-header .model-fc-week{
    height: 42px;
    color: #333333;
    letter-spacing: 0;
    text-align: center;
    line-height: 42px;
    font-weight: 700;
}

.fc-view-basicWeek .model-fc-header .model-fc-day{
    color: #999999;
    letter-spacing: 0;
    text-align: center;
    line-height: 0px;
    font-weight: 700;
    height: 14px;
    font-size: 12px;
}
.lui_calendar_header_tab .lui_calendar_header_left .lui_calendar_header_prev:hover, .lui_calendar_header_tab .lui_calendar_header_left .lui_calendar_header_next:hover, .lui_calendar_header_tab .lui_calendar_header_right .lui_calendar_header_refresh:hover, .lui_calendar_header_tab .lui_calendar_header_right .lui_calendar_header_list:hover, .lui_calendar_header_tab .lui_calendar_header_right .lui_calendar_header_date:hover {
    background-position: 0 0 !important;
}
