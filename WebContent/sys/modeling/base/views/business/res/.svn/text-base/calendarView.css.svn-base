.view_sched>tbody>tr>td{
    font-size: 14px;
    text-align: left;
    color: #666666;
    vertical-align: top;
    min-height: 30px;
    line-height: 30px;

}
.view_sched>tbody>tr>td.title{
    color: #333333;
    text-align: right;
    width: 78px;
    padding: 0 10px;
}
.view_sched div.calendar_dialog{
    white-space: pre-wrap;
    max-height: 85px;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    min-width:100px;
    height: auto;
    line-height: 20px;
    margin-top: 5px;
    overflow: hidden;
    text-overflow: ellipsis; /*超出则...代替*/
    -webkit-line-clamp: 4; /* 行数*/
    line-clamp: 4; /*行数*/
    max-width: 240px;
    word-break: break-all;
}
.lui_calendar_top div.lui_calendar_close{
    position: absolute;
    top: 17px;
    right: 19px;
    width: 12px;
    height: 30px;
}
.lui_calendar_top div.lui_calendar_close>img{
    width: 32px;
    height: 16px;
    margin: 0;
    position: absolute;
    right: -16px;
    top: 50%;
    margin-top: -8px;
    cursor: pointer;
    transition: background-color .3s;
}
#tr_relation_url{
    text-align: right;
    display: inline-block;
    cursor: pointer;
    font-size: 12px;
    color: #4285F4;
    float: right;
}
.fc-event-inner>p{
    position: relative;
}
.fc-event-inner>p>i{
    display: inline-block;
    position: absolute;
    height: 20px;
    width: 4px;
    left: 0;
    top: 0;
}
span.textEllipsis{
    padding-left: 10px;
}
.model-preview-icon{
    border-radius: 12px;
    width: 12px;
    height: 12px;
    background: #4285F4;
    line-height: 8px;
    margin: 15px 7px 10px 15px;
    float: left;
}
.right-arrow{
    margin-bottom: -3px;
    display: inline-block;
    width: 14px;
    height: 14px;
    background: url(../../../resources/images/arrow-down@2x.png) no-repeat center;
    background: url(../../../resources/images/arrow-down.png) no-repeat center \9;
    background-size: contain;
    transform:rotate(90deg);
    -ms-transform:rotate(90deg); 	/* IE 9 */
    -moz-transform:rotate(90deg); 	/* Firefox */
    -webkit-transform:rotate(90deg); /* Safari 和 Chrome */
    -o-transform:rotate(90deg); 	/* Opera */
    vertical-align: -2;
}
