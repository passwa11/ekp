.lui_toolbar_frame .lui_toolbar_btn_on div.lui_toolbar_btn_l, .lui_toolbar_frame .lui_widget_btn_swh div.lui_toolbar_btn_l {
    color: #fff;
    border-color: #4285F4;
    background-color: #4285F4;
}
.lui_calendar_frame div.lui_calendar_header{
    padding: 0;
}
.lui_calendar_header_tab div.lui_calendar_header_right{
    text-align: left;
    margin-bottom: 15px;
}
/* 弹窗 */
div.lui_calendar{
    border: 1px solid #DFE3E9;
    background-color: #fff;
    box-shadow: 0px 0px 3px 0px rgba(0,0,0,0.1);
    z-index: 10;
    padding:12px;
    width: 350px;
    overflow: hidden;
}
div.lui_calendar_top {
    border:0;
}
.lui_calendar_top div.lui_calendar_title{
    font-size: 16px;
    color: #FF9642;
    font-weight: bold;
    position: relative;
    padding:10px 0 12px 24px;
    width: 301px;
}
