ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.modeling_app_nav_preview {
  display: inline-block;
  vertical-align: top;
  width: 180px;
  text-align: center;
}

.nav_left {
  padding-top: 72px;
  margin-right: 300px;
  overflow: auto;
}
.nav_left_wrap{
  min-width: 860px;
  top: 72px;
}
.modeling_app_nav_right {
  width: 300px;
  position: fixed;
  right: 0;
  top: 42px;
  bottom: 0;
  overflow-y: auto;
  overflow-x: hidden;
  background: #fff;
}

.modeling_app_nav_right_head {
  background-color: #F5F5F5;
  text-align:left;
}

.modeling_app_right_title,
.modeling_app_nav_title {
  font-size: 14px;
  color: #354052;
  height: 50px;
  line-height: 50px;
  padding: 0 20px;
  display: inline-block;
}

.modeling_app_nav_preview_head {
  background-color: #F5F5F5;
  text-align: left;
  border-top: 1px solid #ccc;
}

.nav_item_active {
  background: #181E31;
}

.nav-left_head {
  display: inline-block;
  width: 100%;
  color: #FFFFFF;
  text-align: center;
  height: 50px;
  line-height: 50px;
  background: #101627;
}
.model-mask-panel-table{
  padding: 12px 15px;
}
.modeling_app_nav_preview .modeling_app_nav_preview_content {
  margin: 20px 15px;
  background: #21283B;
  text-align: left;
  display: inline-block;
  min-width:150px;
  min-height: 200px;
}

.model-item-expansion .model-sidebar-desc:after {
  content: '';
  display: inline-block;
  border-width: 5px;
  margin-left: 5px;
  border-style: solid;
  border-color: #fff transparent transparent transparent;
  vertical-align: -3px;
}

.item-expansion .model-sidebar-desc:after {
  content: '';
  border-color: transparent transparent #fff transparent;
  vertical-align: 3px;
}

ul.item-expansion-child {
  display: block;
  padding-left: 15px;
  background-color: #1C2235;
}

.model-sidebar-wrap-child {
  display: none;
  font-size: 14px;
}

/* Ã¤Â¾Â§Ã¦Â Â */
.model-sidebar {
  width: 100%;
  /* overflow-y: scroll; */
  transition: width .3s;
}

.model-sidebar.shrink {
  width: 50px;
}

.model-sidebar-icon {
  position: absolute;
  top: 15px;
  left: 20px;
  width: 20px;
  height: 20px;
}

.shrink .model-sidebar-icon {
  left: 15px;
}

.model-sidebar-item {
  height: 50px;
  line-height: 50px;
  font-size: 14px;
  color: #fff;
  padding-left: 20px;
  white-space: nowrap;
  position: relative;
  transition: all .3s;
  cursor: pointer;
}



.model-sidebar-desc {
  transition: all .3s;
  color: #FFFFFF;
}

.model-sidebar-item-box{
  display: block;
  max-width: 180px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  transition-duration: .3s;
}

.aside_selected .model-sidebar-desc,
.model-sidebar-item:hover .model-sidebar-desc {
  color: #4285f4;
}

.model-sidebar-item:hover .model-sidebar-desc:after {
  border-color: #4285F4 transparent transparent transparent;
}

.item-expansion.model-sidebar-item:hover .model-sidebar-desc:after {
  border-color: transparent transparent #4285F4 transparent;
}

.model-sidebar-flag {
  display: none;
  position: absolute;
  left: 60px;
  top: 7px;
  white-space: nowrap;
  padding: 5px 6px;
  background: #071E44;
  border-radius: 4px;
  line-height: 1.5em;
}

.model-sidebar-opt {
  width: 180px;
  height: 20px;
  padding: 20px 0;
  background: #28324C;
  position: fixed;
  bottom: 0;
  left: 0;
  transition: width 0.3s;
  z-index: 5
}

.model-sidebar-back {
  cursor: pointer;
  float: left;
  position: relative;
  margin-top: 2px;
  font-size: 14px;
  color: #FFFFFF;
  padding-left: 44px;
}

.model-sidebar-back:after {
  content: '';
  width: 1px;
  height: 30px;
  background: #333C53;
  position: absolute;
  right: -20px;
  top: -5px;
}

.model-sidebar-back i {
  width: 14px;
  height: 14px;
  background: url(../images/side/zk-back.svg) no-repeat center;
  position: absolute;
  left: 20px;
  top: 0;
}

.model-sidebar-btn {
  float: right;
  width: 20px;
  height: 20px;
  background: url(../images/side/side-shrink.svg) no-repeat center;
  transition: background 0.3s;
  margin-right: 20px;
}

.shrink .model-sidebar-item:hover .model-sidebar-flag {
  display: block;
}

.shrink .model-sidebar-opt {
  width: 50px;
  padding: 0;
  height: 120px;
}

.shrink .model-sidebar-opt .model-sidebar-back {
  width: 50px;
  padding-left: 0;
  font-size: 0;
  height: 60px;
}

.shrink .model-sidebar-opt .model-sidebar-back:after {
  width: 30px;
  height: 1px;
  bottom: 0;
  left: 10px;
  top: auto;
}

.shrink .model-sidebar-opt .model-sidebar-back i {
  width: 20px;
  height: 20px;
  left: 50%;
  margin-left: -10px;
  top: 50%;
  margin-top: -7px;
  background: url(../images/side/sq-back.svg) no-repeat center;
}

.shrink .model-sidebar-opt .model-sidebar-btn {
  margin-top: 20px;
  margin-right: 15px;
  background: url(../images/side/side-scale.svg) no-repeat center;
}

.shrink .model-sidebar-desc {
  display: none;
}

.shrink .model-sidebar-flag:before {
  content: "";
  width: 0;
  height: 0;
  border: 6px solid transparent;
  border-right-color: #071E44;
  position: absolute;
  left: -12px;
  top: 10px;
}

.app_index_sidebar.shrink .model-sidebar-opt {
  height: 60px;
}

