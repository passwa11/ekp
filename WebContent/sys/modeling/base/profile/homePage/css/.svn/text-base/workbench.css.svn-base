@charset "utf-8";
html {
    overflow-x: hidden;
}
body {
overflow-x: hidden;
}
body,
div,
ul,
li,
ol,
h1,
h2,
h3,
h4,
h5,
h6,
input,
textarea,
select,
p,
dl,
dt,
dd,
a,
img,
button,
form,
table,
th,
tr,
td,
tbody,
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
.clearfix:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.clearfix {
    zoom: 1;
}
a{
    text-decoration: none;
}
ul,
ol,
li {
  list-style: none;
}
/*声明字体*/
@font-face {
    font-family: DINAlternate-Bold;
    src: url('./font/DIN\ Alternate\ Bold.ttf');
    src:
      url('../font/DIN\ Alternate\ Bold.ttf') format('embedded-opentype'),
      url('../font/DIN\ Alternate\ Bold.ttf') format('woff'),
      url('../font/DIN\ Alternate\ Bold.ttf') format('truetype'),
      url('../font/DIN\ Alternate\ Bold.ttf') format('svg');
    font-weight: normal;
    font-style: normal;
  }
  * {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-tap-highlight-color: transparent;
  }
/* Workbench */
.workbenchContainer{
    border: 1px solid #DDDDDD;
    background-color: #fff;
    padding: 16px;
    overflow-x: hidden;
}
.workbenchBanner{
    width: 1200px;
    height:280px;
    background: linear-gradient(to right,#FFF,#F9FBFF);
    position: relative;
}
.workbenchBannerContent{
    width: 1200px;
    height:280px;
    background: url(../images/background.png) no-repeat center center;
    background-size: 100% 100%;
    position: absolute;
    right: 0;
    /* right: 50%;
    transform: translate(50%,0); */
}
.workbenchBannerList{
    position: absolute;
    top: 80px;
    left: 32px;
    z-index: 1;
    visibility: hidden;
}
.workbenchBannerList .workbenchBannerListDiv{
    position: absolute;
    height: 1px;
    top: 16px;
    background-color: #BFD8FF;
    left: 0;
    width: 100%;
    visibility: hidden;
}
.workbenchBannerList>li{
    position: relative;
    float: left;
}
.workbenchBannerList>li:nth-child(2){
    padding-left: 0px;
}
.workbenchBannerList>li{
    padding-left: 20px;
    text-align: center;
}
.workbenchBannerListOrder{
    font-family: DINAlternate-Bold;
    display: inline-block;
    width: 24px;
    height: 24px;
    line-height: 24px;
    background: url(../images/banner_icon.png) no-repeat center center;
    background-size: 100% 100%;
    text-align: center;
    color: #fff;
    position: relative;
    z-index: 99;
    border: 4px solid #FEFEFF;
    background-color: #FEFEFF;
    border-radius: 50%;
}
.workbenchBannerListTitle{
    font-size: 16px;
    color: #333333;
    font-weight: 500;
    padding-top: 10px;
}
.workbenchBannerListInfo{
    display: inline-block;
    font-size: 12px;
    color: #666666;
    padding-left: 36px;
    margin: 6px 0 20px 0;
}
.workbenchBannerContentTitle{
    transform: translate(8%,0);
    top: 35px;
    position: absolute;
    line-height: 28px;
    height: 28px;
    color: #2A304A;
    border-radius: 20px;
    z-index: 1;
    font-size: 20px;
    letter-spacing: 4px;
    font-weight: 600;
    padding-left: 16px;
}
.workbenchEntrance{
    margin: 20px 0;
}
.workbenchShortcut{
    width: 1200px;
    float: left;
    position: relative;
}
.workbenchCount{
    width: 50%;
    float: left;
    position: relative;
}
.workbenchEntranceTitle{
    font-size: 14px;
    color: #333333;
    font-weight: 600;
    padding-left: 12px;
    position: relative;
    margin-bottom: 10px;
}
.workbenchEntranceTitle::before{
    content: "";
    position: absolute;
    top:2px;
    left:0;
    width: 4px;
    height: 16px;
    background-color: #4285F4;
}
.workbenchShortcutList>li{
    float: left;
    width: 23%;
    border: 1px solid #EEEEEE;
    border-radius: 4px;
    margin-right: 16px;
}
.workbenchShortcutList>li>a{
    display: inline-block;
    height: 72px;
    width: 100%;
    border-radius: 4px;
    cursor: pointer;
}
.workbenchShortcutList>li>a>img{
    display: inline-block;
    width: 40px;
    height: 40px;
    margin: 16px;
    float: left;
}
.workbenchShortcutList>li>a:hover{
    background-color: #FFFFFF;
    box-shadow: 0px 3px 10px 0px rgba(141,182,230,0.2);
}
.workbenchShortcutList>li>a:hover>.workbenchShortcutListTitle{
    color: #4285F4;
}

.workbenchShortcutList>li>a>.workbenchShortcutListTitle{
    display: inline-block;
    text-align: center;
    margin-top: 16px;
    font-size: 16px;
    color: #2A304A;
    font-weight: 500;
}
.workbenchShortcutList .workbenchShortcutListInfo{
    color: #9497A4;
    font-weight: 400;
    font-size: 12px;
}
.workbenchCountList>li{
    float: left;
    width: 25%;
}
.workbenchCountList>li>a{
    display: inline-block;
    width: 160px;
    height: 94px;
    border-radius: 4px;
    padding-top: 16px;
    cursor: url('../images/cursor.svg'), default;
}
.workbenchCountList>li>a>p{
    font-family: DINAlternate-Bold;
    text-align: center;
    font-size: 32px;
    font-family: 700;
}
.workbenchAppStop>a>p{
    color: #666666;
}
.workbenchAppStop>a:hover>p{
}
.workbenchAppStop>a:hover>span{
    font-weight: 600;
}
.workbenchAppTotal>a:hover>span{
    color: #FF943E;
}
.workbenchAppTotal>a>p{
    color: #FF943E;
}
.workbenchAppReleased>a>p{
    color: #4285F4;
}
.workbenchAppReleased>a:hover>span{
    color: #4285F4;
}
.workbenchAppDraft>a>p{
    color: #2CCC74;
}
.workbenchAppDraft>a:hover>span{
    color: #2CCC74;
}

.workbenchCountList>li>a:hover{
    background-color: #FFFFFF;
    box-shadow: 0px 3px 10px 0px rgba(141,182,230,0.1);
}
.workbenchCountList>li>a>span{
    display: block;
    text-align: center;
    margin-top: 8px;
    font-size: 14px;
    color: #666666;
}
.workbenchBusinessRelation{
    position: relative;
}
.businessRelationContent{
    position: relative;
    background-color: #FCFDFF;
}
.businessRelationContentBg{
    min-width: 1200px;
    height: 560px;
    position: relative;
    margin: 0 auto;
    /*padding: 0 20px;*/
}
#app_relation_echart{
    width: 100%;
    height: 560px;
    margin: 0 auto;
    /*padding-top: 30px;*/
}
.workbenchSearch{
    display: inline-block;
    position: absolute;
}
.workbenchSearch>span{
    position: absolute;
    width: 16px;
    height: 16px;
    background: url(../images/search.png) no-repeat center center;
    background-size: 100% 100%;
    top: 8px;
    right: 8px;
    cursor: pointer;
    z-index: 2;
}
.workbenchSearch>input{
    height: 32px;
    background-color: #FFFFFF;
    border: 1px solid #DDDDDD;
    border-radius: 4px;
    padding: 0 24px 0 8px;
}
.workbenchSearch .appInput{
    width: 224px;
}
.workbenchSearch>input::placeholder{
    font-size: 12px;
    color: #999999;
}

.businessRelationContentLegend{
    position: absolute;
    top: 30px;
    right: 30px;
    z-index: 1;
}
.businessRelationContentLegendItem{
    margin-bottom: 10px;
    position: relative;
}
.businessRelationContentLegendItem>div{
    display: inline-block;
    width: 40px;
    text-align: center;
}
.businessRelationContentLegendItem>div>img{
    display: inline-block;
    width: 14px;
    height: 14px;
    position: relative;
    top:2px;
}
.businessRelationContentLegendItem>div.relationIcon>img{
    width: 30px;
    height: 12px;
}
.businessRelationContentLegendItem>span{
    display: inline-block;
    font-size: 14px;
    color: #9497A4;
}

.workbenchSearch .location_input_selection ul li{
    white-space: nowrap;
}

.modelTitle{
    width: 390px;
    position: absolute;
    top: 0px;
    left: 50%;
    transform: translateX(-50%);
    font-size: 16px;
    font-weight: bold;
    height: 32px;
    line-height: 42px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: center;
}
.searchContent{
    position: absolute;
    top: 10px;
    left: 20px;
    z-index: 1;
}
#searchTypeButton{
    display: inline-block;
    height: 34px;
    line-height: 34px;
    font-size: 14px;
    color: #999999;
    border: 1px solid #ddd;
    padding: 0 16px;
    border-radius: 4px;
    cursor: pointer;
    margin-right: 16px;
}
.workbenchBannerContentButton{
    top: 160px;
    left: 32px;
    position: absolute;
    font-size: 18px;
    color: #fff;
    z-index: 1;
    width: 160px;
    height: 42px;
    line-height: 42px;
    text-align: center;
    background: #4285F4;
    box-shadow: 0px 3px 9px 0px rgba(155,157,176,0.33);
    border-radius: 21px;
}
.workbenchBannerContentButton a,.workbenchBannerContentButton a:hover,.workbenchBannerContentButton a:visited,.workbenchBannerContentButton a:link{
    color:#FFFFFF !important;
    font-size: 16px !important;
}
.workbenchBanner .workbenchBannerContentButton a{
    display: block;
}