@charset "utf-8";
body,
div,
ul,
li,
ol,
h1,
h2,
h3,
h4,
h5,
h6,
input,
textarea,
select,
p,
dl,
dt,
dd,
a,
img,
button,
form,
table,
th,
tr,
td,
tbody,
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  margin: 0;
  padding: 0;
  font-family: "PingFang SC",
  "Lantinghei SC",
  "Helvetica Neue",
  Arial,
  "Microsoft YaHei",
  "WenQuanYi Micro Hei",
  "Heiti SC",
  "Segoe UI",
  sans-serif;
  box-sizing: border-box;
}
.clearfix:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.clearfix {
    zoom: 1;
}
.addViewContentItemIcon.normal{
    background: url(images/new_add_normal.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.card{
    background: url(images/new_add_card.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.board{
    background: url(images/new_add_board.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.calendar{
    background: url(images/new_add_calendar.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.resource{
    background: url(images/new_add_resource.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.gante{
    background: url(images/new_add_gante.png) no-repeat center;
    background-size: contain;
}
.addViewContentItemIcon.mind{
    background: url(images/new_add_mind.png) no-repeat center;
    background-size: contain;
}
.addViewContentList{
    list-style: none;
}
.addViewPopContent{
    display: inline-block;
    width: 706px;
    background-color: #fff;
    border-radius: 4px;
    padding: 8px;
}
.addViewContentItemIcon{
    float: left;
    width: 72px;
    height: 64px;
}
.addViewContentList{
    font-size: 0;
}
.addViewContentList>li{
    display: inline-block;
    width: 328px;
    border-radius: 2px;
    border: 1px solid #EEEEEE;
    padding: 16px;
    margin: 8px;
    cursor: pointer;
}
.addViewContentList>li:hover{
    background: #F5F9FE;
    border: 1px solid #4285F4;
}
.addViewContentList>li.selected{
    background: #F5F9FE;
    border: 1px solid #4285F4;
}
.addViewContentItemInfo{
    float: left;
    width: 210px;
    padding-left: 16px;
}
.addViewContentItemInfo>p{
    font-size: 14px;
    color: #333333;
    margin-bottom: 8px;
    font-weight: 600;
}
.addViewContentItemInfo>span{
    display: inline-block;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 12px;
    color: #666666;
}