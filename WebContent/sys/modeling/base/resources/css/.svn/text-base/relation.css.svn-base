
body {
  background: #FFF;
}
ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.model-mask-panel-output {
  padding: 30px 20px 0px 20px;
  height: 245px;
  overflow-y: auto;
}
.model-mask-panel-output-title {
  font-size: 14px;
  color: #333333;
  margin-bottom: 30px;
}
.model-mask-panel-output-title span {
  color: #4285f4;
}
.model-mask-panel-output-select {
  margin-bottom: 30px;
}
.model-mask-panel-output-select.disable i {
  background: url(../images/checked-gray.png) no-repeat center !important;
}
.model-mask-panel-output-select span {
  color: #999999;
}
.model-mask-panel-output-select-left {
  float: left;
  padding-top: 3px;
}
.model-mask-panel-output-select-left.active label i {
  background: url(../images/checked-blue.png) no-repeat center;
}
.model-mask-panel-output-select-left input {
  display: none;
}
.model-mask-panel-output-select-left input:checked + label i.model-mask-panel-output-i {
  background: url(../images/checked-blue.png) no-repeat center;
}
.model-mask-panel-output-select-left label i {
  display: block;
  width: 14px;
  height: 14px;
  background: url(../images/checkbox.png) no-repeat center;
  transition: all 0.3s;
}
.model-mask-panel-output-select-right {
  margin-left: 20px;
}
.model-mask-panel-output-select-right span {
  font-size: 14px;
  color: #333333;
}
.model-mask-panel-output-select-right label {
  display: block;
}
.model-mask-panel-output-select-right p {
  font-size: 12px;
  color: #999999;
}


/** 筛选项配置页面  */
.model-panel-drag-box {
	position: relative;
}
.model-panel-drag {
	
} 

.model-panel-drag-box .model-mask-panel-edit-title .strong {
	color: #003700;
}

.model-panel-drag-box .model-mask-panel-edit-drag {
	padding: 0;
    background: url(../images/panel/dragText@2x.png) no-repeat center;
    background: url(../images/panel/dragText.png) no-repeat center \9;
   background-size: 124px 30px;
}

.model-panel-drag .model-mask-panel-flag {
	border-bottom: 0;
	padding-bottom: 0;
}

.model-panel-drag-box .model-mask-panel-flag-content {
	height: 174px;
	overflow-y: auto;
	padding: 10px 0 25px 25px;
}

.model-panel-drag-box .model-mask-panel-flag-content:before {
	content: '';
	position: absolute;
	width: 0;
	height: 0;
	border-style: solid;
    border-color: transparent transparent  #F5F6FA;
    border-width: 8px 7px 8px 7px;
    margin-left: 15px;
    margin-top: -30px;
    z-index: 999;
}

.model-panel-drag-box #searchNumber {
	padding-top: 20px;
	margin: 0 20px;
	border-bottom: 1px solid #eee;
}

.model-panel-drag-box #searchNumber .number_title {
	font-size: 14px;
	color: #333;
}
.model-panel-drag-box #searchNumber .number_title .desc {
	color: #999;
	margin-left: 10px;
}

.model-panel-drag-box #searchNumber>div {
	padding-bottom: 15px;
	line-height: 15px;
}

.model-panel-drag-box #searchNumber input {
	margin-right: 6px;
	vertical-align: middle;
	
}

.model-panel-drag-box #searchNumber span {
	margin-right: 30px;
	vertical-align: middle;
	
}

.model-panel-drag-box .toolbar-bottom {
	height: 50px;
    border-top:0px;
}

.model-panel-drag-box .toolbar-bottom .lui_widget_btn {
	margin-top: 5px;
}

.model-panel-drag-box .toolbar-bottom .lui_toolbar_btn_def .lui_toolbar_btn_l,
.model-panel-drag-box .toolbar-bottom .lui_toolbar_btn_gray .lui_toolbar_btn_l {
	width: 90px;
	height: 30px;
	line-height: 30px;
	border-radius: 2px;
}

.model-panel-drag-box .toolbar-bottom .lui_toolbar_btn_def .lui_toolbar_btn_l {
	margin-right: 15px;
}

.model-panel-drag-box .lui_toolbar_btn_gray .lui_toolbar_btn_l {
	color: #333;
	margin-right: 20px;
}


