/* 列表视图预览样式 */
.model-body-content {
	padding: 0px;
	padding-left: 20px;
	background: #fff;
	position: fixed;
	overflow: hidden;
	top: 0px;
	bottom: 0;
	width: 100%;
	box-sizing: border-box;
}

.model-body-content-desc {
	margin: 0;
	color: #4285f4;
	font-size: 14px;
	cursor: pointer;
	transition: all 0.3s;
}

.model-body-content-desc:hover {
	color: #8EB6F8;
}

.model-body-content:before {
	content: '';
	width: 100%;
	height: 1px;
	background: #DDDDDD;
	position: absolute;
	top: -1px;
	left: 0;
	z-index: 5;
}

.model-edit {
	height: 100%;
	min-width: 900px;
}

.model-edit-view-title {
	font-size: 14px;
	color: #333333;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	padding-bottom: 10px;
}

.model-edit-view-title>div {
	cursor: pointer;
	display: inline-block;
	overflow: hidden;
}

.model-edit-view-title>div i {
	float: left;
	width: 14px;
	height: 14px;
	margin-top: 8px;
	background: url(../images/qian.png) no-repeat center;
	background-size: contain;
	margin-right: 6px;
}

.model-edit-view-title>div p {
	float: left;
	margin: 0;
}

.model-edit-view-title>div:hover i {
	animation: arrMove 1s infinite;
}

.model-edit-view-subtitle {
	font-size: 14px;
}

.model-edit-left {
	width: 60%;
	height: 100%;
	overflow: hidden;
	float: left;
	position: relative;
	padding-top: 10px;
	padding-right: 20px;
	box-sizing: border-box;
}

.model-edit-left .model-edit-view-subtitle {
	color: #4285f4;
	margin-bottom: 16px;
}

.model-edit-left-wrap {}

.model-edit-left-over{
	overflow: auto !important;
}
.model-edit-left .model-edit-view-content {
	/* border: 1px solid #DDDDDD; */
	overflow: auto
}

.model-edit-left .model-edit-view-content-top {
	background: #FDFDFD;
	border: 1px solid #DDDDDD;
	border-bottom: 0;
}

.model-edit-left .model-edit-view-content-top.active {
	border: 1px dashed #4285f4;
	position: relative;
}

.model-edit-left .model-edit-view-content-top.active:before {
	content: '';
	width: 100%;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	background: url(../images/edit-mask.png) repeat center;
	z-index: 2;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-subtitle {
	padding-left: 20px;
	padding-top: 16px;
	margin-bottom: 10px;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li {
	padding: 10px 20px;
	border-top: 1px dashed #DDDDDD;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li .model-edit-view-top-list-wrap {
	overflow: hidden;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li:first-child {
	border-top: 0;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li:first-child .model-edit-view-list-bg {
	width: 70%;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li:nth-child(2) .model-edit-view-list-bg {
	width: 75%;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li:nth-child(2) .model-edit-view-list-bg-item>div {
	background: #f0f0f0;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list li:last-child .model-edit-view-list-bg {
	width: 68%;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-title {
	float: left;
	margin: 0;
	font-size: 12px;
	color: #666666;
	border: 1px solid #D4D8DF;
	background: #fff;
	border-radius: 2px;
	height: 24px;
	line-height: 24px;
	padding: 0 10px;
	width: 48px;
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	position: relative;
	z-index: 3;
	cursor: pointer;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-title.disabled {
	color: #999999;
	background: #F7F7F7;
	border-color: #f7f7f7;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-title.active {
	border-color: #4285f4;
	color: #4285f4;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-bg {
	overflow: hidden;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-bg-item {
	float: left;
	width: 20%;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-bg-item>div {
	margin: 0 10px;
	background: #F7F7F7;
	border-radius: 2px;
	height: 26px;
}

.model-edit-left .model-edit-view-content-top .model-edit-view-top-list .model-edit-view-list-bg-item>div:last-child {
	margin-right: 0;
}

.model-edit-left .model-edit-view-content-center {
	overflow: hidden;
}

.model-edit-left .model-edit-view-content-center:first-child>div {
	border-right: 1px solid #DDDDDD;
	border-left: 1px solid #DDDDDD;
}

.model-edit-left .model-edit-view-content-center-wrap {
	padding: 15px 20px;
	border-top: 1px solid #DDDDDD;
	border-right: 1px solid #DDDDDD;
}

.model-edit-left .model-edit-view-content-center .model-edit-view-list {
	overflow: hidden;
	position: relative;
	z-index: 3;
}

.model-edit-left .model-edit-view-content-center .model-edit-view-list p {
	text-align: center;
	background: #fff;
	margin: 0 5px;
	width: 48px;
	font-size: 12px;
	color: #666666;
	border: 1px solid #D4D8DF;
	border-radius: 2px;
	height: 24px;
	line-height: 24px;
