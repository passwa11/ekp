}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-list {
	overflow: hidden;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-list p {
	float: left;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-list p.disabled {
	color: #999999;
	background: #F7F7F7;
	border-color: #f7f7f7;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-list p.active {
	border-color: #4285f4;
	color: #4285f4;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-list p:first-child {
	margin-left: 0;
}

.model-edit-left .model-edit-view-content-center.right {
	float: right;
	width: 50%;
}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-content-center-wrap.active {
	border: 1px dashed #4285f4;
	position: relative;
}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-content-center-wrap.active:before {
	content: '';
	width: 100%;
