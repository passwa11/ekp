}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-list p {
	float: right;
}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-list p.disabled {
	color: #999999;
	background: #F7F7F7;
	border-color: #f7f7f7;
}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-list p.active {
	border-color: #4285f4;
	color: #4285f4;
}

.model-edit-left .model-edit-view-content-center.right .model-edit-view-list p:first-child {
	margin-right: 0;
}

.model-edit-left .model-edit-view-content-bottom {
	border-top: 1px solid #DDDDDD;
	background: #fdfdfd;
	border: 1px solid #DDDDDD;
}

.model-edit-left .model-edit-view-content-bottom.active {
	position: relative;
}

.model-edit-left .model-edit-view-content-bottom.active:before {
	content: '';
	width: 100%;
	position: absolute;
	top: 0;
	bottom: 120px;
	left: 0;
