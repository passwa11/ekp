.model-edit-right-wrap {}

.model-edit-right-wrap .model-edit-view-title {
	padding: 0 20px;
	padding-bottom: 10px;
	border-bottom: 1px solid #DDDDDD;
}

.model-edit-right-wrap .model-edit-view-title p {
	float: left;
	margin: 0;
	font-size: 14px;
}

.model-edit-right-wrap .model-edit-view-title div {
	width: 90px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background: #4285f4;
	color: #fff;
	font-size: 14px;
	border-radius: 2px;
	cursor: pointer;
	transition: all 0.3s;
	float: right;
}

.model-edit-right-wrap .model-edit-view-title div:hover {
	background: #679DF6;
}

.model-edit-right-wrap .model-edit-view-content {
	position: relative;
	bottom: 0;
	left: 0;
	right: 0;
	padding-top: 20px;
	overflow-y: auto;
}

.model-edit-pagination {
	margin: 0 auto;
	margin-top: 20px;
	margin-bottom: 10px;
}

.model-edit-pagination div {
	height: 50px;
	background: url(../images/paging.png) no-repeat center;
}

@keyframes arrMove {
	0% {
		transform: translateX(0);
	}

	50% {
		transform: translateX(2);
	}

	100% {
		transform: translateX(0);
	}
}

