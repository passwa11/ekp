	width: 50%;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-content-center-wrap.active {
	border: 1px dashed #4285f4;
	position: relative;
}

.model-edit-left .model-edit-view-content-center.left .model-edit-view-content-center-wrap.active:before {
	content: '';
	width: 100%;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
