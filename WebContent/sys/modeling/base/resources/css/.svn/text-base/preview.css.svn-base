.model-edit-left .model-edit-view-content-bottom .model-edit-view-table table thead tr td div.active {
	border-color: #4285f4;
	color: #4285f4;
}

.model-edit-left .model-edit-view-content-bottom .model-edit-view-table table tbody tr {
	border-top: 1px solid #DDDDDD;
