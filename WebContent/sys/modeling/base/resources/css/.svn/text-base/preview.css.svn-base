/****新版移动端预览start****/
.model-body-content-phone.new-list-phone{
	height: 660px;
	width: 380px;
	background: #293445;
	border-radius: 40px;
}
.model-body-content-phone.new-list-phone .model-body-content-phone-status-bar{
	background: url(../images/content/phone-status-bar.png) no-repeat center;
	width: 100%;
	height: 37px;
	background-color: #fff;
	border-radius: 30px 30px 0 0;
}
.new-list-phone .model-body-content-phone-wrap{
	padding: 14px;
	border-radius: 30px;
}
.new-list-phone .model-body-content-phone-view {
	height: 84%;
	position: relative;
	overflow-y: auto;
	background-color: #fff;
}
.new-list-phone.model-body-content-phone .model-phone-opt {
	transition: all 0.3s;
	background: #fff;
	border: 1px solid #EEEEEE;
	border-radius: 0 0 30px 30px;
}
.new-list-phone .model-body-content-phone-bottom{
	width: 100%;
	height: 0px;
	position: relative;
	bottom: 0;
}
.new-list-phone.model-body-content-phone .model-phone-opt-item {
	float: unset;
	position: relative;
}
.new-list-phone.model-body-content-phone .model-phone-filter-mask {
	position: absolute;
	left: 160px;
	right: 160px;
	top: 24px;
	bottom: 14px;
	z-index: 2;
	display: none;
	background: rgba(0, 0, 0, 0.3);
	border-radius: 30px;
}
.new-list-phone.model-body-content-phone .model-phone-filter-panel {
	position: absolute;
	left: 250px;
	right: 160px;
	top: 24px;
	bottom: 14px;
	border-top-right-radius: 30px;
	border-bottom-right-radius: 30px;
	z-index: 3;
	overflow-y: auto;
	display: none;
	background: rgb(255, 255, 255);
}
.new-list-phone .model-body-content-phone-title{
	font-size: 16px;
	color: #2A304A;
	letter-spacing: 0;
	text-align: center;
	font-weight: 500;
	height: 40px;
	line-height: 40px;
}

.new-list-phone .model-body-content-phone-sort .model-phone-sort-left{
	height: 30px;
	line-height: 30px;
}
.new-list-phone .model-body-content-phone-sort .model-phone-sort-right{
	height: 30px;
	line-height: 30px;
	width: 100px;
}

.new-list-phone .model-body-content-phone-center-wrap{
	height: 86%;
	background: #FFFFFF;
}
/****新版移动端预览end****/