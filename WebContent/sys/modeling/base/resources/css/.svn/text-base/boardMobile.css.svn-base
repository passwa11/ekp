/* 页签显示数量**/
.muiNavitem .mblDomButtonRedBadge > div {
    padding: 0 .3rem;
    min-width: 2rem;
    height: 1.6rem;
    line-height: 1.7rem;
    text-align: center;
    background-color: transparent;
    border-radius: .8rem;
    color: #333;
    font-size: 1rem;
    position: absolute;
    right: 0;
    top: 1.4rem;
}
/* 看板视图 */
.cardViewMobileConfigContainer{
    background-color: #F5F6FB;
    padding: 16px;
}
.cardViewMobileConfigContainer.boardViewContainer{
    padding: 0px;
}
.cardViewMobileConfigCardContent{
    background-color: #fff;
    margin-bottom: 16px;
    border-radius: 4px;
    zoom: 1;
}
.cardViewMobileConfigCardContent::after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.cardViewMobileConfigCardCoverBox{
    float: right;
    border-radius: 4px;
    height: 72px;
    width: 72px;
    padding: 16px 16px 0 0;
}
.cardViewMobileConfigCardCover{
    background-size: contain;
}
.cardViewMobileConfigCardCover.default{
    display: inline-block;
    width: 72px;
    height: 72px;
    background: url(../images/board/board-default.png) no-repeat center center;
    background-size: contain;
    border-radius: 4px;
}
.cardViewMobileConfigCardTextBox{
    float: left;
    padding: 24px;
}
.cardViewMobileConfigCardTextTitle{
    width: 100%;
    font-size: 15px;
    color: #2A304A;
    margin: 0 0 16px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-weight: 600;
    word-break: break-all;
}
.cardViewMobileConfigCardTextList{
    width: 100%;
}
.cardViewMobileConfigCardTextList div{
    display: flex;
    font-size: 13px;
    color: #979DB1;
}
.cardViewMobileConfigCardSummary{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 100%;
}
.cardViewMobileConfigCardSummary div{
    display: inline-block;
}
.cardViewMobileConfigCardSummary .cardViewMobileConfigCardSummaryItem{
    /* margin-right: 32px; */
    flex: 1;
}
.cardViewMobileConfigCardSummary .cardViewMobileConfigCardSummaryItem:last-child{
    margin-right: 0px;
}
.cardViewMobileConfigCardSummary .cardViewMobileConfigCardSummaryItem div{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.cardViewMobileConfigCardSummaryItem .cardViewMobileConfigCardSummaryItemTitle{
    max-width: 50%;
}
.cardViewMobileConfigCardSummaryItem .cardViewMobileConfigCardSummaryItemContent{
    max-width: 50%;
 }
.cardViewMobileConfigCardTextList>li{
    display: inline-block;
    width: 30%;
    font-size: 13px;
    color: #979DB1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.cardViewMobileConfigCardTextList .width50{
    display: inline-block;
    max-width: 45%;
    font-size: 13px;
    color: #979DB1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.viewMobileConfigCardContentMore{
    display: inline-block;
    width: 100%;
    height: 12px;
    background: url(../images/board/icon_down.png) no-repeat center center;
    background-size: 12px 12px;
}

.board-list{
    padding: 0rem 1.5rem 0rem 1.5rem;
    border-bottom: 0px solid #eaebf0;
}

.cardViewMobileConfigCardTextList.boardContentMore{
    display: none;
}

.cardViewMobileConfigCardTextList.boardContentMore.active{
    display: block;
}

/** 项目分类 */
.criteriaNode{
    display:flex;
    justify-content: space-between;
}

.board-select{
    height: 4rem;
    position: relative;
    line-height: 4rem;
    margin: 0rem 5rem -1.5rem 0.5rem;
    width: 7.5rem;
    display: inline-block;
    float: left;
}
.board-list-criteria{
    height:4.4rem;
    background-color:#fff;
    position:relative;
    z-index:3;
}

.board-criteria-mask{
    position:absolute;
    background: rgba(0,0,0,0.40);
    width:100%;
    height:100%;
    z-index:2;
    top:0;
}

.board-select-dropdown{
    position: absolute;
    width: 100%;
    background-color: #fff;
    max-height: 44rem;
    left: 0;
    overflow: auto;
    z-index: 999;
}
.board-select-dropdown-item{
    height: 5rem;
    line-height: 5rem;
    font-size: 1.4rem;
    color: #586880;
    letter-spacing: 0.0045rem;
    padding-left: 1.5rem;
    border-top: 1px solid #DFE3E9;
    display: flex;
}
.board-select-dropdown-item .board-select-selected{
    transform: rotate(-135deg);
    margin-right: 1rem;
    align-self: center;
    position: relative;
    width: 1.6rem;
    height: 1.6rem;
    background: #ffffff;
    border-radius: 1.5rem;
    border: 0.1rem solid #DFE3E9;
}
.board-select-selected-active .board-select-item{
    color:#318FED;
    float: left;
}
.board-select-selected-active .board-select-selected{
    border-bottom: none;
    border-right: none;
    transform: rotate(-135deg);
    margin-right: 1rem;
    align-self: center;
    position: relative;
    width: 1.6rem;
    height: 1.6rem;
    background: #4285F4;
    border-radius: 1.5rem;
}
.board-select-selected-active .board-select-selected div{
    width: 0.6rem;
    height: 0.6rem;
    background: #FFFFFF;
    border-radius: 1.5rem;
    border: 0.1rem solid #DFE3E9;
    border-bottom: none;
    border-right: none;
    align-self: center;
    position: relative;
    margin: 0.4rem;
}
.board-select-content{
    font-size: 1.4rem;
    color: #50617A;
    align-items:center;
    height: 4rem;
    float: left;
}
.board-select-content-down{
    width: 24px;
    height: 24px;
    background: url(../images/listview/close@2x.png) no-repeat center;
    background-size: contain;
    right: -4px;
    top: 8px;
    position: absolute;
}
.board-select-content-up{
    width: 24px;
    height: 24px;
    background: url(../images/listview/open@2x.png) no-repeat center;
    background-size: contain;
    right: -4px;
    top: 8px;
    position: absolute;
}

.board-select-content span:first-child{
    width: 6rem;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.trigger-height-hidden{
    height:0;
    transition:height 1s;
    overflow:hidden;
}
.board-select-mark{
    height: 100%;
    width: 100%;
    background-color: rgba(0,0,0,0.5);
    position: fixed;
    z-index: 88;
    opacity: 0.5;
    display: block;
}
.trigger-height-hidden .board-select-mark{
    display: none;
}
/************************************/
.boardBody{
    overflow-y: hidden;
}

.width100{
    max-width: 100%;
}
.mblEdgeToEdgeList{
    margin-bottom: 4.5rem;
}
/* 0910 */
li.muiNavitem.muiNavitemBadge{
    padding-left: 0.5rem;
}
li.muiNavitem.muiNavitemSelected span{
    min-width: auto;
}
.muiNavitem .mblDomButtonRedBadge > div{
    padding: 0 .4rem;
}
@media only screen and (device-width: 375px) and (device-height: 812px) and (-webkit-device-pixel-ratio: 3) {
    .muiTabBarGrid.mblTabBarTabBar .mblTabBarButton{
        padding-bottom: 20px !important;
    }
}
.muiPortalProcessSimple .muiDocItem:first-child {
    padding-top: 1.2rem;
}