.model-query-wrap .model-query-add {
  height: 30px;
  border: 1px dashed #CCCCCC;
  border-radius: 2px;
  font-size: 12px;
  line-height: 30px;
  color: #999999;
  text-align: center;
}

.model-query-wrap .model-query-add:before {
  content: '';
  display: inline-block;
  width: 14px;
  height: 14px;
  vertical-align: middle;
  margin-top: -2px;
  margin-right: 10px;
  background-size: 14px;
}
