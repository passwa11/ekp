.model-query {
  background: #FFFFFF;
}

.clearfix:after {
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.clearfix {
  *zoom: 1;
}

.model-query-wrap {
  width: 100%;
  margin: 12px auto 5px;
}

.model-query-wrap .model-query-tab {
	margin-bottom: 20px;
}

.model-query-wrap .model-query-tab ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.model-query-wrap .model-query-tab ul li {
  width: 50%;
  float: left;
  font-size: 14px;
  letter-spacing: 0;
  text-align: center;
  line-height: 14px;
  padding-bottom: 12px;
  color: #333;
  cursor: pointer;
  border-bottom: 1px solid #eee;
}

.model-query-wrap .model-query-tab ul li.active {
  color: #4285f4;
}

.model-query-wrap .model-query-tab ul li.active span {
  padding-bottom: 10px;
  border-bottom: 2px solid #4285f4;
}

.model-query-wrap .model-query-content {
  padding: 0 10px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.model-query-wrap .model-query-content-cust {
  display: block;
}

.model-query-wrap .model-query-content-sys {
  display: none;
}

.model-query-wrap .model-query-content-rule {
  margin-bottom: 20px;
  font-size: 14px;
  color: #333333;
  letter-spacing: 0;
  line-height: 14px;
  width: 380px;
}

.model-query-wrap .model-query-content-rule p {
  width: 20%;
  float: left;
  margin: 0;
  font-size: 14px;
  overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.model-query-wrap .model-query-content-rule div {
  width: 80%;
  float: left;
}

.model-query-wrap .model-query-content-rule div span {
  float: left;
  padding: 0 10px 0 20px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

