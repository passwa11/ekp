.model-mask-panel-table .model-table-left p {
    font-size: 14px;
    color: #333333;
    margin-bottom: 30px;
    margin-top: 0;
}

.model-mask-panel-table .model-table-right {
    margin-left: 32px;
}

.model-mask-panel-table .model-table-right span {
    vertical-align: middle;
}

.model-mask-panel-table .td_normal_title {
    font-size: 14px;
    color: #333333;
    text-align: right;
    vertical-align: top;
    line-height: 33px;
}

.model-mask-panel-table .modeling_form_table {
    width: 100%;
}

.model-mask-panel-table .comp,
.model-mask-panel-table input[type=text],
.model-mask-panel-table textarea {
    border: 1px solid #DFE3E9;
    border-radius: 2px;
    height: 30px;
    line-height: 30px;
    margin-left: 16px;
    color: #333333;
}

.model-mask-panel-table select {
    border: 1px solid #DFE3E9;
    border-radius: 2px;
    height: 30px;
    line-height: 30px;
}

.model-mask-panel-table div[_xform_type=radio],
.model-mask-panel-table div[_xform_type=checkbox] {
    height: 33px;
    line-height: 30px;
    margin-left: 16px;
    margin-bottom: 25px;
}
.model-mask-panel-table div[_xform_type=checkbox] {
    height: auto !important;
}

.model-mask-panel .model-mask-panel-table-radio-item {
    float: left;
    margin-right: 50px;
    margin-bottom: 16px;
    cursor: pointer;
}

.model-mask-panel .model-mask-panel-table-radio-item i {
    background: url(../images/panel/icon-radio.png);
    background: url(../images/panel/icon-radio@2x.png);
    background-size: contain;
}

.model-mask-panel .model-mask-panel-table-radio-item input {
    display: none;
}

.model-mask-panel .model-mask-panel-table-radio-item input:checked+i.model-mask-panel-output-i {
    background: url(../images/panel/icon-radio-active.png) no-repeat center;
    background: url(../images/panel/icon-radio-active@2x.png) no-repeat center;
    background-size: contain;
}

.model-mask-panel .model-mask-panel-table-radio {
    overflow: hidden;
    margin-left: 20px;
}

.tb_simple>tbody>tr>td {
    padding: 0px;
}

.model-mask-panel td>div {
    margin-bottom: 20px;
}

.model-mask-panel .validation-table td>.lui_icon_s_icon_validator {
    margin-bottom: 0px;
}

.model-mask-panel .model-mask-panel-table-base {
    margin-left: 16px;
}

.model-mask-panel .model-mask-panel-table-base table {
    width: 100%;
    border-spacing: 0;
}

.model-mask-panel .model-mask-panel-table-base table tr {
    height: 40px;
    /* line-height: 40px; */
}

.model-mask-panel .model-mask-panel-table-base table td {
    border: 1px solid #DFE3E9;
    border-left: 0;
    border-top: 0;
    padding-left: 10px;
}

.model-mask-panel .model-mask-panel-table-base table thead tr {
    background: #F8F8F8;
}

.model-mask-panel .model-mask-panel-table-base table thead tr td {
    border-top: 1px solid #DFE3E9;
    font-size: 14px;
    color: #354052;
    padding: 0;
    padding-left: 10px;
}

.model-mask-panel .model-mask-panel-table-base table thead tr td:first-child {
    border-left: 1px solid #DFE3E9;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td {
    font-size: 12px;
    color: #999999;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td p {
    margin: 0;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td:first-child {
    border-left: 1px solid #DFE3E9;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td input[type=text] {
    outline: none;
    border: 0;
    border-bottom: 1px solid #d9d9d9;
    margin-left: 0;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td.model-mask-panel-table-opt p {
    color: #4285f4;
    cursor: pointer;
    transition: all 0.3s;
    float: left;
    padding: 0 10px;
    position: relative;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td.model-mask-panel-table-opt p:before {
    content: '';
    position: absolute;
    width: 1px;
    height: 10px;
    background: #D9D9D9;
    left: 0;
    top: 50%;
    margin-top: -5px;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td.model-mask-panel-table-opt p:first-child:before {
    display: none;
}

.model-mask-panel .model-mask-panel-table-base table tbody tr td.model-mask-panel-table-opt p:hover {
    color: #8EB6F8;
}

.model-mask-panel .model-mask-panel-table-select {
    display: inline-block;
    position: relative;
}

.model-mask-panel .model-mask-panel-table-select:before {
    content: '';
    border: 4px solid transparent;
    border-top: 6px solid #999999;
    position: absolute;
    top: 50%;
    right: -12px;
    margin-top: -2px;
}

.model-mask-panel .model-mask-panel-table-show {
    margin-left: 16px;
    border: 1px solid #DFE3E9;
    border-radius: 2px;
    /* padding: 0 4px; */
    padding: 0 26px 0 4px;
    overflow: hidden;
    position: relative;
    height: 30px;
    cursor: pointer;
}

.model-mask-panel .model-mask-panel-table-show:before {
    content: '';

    position: absolute;
    width: 14px;
    height: 14px;
    background: url(../images/panel/show.png) no-repeat center;
    background: url(../images/panel/show@2x.png) no-repeat center;
    background-size: contain;
    right: 6px;
    top: 8px;
}

.model-mask-panel .model-mask-panel-table-show p {
    float: left;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    color: #333333;
    margin: 0 4px;
}

.model-mask-panel .model-mask-panel-table-show input {
    border: none;
    font-size: 12px;
    color: #333333;
    width: 100%;
    margin-left: 0px;
    cursor: pointer;
    height: 28px;
}

.model-mask-panel .model-mask-panel-table-create {
    margin-top: 21px;
    margin-left: 16px;
    border: 1px dashed #CCCCCC;
    border-radius: 2px;
    text-align: center;
    height: 30px;
    line-height: 30px;
    cursor: pointer;
}


.model-mask-panel .model-mask-panel-table-create div {
    display: inline-block;
    padding-left: 20px;

    background: url(../images/panel/table-create.png) no-repeat left;
    background: url(../images/panel/table-create@2x.png) no-repeat left;
    background-size: 14px;
}

.model-mask-panel .detail-delete{
    margin: 0px;
    border: 1px dashed #CCCCCC;
    border-radius: 2px;
    background-color: #f6f6f6;
    text-align: center;
    height: 30px;
    line-height: 30px;
    cursor: pointer;
}
.model-mask-panel .detail-delete div {
    display: inline-block;
    padding-left: 20px;
    background: url(../images/delete.png) no-repeat left;
    background-size: 14px;
}

.model-mask-panel-output-select {
    margin-bottom: 30px;
}

.model-mask-panel-output-select.disable i {
    background: url(../images/panel/xuanzhong-hui.png) no-repeat center !important;
    background: url(../images/panel/xuanzhong-hui@2x.png) no-repeat center !important;
    background-size: 14px !important;
}

.model-mask-panel-output-select span {
    color: #999999;
}

.model-mask-panel-output-select-left {
    float: left;
    padding-top: 3px;

}

.model-mask-panel-output-select-left.active i {
    background: url(../images/panel/xuanzhong-lan.png) no-repeat center;
    background: url(../images/panel/xuanzhong-lan@2x.png) no-repeat center;
    background-size: contain;
}

.model-mask-panel-output-select-left input {
    display: none;
}

.model-mask-panel-output-select-left input:checked+i.model-mask-panel-output-i {
    background: url(../images/panel/xuanzhong-lan.png) no-repeat center;
    background: url(../images/panel/xuanzhong-lan@2x.png) no-repeat center;
    background-size: contain;
}

.model-mask-panel-output-select-left i {
    display: block;
    width: 14px;
    height: 14px;
    background: url(../images/panel/kexuan.png) no-repeat center;
    background: url(../images/panel/kexuan@2x.png) no-repeat center;
    background-size: contain;
    transition: all 0.3s;
}

.model-mask-panel-output-select-right {
    margin-left: 20px;
}

.model-mask-panel-output-select-right span {
    font-size: 14px;
    color: #333333;
}

.model-mask-panel-output-select-right label {
    display: block;
    cursor: pointer;
}

.model-mask-panel-output-select-right p {
    font-size: 12px;
    color: #999999;
}

.hiddenField {
    display: none;
}

td div.modeling_data_select.inputselectsgl {
    width: 188px;
    margin-bottom: 0px;
    border:none;
    border-bottom: 1px solid #d9d9d9;
}


#_xform_fdOutExtend label {
	margin-right: 0;
}

#_xform_fdOutExtend label:first-child {
    margin-right: 12px;
}
.modeling_form_table.operationMainForm>tbody>tr>td{
    padding: 8px 5px;
}

.modeling-filling-table .modeling-form-left select{
    width: 97%;
}
.modeling_data_source_table .modeling-out-select,.modeling-filling-table .modeling_data_source_table .model-mask-panel-table-show {
    width: 90%;
    display: inline-flex;
}
.model-table-right .modeling_data_source_table span{
    vertical-align: top;
}
