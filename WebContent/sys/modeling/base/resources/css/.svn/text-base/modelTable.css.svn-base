@charset "UTF-8";

.model-mask-panel {
    width: 100%;
    padding-bottom: 25px;
}

.model-mask-panel-table {
    /*padding: 14px 24px;*/
    /*padding-right: 28px;*/
    padding: 20px 71px 20px 28px;
}

.model-mask-panel-table .model-table-item {
    position: relative;
    overflow: hidden;
    /*padding-bottom: 15px;*/
}

.model-mask-panel-table .model-table-item-line {
    margin-bottom: 30px;
}

