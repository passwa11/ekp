.model-print-tips {
    margin-top: 10px;
    height: 40px;
    background-color: #f8f8f8;
    border-radius: 4px;
    padding-left: 15px;
    font-size: 14px;
    line-height: 40px;
}

.model-print-tips p {
    color: #333333;
    display: inline-block;
    margin: 0;
}

.model-print-tips p:before {
    content: '';
    display: inline-block;
    width: 14px;
    height: 14px;
    margin-right: 10px;
    vertical-align: middle;
    background: url(../images/mechanism/icon_tips@2x.jpg) no-repeat;
    background: url(../images/mechanism/icon_tips.jpg) no-repeat \9;
    background-size: 14px;
}

.model-print-tips a {
    text-decoration: none;
    color: #4285F4;
    cursor: pointer;
    padding: 0 20px;
}

