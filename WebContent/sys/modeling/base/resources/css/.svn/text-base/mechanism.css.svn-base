.sideMenu li {list-style:none;}
.sideMenu h3{ height:32px; line-height:32px; padding-left:10px; background:#f4f4f4; cursor:pointer; font-family: 'Microsoft YaHei'; font-size: 14px; color: #333;margin:0px;
}
.sideMenu ul .nLi.on .sub{ display: block;}
.sideMenu ul .nLi .sub{ padding:8px 0; display:none; }
.sideMenu ul .nLi .sub li{ height: 30px; line-height: 30px; list-style: none;}
.sideMenu ul .nLi .sub li a{ display: block; width: 100%; height: 100%; font-family: 'Microsoft YaHei'; font-size: 14px; color: #333; text-decoration: none; padding:0 25px; box-sizing:border-box;}
.sideMenu ul .nLi .sub li a:hover{ background:#eee;}
.sideMenu .arrow {
    background-image: url(./../images/ui-icons.png);
    width: 16px;
    height: 16px;
    display: inline-block;
    vertical-align: middle;
    margin-top: -.25em;
    position: relative;
    text-indent: -99999px;
    overflow: hidden;
    background-repeat: no-repeat;
    background-position: -32px -16px;
}
.sideMenu .on .arrow {
    background-position: -65px -16px;
}
