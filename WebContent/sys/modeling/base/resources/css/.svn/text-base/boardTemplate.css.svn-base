@charset "utf-8";


.clearfix:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
  .clearfix {
    zoom: 1;
}
::-webkit-scrollbar{
    width: 6px;
    height: 6px;
}
::-webkit-scrollbar-thumb{
    border-radius: 10px;
    background: #ddd;
}
.cardClassifyContent::-webkit-scrollbar-track{
    /*background: transparent;*/
    background-color: rgb(247, 250, 255)!important;
}
.boardContainer{
    background-color: #F2F2F2;
    width: 100%;
    /*min-height: 600px;*/
    overflow-x: scroll;
    padding: 16px;
    white-space: nowrap;
}
.tb_simple>tbody>tr>td{
    padding-top: 4px;
    padding-bottom: 0px;
}
.boardContainer .listview{
    background-color: #F7FAFF;
}
.cardClassifyContent{
    display: inline-block;
    width: 262px;
    height: auto;
    overflow-y: auto;
    padding: 0px 16px 32px 16px;
    background-color: #F7FAFF;
    border: 1px solid #E7EAEE;
    box-shadow: 0px 0px 6px 0px rgba(51,51,51,0.13);
    border-radius: 4px;
    margin-right: 16px;
    vertical-align: top;
    position: relative;
}
.cardClassifyHeader{
    height: 40px;
    line-height: 40px;
    position: -webkit-sticky;
    position: sticky;
    top: 0px;
    z-index: 10;
    background-color: #F7FAFF;
    padding: 9px 0px 0px 0px;
    text-align: center;
}
.cardClassifyHeader::before{
    content: "";
    position: absolute;
    width: 4px;
    height: 24px;
    background: url(../images/board/board-title.svg) no-repeat center;
    background-size: contain;
    top: 16px;
    left: -16px;
}
.cardClassifyHeaderText{
    float: left;
    font-size: 16px;
    color: #333333;
    font-weight: 600;
    max-width: 150px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.cardClassifyHeaderCount{
    float: left;
    min-width: 24px;
    height: 16px;
    line-height: 16px;
    text-align: center;
    font-size: 12px;
    color: #FFFFFF;
    background: url(../images/board/header-count.png) no-repeat center;
    background-size: contain;
    position: relative;
    top: 10px;
    margin-left: 8px;
}
.cardClassifyHeaderBtn{
    display: block;
    float: right;
    width: 16px;
    height: 16px;
    cursor: pointer;
    position: relative;
    top:10px;
}
.cardClassifyContent .cardClassifyHeaderBtn{
    background: url(../images/board/header-pickup.png) no-repeat center;
    background-size: contain;
}
.cardClassifyContent .cardClassifyHeaderBtn:hover{
    background: url(../images/board/header-pickup-hover.png) no-repeat center;
    background-size: contain;
}
.cardClassifyContent.minimize .cardClassifyHeaderBtn{
    background: url(../images/board/header-spread.png) no-repeat center;
    background-size: contain;
    position: absolute;
    margin-left: 4px;
    top: calc(100% - 32px);
    display: block;
}
.cardClassifyContent > .cardClassifyHeaderBtn{
    display: none;
}
.cardClassifyContent.minimize .cardClassifyHeader > .cardClassifyHeaderBtn{
    display: none;
}
.cardClassifyContent .cardClassifyHeader > .cardClassifyHeaderBtn{
    display: block;
}
.cardClassifyContent.minimize .cardClassifyHeaderBtn:hover{
    background: url(../images/board/header-spread-hover.png) no-repeat center;
    background-size: contain;
}
.cardClassifyDetails{
    margin-top: 16px;
    position: relative;
    border: 1px solid transparent;
    border-radius: 4px;
}
.cardClassifyContent.minimalism .cardClassifyDetails{
    margin-top: 8px;
}
.cardClassifyDetailsCover{
    box-sizing: border-box;
    width: 100%;
    height:128px;
    background: url(../images/board/board-cover.png) no-repeat center;
    background-size: 100% 100%;
    border: 1px solid #eee;
    border-radius: 4px 4px 0 0;
    border-bottom-color: transparent;
}
.cardClassifyDetailsText{
    padding: 16px;
    background-color: #fff;
    border: 1px solid #eee;
    border-radius: 0 0 4px 4px;
    border-top-color: transparent;
    position: relative;
}
.cardClassifyDetailsTitle{
    font-size: 14px;
    font-weight:600;
    color: #333333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    white-space: normal;
}
.cardClassifyAbstract{
    margin-top: 16px;
}
.cardClassifyAbstractTitle{
    font-size: 12px;
    color: #999999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 4px;
    padding-left: 8px;
    position: relative;
}
.cardClassifyAbstractTitle::before{
    content: "";
    position: absolute;
    width: 2px;
    height: 12px;
    background-color: #D4D6DB;
    top: 2px;
    left: 0;
    border-radius: 1px;
}
.cardClassifyAbstractContent{
    font-size: 14px;
    color: #666666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.cardClassifyDetails.selected{
    border: 1px solid #4285f4;
    border-radius: 4px;
}
.cardClassifyDetails.selected .cardClassifyDetailsCover{
    /*border: 1px solid #4285f4;*/
    /*border-bottom-color: transparent;*/
    /*width: 100%;*/
}
.cardClassifyDetails.selected .cardClassifyDetailsText{
    /*border: 1px solid #4285f4;*/
    /*border-top-color: transparent;*/
}
.cardClassifyDetailsOpt{
    width: 20px;
    height: 20px;
    cursor: pointer;
    position: absolute;
    top: 26px;
    right: 10px;
    opacity: .2;
    z-index: 1;
}
.cardClassifyDetailsOpt:checked{
    opacity: .8;
}
.cardClassifyDetails .cardClassifyDetailsOpt{
    visibility: hidden;
}
.cardClassifyDetails.selected .cardClassifyDetailsOpt{
    visibility: visible;
}
.cardClassifyDetails:hover .cardClassifyDetailsOpt{
    visibility: visible;
}
.cardClassifyDetails:hover{
    border: 1px solid #4285f4;
    border-radius: 4px;
}
.cardClassifyDetailsNull{
    text-align: center;
    font-size: 14px;
    color: #666666;
    margin: 16px 0;
}
/* 卡片最小化展示 */
.cardClassifyContent.minimize{
    width: 25px;
    min-height: 170px;
}
.cardClassifyContent.minimize .cardClassifyHeader::before {
    top: -26px;
    left: 6px;
    transform: rotate(90deg);
}
.cardClassifyContent.minimize .cardClassifyHeader{
    line-height: normal;
    height: 100%;
    min-height: 136px;
}
.cardClassifyContent.minimize .cardClassifyDetails{
    display: none;
}
.cardClassifyContent.minimize .cardClassifyDetailsNull{
    display: none;
}
.cardClassifyContent.minimize .cardClassifyHeaderText{
    white-space: normal;
    width: 20px;
}
.cardClassifyContent.minimize .cardClassifyHeaderCount{
    margin: 4px 0 32px 0;
}
/* 概览模式 */
.cardClassifyContent.overview .cardClassifyDetails>.cardClassifyDetailsCover{
    display: none;
}
.cardClassifyContent.overview .cardClassifyDetails>.cardClassifyDetailsText{
    border-radius: 4px;
    border-top-color: #eee;
}
.cardClassifyContent.overview .cardClassifyDetails.selected>.cardClassifyDetailsText{
    border-top-color: #4285f4;
}
/* 极简模式 */
.cardClassifyContent.minimalism .cardClassifyDetails>.cardClassifyDetailsCover{
    display: none;
}
.cardClassifyContent.minimalism .cardClassifyDetails .cardClassifyAbstract{
    display: none;
}
.cardClassifyContent.minimalism .cardClassifyDetails>.cardClassifyDetailsText{
    border-radius: 4px;
    border-top-color: #eee;
}
.cardClassifyContent.minimalism .cardClassifyDetails.selected>.cardClassifyDetailsText{
    /*border-top-color: #4285f4;*/
}
.cardClassifyContent::-webkit-scrollbar-thumb{
    background: transparent;
}
.cardClassifyContent:hover::-webkit-scrollbar-thumb{
    background: #ddd;
}
.cardClassifyDetailsPreview{
    position: absolute;
    /* top: 16px;
    left: 100%; */
    z-index: 10;
    width: 230px;
    box-shadow: 0px 0px 6px 0px rgba(51,51,51,0.13);
    border-radius: 4px;
    background-color: #fff;
}
.cardClassifyContent.minimalism .cardClassifyDetails .cardClassifyDetailsPreview .cardClassifyAbstract{
    display: block;
}
.cardClassifyDetailsText:hover>span{
    visibility: visible;
}
.cardClassifyDetailsText>span{
    visibility: hidden;
    width: 222px;
    font-size: 12px;
    position: absolute;
    color:#fff;
    font-weight: normal;
    border-radius: 2px;
    background-color: #393C41;
    box-shadow: 0 2px 5px 0 rgba(0,0,0,0.1);
    padding:10px;
    top: 0%;
    right: 50%;
    transform: translate(50%,60px);
    cursor:default;
    line-height: initial;
    z-index: 10;
    white-space: normal;
}
.cardClassifyDetailsText>span::after{
    content: "";
    position: absolute;
    z-index: 100;
    top: -15px;
    left: 50%;
    transform: translate(0,-1px);
    border-width: 8px;
    border-style: solid;
    border-color: transparent transparent #393C41 transparent;
}
.displayCss-pre .model-mask-panel{
    text-align: center;
}
.displayCss-pre .model-mask-panel .cardClassifyContent{
    text-align: left;
}

