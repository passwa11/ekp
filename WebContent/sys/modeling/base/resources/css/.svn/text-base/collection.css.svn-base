    background-color: rgba(66,133,244,0.1);
}
.open_rule_setting_show_num_unit,.open_rule_setting_show_cus_unit,.open_rule_setting_show_point_count{
    padding: 0 16px;
    margin-bottom: 8px;
}
.open_rule_setting_show_num_unit>select.inputsgl,.open_rule_setting_show_cus_unit>input.inputsgl,.open_rule_setting_show_point_count>input.inputsgl{
    display: block;
    width: 100%;
    height: 30px;
    background: #FFFFFF;
    border: 1px solid #DDDDDD;
    border-radius: 2px;
}
.open_rule_setting_show_thousands{
    padding: 0 16px;
}
.open_rule_setting_show_thousands>input{
    position: relative;
    top: 2px;
    margin-left: 4px;
}

.operateTips{
    display:inline-block;
    margin: 5px 0 5px 14px;
    font-size: 12px;
    padding-right: 2px;
    padding-left: 16px;
    color:#FF9431;
    background: url(../images/form-tip@2x.png) no-repeat left;
    background: url(../images/form-tip.png) no-repeat \9;
    background-size: 12px 12px;
    word-break:break-word;
    white-space: normal;
}

