}

.model-edit-view-title ul li{
    width: 50%;
    display: inline-block;
    text-align: center;
    height: 40px;
    line-height: 40px;
    box-sizing: border-box;
    cursor: pointer;
    background-color: #F8F8F8;
    border-bottom: 1px solid #DDDDDD;
    box-sizing: border-box;
}
.model-edit-view-title ul li>span{
