body {
    overflow: hidden;
}
html,body{
    height: 100%;
}
.modeling-pam-top{
    width: 100%;
    position: absolute;
    top: 0px;
    height: 50px;
    padding: 0px 0 17px 0;
    background-color: #FFFFFF ;
    margin-top: 10px;
    text-align: center;
}
.modeling-pam-back{
    width: 60px;
    height: 30px;
    background: rgba(66,133,244,0.08);
    border-radius: 2px;
    float: left;
}
.modeling-pam-back > div {
    cursor: pointer;
    display: inline-block;
    overflow: hidden;
}
.modeling-pam-back > div i {
    float: left;
    width: 14px;
    height: 14px;
    margin-top: 8px;
    background: url(../images/qian.png) no-repeat center;
    background-size: contain;
    margin-right: 6px;
}
.modeling-pam-back > div p {
    float: left;
    margin-top: 6px;
    margin-right: 6px;
    width: 24px;
    height: 17px;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #4285F4;
    letter-spacing: 0;
    font-weight: 400;
}
.modeling-pam-top-center {
    width: 195px;
    height: 32px;
    line-height: 32px;
    font-size: 14px;
    text-align: center;
    display: inline-block;
}
.modeling-pam-top ul {
    list-style: none;
}
.modeling-pam-top-center ul li.active {
    width: 86px;
    height: 30px;
    background: #4285F4;
    border: 1px solid #4285F4;
    border-radius: 2px;
    color:#ffffff;
}
.modeling-pam-top-center ul li {
    float: left;
    margin-right: 32px;
}
.modeling-pam-top ul li {
    cursor: pointer;
}
.modeling-pam-top-right {
    padding-right: 23px;
    float: right;
    box-sizing: border-box;
}
.modeling-pam-top-right ul li.active {
    background: #4285F4;
    border-radius: 2px;
    color: #FFFFFF;
}
.modeling-pam-top-right ul li {
    float: right;
    margin-left: 10px;
    background: #FFFFFF;
    /*border: 1px solid #DDDDDD;*/
    border-radius: 2px;
    padding: 0 7px;
    min-width: 90px;
    text-align: center;
    padding: 5px;
}

.fdViewFlag:first-child {
    margin-left: 0;
}

.lui_custom_list_boxs {
    border-top: 1px solid #d5d5d5;
    position: fixed;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 1000;
    height: 63px;
}

table.model-view-panel-table > tbody > tr > td.td_normal_title {
    text-align: left;
    padding: 4px 20px 4px 20px;
    font-weight: 600;
    font-size: 14px;
    margin-bottom: -13px;
    margin-top: -8px;
}

.model-view-panel-table .model-view-panel-table-td {
    padding-left: 20px !important;
    padding-right: 20px !important;
    padding-bottom: 10px !important;
}

.inputselectsgl input {
    padding-left: 8px !important;
}

.model-edit-view-oper-content-item div.operation > div.inputselectsgl {
    height: 28px !important;
}

.model-edit-view-oper-content-item div.operation > div.inputselectsgl input {
    height: 28px !important;
}

.liHidden {
    display: none;
}

.modeling-pam-preview {
    width: 70%;
    height: 100%;
    overflow: hidden;
    float: left;
    position: relative;
    padding-top: 10px;
    padding-right: 20px;
    box-sizing: border-box;
}

.modeling-pam-nav {
    margin-top: 10px;
}

.terminal {
    width: 86px;
    height: 30px;
    background: #4285F4;
    border: 1px solid #4285F4;
    border-radius: 2002px;
}

.listviewName {
    height: 25px;
    font-family: PingFangSC-Medium;
    font-size: 18px;
