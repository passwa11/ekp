.formatSelectTab input[type='radio']{
    margin-right: 20px;
}
.formatReviewBox{
    float: right;
    margin-right: 20px;
    width: 200px;
    height: 100px;
    background-color: #a0a0a3;
}
.btnBar{
    padding: 5px 0px;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 30px;
    line-height: 30px;
    background-color: #FFFFFF;
    border-top: 1px solid #dedede;

    text-align: right;
    padding:15px 0;
}
.btnBar .lui_toolbar_btn_def{
    float: right;
    margin: 0px 15px;
    padding-left: 3px;
    padding-right: 3px;
    letter-spacing: 2px;

}
.btn_cancel{
    line-height: 19px;
    background-color: #fff;
    color: #333;
    padding: 6px 10px;
    min-width: 80px;
    text-align: center;
    border-radius: 4px;
    border: 1px solid #e0e0e0;
    margin-left: 8px;
    display: inline-block;
    transition-duration: .3s;
    margin-right:30px;
}
.btn_cancel:hover{
    color: #333;
    background-color: #eee;
}
.btn_submit{
    line-height: 19px;
    background-color: #37a8f6;
    color: #fff;
    padding: 6px 10px;
    min-width: 80px;
    text-align: center;
    border-radius: 4px;
    border: 1px solid #37a8f6;
    margin-left: 8px;
    display: inline-block;
    transition-duration: .3s;
}
.btn_submit:hover{
    color: #fff;
    border-color: #0d94f1;
    background-color: #0d94f1;
}

.format_flag_no{
    width:14px;
    height:14px;
    background: url(../images/panel/icon-radio.png) no-repeat center;
    display: inline-block;
    cursor: pointer;
    margin-right: 20px;
}
.format_flag_yes{
    width:14px;
    height:14px;
    background: url(../images/panel/icon-radio-active.png) no-repeat center;
    display: inline-block;
    cursor: pointer;
    margin-right: 20px;
}
.lui_custom_list_boxs{
    border-top: 1px solid #d5d5d5;
    position:fixed;
    bottom:0;
    width:100%;
    background-color: #fff;
    z-index:1000;
    height:63px;
    text-align: right;
    padding-top: 15px;
    box-sizing: border-box;
}
html,body{
    height:100%;
}
.format_select_content{
    height: 140px;
    position: relative;
    border: 1px solid #DFE3E9;
    border-radius: 2px;
    margin: 0 auto;
    padding-left: 20px;
    box-sizing: border-box;
    width: 90%;
    cursor: pointer;
}
.format_select_content_left{
    display: inline-block;
    position: absolute;
    top: 25px;
}
.format_select_content_right{
    display: inline-block;
    position: absolute;
    right: 0;
    top: 25px;
}
.active.format_select_content,.format_select_content:hover{
    background-color: #EDF2FD;
    color: #3b65b8;
    border: 1px solid #4285F4;
}

