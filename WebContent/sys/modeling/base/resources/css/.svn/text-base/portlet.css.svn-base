@charset "UTF-8";
.formatSelectTab{
    padding: 10px;
}
.formatSelectTab tr,
.formatSelectTab tr td {
    height: 100px;
    line-height: 100px;
    margin-bottom: 10px;
}
.formatSelectTab label{
    width: 100%;
    height: 100%;
    display: inline-block;
    cursor: pointer;
}
