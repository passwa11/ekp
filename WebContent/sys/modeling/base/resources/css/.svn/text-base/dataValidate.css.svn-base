/*************** 穿梭框 start *****************/
.shuttleBox {

}

.shuttle-box-content {
	
}

.shuttle-box-content .content-table-select {
	padding: 15px 0px;
	height: 35px;
	line-height: 35px;
}

.content-table-select .table-select-wrap {
	display: inline-block;
}

.content-table-select .table-select-tip {
	margin-left: 15px;
	display: inline-block;
}

.table-select-tip span {
	color: #979797;
}

.shuttle-box-content .box-wrap {
	padding-bottom: 15px;
}

.shuttle-box-content .box-wrap {
	overflow: auto;
}
.box-wrap > div{
	float: left;
}
.box-wrap > div:last-child{
	float: right;
}
.box-wrap .box-container {
	width:300px;
}
.box-wrap .box-container::after {
	display: block;
	height: 0;
	content: '';
	clear: both;
}

.box-container .content-box {
	overflow: hidden;
	border: 1px solid #dddddd;
	min-height: 1px;
	box-sizing: border-box;
}

.content-box .box-header {
	background: #F6F7FA;
	padding:0 15px;
	border-bottom: 1px solid #dddddd;
	height: 40px;
	line-height:40px;
	font-size:14px;
	color:#666;
}

.content-box .box-content {
	background-color: #fefefe;
	height: 300px;
	overflow: auto;
}

.box-content .box-item {
	padding:0 15px;
	cursor: pointer;
	height: 40px;
	line-height: 40px;
	transition:all .3s;
}
.box-item-txt{
	float:left;
}
.box-item-close{
	float:right;
	visibility:hidden;
}
.box-item:hover .box-item-close{
	visibility:visible;
	color:#4285f4;
}
.content-box.selected-box .box-item:hover {
	background: #EAF4FE;
}

.content-box.to-choose-box .box-item.active {
	color: #4285f4;
}

.box-wrap .choose-box-oper {
	margin: auto;
	padding: 0px 55px;
}

.choose-box-oper .oper-btn {
	width: 119px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	font-size: 14px;
	border-radius: 7px;
	cursor: pointer;
	transition: all 0.3s;
	border: 1px solid #e0e0e0;
	margin: 15px 0px;
}

.oper-btn span {
	
}

.choose-box-oper .high-light {
	background: #4285f4;
	color: #fff;
}
.box-btn{
	margin-left:10px;
	margin-top:110px
}
.box-btn-item{
	cursor: pointer;
    height: 40px;
    line-height: 40px;
    width: 126px;
    background: #F8F8F8;
    border: 1px solid #DDDDDD;
    text-align: center;
    position: relative;
    border-radius: 4px;
    font-size: 14px;
    color: #999999;
    transition: all .3s;
    position:relative;
}
.box-btn-item:first-child{
	text-align:left;
	text-indent:30px;
}
.box-btn-item:hover{
	background: #4285f4;
    border-color: #4285f4;
    color: #fff;
}
.box-btn-item:first-child{
	margin-bottom:10px;
}
.box-btn-item:first-child:before{
	content:'';
    width: 6px;
    height: 10px;
    background: url(../images/nav/you@2x.png) no-repeat center;
    background: url(../images/nav/you@1x.png) no-repeat center \9;
    background-size: contain;
    position: absolute;
    right: 28px;
    top: 50%;
    margin-top: -5px;
}
.box-btn-item:first-child:hover:before{
	background: url(../images/nav/you-active@2x.png) no-repeat center;
    background: url(../images/nav/you-active.png) no-repeat center \9;
    background-size: contain;
}
/*************** 穿梭框 end *****************/
/*************** 自定义校验规则 **************/
.data-validate-where > div{
	margin-bottom: 20px;
}
.model-mask-panel-table-base table {
	width: 100%;
	border-spacing: 0;
}
.model-mask-panel-table-base table thead tr {
	background: #F8F8F8;
}
.model-mask-panel-table-create {
	margin-top: 21px;
	margin-left: 16px;
	border: 1px dashed #CCCCCC;
	border-radius: 2px;
	text-align: center;
	height: 30px;
	line-height: 30px;
	cursor: pointer;
}
.model-mask-panel-table-create div {
	display: inline-block;
	padding-left: 20px;
	background: url(../images/panel/table-create.png) no-repeat left;
	background: url(../images/panel/table-create@2x.png) no-repeat left;
	background-size: 14px;
}
.validate_type .radio-label {
	margin-right: 10px;
}
.judge_type span{
	margin-right: 10px;
}
.modeling-viewcover-tip {
	display: inline-block;
	width: 16px;
	height: 16px;
	position: relative;
	background: url(../images/modeling-tips.png) no-repeat center;
	background-size: contain;
	cursor: pointer;
	top: 3px;
	left: 3px;
}
.modeling-viewcover-tip:hover>span {
	visibility: visible;
}
.modeling-viewcover-tip:hover {
	background: url(../images/modeling-tips-hover.png) no-repeat center;
	background-size: contain;
}
.modeling-viewcover-tip>span {
	 visibility: hidden;
	 width: 330px;
	 position: absolute;
	 vertical-align: middle;
	 color: #0C0C0C;
	 font-size: 12px;
	 font-weight: normal;
	 border-radius: 2px;
	 background-color: #FFFFFF;
	 box-shadow: 0 2px 5px 0 rgb(0 0 0 / 10%);
	 padding: 10px;
	 z-index: 99;
	 bottom: 257%;
	 right: -120px;
	 transform: translate(55%,110px);
	 cursor: default;
	 line-height: initial;
	 z-index: 10;
 }
.modeling_data_select{
	width: 100%;
}