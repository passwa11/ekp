	border: 1px solid #ccc;
	padding: 5px 5px;
	word-break: break-all;
}

.txt-wrap span {
	
}

.txt-wrap .txt-no-record {
	
}

.data-validate-main .data-validate-content {
	position: fixed;
	top: 10px;
	left: 0;
	right: 0;
	bottom: 53px;
	padding: 0 15px;
	overflow-y: auto;
	overflow-x: hidden;
}

.data-validate-main .data-validate-bottom {
	text-align: right;
	padding: 10px 10px 10px 0;
