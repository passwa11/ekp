.archives_template_setting{
    margin: 29.5px auto;
    width: 95%;
}
.archives_type_0 i ,.archives_type_1 i {
    width: 14px;
    height: 14px;
    background: url(../../../base/resources/images/icon-tips@2x.png) no-repeat;
    position: absolute;
    margin: 13px;
    background-size: contain;
}
.archives_type_1,.archives_type_0_info{
    background: #F8F8F8;
    border-radius: 4px;
    margin: 10px;
    height: 40px;
}
.archives_type_0_tips,.archives_type_1_tips {
    margin: 9px 33px;
    font-size: 14px;
    color: #333333;
    line-height: 40px;
    font-weight: 400;
}
.switch_submit_div{
    margin-left: 20px;
    margin-top: 30px;
}
.config_div,.archives_auto_div{
    margin-top: 28px;
}
.archives_type_div,.archives_auto_div,.archives_type_0_info,.archives_type_1{
    margin-left: 20px;
}
.archives_type_div input{
    position: relative;
    top: 1px;
    margin-left: 13px;
    margin-right: 8px;
}
.archives_auto_div div ,.switch_div div{
    display: inline-block;
    margin-left: 10px;
}
.lui_toolbar_btn_def{
    width: 90px;
    /*height: 30px;*/
    background: #4285F4;
    border-radius: 4px;
}
.lui_toolbar_btn_def.lui_icon_on .lui_toolbar_btn_l{
    background-color: #4285F4;
}
.archives_type_0_info,.archives_type_1{
    margin-top: 25px;
}
