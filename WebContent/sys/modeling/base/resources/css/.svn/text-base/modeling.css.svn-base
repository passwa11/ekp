@charset "UTF-8";

/*
业务建模通用控件css
 */
html {
  overflow-x: hidden;
}
/*==通用框架、样式*/
body {
  background-color: #E8ECEF;
}

body.lui_profile_listview_body{
  background-color: #f6f6f6;
}

ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.clearfix:after {
  content: '';
  display: block;
  clear: both;
}

/*颜色*/
.green_backgroud_color {
  background-color: #55d633;
}

.blue_backgroud_color {
  background-color: #7a9edf;
}

.black_backgroud_color {
  background-color: #818282;
}

.white_backgroud_color {
  background-color: white;
  color: #000;
}

/*小组件*/
.down_arrow {
  transform: scaleX(-1) rotate(45deg);
  /*-ms-transform:scaleX(-1)  rotate(45deg); !* IE 9 *!*/
  /*-webkit-transform:scaleX(-1) rotate(45deg); !* Safari and Chrome *!;*/
  border-right: 1px solid #ffffff;
  border-bottom: 1px solid #ffffff;
  width: 4px;
  height: 4px;
  display: inline-block;
  vertical-align: 2px;
}

.up_arrow {
  transform: scaleX(-1) rotate(-135deg);
  /*-ms-transform:scaleX(-1)  rotate(-135deg); !* IE 9 *!*/
  /*-webkit-transform:scaleX(-1) rotate(-135deg); !* Safari and Chrome *!;*/
  border-right: 1px solid #ffffff;
  border-bottom: 1px solid #ffffff;
  width: 4px;
  height: 4px;
  display: inline-block;
  vertical-align: 2px;
}

.toolbar-bottom {
  width: 100%;
  height: 55px;
  background-color: #ffffff;
  line-height: 70px;
  border-top: 1px solid #eeeeee;
  position: fixed;
  bottom: 0px;
  left: 0px;
  z-index: 99;
}

.toolbar-bottom .lui_widget_btn {
  float: right;
  margin-top: 15px;
  margin-right: 15px;
}

.toolbar-bottom .lui_toolbar_btn_l {
  width: 90px;
  line-height: 30px;
  height: 30px;
}

.modeling_form_table input {
  border: none;
  border-bottom: 1px solid #C1C1C1;
  vertical-align: middle;
  margin-right: 6px;
}

label.lui-lbpm-radio {
  margin-right: 24px;
}

/*框架*/
.lui_modeling {
  background-color: #f6f6f6;
  height: 100%;
  padding: 10px 10px 0px 10px;
  position: relative;
}

.lui_modeling_index {
  background: #f7f7f7;
}

.lui_modeling_main {
  padding-top: 10px;
  /*height: 100%;*/
  min-height: 100%;
  border: 1px solid #dddddd;
  background-color: #FFFFFF;
}

.lui_modeling_iframe_body {
  width: 100%;
  height: 100%;
}

.lui_list_body_frame {
  padding: 0;
}

.lui_list_content_padding {
  background-color: #fff;
  position: fixed;
  top: 10px;
  bottom: 0px;
  left: 10px;
  right: 10px;
  margin: 10px;
  background-color: #fff;
  overflow-y: scroll;
}

/*============= 应用/表单首页框架 start================*/

/* 页眉 */
.lui_modeling_index .model-header {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  height: 50px;
  line-height: 50px;
  background: #101627;
  z-index: 58;
  padding-right: 20px;
  padding-left: 10px;
}

.model-header-left {
  float: left;
}

.model-header-origin {
  color: #fff;
  display: inline-block;
}

.model-header-origin:hover {
  color: #4285F4;
}

.model-header-nav {
  margin-left: 10px;
  color: #fff;
  display: inline-block;
  cursor: pointer;
  position: relative;
}

.model-header-nav:before {
  content: '';
  transform: scaleY(1) rotate(45deg);
  /*-ms-transform:scaleX(-1)  rotate(45deg); !* IE 9 *!*/
  /*-webkit-transform:scaleX(-1) rotate(45deg); !* Safari and Chrome *!;*/
  border-right: 2px solid #ffffff;
  border-top: 2px solid #ffffff;
  width: 6px;
  height: 6px;
  display: inline-block;
  margin-right: 12px;
  vertical-align: 2px;
  cursor: text;
}

.model-header-nav:after {
  content: '';
  display: inline-block;
  border-width: 5px;
  margin-left: 5px;
  border-style: solid;
  border-color: #fff transparent transparent transparent;
  vertical-align: -3px;
}

.model-header-origin i {
  padding-right: 7px;
}

.model-header-operation {
  float: right;
  font-size: 14px;
  color: #FFFFFF;
  background: url(../images/appdesign/header/ques.svg) no-repeat left;
  padding-left: 20px;
}

 .oprationHelp {
    cursor: pointer;
    float:right;
    font-size: 14px;
    padding-right:10px;
    padding-left: 20px;
    background: url(../images/appdesign/header/ques_black.svg) no-repeat left;
 }

.model-ques {
  font-size: 14px;
  background: url(../images/appdesign/header/ques_black.svg) no-repeat left;
  padding-left: 20px;
}

/*选择表单*/
.model_select_box {
  display: none;
  position: absolute;
  background-color: #101627;
  color: #fff;
  left: 5px;
  top: 48px;
  transition: all 1s;
  box-shadow: 0 1px 12px 0 rgba(0, 0, 0, 0.25);
}

.model-header-nav:hover .model_select_box {
  display: block;
}

.model-header-nav:hover .model_select_selected {
  color: #4285F4;
}

.model-header-nav:hover:after {
  border-color: transparent transparent #4285F4 transparent;
  vertical-align: 3px;
}

.model_select_box ul {
  list-style: none;
  padding: 8px 26px;
  background: #fff;
  max-height: 240px;
  overflow-y: scroll;
}

.model-sidebar-box .model-sidebar {
  overflow-y: auto;
  /*兼容firefox**/
  scrollbar-color: #21283B #21283B;
  scrollbar-width: thin;
  /*兼容ie**/
  -ms-scrollbar-base-color:#21283B;
  -ms-scrollbar-arrow-color:#21283B;
  -ms-scrollbar-track-color:#21283B;
  -ms-scrollbar-shadow-color:#21283B;
}

.model-sidebar-box.change .model-sidebar {
  overflow: visible;
}

.model-sidebar-mask {
  position: fixed;
  left: 172px;
  top: 50px;
  bottom: 0;
  width: 8px;
  background: #21283B;
  z-index: 9;
  transition: all .3s;
}

.model-sidebar-mask:before {
  content: '';
  height: 60px;
  width: 8px;
  background: #28324C;
  position: absolute;
  bottom: 0;
  right: 0;
}

.model-sidebar-box.change .model-sidebar-mask {
  left: 42px;
  z-index: 0;
}

.model-sidebar-box.change .model-sidebar-mask:before {
  height: 120px;
}

.model-sidebar-item-box {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.item-expansion-child .model-sidebar-item {
  padding-left: 20px;
}

.model_select_box ul li {
  font-size: 14px;
  color: #333;
  height: 40px;
  line-height: 40px;
  width: 60px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  transition: all .3s;
}

.model_select_box ul li:hover {
  color: #4285F4;
}

.shrink .this-sidebar-item>div {
  background: #1C2235;
}

.shrink .this-sidebar-item>div>i {
  color: #4285F4;
}

/* 展开*/
.model-item-expansion .model-sidebar-desc:after {
  content: '';
  display: inline-block;
  border-width: 5px;
  margin-left: 5px;
  border-style: solid;
  border-color: #fff transparent transparent transparent;
  vertical-align: -3px;
  width:0;
  height:0;
}

.item-expansion .model-sidebar-desc:after {
  content: '';
  border-color: transparent transparent #fff transparent;
  vertical-align: 3px;
}

.model-sidebar-child-item .model-sidebar-desc:after {
  content: '';
  border-color: transparent !important;
  border: none !important;
}

.model-sidebar-item-box:hover .model-sidebar-desc {
  color: #4285f4;
}

.model-sidebar-item-box:hover i {
  color: #4285f4;
}

.model-sidebar-item-box i {
  transition: all .3s;
}

/* .item-expansion .item-expansion-child .model-sidebar-desc{
    color: #FFFFFF;
} */
.item-expansion-child .model-sidebar-child-item:hover .model-sidebar-desc {
  color: #4285F4;
}

.aside_selected>.model-sidebar-item-box .model-sidebar-desc {
  color: #4285f4;
}

.aside_selected {
  background: #1C2235;
}

.aside_selected>.model-sidebar-item-box>i {
  color: #4285F4;
}

.model-sidebar-item-box .model-sidebar-desc {
  transition: all .3s;
}

.model-sidebar-item-box:hover .model-sidebar-desc:after,
.aside_selected.model-item-expansion .model-sidebar-desc:after {
  border-color: #4285F4 transparent transparent transparent;
  vertical-align: -3px;
}

.item-expansion .model-sidebar-item-box:hover .model-sidebar-desc:after,
.aside_selected.item-expansion .model-sidebar-desc:after {
  border-color: transparent transparent #4285F4 transparent;
  vertical-align: 3px;
}

