.shrink .item-expansion .model-sidebar-desc:after {
  content: '';
  border-color: #fff transparent transparent transparent;
  vertical-align: -3px;
}

ul.item-expansion-child {
  display: block;
  text-indent: 36px;
}

.shrink ul.item-expansion-child {
  /* text-indent: 0; */
}

.shrink .model-sidebar-wrap-child {
  display: none;
  position: absolute;
  padding-left: 11px;
  top: 0px;
  left: 50px;
}

.shrink .model-sidebar-wrap-child .model-sidebar-child-item {
  padding: 0 20px;
  background: #21283B;
}

.shrink .model-sidebar-wrap>.model-sidebar-item:hover .model-sidebar-wrap-child {
  display: block;
}

.shrink .model-sidebar-wrap-child:before {
  content: "";
  width: 0;
  height: 0;
  border: 6px solid transparent;
  border-right-color: #21283B;
  position: absolute;
  left: 0px;
  top: 20px;
}

.shrink ul.item-shrink-expansion-child {
  display: block;
  position: absolute;
}

.model-sidebar-wrap-child {
  display: none;
  font-size: 14px;
}

/* 侧栏 */
.model-sidebar {
  margin-top: 50px;
  padding-bottom: 40px;
  width: 180px;
  background-color: #21283B;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  z-index: 1;
  transition: width .3s;
}

.model-sidebar.shrink {
  width: 50px;
}

.model-sidebar-icon {
  position: absolute;
  top: 15px;
  left: 20px;
  width: 20px;
  height: 20px;
}

.shrink .model-sidebar-icon {
  left: 15px;
}

.model-sidebar-item {
  line-height: 50px;
  font-size: 14px;
  color: #FFFFFF;
  text-indent: 20px;
  white-space: nowrap;
  position: relative;
  transition: all .3s;
  cursor: pointer;
}

.model-sidebar-item i {
  margin-right: 10px;
}


.model-sidebar-flag {
  display: none;
  position: absolute;
  left: 60px;
  top: 7px;
  white-space: nowrap;
  padding: 5px 6px;
  background: #071E44;
  border-radius: 4px;
  line-height: 1.5em;
}

.model-sidebar-opt {
  width: 180px;
  height: 20px;
  padding: 20px 0;
  background: #28324C;
  position: fixed;
  bottom: 0;
  left: 0;
  transition: width 0.3s;
  z-index: 5
}

.model-sidebar-back {
  cursor: pointer;
  float: left;
  position: relative;
  font-size: 14px;
  color: #FFFFFF;
  padding-left: 20px;
  transition: color .3s;
}

.model-sidebar-back i {
  margin-right: 10px;
}

.model-sidebar-back:hover,
.model-sidebar-back:hover i {
  color: #4285F4;
}

.model-sidebar-back:after {
  content: '';
  width: 1px;
  height: 30px;
  background: #333C53;
  position: absolute;
  right: -20px;
  top: -5px;
}

.model-sidebar-btn {
  float: right;
  color: #fff;
  width: 20px;
  height: 20px;
  transition: color 0.3s;
  margin-right: 20px;
  cursor: pointer;
}

.model-sidebar-btn:hover {
  color: #4285F4;
}

.shrink .model-sidebar-item:hover .model-sidebar-flag {
  display: block;
}

.shrink .model-sidebar-opt {
  width: 50px;
  padding: 0;
  height: 120px;
}

.shrink .model-sidebar-opt .model-sidebar-back {
  width: 50px;
  padding-left: 0;
  font-size: 0;
  height: 60px;
  text-align: center;
}

.shrink .model-sidebar-opt .model-sidebar-back:after {
  width: 30px;
  height: 1px;
  bottom: 0;
  left: 10px;
  top: auto;
}

.shrink .model-sidebar-opt .model-sidebar-back i {
  line-height: 60px;
  margin-right: 0;
}

.shrink .model-sidebar-opt .model-sidebar-btn {
  margin-top: 20px;
  margin-right: 16px;
  transform: rotate(180deg);
}

.shrink .model-sidebar-desc {
  display: none;
}

.shrink .model-sidebar-wrap-child .model-sidebar-desc {
  display: block;
  text-indent: 4px;
}

.shrink .model-sidebar-wrap-child i {
  display: none;
}

.shrink .model-sidebar-flag:before {
  content: "";
  width: 0;
  height: 0;
  border: 6px solid transparent;
  border-right-color: #071E44;
  position: absolute;
  left: -12px;
  top: 10px;
}

.app_index_sidebar.shrink .model-sidebar-opt {
  height: 60px;
}

/* 内容区 */

.model-body-iframe {
  margin-top: 50px;
  margin-left: 180px;
  transition: all .3s;
  position: fixed;
  top: 0px;
  right: 0px;
  left: 0px;
  bottom: 0px;
}

.model-body-iframe.spread {
  margin-left: 50px;
}

.model-body-wrap-iframe {
  /*padding-top: 10px;*/
  /*padding-left: 10px;*/
  /*padding-right: 10px;*/
  height: 100%;
}

.model-body-iframe iframe {
  border: 0;
  width: 100%;
  height: 100%;
}
.lui_app_moduleMain_frame{
  min-height:550px
}
