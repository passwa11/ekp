/*********** 移动首页设置的模板 start **************/
/****************遮罩层要在最顶层，样式需要添加在顶层********************/
.model-mask {
  z-index: 20;
  opacity: 0;
}

.model-mask.active {
  opacity: 1;
}

.model-mask-bg {
  position: fixed;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 998;
}

.model-mask-panel {
  width: 530px;
  background: #fff;
  margin: 0 auto;
  position: relative;
  z-index: 20;
}

.model-mask-panel-edit {
  padding: 20px;
}

.model-mask-panel-edit-title {
  margin-bottom: 20px;
}

.model-mask-panel-edit-title p {
  font-size: 14px;
  color: #666666;
  margin: 0;
}

.model-mask-panel-edit-content {
  height: 120px;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
  padding: 8px;
  background: url(../images/panel/edit.png) no-repeat center;
  background-size: 82px;
  transition: border-color 0.3s;
}

.model-mask-panel-edit-content textarea {
  opacity: 0;
  filter: alpha(opacity=0);
  width: 100%;
  height: 100%;
  padding: 0;
  font-size: 12px;
  color: #333333;
  outline: none;
  resize: none;
  border: 0;
}

.model-mask-panel-edit-content.active {
  border-color: #4285f4;
}

.model-mask-panel-edit-content.active textarea {
  opacity: 1;
  filter: alpha(opacity=100);
}

.model-mask-panel-flag {
  padding-bottom: 20px;
  border-bottom: 1px solid #eeeeee;
}

.model-mask-panel-flag-title {
  font-size: 14px;
  color: #666666;
  padding-left: 20px;
  margin-bottom: 10px;
}

.model-mask-panel-flag-content {
  height: 240px;
  padding: 15px 25px;
  background: #F5F6FA;
  overflow-y: auto;
}

.model-mask-panel-flag-content .model-mask-flag-item {
  float: left;
  width: 20%;
}

.model-mask-panel-flag-content .model-mask-flag-item div {
  cursor: pointer;
  margin: 5px;
  padding: 0 8px;
  border-radius: 2px;
  background: #fff;
  height: 32px;
  border: 1px solid #DDDDDD;
  transition: all 0.3s;
}

.model-mask-panel-flag-content .model-mask-flag-item div p {
  line-height: 32px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 12px;
  color: #666666;
  margin: 0;
}

.model-mask-panel-flag-content .model-mask-flag-item div:hover {
  color: #4285f4;
  border-color: #4285f4;
}

