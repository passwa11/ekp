 html{
    line-height: 1.15;
}
.listview {
    padding-bottom: 0px;
}
.div.lui_dialog_common_buttons {
    text-align: right;
}
.com_gototop{
    display: none;
}
.topline{
    border-bottom: 0 !important;
}

.toolbar-bottom {
    width: 100%;
    height: 70px;
    background-color: #ffffff;
    line-height: 70px;
    border-top: 1px solid #eeeeee;
    position: fixed;
    bottom: 0px;
    left: 0px;
}
.toolbar-bottom .lui_widget_btns {
    float: right;
    margin-top: 20px;
    margin-right: 5px;
    line-height: 30px;
    height: 30px;
}
.toolbar-bottom .lui_widget_btn {
    margin-right: 15px;
}
.toolbar-bottom .lui_left_text {
    float: left;
    margin-top: 20px;
    margin-left: 20px;
    line-height: 28px;
}

.model-mask-panel .model-list {
    padding: 20px 24px 0px 24px;
}
.model-mask-panel .model-list.app .model-list-content li {
    padding: 20px 30px;
    height: 120px;
    box-sizing: border-box;
}
.model-mask-panel .model-list-title {
    margin-top: 0;
    margin-bottom: 20px;
}
.model-mask-panel .model-list-content {
    height: 380px;
    overflow: scroll;
}
.model-mask-panel .model-list-content li {
    border: 1px solid #EEEEEE;
    border-radius: 2px;
    transition: all 0.3s;
}
.model-mask-panel .model-list-content li.active {
    border-color: #4285f4;
}
.model-mask-panel .model-list-content li.active .model-list-content-radio {
    background: url(../images/listview/icon-radio-active.png) no-repeat center;
    background: url(../images/listview/icon-radio-active@2x.png) no-repeat center;
    background-size: contain;
}
.model-mask-panel .model-list-content-item {
    overflow: hidden;
    position: relative;
}
.model-mask-panel .model-list-content-radio {
    position: absolute;
    top: 38px;
    margin-top: -7px;
    left: 0;
    width: 14px;
    height: 14px;
    background: url(../images/listview/icon-radio.png);
    background: url(../images/listview/icon-radio@2x.png);
    background-size: contain;
    transition: all 0.3s;
}
.model-mask-panel .model-list-content-create {
    cursor: pointer;
    position: absolute;
    top: 37px;
    margin-top: -7px;
    right: 0;
    width: 18px;
    height: 18px;
    background: url(../images/listview/icon-edit.png);
    background: url(../images/listview/icon-edit@2x.png);
    background-size: contain;
}
.model-mask-panel .model-list-content-create:hover{
    background: url(../images/listview/icon-edit_hover.png);
    background: url(../images/listview/icon-edit_hover@2x.png);
    background-size: contain;
}
.model-mask-panel .model-list-content-desc {
    margin-left: 32px;
}
.model-mask-panel .model-list-content-desc > p {
    font-size: 16px;
    color: #999999;
    margin: 0;
    margin-bottom: 10px;
}
.model-mask-panel .model-list-content-desc-title {
    overflow: hidden;
    color: #333333;
}
.model-mask-panel .model-list-content-desc-title p {
    margin: 2px 10px 14px 0;
    font-size: 16px;
    float: left;
}
.model-mask-panel .model-list-content-desc-title span {
    font-size: 12px;
    color: #4285f4;
    border: 1px solid #4285f4;
    border-radius: 12px;
    float: left;
    padding: 3px;
}
.model-mask-panel .model-list-content-desc-title div {
    background: url(../images/listview/icon-version-new@2x.png) left center / contain no-repeat;
    margin: 2px;
    width: 36px;
    height: 16px;
    display: inline-block;
}

.model-mask-panel .model-radios {
  /*  height: 275px;
    overflow: auto;*/
}
.model-mask-panel .model-radios-item {
    padding: 30px 0;
    margin: 0 20px;
    border-top: 1px solid #eeeeee;
}
.model-mask-panel .model-radios-item:first-child {
    border-top: 0;
}
.model-mask-panel .model-radios-title {
    font-size: 14px;
    color: #333333;
    margin: 0;
    margin-bottom: 20px;
}
.model-mask-panel .model-radios-opt-item {
    overflow: hidden;
    margin-top: 20px;
}
.model-mask-panel .model-radios-opt-item.active i {
    background: url(../images/listview/icon-radio-active@2x.png) no-repeat center;
    background: url(../images/listview/icon-radio-active.png) no-repeat center \9;
    background-size: contain;
}
.model-mask-panel .model-radios-opt-item:first-child {
    margin-top: 0;
}
.model-mask-panel .model-radios-opt-item div {
    width: 14px;
    height: 14px;
    float: left;
    margin-right: 6px;
    position: relative;
    margin-top: 1px;
}
.model-mask-panel .model-radios-opt-item div input {
    visibility: hidden;
}
.model-mask-panel .model-radios-opt-item div i {
    width: 14px;
    height: 14px;
    position: absolute;
    left: 0;
    top: 0;
    background: url(../images/listview/icon-radio@2x.png);
    background: url(../images/listview/icon-radio.png) \9;
    background-size: contain;
}
.model-mask-panel .model-radios-opt-item p {
    margin: 0;
    font-size: 14px;
    color: #333333;
    line-height: 1.15;
}
.top-tip-content{
    height: 40px;
    position: fixed;
    top: 10px;
    left: 2%;
    width: 90%;
    z-index: 99;
    background: #FFF8F2;
    border: 1px solid rgba(255,148,62,1);
    border-radius: 4px;
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #FF943E;
    line-height: 40px;
}
.top-tips{
    background: url(../images/icon-orange.png) no-repeat center;
    display: inline-block;
    width: 30px;
    height: 15px;
    background-size: contain;
    cursor: pointer;
    margin-bottom: -2px;
    margin-left: 10px;
    position: absolute;
    top: 13px;
}
.del-tips{
    background: url(../images/button-del.png) no-repeat center;
    display: inline-block;
    width: 9px;
    height: 9px;
    background-size: contain;
    cursor: pointer;
    position: absolute;
    top: 15px;
    left: 447px;
}
 .bottom-tips{
     background: url(../images/icon-grey.png) no-repeat center;
     display: inline-block;
     width: 28px;
     height: 15px;
     background-size: contain;
     cursor: pointer;
     margin-bottom: -2px;
     margin-left: 10px;
 }
 .precaution{
     float: left;
     width: 250px;
     height: 17px;
     font-family: PingFangSC-Regular;
     font-size: 12px;
     color: #999999;
     font-weight: 400;
 }