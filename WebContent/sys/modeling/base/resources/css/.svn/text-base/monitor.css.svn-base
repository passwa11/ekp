@charset "UTF-8";

/*
业务建模通用控件css
 */
/*==通用框架、样式*/
body {
  background-color: transparent;
}

body.lui_profile_listview_body{
  background-color: #f6f6f6;
}

ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.clearfix:after {
  content: '';
  display: block;
  clear: both;
}

/*颜色*/
.green_backgroud_color {
  background-color: #55d633;
}

.blue_backgroud_color {
  background-color: #7a9edf;
}

.black_backgroud_color {
  background-color: #818282;
}

.white_backgroud_color {
  background-color: white;
  color: #000;
}

.monitorContainer{
  background: #FFFFFF;
  border: 1px solid #DDDDDD;
  box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.06);
  padding: 16px;
  min-width: 976px;
}
.monitorTitle{
  padding-left: 12px;
  position: relative;
  margin-bottom: 11px;
  margin-top: 11px;
  font-size: 18px;
  color: #000000;
  line-height: 28px;
  font-weight: 600;
}
.monitorStatistics{
  height: 140px;
  min-width: 1058px;
}
.monitorStatisticsApp{
  border: 1px solid #EEEEEE;
  border-radius: 4px;
  height: 140px;
  display: inline-block;
}

.monitorStatisticsForm{
  height: 140px;
  border: 1px solid #EEEEEE;
  border-radius: 4px;
  display: inline-block;
}
.monitorStatisticsAppTitle{
  height: 48px;
  border-radius: 44px;
}
.monitorStatisticsAppTitle>div{
  font-size: 16px;
  color: #333333;
  line-height: 24px;
  font-weight: 500;
  padding: 12px 0 12px 16px;
}
.monitorStatisticsAppList{
  padding-top: 10px;
}
.monitorStatisticsAppList>li{
  float: left;
  width: calc(20% - 1px);
  position: relative;
}
.monitorStatisticsAppList>li>a{
  display: inline-block;
  width: 84px;
  border-radius: 4px;
  padding-left: 20px;
  padding-bottom: 26px;
  cursor:default;
}
.monitorStatisticsAppList>li>a>p{
  text-align: left;
  letter-spacing: 0;
  line-height: 26px;
  font-weight: 700;
  font-size: 26px;
}
.monitorStatisticsAppTotal>a>p{
  color: #4285F4;
}
.monitorStatisticsAppPublish>a>p{
  color: #00C980;
}
.monitorStatisticsAppNotPublish>a>p{
  color: #00C980;
}
.monitorStatisticsAppSelfBuild>a>p{
  color: #FF943E;
}
.monitorStatisticsAppInstall>a>p{
  color: #FF943E;
}
.monitorStatisticsAppPublish::before{
  content: '';
  border-left: 1px solid #F2F2F2;
  height: 40px;
  top:6px;
  position: absolute;
}
.monitorStatisticsAppSelfBuild::before{
  content: '';
  border-left: 1px solid #F2F2F2;
  height: 40px;
  top:6px;
  position: absolute;
}
.monitorStatisticsAppList>li>a>span{
  display: block;
  text-align: left;
  padding-top: 4px;
  font-size: 14px;
  color: #666666;
  font-weight: 400;
}
.monitorStatisticsFormTitle{
  height: 48px;
  border-radius: 44px;
}
.monitorStatisticsFormTitle>div{
  font-size: 16px;
  color: #333333;
  line-height: 24px;
  font-weight: 500;
  padding: 12px 0 12px 16px;
}
.monitorStatisticsFormList>li{
  float: left;
  width: 29%;
  background: #F8F8F8;
  margin-left: 20px;
}
.monitorStatisticsFormList>li>a{
  display: inline-block;
  width: 84px;
  padding-left: 20px;
  padding-bottom: 12px;
  border-radius: 2px;
  cursor: default;
}
.monitorStatisticsFormList>li>a>p{
  text-align: left;
  letter-spacing: 0;
  line-height: 26px;
  font-weight: 700;
  font-size: 26px;
  color: #333333;
  padding-top: 10px;
  padding-bottom: 4px;
}
.monitorStatisticsFormList>li>a>span{
  text-align: left;
  font-size: 14px;
  color: #666666;
  font-weight: 400;
}

.monitorChart{
  margin-top: 16px;
  margin-bottom: 16px;
  min-width: 1058px;
}
.monitorChartTriggerExecQueue{
  height: 334px;
  border: 1px solid #EEEEEE;
  border-radius: 4px;
  min-width: 514px;
  display: inline-block;
  float: left;
}
.monitorChartTriggerExecQueueContent{
  height: 304px;
  width: 100%;
  display: inline-block;
}
.monitorChartTriggerExecQueueFoot{
  height: 24px;
  width: 100%;
  display: inline-block;
}

.monitorChartTriggerExecTime{
  position: relative;
  height: 334px;
  border-radius: 4px;
  min-width: 514px;
  border: 1px solid #EEEEEE;
  display: inline-block;
}

.monitorChartTriggerExecTimeContent{
  width: 100%;
  height: 304px;
  border-radius: 4px;
  display: inline-block;
}

.monitorChartTriggerExecTimeFoot{
  height: 24px;
  width: 100%;
  line-height: 24px;
  font-weight: 100;
  color: #999999;
  display: inline-block;
  text-align: left;
  font-size: 12px;
  margin-left: 14px;
}

.monitorChartButtons {
  position: absolute;
  width: 310px;
  z-index: 2;
  right: 0;
  top: 9px;
}

.monitorChartButtons > div{
  border: 1px solid #DDDDDD;
  width: 80px;
  display: inline-block;
  font-size: 14px;
  color: #333333;
  line-height: 30px;
  font-weight: 400;
  border-radius: 2px;
  height: 30px;
}

.monitorChartButtons div select{
  color: #333;
}

.monitorLog{
  width: 100%;
  background: #FFFFFF;
  border: 1px solid #EEEEEE;
  border-radius: 4px;
}

.triggerExecTimeTotal{
  margin-right: 10px;
  text-align: center;
}

.triggerExecTimeAverage{
  margin-right: -4px;
  text-align: center;
}
.monitorLogView a{
  font-size: 14px;
  color: #4285F4;
  line-height: 20px;
  font-weight: 400;
}

.monitorLogStatus .success{
  display: inline-block;
  border-radius: 6px;
  height: 6px;
  width: 6px;
  background-color: #00C980;
}
.monitorLogStatus .fail{
  display: inline-block;
  border-radius: 6px;
  height: 6px;
  width: 6px;
  background-color: #F95A5A;
}

.monitorLogStatus .doing{
  display: inline-block;
  border-radius: 6px;
  height: 6px;
  width: 6px;
  background-color: #999999;
}

.monitorLogStatus .suspend{
  display: inline-block;
  border-radius: 6px;
  height: 6px;
  width: 6px;
  background-color: #FF943E;
}

.monitorStatisticsApp,.monitorChartTriggerExecQueue{
  margin-right: 8px;
}
.monitorStatisticsForm{
  margin-left: 8px;
}
.monitorChartTriggerExecTime{
  margin-left: 12px;
}
.monitorStatisticsApp,.monitorStatisticsForm,.monitorChartTriggerExecTime,.monitorChartTriggerExecQueue{
  width: calc(50% - 13px);
  min-width: 514px;
  max-width: 2656px;
}

.tooltipName{
  font-size: 14px;
  color: #333333;
  font-weight: 500;
}

.tooltipExecTime{
  display:inline-block;
}

.tooltipExecTime span{
  display:inline-block;
  margin-right:5px;
  border-radius:1px;
  width:8px;
  height:8px;
  background-color:#4878FA;
}

.tooltipExecTimeSpan{
  float: left;
}

.tooltipExecTimeContent{
  display:inline-block;
  font-size: 12px;
  color: #333333;
  font-weight: 400;
}

.triggerExecTimeSelect{
  border: 0px;
  margin-right: 26px;
  text-align: left;
}

.triggerExecTimeSelectInput{
  width: 100px;
  background: #FFFFFF;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
}

.triggerExecTimeSelectInputText{
  float: left;
  margin-left: 8px;
  width: 70px;
}

.triggerExecTimeSelectIcon{
  background: url(../images/icon-panel_down@2x.png);
  height: 14px;
  width: 14px;
  background-size: 14px;
  display: inline-block;
}

.triggerExecTimeSelectList{
  display: none;
  position: relative;
  width: 100px;
  z-index: 2;
  top: 0;
  float: right;
  left: 22px;
  border: 1px solid #DDDDDD;
  background: #FFFFFF;
}

.triggerExecTimeSelectList span{
    display: inline-block;
    width: 100%;
    padding-left: 8px;
}

.triggerExecTimeSelectList div:hover{
  background: #D7D7D7;
}

.xAxisTip{
  display: none;
  position: absolute;
  padding: 5px 5px;
  font-size: 12px;
  line-height: 18px;
  color: #575757;
  background: #FFFFFF;
  box-shadow: 0 1px 4px 0 rgba(0,0,0,0.20);
  border-radius: 4px;
}

.criterion-value .lui_criteria_date_span, .criteria-popup-value .lui_criteria_date_span{
  margin-top: 1px !important;
}