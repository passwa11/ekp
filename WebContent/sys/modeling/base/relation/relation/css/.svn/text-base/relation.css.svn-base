@charset "UTF-8";

.upBtn {
    cursor: pointer;
    width: 21px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/up-grew.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.downBtn {
    cursor: pointer;
    width: 21px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/down-grew.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.relation_addLine {
    cursor: pointer;
    width: 20px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/add.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.relation_delLine {
    cursor: pointer;
    width: 21px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/quit-grew.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.relation_editLine {
    cursor: pointer;
    width: 20px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/edit.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.table_relation td,
.table_relation {
    border: none !important;
}

.table_relation tr {
    border-top: none !important;
    border-left: none !important;
    border-right: none !important;
}

.table_relation select,
.table_relation input {
    border-top: none !important;
    border-left: none !important;
    border-right: none !important;
    border-bottom: 1px solid #000 !important;
}

#fdInWhereTable .table_opera {
    color: rgba(0, 0, 0, 0);
    cursor: pointer;
    width: 20px;
    height: 25px;
    display: inline-block;
    background: url(../../../resources/images/delete.png);
    background-repeat: no-repeat;
    background-position: center center;
    float: right;
    margin-top: 7px;
    margin-right: 10px;
}

.paramSelect {
    cursor: pointer;
    color: #fff;
    background-color: #39c;
    padding: 3px 8px;
    margin-left: 5px;
    font-size: 12px;
    line-height: 20px;
}

.red_btn {
    cursor: pointer;
    float: right;
    padding: 3px;
    background-color: #fe4323;
    color: #fff;
    border-radius: 5px;
    font-size: 14px;
}

/**

*/
#viewSetModel {
    display: inline-block;
    min-width: 100px;
    padding: 0px 10px;
    border-top: none !important;
    border-left: none !important;
    border-right: none !important;
    border-bottom: 1px solid #000 !important;
    border-top: none !important;
}

#relationSetMain {
    background-color: #ffffff;
    border-collapse: collapse;
    border: 1px #d2d2d2 solid;
    padding: 8px !important;
    text-align: left;
    margin: 0 auto;
}

/*left*/
#relationSetMain #relationNav {
    width: 280px;
    background-color: #f9f9f9;
}

#relationSetMain #relationMain {
    width: 280px;
    background-color: #f6f6f6;
}

#relationNav .relation_delLine {
    width: 19px;
    height: 19px;
    background: url(./del.png);
}

#relationNav .relation_addLine {
    width: 19px;
    height: 19px;
    background: url(./add_white.png);
}

#relationNav .relation_editLine {
    width: 19px;
    height: 19px;
    background: url(./edit.png);
}

#relationNav .relationNavTitle {
    padding: 2px 10px;
    height: 30px;
    line-height: 30px;
    width: 100%;
    font-size: 18px;
    text-align: center;
    padding: 8px 0;
    background-color: #3e9ece;
    color: #fff;
}

#relationNav .relationNavItem ul li {
    list-style: none;
    line-height: 40px;
    min-height: 40px;
    padding-left: 10px;
    padding-right: 10px;
    margin-top: 5px;
}

#relationNav .relationNavItem ul li.selected,
#relationNav .relationNavItem ul li:hover {
    background-color: #99bbca;
    color: #FFFFFF;
}

#relationNav .relationNavItem ul li.selected .relation_delLine,
#relationNav .relationNavItem ul li:hover .relation_delLine {
    background: url(./del_white.png);
}

#relationNav .relationNavItem ul li.selected .relation_editLine,
#relationNav .relationNavItem ul li:hover .relation_editLine {
    background: url(./edit_white.png);
}

#relationNav .relationNavItem ul li .itemTitle {
    float: left;
    width: 150px;
    font-size: 16px;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.template {
    display: none;
}

/*view set */
.viewSetModel .viewSetModel_name {
    width: 100px;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin: 0px 5px;
    padding: 0px 5px;
    display: inline-block;
    vertical-align: bottom;
    border-bottom: 1px solid #99bbca;
}

.vieSetTable tr {
    border-bottom: none !important;
}

/*<%--   临时样式 --%>*/
#dContainer,
#dContainerPut {
    margin: 5px;
    padding: 10px;
    background-color: #E6EFC2;
    height: 260px;
}

#dContainer ul li {
    float: left;
    width: 200px;
    height: 60px;
    line-height: 60px;
    background-color: #439ce1;
    margin: 5px;
    text-align: center;
    cursor: pointer;
}

#dContainer ul li.disDrag {
    background-color: #d3d3d3;
    cursor: no-drop;
}

#graphContainer {
    width: 700px;
    height: 100%;
    position: relative;
    min-height: 600px;
    margin: auto;
}

.model-rightbar-model-item {
    display: inline-block;
    width: 52px;
    height: 74px;
    margin: 20px;
    cursor: pointer;
    color: #999;
}

.model-rightbar-model-item i {
    background: #F6F7FA;
    display: block;
    line-height: 52px;
    text-align: center;
    font-size: 40px;
    width: 52px;
    height: 52px;
    border-radius: 8px;
    margin-bottom: 6px;
}

.model-rightbar-model-item:hover .lui_iconfont_nav_kms_homework:before {
    color: #333;
}

.model-rightbar-model-item p {
    font-size: 12px;
    color: #666666;
    text-align: center;
    margin-left: -10px;
    width: 135% !important;
}

.model-rightbar-model-item .textEllipsis{
    margin-left: 2px;
}

.relation-right-table-tr td {
    font-size: 12px;
}

.relation-operation span {
    display: inline-block;
    cursor: pointer;
    padding: 0 6px;
    font-size: 12px;
    color: #4285F4;
}

.relation-operation span:first-child {
    border-right: 1px solid #D9D9D9;
}

.relation-operation span:hover {}

/* 搜索覆写 */
.lui_profile_listview_searchWrap {
    width: 224px;
    background: #fff;
    margin-right: 28px;
    position: relative;
    float: right;
    vertical-align: middle;
    position: relative;
    margin: 0 0 0 20px !important;
}

.lui_profile_search_input {
    display: inline-block;
    border: 1px solid #dbdbdb;
    border-radius: 2px;
    font-size: 12px;
    padding: 8px 10px;
    padding-left: 8px;
    width: 200px;
    line-height: 12px;
    -webkit-appearance: textfield;
    outline: none;
}

.lui_icon_s_icon_search {
    position: absolute;
    top: 0px;
    bottom: 0px;
    right: 0px;
    margin: auto 10px;
}

.lui_profile_listview_icon {
    display: inline-block;
    width: 16px;
    height: 16px;
}

/* 搜索覆写 end */
.fullscreen {
    background-color: #E8ECEF;
}

.disDrag.model-rightbar-model-item,
.disDrag.model-rightbar-model-item p {
    color: #ccc;
    cursor: no-drop;
}

.disDrag.model-rightbar-model-item i {
    background-color: #fff;
}

/*覆写输入框*/
td div.modeling_data_select.inputselectsgl {
    width: 188px;
    margin-bottom: 0px;
    border:none;
    border-bottom: 1px solid #d9d9d9;
}

.model-body-desc{
    width: 260px;
    height: 80px;
    background: #FFFFFF;
    border-radius: 3px;
    margin-left: 14px;
    margin-top: 8px;
}

.model-body-desc .model-body-desc-wrap{
    height: 40px;
    line-height: 40px;
    padding-left: 20px;
}

.model-body-desc .model-body-desc-wrap p{
    float: left;
    margin: 0 8px 0 0;
}
.model-body-desc-wrap .model-body-header-title {
    font-size: 14px;
    color: #000000;
    letter-spacing: 0;
    font-weight: 400;
}
.model-body-desc-wrap .model-body-header-desc {
    font-size: 12px;
    color: #666666;
    font-weight: 400;
    height: 40px;
}

.model-body-desc-wrap .model-body-header-desc span {
    color: #4285F4;
}
.model-body-desc .model-body-desc-wrap{
    position: relative;
}
.model-body-header-desc-tip {
    position: absolute;
    right: -121px;
    top: 25px;
    background: #FFFFFF;
    border: 1px solid #DDDDDD;
    -webkit-box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.1);
    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.1);
    padding: 5px;
    z-index: 99;
     display: none;
    border-radius: 4px;
}
.model-body-header-desc-tip.active{
    display: block;
}
.model-body-header-desc-tip .model-body-desc-wrap{
    font-size: 12px;
    color: #666666;
    font-weight: 400;
    height: 20px;
    line-height: 20px;
    padding: 3px;
}

.model-body-header-desc-tip .model-body-desc-wrap .model-body-header-desc{
    height: 20px;
    line-height: 20px;
}

.model-body-header-desc-tip .model-body-desc-wrap span{
    color: #4285f4;
}
.model-mask-panel-outparam{
    width: 98%;
    display: inline-block;
}