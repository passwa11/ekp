@charset "UTF-8";

body{
	background-color: #FFF!important;
}

.modelTargetNameBox{
	display: inline-block;
	min-width: 150px;
	border-bottom: 1px solid #b4b4b4;
}

.highLight{
	cursor : pointer;
	color : #39c;
}

.field_table{
	
}

.field_table thead td{
	background-color: #f6f6f6;
	padding: 5px;
	text-align: left;
	word-break: break-word;
	border: 1px solid #d2d2d2;
}

.field_table .table_opera{
	cursor:pointer;
	color:#1b83d8;
}

.view_field_tr td{
	text-align: left;
}
.fdMsgKeyTr .inputsgl{
	width: 50%;
}

.beanInputDiv {
	width: 100%;
	border: 0;
	display: inline-block;
	position: relative;
}

.inputSelectItem {
	background: url(../../../../resources/images/show.png);
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 14px;
	border: 1px;
	position: absolute;
	width: 23px;
	height: 100%;
	right: 14px;
	top: 0;
}

.view_pre_query_back_value{
	margin-left: 15px;
	margin-top: 6px;
}
.view_pre_query_back_value>div{
	margin-bottom: 20px;
	float:left;
	width:100%;
	line-height: 32px;
}
.view_field_pre_query_table .table_opera,.view_field_detail_query_table .table_opera{
	margin-left: 5px;
}
.view_field_pre_query_table .model-mask-panel .model-mask-panel-table-show,.view_field_detail_query_table .model-mask-panel .model-mask-panel-table-show{
	margin-left: 0px;
}
.view_field_target_table .validation-advice>table tbody tr td,.view_field_pre_query_table .validation-advice>table tbody tr td{
	border: 0px!important;
}
.view_field_target_table .validation-advice>table tr,.view_field_pre_query_table .validation-advice>table tr{
	height: 20px!important;
}
.model-mask-panel td>div.pre_query_content{
	margin-bottom: 0px;
	display: inline-block;
	width: 144px;
}
.select_view .select_content_view{
	/* position: absolute; */
	left:16px;
	background-color: #FFFFFF;
	border: 1px solid #DDDDDD;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.15);
	border-radius: 0 0 2px 2px;
	border-radius: 0px 0px 2px 2px;
	z-index:9999;
	width: 100%;
	box-sizing: border-box;
}
.select_result_view .status_hover{
	background: #ECF2FD;
}
ul {
	list-style:none;
}
.panel-main .panel-main-right{
	top:40px;
}
.panel-main .panel-main-left{
	overflow-y: auto;
}
.select_search_view{
	border-bottom: 1px solid #eee;
	position: relative;
}
.select_search_view>input{
	padding-left: 9px !important;
	height: 38px !important;
	border:0;
	width: 100%;
	box-sizing: border-box;
}
.select_search_view::after{
	/*content: "";*/
	/*position: absolute;*/
	/*width: 18px;*/
	/*height: 18px;*/
	/*background: url(../../../../resources/images/index-search@2x.png) no-repeat center;*/
	/*background-size: 14px 14px;*/
	/*top: 10px;*/
	/*left: 6px;*/
}
.select_result_view>li{
	font-size: 12px;
	padding: 0 7px;
	height: 30px;
	line-height: 30px;
	position: relative;
}
.select_result_view>li:hover{
	color: #4285F4;
	background-color: rgba(66,133,244,0.10);
}
.showSelect::before{
	content: "";
	position: absolute;
	z-index: 9;
	top: 14px;
	left: 50%;
	transform: translateX(-50%);
	border-width: 8px;
	border-style: solid;
	border-color: transparent transparent #fff transparent;
}
.hide{
	display: none;
}
.selectedItem{
	display:flex;
	display: -webkit-flex;
	align-items:center;
	height: 28px;
	padding-left: 8px;
	position: relative;
}
.selectedItem>label{
	font-size: 14px;
	color: #333333;
	box-sizing: border-box;
	padding: 0px 30px 0px 8px;
	background: #F5F6F7;
	border: 1px solid #EEEEEE;
	border-radius: 2px;
	height: 22px;
	line-height: 22px;
	margin-right: 4px;
	position: relative;
	cursor: pointer;
	word-break: keep-all;
	white-space: nowrap;
	max-width: 160px;
	text-overflow: ellipsis;
	overflow: hidden;
}
.selectedItem>label>i,.dropDownSelectField>i{
	content: '';
	position: absolute;
	right: 6px;
	top: 2px;
	width: 18px;
	height: 18px;
	background: url(../../../../resources/images/views/modeling-close.png) no-repeat center;
	background-size: 9px 9px;
}


.view_pre_query_back_value .inputselectsgl {
	border: 1.5px solid #dfdfdf;
}
.inputselectsgl .input{display:block;border: 0px;position: absolute;top:0px;bottom:0px;left: 0px;right: 23px;cursor: text;line-height: 21px;}

.behaviorCheckBox{
	display: inline-block;
	width: 80px;
}
/* 下拉框 */
.muiPerformanceDropdownBox{
	align-items: center;
	justify-content: space-between;
	height:30px;
	border: 1px solid #DDDDDD;
	background-color: #fff;
	border-radius: 2px;
	position: relative;
	padding:0 8px;
	width: 256px;
	display: inline-flex;
	margin-left: 10px;
}
.muiPerformanceDropdownBox.active{
	background-color: #F6F9FF;
	border: 1px solid #4285F4;
}
.muiPerformanceDropdownBox>span{
	font-size: 12px;
	color: #999999;
}
.muiPerformanceDropdownBox>.muiPerformanceDropdownBoxIcon{
	float: right;
	width: 20px;
	height: 20px;
	background: url(../../../../resources/images/select-down@2x.png) no-repeat center;
	background-size: 12px 8px;
}
.muiPerformanceDropdownBox.active>.muiPerformanceDropdownBoxIcon{
	transform: rotate(180deg);
}
.select_view{
	display: none;
	width: 100%;
	position: absolute;
	top: 100%;
	right:-1px;
	background-color: #fff;
	border: 1px solid #DDDDDD;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.15);
	border-radius: 2px;
	z-index: 11;
	box-sizing: content-box;
	transform: translateY(1px);
	left: 0;
	max-height: 260px;
	overflow-y: auto;
	overflow-x: hidden;
}
.select_view>ul>li{
	display: flex;
	display: -webkit-flex;
	align-items: center;
	height: 30px;
	height:30px;
	font-size: 12px;
	padding:0 6px;
	background-color: #fff;
	cursor: pointer;
	color:#999999;
}
.select_view>ul>li:hover{
	background-color: rgba(66,133,244,0.10);
}

.view_field_pre_query_table tbody tr td{
	position: relative;
}
.view_field_pre_query_table .txtstrong{
	position: absolute;
	top: 19px;
	right: 6px;
}
.behaviorCheckBox{
	float: left;
}
/* 复选框 */
.muiPerformanceManagementCheckbox{
	display: inline-block;
	height:30px;
	line-height: 30px;
	position: relative;
	cursor: pointer;
	vertical-align: middle;
}
.muiPerformanceManagementCheckbox input[type="checkbox"] {
	position: relative;
	left:0px;
	width: 15px;
	height: 15px;
	background-color: #fff;
	-webkit-appearance: none;
	border: 1px solid #c9c9c9;
	border-radius: 2px;
	outline: none;
	margin-right: 4px;
}
.muiPerformanceManagementCheckbox input[type="checkbox"]:checked {
	background: url(../../../../resources/images/check_box.png)
	no-repeat center;
	background-size: 100% 100%;
	border: 0;
}
.muiPerformanceManagementCheckbox span{
	font-size: 14px;
	color: #333333;
	user-select: none;
}
.muiPerformanceManagementCheckbox strong{
	font-size: 12px;
	color: #333333;
	user-select: none;
	font-weight: bold;
}
.muiPerformanceManagementCheckbox input[type="checkbox"]:checked+em{
	color:#4285f4;
}
.muiHrArchivesPreviewBoxLabel{
	float: left;
	margin-right: 10px;
	height:32px;
	line-height: 32px;
	font-size: 14px;
	color: #565656;
}
/* 复选框 END */
.backValueTr{
	display: none;
}
.backValueTr.active{
	display: block;
}

.modeling-viewcover-tip{
	display: inline-block;
	width: 16px;
	height: 16px;
	position: relative;
	background: url(../images/modeling-tips.png) no-repeat center;
	background-size: contain;
	cursor: pointer;
	top: 3px;
	left: 3px;
}
.modeling-viewcover-tip:hover {
	background: url(../images/modeling-tips-hover.png) no-repeat center;
	background-size: contain;
}
.modeling-viewcover-tip:hover>span{
	visibility: visible;
}
.modeling-viewcover-tip>span{
	visibility: hidden;
	width: 630px;
	position: absolute;
	color:#0C0C0C;
	font-size: 12px;
	font-weight: normal;
	border-radius: 2px;
	background-color: #FFFFFF;
	box-shadow: 0 2px 5px 0 rgba(0,0,0,0.1);
	padding:10px;
	z-index: 99;
	bottom: 100%;
	right: -120px;
	transform: translate(55%,110px);
	cursor:default;
	line-height: initial;
	z-index: 10;
}
.detailSelect{
	margin-left: 15px;
}