@charset "utf-8";
html {
  font-size: 62.5%;
  -ms-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
  -webkit-overflow-scrolling : touch;
  overflow-x: hidden;
}
body {
  font-size: 1.6rem;
  overflow-x: hidden;
}
body,
div,
ul,
li,
ol,
h1,
h2,
h3,
h4,
h5,
h6,
input,
textarea,
select,
p,
dl,
dt,
dd,
a,
img,
button,
form,
table,
th,
tr,
td,
tbody,
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  margin: 0;
  padding: 0;
  font-family: "PingFang SC",
  "Lantinghei SC",
  "Helvetica Neue",
  Arial,
  "Microsoft YaHei",
  "WenQuanYi Micro Hei",
  "Heiti SC",
  "Segoe UI",
  sans-serif;
  font-size: 1.4rem;
  box-sizing: border-box;
}
a,
a:link,
a:active,
a:hover,
input {
  outline: 0;
  background: none;
  text-decoration: none;
  -webkit-tap-highlight-color: transparent;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
input,
button,
textarea,
select,
optgroup,
option {
  font-family: inherit;
  font-size: inherit;
  font-style: inherit;
  font-weight: inherit;
  outline: medium;
  outline:none;
  border:none;
  background:none;
}
address,
caption,
cite,
code,
dfn,
em,
strong,
th,
i {
  font-weight: normal;
  font-style: normal;
}
ul,
ol,
li {
  list-style: none;
}
.line-cut {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.lines-cut {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.clearfix:after {
  content: " ";
  display: block;
  visibility: hidden;
  height: 0;
  clear: both;
}
.clearfix {
  zoom: 1;
}
.left{
  float: left;
}
.right{
  float: right;
}
/*声明字体*/
@font-face {
  font-family: DINAlternate-Bold;
  src: url('../font/DIN\ Alternate\ Bold.ttf');
  src:
    url('../font/DIN\ Alternate\ Bold.ttf') format('embedded-opentype'),
    url('../font/DIN\ Alternate\ Bold.ttf') format('woff'),
    url('../font/DIN\ Alternate\ Bold.ttf') format('truetype'),
    url('../font/DIN\ Alternate\ Bold.ttf') format('svg');
  font-weight: normal;
  font-style: normal;
}
* {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-tap-highlight-color: transparent;
}
ul::-webkit-scrollbar {
  display: none;
}
