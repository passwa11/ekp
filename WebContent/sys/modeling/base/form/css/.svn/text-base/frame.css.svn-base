@charset "UTF-8";

/******************** 头部导航样式 start *********************/
.form_head{
	height: 70px;
	line-height: 70px;
	/* margin: 0 auto; */
	position: relative;
}

.form_head .form_head_historyback{
	padding: 0px 15px 0 5px;
	width: 195px;
	height: 100%;
	overflow: hidden;
	/* float: left; */
	background-color: #046eb5;
	cursor: pointer;
}

.form_head_historyback .form_head_back_icon{display: inline-block;background-image: url(../images/back.png);width: 32px;height: 32px;background-repeat: no-repeat;background-position: center;background-size: cover;vertical-align: middle;position: absolute;top: 20px;}

.form_head_historyback .form_head_back_des{
	
display: inline-block;
	
width: 148px;
	
margin-left: 33px;
	
text-overflow: ellipsis;
	
overflow: hidden;
	
white-space: nowrap;
}

.form_head .form_head_operation{
	position: absolute;
	top: 0;
	left: 215px;
	right: 0px;
	overflow: hidden;
	height: 100%;
	/* padding: 0 18px; */
	background-color: #4b75c2;
}

.form_head_operation .form_operation_ul{
	/* text-align : center; */
	/* height: 100%; */
}

.form_head_operation ul li{
	margin: 0px;
	padding: 0px 20px;
	list-style: none;
	display: inline-block;
	cursor: pointer;
	width: 150px;
	font-size: 20px;
	color: rgba(255,255,255,.8);
	border: 1px solid #b0daba;
	text-align: center;
}

.form_head_operation ul li.head_selected{
	background-color: #413cad;
	color: #fff;
}
/******************** 头部导航样式 end *********************/

/******************** 内容主体样式 start *********************/
.form_main{
    position: absolute;
    top: 70px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    overflow-y: hidden;
	
}

.form_main iframe{
	width:100%;
	height:100% !important;
}

/******************** 内容主体样式 end *********************/