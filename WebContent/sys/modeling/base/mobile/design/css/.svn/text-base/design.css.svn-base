}

.designerMainLeft .designerMainLeft-desc {
  margin: 0;
  color: #4285f4;
  font-size: 14px;
  cursor: pointer;
  transition: all 0.3s;
}

.designerMainLeft .designerMainLeft-desc:hover {
  color: #8EB6F8;
}

.mobileDesignerMain .designerMainright,
.mobileDesignerMain .designerMainrightSetting{
  position: absolute;
  right: 0;
  top: 0px;
  bottom: 0;
  width: 500px;
  background: #fff;
  overflow: auto;
}
.mobileDesignerMain .designerMainrightSetting{
  display: none;
}

.designerMainLeft .mobileView {}

.mobileView .model-body-content-phone {
  width: 304px;
  height: 537px;
  margin: 10px auto 0;
  background: url(../images/phone@2x.png) no-repeat center;
  background: url(../images/phone.png) no-repeat center \9;
  background-size: contain;
}

.mobileView .model-body-content-phone-wrap {
  padding: 53px 25px 0px 40px;
}

.mobileView .model-body-content-phone-view {
  width: 93%;
  height: 437px;
  overflow-y: auto;
  background-color: #fff;
}

.mobileView .mobileBlock {
  border: 1px dashed transparent;
  border-radius: 4px;
  cursor: pointer;
  transition: all .3s;
  position: relative;
}

.mobileBlock-mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  transition: background .3s;
}

.mobileView .defaultView_head_notify.mobileBlock:before {
  display: none;
}

.mobileView .mobileBlock:hover .mobileBlock-mask,
.mobileView .mobileBlock.active .mobileBlock-mask {
  background: url(../images/zz.png) repeat center;
}

.mobileView .mobileBlock:before {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  transition: background .3s;
}

.mobileView .mobileBlock:hover:before,
.mobileView .mobileBlock.active:before {
  /* background: url(../images/zz.png) repeat center; */
  background: url(../images/edit-mask.png) repeat center;
}

.mobileView .mobileBlock.active {
  border-color: #4285f4;
}

.mobileView .mobileBlock.warning {
  /* border-color: #FF4444; */
}

.mobileDesignerMain .mobileAttrPanel {
  background-color: #fff;
  /* position: absolute; */
  top: 544px;
  bottom: 0;
  left: 0;
  right: 0;
}

.mobileAttrPanelMain {
  position: absolute;
  top: 52px;
  left: 0;
  right: 0;
  bottom: 5px;
  overflow: auto;
}
