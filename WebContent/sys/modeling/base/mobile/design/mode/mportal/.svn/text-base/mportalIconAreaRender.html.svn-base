if(!data){
	return;
}
var wgt = render.parent;
var lang = wgt.getModelingLang();
var dataValue = data.value || {};
var title = dataValue.title || "未定义";
data.info = data.info || {};
var dataValidate = data.info.validate || {};
{$
	<div class="field_item_value multi-list-block">
		<div class='content_name'>
			<p class="form_item_title">{%title%}</p>
			<i class="form_item_delete"></i>
		</div>
		<div class="field_item_content">
			<div class="content_wrap">
				<div class="content_item_form_element content_input">
					<p class="content_body_title">{%lang['modelingAppMobile.docSubject']%}</p>
					<div class="content_opt">
						<input name='{%data.uuId%}_formItemTitle' title="{%lang['modelingAppMobile.docSubject']%}" type="text" placeholder="{%lang['modeling.please.enter']%}" value="{%(dataValue.title) || ''%}" data-validate="{%dataValidate.title%}" data-update-event='input' data-updateView="true"/>
					</div>
					<p class="content_opt_info"></p>
				</div>
				<div class="content_item_form_element">
					<p class="content_body_title">{%lang['modeling.model.icon']%}</p>
					<input name="{%data.uuId%}_icon" value="{%dataValue.icon || 'mui-docRecord'%}" type="hidden" data-update-event='change' data-updateView="true" data-validate="{%dataValidate.icon%}"/>
					<div class="content-icon-choose">
						<i class="mui {%dataValue.icon || 'mui-docRecord'%}"></i>
					</div>
					<p class="content_opt_info"></p>
				</div>
				<div class="content_item_form_element content_choose">
					<p class="content_body_title">{%lang['table.modelingCollectionView']%}</p>
					<input name="{%data.uuId%}_listView" title="{%lang['table.modelingCollectionView']%}" value="{%dataValue.listView%}" type="hidden" data-update-event='change' data-validate="{%dataValidate.listView%}"/>
					<input name="{%data.uuId%}_listViewText" value="{%dataValue.listViewText%}" type="hidden" />
					<input name="{%data.uuId%}_listViewAppId" value="{%dataValue.listViewAppId%}" type="hidden" />
					<input name="{%data.uuId%}_nodeType" value="{%dataValue.nodeType%}" type="hidden" />
					<div class="content_opt listVieElement">
						<p class="listViewText">{%dataValue.listViewText || lang['modeling.page.choose']%}</p>
						<i></i>
					</div>
					<p class="content_opt_info"></p>
				</div>$}
				if(this.isCount){
					{$<div class="content_item_form_element content_select" style="display: none">
						<p class="content_body_title">tab</p>
						<div class="lvCountWrap"></div>
						<input name='{%data.uuId%}_countLv' title="tab" data-update-event='change' type='hidden' value="{%dataValue.countLv%}"/>
						<input name="{%data.uuId%}_lvCollection" type="hidden" />
						<p class="content_opt_info"></p>
					</div>$}
				}
			{$</div>
		</div>
	</div>
$}