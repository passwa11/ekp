
.model-body {


}

.model-body .model-body-wrap {
    position: fixed;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
    background: #fff;
    margin: 10px;
    border: 1px solid #DDDDDD;
    box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.06);
    border-radius: 4px;
    overflow: hidden;
}

.model-body-phone-content .model-body-phone-empty {
    position: absolute;
    top: 50%;
    margin-top: -110px;
    left: 50%;
    margin-left: -140px;
}

.model-body-phone-content .model-body-phone-empty-img div {
    width: 164px;
    height: 127px;
    margin: 0 auto;
    background: url(../images/empty.png) no-repeat center;
}

.model-body-phone-content .model-body-phone-empty-img p {
    font-size: 28px;
    color: #333333;
    text-align: center;
    margin: 30px 0px 40px;
}

.model-body-phone-content .model-body-phone-empty-btn {
    width: 90px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background: #4285f4;
    color: #fff;
    font-size: 14px;
    border-radius: 4px;
    cursor: pointer;
    transition: all 0.3s;
    margin: 0 auto;
}

.model-body-phone-content .model-body-phone-empty-btn:hover {
    background: #679DF6;
}

.model-mask {
    z-index: 999;
}

.model-mask.active {
    opacity: 1;
}

.model-mask-bg {
    position: fixed;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 998;
}

.model-mask-panel {
    width: 540px;
    background: #fff;
    margin: 0 auto;
    position: relative;
    z-index: 999;
}

.model-mask-panel-edit {
    padding: 20px;
}

.model-mask-panel-edit-title {
    margin-bottom: 20px;
}

.model-mask-panel-edit-title p {
    font-size: 14px;
    color: #666666;
    margin: 0;
}

.model-mask.active .model-mask-phone {
    z-index: 999;
}

.model-mask-phone {
    width: 790px;
    height: 518px;
    border-radius: 4px;
    background: #fff;
    position: absolute;
    top: 50%;
    margin-top: -245px;
    left: 50%;
    margin-left: -325px;
    overflow: hidden;
}

.model-mask-phone-wrap {
    /* padding: 40px 30px 0px 50px; */
}

.model-mask-phone-left {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 160px;
    padding: 20px;
    overflow: auto;
    background: #F5F6FA;
}
.model-mask-phone-right {
    margin-left: 200px;
}
.model-mask-phone-right-item {
    padding: 30px 40px 40px 60px;
    overflow: hidden;
}
.model-mask-phone-right-item .model-mask-phone-img-default{
    background: url(../images/mask-phone@2x.png) no-repeat center;
    background: url(../images/mask-phone.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-right-item .model-mask-phone-img-list{
    background: url(../images/mask-phone-list@2x.png) no-repeat center;
    background: url(../images/mask-phone-list.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-right-item .model-mask-phone-img-mportal{
    background: url(../images/mask-phone-mportal@2x.png) no-repeat center;
    background: url(../images/mask-phone-mportal.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-right-item .model-mask-phone-img-mportalList{
    background: url(../images/mask-phone-mportalList@2x.png) no-repeat center;
    background: url(../images/mask-phone-mportalList.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-right-img {
    width: 220px;
    height: 446px;
    margin-right: 30px;
    float: left;
}
.model-mask-phone-right-desc {
    margin-top: 30px;
    margin-left: 250px;
}
.model-mask-phone-right-desc p {
    font-size: 14px;
    color: #999999;
    margin: 0;
    margin-bottom: 10px;
}
.model-mask-phone-right-desc .model-mask-phone-title {
    color: #333333;
}
.model-mask-phone-right-btn {
    border-radius: 2px;
    background: #4285f4;
    height: 30px;
    line-height: 30px;
    text-align: center;
    font-size: 14px;
    color: #FFFFFF;
    width: 90px;
    transition: all 0.3s;
    cursor: pointer;
    margin-top: 50px;
}
.model-mask-phone-right-btn:hover {
    background: #679DF6;
}
.model-mask-phone-slide-item {
    width: 158px;
    height: 158px;
    background: #FFFFFF;
    border: 1px solid #E7EAF3;
    border-radius: 2px;
    margin-bottom: 10px;
    transition: all 0.3s;
    cursor: pointer;
}
.model-mask-phone-slide-item:hover {
    border: 1px solid #99A8E5;
}
.model-mask-phone-slide-item.model-mask-phone-item-img .model-mask-phone-slide-img {
    background: url(../images/thumb-default@2x.png) no-repeat center;
    background: url(../images/thumb-default.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-slide-item.model-mask-list-item-img .model-mask-phone-slide-img {
    background: url(../images/thumb-list@2x.png) no-repeat center;
    background: url(../images/thumb-list.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-slide-item.model-mask-mportal-item-img .model-mask-phone-slide-img {
    background: url(../images/thumb-mportal@2x.png) no-repeat center;
    background: url(../images/thumb-mportal.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-slide-item.model-mask-mportalList-item-img .model-mask-phone-slide-img {
    background: url(../images/thumb-mportalList@2x.png) no-repeat center;
    background: url(../images/thumb-mportalList.png) no-repeat \9;
    background-size: contain;
}
.model-mask-phone-slide-item.active {
    background: #EEF1FB;
    border: 1px solid #99A8E5;
    box-shadow: 0 2px 6px 0 #e3e8ff;
}
.model-mask-phone-slide-img {
    width: 78px;
    height: 160px;
    margin: 0 auto;
}
.index-close-icon{
	background: url(../images/close.png) no-repeat center;
	background-size: contain;
	width: 12px;
    height: 12px;
	float:right;
	margin:10px 15px;
	cursor:pointer;
}

@keyframes arrMove {
    0% {
        transform: translateX(0);
    }
    50% {
        transform: translateX(2px);
    }
    100% {
        transform: translateX(0);
    }
}