@charset "UTF-8";

.clearfix:after {
    /*伪元素是行内元素 正常浏览器清除浮动方法*/
    content: "";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.clearfix {
    *zoom: 1;
    /*ie6清除浮动的方式 *号只有IE6-IE7执行，其他浏览器不执行*/
}

.model-maintenance-toolbox {
    padding: 20px;
}

.model-maintenance-toolbox i {
    display: inline-block;
    width: 14px;
    height: 14px;
    border: 1px solid #ddd;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    border-radius: 2px;
    margin-right: 6px;
    vertical-align: middle;
}

.model-maintenance-toolbox i.allSelected, .model-maintenance-toolbox i.formSelected, .model-maintenance-toolbox i.itemSelected {
    border: 0;
    background: url(../../resources/images/checked-blue.png) no-repeat center;
    background: url(../../resources/images/checked-blue.png) no-repeat center \9;
    background-size: 14px;
}


.maintenance-toolbox-return {
    font-size: 14px;
    color: #333333;
    height: 30px;
    line-height: 30px;
    overflow: hidden;
    padding-bottom: 10px;

}

.maintenance-toolbox-return > div {
    cursor: pointer;
    display: inline-block;
    overflow: hidden;
    width: 80px;
}

.maintenance-toolbox-return > div i {
    float: left;
    width: 14px;
    height: 14px;
    margin-top: 8px;
    border: none;
    background: url(../../resources/images/qian.png) no-repeat center;
    background-size: contain;
    margin-right: 6px;
}

.model-edit-view-title > div p {
    float: left;
    font-size: 12px;
    margin: 0;
}

.maintenance-toolbox-return > div:hover i {
    animation: arrMove 1s infinite;
}

