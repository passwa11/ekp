.maintenance-toolbox-dialog .toolbox-dialog-container .item-status-result.success:before, .maintenance-toolbox-dialog .toolbox-dialog-container .item-status-result.error:before {
    content: '';
    display: inline-block;
    vertical-align: middle;
    margin-right: 10px;
    width: 14px;
    height: 14px;
    background-size: 14px 14px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple {
    width: 100%;
    table-layout: fixed;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple th, .maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple td {
    padding: 0;
    padding-top: 10px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple th {
    font-size: 14px;
    color: #333333;
    font-weight: normal;
    letter-spacing: 0;
    line-height: 20px;
    /*overflow: hidden;*/
    /*text-overflow: ellipsis;*/
    /*white-space: nowrap;*/
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple th.td-left {
    padding-right: 50px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple th span {
    color: #4285f4;
    margin-top: -3px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple td {
    font-size: 12px;
    color: #999999;
    /*overflow: hidden;*/
    /*text-overflow: ellipsis;*/
    /*white-space: nowrap;*/
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple .td-left {
    text-align: left;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple .td-left span {
    display: inline-block;
    vertical-align: middle;
    max-width: 50%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .tb-simple .td-right {
    text-align: right;
    width: 50%;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .item-promotion-table .td-left i {
    display: inline-block;
    float: left;
    width: 14px;
    height: 14px;
    border: 1px solid #ddd;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    border-radius: 2px;
    margin-right: 6px;
    vertical-align: middle;
    margin-top: 2px;
}


.maintenance-toolbox-dialog .toolbox-dialog-container .item-promotion-table .td-left i.itemSelected,
.maintenance-toolbox-dialog .toolbox-dialog-container .item-promotion-table .td-left i.formSelected {
    border: 0;
    background: url(../../resources/images/checked-blue.png) no-repeat;
    background: url(../../resources/images/checked-blue.png) no-repeat \9;
    background-size: 14px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 70px;
    background-color: #FFFFFF;
    border-top: 4px solid #f8f8f8;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state {
    height: 100%;
    margin-top: -4px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .bar {
    display: block;
    height: 4px;
    overflow: hidden;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content {
    padding: 0 20px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content > div {
    height: 30px;
    margin-top: 18px;
    vertical-align: middle;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content .desc {
    float: left;
    max-width: 50%;
    font-size: 14px;
    color: #333333;
    line-height: 30px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content .cancel,
.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content .continue {
    cursor: pointer;
    float: right;
    width: 90px;
    height: 30px;
    line-height: 28px;
    border-radius: 2px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    text-align: center;
    font-size: 14px;
    margin-left: 10px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content .cancel {
    color: #333;
    border: 1px solid #DDDDDD;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state .state-content .continue {
    color: #fff;
    border: 1px solid #4285f4;
    background-color: #4285f4;
    margin-right: 15px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.detecting .bar {
    background: #4285f4;
    border-radius: 0 100px 100px 0;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.finish .bar {
    background: #2CCC74;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.finish .desc:before {
    content: '';
    display: inline-block;
    width: 14px;
    height: 14px;
    /*background: url(../assets/correct@2x.png) no-repeat;*/
    /*background: url(../assets/correct.png) no-repeat \9;*/
    background-size: 14px 14px;
    vertical-align: middle;
    margin-right: 10px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.incomplete .bar {
    background: #FF943E;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.incomplete .desc:before {
    content: '';
    display: inline-block;
    width: 14px;
    height: 14px;
    /*background: url(../assets/tips@2x.png) no-repeat;*/
    /*background: url(../assets/tips.png) no-repeat \9;*/
    background-size: 14px 14px;
    vertical-align: middle;
    margin-right: 10px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status i {
    display: inline-block;
    width: 14px;
    height: 14px;
    border: 1px solid #DDDDDD;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    border-radius: 2px;
    margin-right: 6px;
    vertical-align: middle;
}
.maintenance-toolbox-dialog .toolbox-dialog-status i.itemSelected {
    border: 0;
    background: url(../../resources/images/checked-blue.png) no-repeat;
    background: url(../../resources/images/checked-blue.png) no-repeat \9;
    background-size: 14px;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.promotion .desc {
    font-size: 0;
}

.maintenance-toolbox-dialog .toolbox-dialog-status .state.promotion .desc span {
    font-size: 12px;
    vertical-align: middle;
    margin-right: 20px;
}

.form-item-testing, .form-item-promotion {
    display: none;
}
