.model-maintenance-toolbox .maintenance-toolbox-actionBar {
    margin-bottom: 10px;
}

.model-maintenance-toolbox .maintenance-toolbox-actionBar .selectAll {
    float: left;
    font-size: 12px;
    line-height: 24px;
    color: #333;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    vertical-align: middle;
    cursor: pointer;
}

.model-maintenance-toolbox .maintenance-toolbox-actionBar .selectAll i {
    margin-top: -2px;
}

.model-maintenance-toolbox .maintenance-toolbox-actionBar .doCheck {
    float: right;
    background: #4285F4;
    border-radius: 4px;
    width: 70px;
    height: 24px;
    line-height: 24px;
    font-size: 12px;
    color: #FFFFFF;
    letter-spacing: 0;
    text-align: center;
    cursor: pointer;
}

.model-maintenance-toolbox .maintenance-toolbox-container .toolbox-form-item {
    width: 100%;
    border: 1px solid #EEEEEE;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin-bottom: 10px;
    padding-bottom: 10px;
    overflow: hidden;
    font-size: 0;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-title {
    padding: 9px 10px;
    width: 100%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

i {
    cursor: pointer;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-title span {
    font-size: 14px;
    line-height: 20px;
    color: #333333;
    font-weight: 550;
    display: inline-block;
    vertical-align: middle;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    vertical-align: middle;
    cursor: pointer;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-title span:before {
    content: '';
    display: inline-block;
    width: 0;
    height: 0;
    border-style: solid;
    border-color: transparent transparent transparent #999;
    border-width: 4px;
    margin-left: 8px;
    margin-right: 4px;
    margin-top: -2px;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-title span.develop:before {
    border-color: #999 transparent transparent transparent;
    margin-left: 6px;
    margin-right: 6px;
    margin-bottom: -2px;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-list {
    padding-left: 20px;
    display: none;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-list li {
    display: inline-block;
    font-size: 14px;
    color: #333333;
    letter-spacing: 0;
    margin-right: 30px;
    padding: 10px 0;
    cursor: pointer;
}

.model-maintenance-toolbox .maintenance-toolbox-container .form-item-list li p {
    display: inline-block;
    max-width: 140px;
    line-height: 20px;
    margin: 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    vertical-align: middle;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}


.maintenance-toolbox-dialog {
    position: relative;
    height: 100%;
    margin-top: -50px;
    padding-top: 50px;
    padding-bottom: 70px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.maintenance-toolbox-dialog .toolbox-dialog-container {
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;
    padding: 0 25px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container > div:first-child .item-status-result {
    border: 0;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing,
.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-promotion {
    padding-bottom: 20px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing .form-item-content,
.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-promotion .form-item-content {
    position: relative;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing .form-item-content > img,
.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-promotion .form-item-content > img {
    position: absolute;
    top: 0;
    width: 40px;
    height: 40px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing {
    padding-left: 70px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing .item-status-result {
    margin-left: -70px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-testing .form-item-content > img {
    left: -70px;
}

