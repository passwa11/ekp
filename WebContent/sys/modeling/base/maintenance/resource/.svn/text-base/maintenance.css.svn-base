/*    padding-left: 50px;*/
/*}*/

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-promotion .item-status-result {
    margin-left: -50px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .form-item-promotion .form-item-content > img {
    left: -50px;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .item-status-result {
    text-align: center;
    font-size: 14px;
    color: #333333;
    padding: 20px 0;
    border-top: 1px solid #eee;
}

.maintenance-toolbox-dialog .toolbox-dialog-container .item-status-result span {
    color: #4285f4;
}

