@charset "utf-8";
body,html{
    height:100%;
}

.clearfix:after {
    content: " ";
    display: block;
    visibility: hidden;
    height: 0;
    clear: both;
}
.clearfix {
    zoom: 1;
}
::-webkit-scrollbar{
    width: 6px;
    height: 6px;
}
::-webkit-scrollbar-thumb{
    border-radius: 10px;
    background: #ddd;
}
::-webkit-scrollbar-track{
    background: transparent;
}
/* ViewConfig */

.viewMobileCardTextBox{
    background: #FFFFFF;
    border-radius: 8px;
}
.viewMobileConfigCardTextTitle{
    height: 20px;
    padding: 1rem;
}
.viewMobileCardTextTitleLeft{
    width: 30%;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #000000;
    line-height: 16px;
    font-weight: 400;
    float: left;
    max-height: 32px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.viewMobileCardTextTitleRight{
    width: 70%;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #000000;
    line-height: 16px;
    font-weight: 400;
    float: left;
    max-height: 32px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
}


.viewMobileCardTextContent{
    
}

.viewMobileCardTextContentTr{
    min-height: 20px;
    padding: 1rem;
    
}

.viewMobileCardTextContentTdLeft{
    width: 30%;
    min-height: 20px;
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #999999;
    line-height: 20px;
    font-weight: 400;
    display: inline-block;
    vertical-align: top;
    
}

.viewMobileCardTextContentTdRight{
    width: 70%;
    min-height: 20px;
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #000000;
    line-height: 20px;
    font-weight: 400;
    display: inline-block;
    vertical-align: top;
    word-break: break-all;
}

.viewMobileCardTextContentShow{
    width: 100%;
    font-family: PingFangSC-Regular;
    font-size: 1rem;
    color: #999999;
    font-weight: 400;
    text-align: center;
}

.viewMobileCardTextContentHide{
    width: 100%;
    font-family: PingFangSC-Regular;
    font-size: 1rem;
    color: #999999;
    font-weight: 400;
    text-align: center;
}

.down-icon {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../../../base/resources/images/icon-panel_down@2x.png) no-repeat center;
    background-size: contain;
    position: relative;
    top:4px;
}

.up-icon {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../../../base/resources/images/icon-panel_open@2x.png) no-repeat center;
    background-size: contain;
    position: relative;
    top:4px;
}

.title-icon{
    display: inline-block;
    width: 12px;
    height: 12px;
    background: url(../images/iconCommon_surface_16_form.svg) no-repeat center;
    background-size: contain;
    position: relative;
    top: 2px;
    margin-right: 2px;
}

/* 覆盖样式 start*/
.muiNavitem span{
    padding: 0 0.1rem;
    display: block;
    max-width: 100%;
    margin: 0 auto;
    color: #999fb7;
    box-sizing: border-box;
    height: 85%;
}

.mui-scroll-view {
    width: 100%;
    height: 100%;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.mui-scroll-view {
    background: rgb(237, 240, 245);
    width: 100%;
    height: 100%;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.muiDocItem {
    padding-top: 1.3rem;
    padding-bottom: 0.8rem;
    padding-left: 1.5rem;
    border-bottom: 0;
    overflow: auto;
}

/* 覆盖样式 end*/