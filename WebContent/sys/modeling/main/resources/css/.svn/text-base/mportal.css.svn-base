html, body {
	background-color: #fff !important;
}

.mportal {
	background: #fff url(../images/collapse-bg.png) no-repeat center 0px;
	background-size: contain;
}

.mportal-box {
	padding: 0 1rem;
}

.mportal-box .mportal-content {
	
}

.mportal-content>div {
	margin: 0.5rem 0;
	padding: 0 0.3rem;
}

.mportal-content .block-title {
	
}

.block-title p {
	font-size: 2rem;
	color: #333333;
	font-weight: 600;
	margin: 1rem 0;
}

.mportal-content .mportal-statistics {
	margin-top: 0.6rem;
}

.mportal-statistics .mportal-collapse {
	position: relative;
	border-radius: 0.4rem;
	background: #fff;
	box-shadow: 0 0.2rem 0.8rem -0.2rem rgba(0, 0, 0, 0.125);
	margin-bottom: 1rem;
	padding-bottom: 1rem;
}

.mportal-collapse-item {
	float: left;
	width: 33.3%;
}

.mportal-collapse-item-wrap {
	padding: 1rem 1rem;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	text-align: center;
}

.mportal-collapse-item-wrap div {
	font-size: 2rem;
	color: #4285f4;
	height: 2.5rem;
	line-height: 2.5rem;
}

.mportal-collapse-item-wrap p {
	margin: 0;
	font-size: 1.2rem;
	color: #333333;
	height: 1.8rem;
	line-height: 1.8rem;
	text-overflow: ellipsis;
	overflow: hidden;
}

.mportal-collapse-wrap:after {
	content: '';
	display: block;
	clear: both;
}

.mportal-content .mportal-iconArea {
	
}

.mportal-slide {
	float: left;
}

.mportal-slide-wrap {
	padding: 0.5rem;
}

.mportal-slide-icon {
	width: 4rem;
	height: 4rem;
	margin: 0 auto;
	background: #4285f4;
	border-radius: 0.8rem;
	margin-bottom: 0.4rem;
	font-size: 3rem;
	text-align: center;
	color: #fff;
}

.mui_ekp_portal_item ul.withbg {
	background: none !important;
}

.mportal-slide-wrap p {
	margin: 0;
	font-size: 1.2rem;
	color: #333333;
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.slide-pagination {
	text-align: center;
}

.pagination {
	display: inline-block;
	margin: 0 auto;
	overflow: hidden;
	height: 0.4rem;
	border-radius: 0.4rem;
}

.pagination .swiper-pagination-switch {
	float: left;
	width: 1rem;
	height: 1rem;
	background: #eee;
}

.pagination .swiper-active-switch {
	background: #4285f4;
}

.mportal-content .mportal-listView {
	
}

.mportal-listView .list-item {
	height: 5rem;
	position: relative;
}

.list-item .item-icon {
	width: 3rem;
	height: 3rem;
	background: url(../images/doc@2x.png) no-repeat center;
	background: url(../images/doc.png) no-repeat center \9;
	background-size: contain;
	position: absolute;
	left: 0;
	top: 50%;
	margin-top: -1.5rem;
}

.list-item .item-detail {
	height: 5rem;
	line-height: 5rem;
	padding: 0 3rem;
	padding-left: 3.8rem;
	box-sizing: border-box;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.item-detail p {
	font-size: 1.4rem;
	color: #333333;
	margin: 0;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.list-item .item-arrow {
	width: .7rem;
	height: 1.2rem;
	background: url(../images/arrow@2x.png) no-repeat center;
	background: url(../images/arrow.png) no-repeat center \9;
	background-size: contain;
	position: absolute;
	right: 0;
	top: 50%;
	margin-top: -0.6rem;
}

.swiper-wrapper {
	margin: 0 auto;
}

.content-icon-choose {
	width: 4rem;
	height: 4rem;
	background: #4285f4;
	border-radius: 8px;
	margin-bottom: 4px;
	font-size: 3rem;
	color: #fff;
	text-align: center;
}

.mportal-collapse.shrink .mportal-collapse-wrap.bottom {
	display: none;
}

.mportal-collapse .mportal-collapse-wrap.bottom {
	display: block;
}

.mportal-collapse.shrink .mportal-collapse-btn {
	transform: rotate(0deg);
	background-size: contain;
}

.mportal-collapse-btn {
	position: absolute;
	transition: all 0.3s;
	bottom: 0.2rem;
	left: 50%;
	margin-left: -8px;
	width: 2rem;
	height: 1.5rem;
	transform: rotate(180deg);
	background: url(../images/down@2x.png) no-repeat center;
	background: url(../images/down.png) no-repeat center\9;
	background-size: contain;
	cursor: pointer;
	z-index: 2;
}

.mui_ekp_portal_item .muiPortalIcon .mui {
	line-height: 5.2rem;
	font-size: 2.6rem;
}

.mportal-menu-slide .muiPortalMenuSlideItem .muiPortalIcon {
	background-image: linear-gradient(-180deg, #629DFF 2%, #2368DC 96%)
		!important;
}

.no-auth-div{
	margin: 0 auto;
	width: 24rem;
	height: 13rem;
}
.no-auth-image{
	width: 5.59rem;
    height: 5rem;
	background: url(../images/noAuth.png) no-repeat center;
    background-size: contain;
    display: inline-block;
    margin: 0 .2rem;
    margin-top:4.6rem;
}
.no-auth-text{
	display:inline-block;
	position:absolute;
	margin-top:4rem;
	margin-left: 1.3rem;
}
.no-auth-message{
	font-size: 1.82rem;
	letter-spacing: .14rem;
	color: #2A304A ;
	margin-bottom: 0.1rem;
	padding: .4rem 0;
}
.no-auth-tip{
	font-size: 1.48rem;
	letter-spacing: .14rem;
	color: #AAACB7;
	padding: .4rem 0;
}
/* 150303 */
.mblScrollableViewContainer ul.withbg.mblView.mblSwapView{
	padding: 0;
}
.mblScrollableViewContainer .mui_ekp_portal_item ul>li>a>div{
	transform: scale(0.8);
}