@charset "utf-8";  

.muiDocItem{
	padding-top: 1.3rem;
	padding-bottom: 0.8rem;
	padding-left: 1.5rem;
	border-bottom: .5px solid #eaebf0;
	overflow: auto;
}

.mui_modeling_list_container{
	padding-top: 1.6rem;
    padding-bottom: 3.8rem;
    border-bottom: .5px solid #eaebf0;
	border: none;
}
.lines-cut {
	overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 2.5rem;
    font-size: var(--muiFontSizeM);
    color : var(--muiFontColorInfo)
}
.summaryBlock{
	margin-top: 1rem;
	width: 100%;
	color: var(--muiFontColorMuted);
	font-size: var(--muiFontSizeS);
	vertical-align: middle;
	height: 1.7rem;
	line-height: 1.7rem;
}
.summaryBlock>div{
    margin-right: 1.4rem;
    float: left;
	max-height: 1.9rem;
	overflow: hidden;
	max-width: 30rem;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.muiComplexlReadNum{
	margin-right: .4rem;
    font-family: "landray-number", fontnumber,-apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
}

.mui_modeling_list_container .imgBlock{
	float: right;
}

.mui_modeling_list_container .imgBlock > img{
	width: 72px;
	height: 72px;
}

.mui_modeling_list_container .boardImgBlock{
	width: 7.2rem;
	height: 7.2rem;
	float: right;
	margin: 0rem 1rem 0rem 0rem;
}
.mui_modeling_list_container.board-list-container{
	padding: 0rem;
	margin: 0rem;
}
.boardNavHeader .muiNavbarContainer{
	text-align: left;
}

.muiTabBarGrid.mblTabBarTabBar .mblTabBarButton:not(:last-child):after {
	content: '';
	display: block;
	height: 20px;
	border-left: 1px solid #efefef;
	position: absolute;
	right: 0;
	top: 50%;
	margin-top: -10px
}
.fontmuis.muis-form-selected-cor.muiCheckBlock{
	visibility: hidden;
}