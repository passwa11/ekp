.modelAppSpaceWidgetDemoTypeSignItemInfo>i{
	float: left;
}
.modelAppSpaceWidgetDemoTypeSignItemInfo>span{
	float: right;
}
.modelAppSpaceWidgetDemoTypeSignItemInfo{
	zoom: 1;
}
.modelAppSpaceWidgetDemoTypeSignItemInfo:after{
	content: " ";
	display: block;
	visibility: hidden;
	height: 0;
	clear: both;
}
.modelAppSpaceWidgetDemoTypeSignMore>span>i{
	display: inline-block;
	width: 16px;
	height: 16px;
	/*background: url(../images/more.svg) no-repeat center center;*/
	background-size: contain;
	background-color: #F8F8F8;
	position: relative;
	top: 4px;
}
.modelAppSpaceWidgetDemoTypeSignMore{
	float: right;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background-color: #F7F7F8;
	border-radius: 20px;
	font-size: 12px;
	color: #999FB7;
	padding: 0 20px;
	border-radius: 30px;
	cursor: pointer;
	box-shadow: 0px 0px 15px 0px rgba(153,179,210,0.2);
}
.modelAppSpaceWidgetListViewWithCount,.modelAppSpaceWidgetListViewWithIcon{
	margin-top: 0px;
	margin-bottom: 0px;
	padding-bottom: 0px;
	padding-top: 0px;
}
.modelAppSpaceWidgetListViewWithCount .modelAppSpaceWidgetDemoTypeEnter,.modelAppSpaceWidgetListViewWithIcon .modelAppSpaceWidgetDemoTypeEnter{
	margin-top: 8px;
	margin-bottom: 8px;
	padding-bottom: 8px;
	padding-top: 8px;
}
.mblEdgeToEdgeList{
	margin-bottom: 0rem;
}
.mui_pph_toast {
	height: 3.68rem;
	display: flex;
	margin-top: 0.6rem;
	background: rgba(255, 255, 255, .2);
	border-radius: 6px;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.mportalList-slide {
	background: inherit !important;
}
.modelAppSpaceWidgetDemoTypeTitle{
	padding: 0.5rem;
	max-width: 12rem;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	margin: 4px 0px;
}
.muiPortalMenuSlide{
	padding: 0.5rem;
}
.mportal-collapse{
	padding: 0.5rem;
}
.mui_pph_toast_icon {
	width: 2.48rem;
	height: 2.32rem;
	margin: 0 1.2rem;
}
.mui_pph_tc_text{
	font-size: 14px;
	margin-left: 0.23rem;
	color: #fff;
}
.mui_pph_toast{
	 background-color: #4285f4;
 }
.mui_pph_close_icon {
	width: 1.23rem;
	height: 1.23rem;
	margin-right: 1.15rem;
}
.mui_pph_new_icon {
	width: 1.56rem;
	height: 1.28rem;
}
.muiDocItem.muiPortalClickItem {
	padding: 0rem 1rem;
	overflow:hidden;
}
.muiPortalCard, .muiPortalTabCard{
	padding: 0px;
}
.mui_ekp_portal_approval_process_tab>li>span{
	max-width: 7.5rem;
}