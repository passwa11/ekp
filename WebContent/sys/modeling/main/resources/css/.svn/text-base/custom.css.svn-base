.mui_personnel_process_head {
	color: unset;
	height: 15rem;
	background: #FFF;
	background-size: cover;
	padding: 0 1rem;
	padding-top: 1rem;
	box-sizing: border-box;
}
.mui_personnel_process_head.blueBg {
	background: url(../images/bg.jpg) no-repeat center;
	background-size: cover;
}
.mui_personnel_process_head.collapseBg {
	background: #fff url(../images/collapse-bg.png) no-repeat center 0px;
	background-size: cover;
}
.muiPortletIcon{
	display: inline-block;
	margin-right: .2rem;
}
.modelAppSpaceWidgetDemoModel{
	padding: 8px 16px;
	position: relative;
	margin: 6px 0;
	border: 2px solid transparent;
}
.modelAppSpaceWidgetDemoModel.selected{
	background-color: #ffffff;
	border: 2px solid #4285f4;
}
.modelAppSpaceWidgetDemoModel>span{
	display: none;
}
.modelAppSpaceWidgetDemoModel.selected>span{
	display: inline-block;
	font-size: 12px;
	color: #FFFFFF;
	background-color: #4285F4;
	border-radius: 2px;
	text-align: center;
	z-index: 1;
	position: absolute;
	right: -2px;
	bottom: 100%;
	padding: 4px 8px;
	cursor: pointer;
}

.modelAppSpaceWidgetDemoTypeCover{
	width: 100%;
	height: 160px;
	/*background: url(../images/chartC.svg) no-repeat center center;*/
	background-size: 64px 64px;
	background-color: #F8F8F8;
}
.modelAppSpaceWidgetDemoModelBtn{
	height:48px;
	line-height: 48px;
	border-top: 1px dashed rgba(221,221,221,1);
	border-bottom: 1px dashed rgba(221,221,221,1);
	background-color: #ffffff;
	font-size: 14px;
	color: #333333;
	text-align: center;
	cursor: pointer;
}
.modelAppSpaceWidgetDemoModelBtn>i{

}
.modelAppSpaceWidgetDemoTypeTodo{
	height: 50px;
	line-height: 50px;
	color: #fff;
	font-size: 16px;
	background-color: #4285f4;
	border-radius: 2px;
	padding: 2px 16px;
}
.modelAppSpaceWidgetDemoTypeText{
	color: #333333;
	font-size: 14px;
	word-break: break-word;
	padding: 0.5rem;
	white-space: pre-wrap;
}
.modelAppSpaceWidgetDemoIconLink{
	float: right;
	width: 16px;
	height: 16px;
	background: url(../images/link.svg) no-repeat center center;
	background-size: contain;
}
.modelAppSpaceWidgetDemoTypeTodo>.modelAppSpaceWidgetDemoIconLink{
	position: relative;
	top: 16px;
}
/* 0705 */

.modelAppSpaceWidgetDemoTypeEnter>i{
	display: inline-block;
	width: 18px;
	height: 18px;
	background-size: contain;
	top: -0.3rem;
	padding-left: 0.5rem;
}
.modelAppSpaceWidgetDemoTypeEnter>strong{
	display: inline-block;
	font-size: 14px;
	color: #3E4665;
	margin: 0 1.6rem;
	position: relative;
	top: 0px;
	width: 10rem;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.modelAppSpaceWidgetDemoNum{
	background-color: #FF725A;
	padding: 2px 6px;
	border-radius: 8px;
	font-size: 10px;
	color: #FFFFFF;
	transform: scale(0.8);
	float: right;
	margin-right: 4px;
}
.modelAppSpaceWidgetDemoIconLinkN{
	float: right;
	width: 14px;
	height: 14px;
	background: url(../images/link2.svg) no-repeat center center;
	background-size: contain;
	position: relative;
	top: 2px;
}
/* 0708 */
.modelAppSpaceWidgetListWidget {
	margin-bottom: 12px;
}
.modelAppSpaceWidgetListWidget>ul{
	font-size: 0;
}
.modelAppSpaceWidgetListWidget>ul>li{
	float: left;
	border: 1px solid rgba(238,238,238,1);
	border-radius: 2px;
	margin-bottom: 10px;
	cursor: pointer;
}
.modelAppSpaceWidgetListWidget>ul>li:nth-child(odd){
	margin-right: 10px;
}
.modelAppSpaceWidgetListWidgetContent{
	width: 100px;
	height: 64px;
}
.modelAppSpaceWidgetListWidgetInfo{
	width: 100px;
	font-size: 12px;
	color: #333333;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	padding: 2px 8px;
}
.modelAppSpaceWidgetLeftBoxContent{
	overflow: scroll;
}
.modelAppSpaceWidgetLeftBoxContentPanel{
	overflow: scroll;
	height: 100%;
}
.modelAppSpaceWidgetLeftBoxContent::-webkit-scrollbar{
	display: none;
}
.modelAppSpaceWidgetLeftBoxContentPanel::-webkit-scrollbar{
	display: none;
}
.modelAppSpaceWidgetDemoTypeSignTitle{
	margin-bottom: 12px;
}
.modelAppSpaceWidgetDemoTypeSignTitle>span{
	float: left;
	font-size: 18px;
	color: #2A304A;
}
.modelAppSpaceWidgetDemoTypeSignTitle>ul{
	float: right;
	position: relative;
	top: 4px;
	font-size: 0;
}
.modelAppSpaceWidgetDemoTypeSignTitle>ul>li{
	display: inline-block;
	font-size: 14px;
	color: #999FB7;
	cursor: pointer;
}
.modelAppSpaceWidgetDemoTypeSignTitle>ul>li:not(:last-child){
	margin-right: 28px;
}
.modelAppSpaceWidgetDemoTypeSignTitle>ul>li.active{
	color: #4285F4;
}
.modelAppSpaceWidgetDemoTypeSignContentItem{
	display: none;
}
.modelAppSpaceWidgetDemoTypeSignContentItem:first-child{
	display: block
}
.modelAppSpaceWidgetDemoModel{
	text-align: left;
}
