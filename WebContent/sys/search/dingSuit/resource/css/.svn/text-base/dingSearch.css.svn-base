.lui-ding-search-category-btnWrap {
    position: fixed;
    bottom:0;
    width: 100%;
    height:50px;
    background-color: #fff;
    font-size:0;
    text-align:right;
    border-top: 1px solid  rgba(17,31,44,0.12);
}
.lui-ding-search-category-btnWrap>.lui-component.lui_widget_btn{
    position: relative;
    top:6px;
}
.lui-ding-search-category-btnWrap>.lui-component.lui_widget_btn{
    margin-right: 10px;
}
.lui-ding-search-category-btnWrap .lui_toolbar_btn_def .lui_toolbar_btn_l{
    height: 36px;
    line-height: 36px;
    color: #fff;
    text-align: center;
    display: block;
    border: 1px solid #0089ff;
    border-radius: 12px;
    background-color: #0089ff;
    transition-duration: .3s;
    font-size: 17px;
}
.lui-ding-search-category-btnWrap .lui_widget_btn .lui_widget_btn_txt,.lui-ding-search-category-btnWrap .lui_toolbar_btn_gray .lui_widget_btn_txt{
    padding:0 14px;
}
.lui-ding-search-category-btnWrap .lui_toolbar_btn_gray .lui_toolbar_btn_l{
    height: 36px;
    line-height: 36px;
    color: #fff;
    text-align: center;
    display: block;
    border: 1px solid rgba(17,31,44,0.12);
    border-radius: 12px;
    background-color: #F2F4F5;
    transition-duration: .3s;
    font-size: 17px;
    color: #111F2C;
}