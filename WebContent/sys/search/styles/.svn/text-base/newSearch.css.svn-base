.lui-search-panel-heading{
  background:#f9fbfe;
  border-bottom: 1px solid #4285f4;
}
.lui-search-panel-heading:after,
.lui-search-panel-heading:before,
.lui-search-panel-list:after,
.lui-search-panel-list:before,
.lui-search-panel-list ul:after,
.lui-search-panel-list ul:before,
.lui-search-panel-list > li a:after,
.lui-search-panel-list > li a:before{
  content: '';
  display: table;
  visibility: hidden;
}
.lui-search-panel-heading:after,
.lui-search-panel-list:after,
.lui-search-panel-list ul:after,
.lui-search-panel-list > li a:after{
  clear: both;
}
.lui-search-panel-list#expandArea{ display:none; }
.lui-search-panel-heading-title{
  margin:0;
  padding:5px 10px 3px;
  line-height: 20px;
  color: #4285F4;
  font-size: 16px;
  font-weight: normal;
  float: left;
  margin: 0px;
}
.lui-search-panel{
  padding-right: 60px;
  overflow: hidden;
  position: relative;
  height:55px
}
.lui-search-panel-list,
.lui-search-panel-list ul{
  margin: 0;
  padding: 15px 15px 15px 15px;
  list-style: none;
}
.lui-search-panel-list li{
  margin: 10px 0;
  width: 260px;
  float: left;
  vertical-align: middle;
  background:url(../images/icon-list.png) no-repeat 0 50%;
  background-size:16px;
}
.lui-search-panel-list > li a{
  max-width: 98%;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  vertical-align: bottom;
  transition: all .3s;
}
.lui-search-panel-list li.lui-search-panel-list-item a {
  line-height: 20px;
  max-width: 87%;
  padding-left: 22px;
}
.lui-search-panel-list > li a span{
	width:100%
}

.lui-search-panel-list > li .txt{
  color: #323941;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  float: left;
  transition: all .3s;
}
.lui-search-panel-list  li.lui-search-panel-list-item a.selected{ color:#4285F4;}
.lui-search-panel-list  li a .txt{
  color: #443c3c;
}
.lui-search-extra-wrap{
  line-height:20px;
  padding:1px 0;
	cursor: pointer;
}
.lui-search-extra-more{
	padding-left: 3px;
	/* background:#d8d8d8; */
	/* margin-top: 2px;
	border-bottom-left-radius: 8px;
  border-bottom-right-radius: 8px; */
  position: absolute;
  right: 10px;
  top: 25px;
}
.lui-search-panel-heading .lui-search-extra-more{margin:0 0 0 -4px;}
.lui-search-extra-more .arrow{
	display:inline-block;
	vertical-align:middle;
	width:16px;
	height: 11px;
	background: url(../images/icon-collapse.png) no-repeat center;
}
.lui-search-extra-more.expand .arrow{
	background-image: url(../images/icon-expand.png);
}
	
.lui-cate-queryEmpty-wrap {
	width: 100%;
	text-align: center;
	position: relative;
	padding:30px 0;
	margin-top: 120px;
}

.lui-cate-queryEmpty {
	display: inline-block;
	vertical-align: middle;
	width: 50%;
	height: 106px;
	min-width: 300px;
}
.lui-cate-queryEmpty .queryEmpty-icon{
	display:inline-block; 
	width: 124px; 
	height:106px;
	 background:url(../images/query-empty-icon.png) no-repeat;
}
.queryEmpty-tips{
	position: relative;
	top: -90px;
	line-height: 20px;
	font-size:16px;
}
.queryEmpty-suggest{
	color:#666;
	margin-top: -5px;
	word-wrap: normal;
	white-space: nowrap;
}

.lui_tabpanel_frame {
	border-radius: 4px;
	border: 1px solid #d5d5d5\9;
	background-color: #fff;
	-webkit-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
		 -moz-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
			-ms-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
			 -o-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
					box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
}
.lui_tabpanel_navs_c {
	font-size: inherit;
	background-color: transparent;
	height: auto;
	color: #636363;
	line-height: 1;
	padding-left: 5px;
	border-bottom: 1px solid #d5d5d5;
	vertical-align: middle;
}
.lui_tabpanel_navs_item_l {
	float: left;
	overflow: visible;
	height: auto;
}
.lui_tabpanel_navs_item_r{ }
.lui_tabpanel_navs_item_c {
	margin: 0 0 -1px 0px;
	padding: 8px 10px;
	max-width: 90%;
	height: 20px;
	line-height: 20px;
	color: #666;
	font-size: 0;
	*white-space: -1px;
	font-weight: normal;
	border-bottom: 2px solid transparent;
	position: relative;
	transition-duration: .3s;
}
.lui_tabpanel_navs_item_c:hover {
	color: #4285f4;
}
.lui_tabpanel_navs_item_c .com_prompt_num{
	position: relative;
	top: -4px;
}
.lui_tabpanel_navs_c.tabLearn .lui_tabpanel_navs_item_c{ text-align: center; }
.lui_tabpanel_navs_item_selected .lui_tabpanel_navs_item_r{ }
.lui_tabpanel_navs_item_selected .lui_tabpanel_navs_item_c {
	background-color: transparent;
	color: #4285f4;
	font-weight: normal;
	border-top: 0;
	border-bottom: 2px solid #4285f4;
}
.lui_tabpanel_content_l {
	background-color: transparent;
}

/*搜索机制改造*/
.lui_search_container{
	padding-left: 7px;
	position: relative;
}
.lui_search_doc{
	height: 40px;
    line-height: 40px;
    border-bottom: 1px solid #EEEEEE;
}
.lui_search_doc>span{
	font-size: 14px;
	color: #333333;
	cursor: pointer;
}
.lui_search_doc>span>i{
	background: url(../images/close@2x.png) no-repeat center;
	background-size: 18px;
    display: inline-block;
    width: 10px;
    height: 10px;
    margin-left: 6px;
}
.lui_search_doc>span.active{
	color: #4285F4;
}
.lui_search_doc>span.active>i{
	background: url(../images/open_active@2x.png) no-repeat center;
	background-size: 18px;
}
.lui_search_content{
	width: 250px;
	background: #FFFFFF;
	border: 1px solid #DDDDDD;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.15);
	border-radius: 0 0 2px 2px;
	position: absolute;
}
.lui_search_input{
	margin: 9px 8px;
	position: relative;
}
.lui_search_input .lui_search_keyword{
	width: 100%;
    background: #FFFFFF;
    border: 1px solid #DDDDDD;
    border-radius: 2px;
    height: 30px;
    padding-left: 8px;
    padding-right: 50px;
    box-sizing: border-box;
}
.lui_search_input .lui_search_empty{
	background: url(../images/relevance_del_gray.png) no-repeat;
    width: 15px;
    height: 15px;
    cursor: pointer;
    position: absolute;
    right: 34px;
    top: 7.5px;
}
.lui_search_input .lui_search_btn{
	display: inline-block;
    width: 16px;
    height: 16px;
	background: url(../images/index-search@2x.png) no-repeat center;
    background: url(../images/index-search.png) no-repeat center \9;
    background-size: 16px;
    position: absolute;
    right: 8px;
    top: 7.5px;
}
.lui_search_list .lui_search_list_ul{
	list-style: none;
    min-height: 120px;
    max-height: 270px;
    overflow: hidden;
    overflow-y: auto;
}
.lui_search_list .lui_search_list_item{
	line-height: 30px;
    padding-left: 8px;
    font-size: 12px;
	color: #666666;
}
.lui_search_list .lui_search_list_item:hover {
	color: #4285F4;
	background: rgba(66,133,244,0.10);
}