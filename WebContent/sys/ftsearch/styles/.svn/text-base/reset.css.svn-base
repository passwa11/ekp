﻿@charset "utf-8";
/* CSS Document */

/* CSS reset */
body,td,th,button,div,a{font-family:"微软雅黑", Geneva, "sans-serif" , "微软雅黑" , "宋体"; font-size: 12px;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,iieldset,input,textarea,p,blockquote,th,td{ margin:0; padding:0;}
table{ border-collapse:collapse; border-spacing:0;}
fieldset,img{ border:0;}
address,caption,cite,code,dfn,em,strong,th,var{ font-style:normal; font-weight:normal;}
ol,ul{ list-style:none;}
a{ text-decoration:none; transition:background-color 0.3s ease 0s, border 0.3s ease 0s, color 0.3s ease 0s, opacity 0.3s ease-in-out 0s;}
caption,th{ text-align:left;}
h1,h2,h3,h4,h5,h6{ font-size:100%; font-weight:normal;}
q:before,q:after{ content:''}
abbr,acrnym{ border:0;}
.textEllipsis{ width:90%; overflow:hidden; display:block; white-space:nowrap; text-overflow:ellipsis; }
.clr{ clear:both; line-height:0px;}



.lui_btn{  padding:0 14px; text-align:center; line-height:26px; color:#fff; font-size:14px; border:1px solid #35a1d0; background-color:#47b5e6; display:block;}
.lui_btn:hover{ background:#04609b; text-decoration:none; border-color:#004376; color:#fff}

.lui_btn_list{  padding:0 10px; text-align:center; line-height:22px; color:#909090; border:1px solid #c9c9c9; background-color:#fff; display:block;}
.lui_btn_list:hover{background-color:#fff;  text-decoration:none; border-color:#30ade4; color:#3eb2e6}

