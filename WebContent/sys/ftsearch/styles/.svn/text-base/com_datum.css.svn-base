.com_datum_w{ background:#fff; padding:15px 20px; margin-bottom:18px;}

.com_datum_item_w{ margin-bottom:20px;}
.com_datum_itemHead{ margin-bottom:15px; font-size:21px; font-style:italic; color:#f99f47; height:46px; padding-left:47px; line-height:46px;  font-weight:700; border-bottom:1px solid #ededed; position:relative;}
.com_datum_itemHead .ico_head{ display:inline-block; width:47px; height:46px; background:#fff url(images/com_datum_item_head.png) no-repeat left top; position:absolute; bottom:-1px; left:0px;}

.com_datum_itemHead .turn_w{ width:64px; height:28px; position:absolute; bottom:4px; right:4px; cursor:pointer;}
.com_datum_itemHead .turn_w a{ display:inline-block; width:23px; height:23px; background-color:#f4f4f4; border:1px solid #e6e6e6;}
.com_datum_itemHead .turn_w .arrow_L{ margin-right:9px; background:#f4f4f4 url(images/arrow_L.png) no-repeat center;}
.com_datum_itemHead .turn_w .arrow_R{ background:#f4f4f4 url(images/arrow_R.png) no-repeat center;}
.com_datum_itemHead .turn_w a:hover{ background-color:#e1e1e1;}

.com_datum_itemContent .turn_w{ width:64px; height:28px; position:absolute; top:-22px; right:4px; cursor:pointer;}
.com_datum_itemContent .turn_w a{ display:inline-block; width:23px; height:23px; background-color:#f4f4f4; border:1px solid #e6e6e6;}
.com_datum_itemContent .turn_w .arrow_L{ margin-right:9px; background:#f4f4f4 url(images/arrow_L.png) no-repeat center;}
.com_datum_itemContent .turn_w .arrow_R{ background:#f4f4f4 url(images/arrow_R.png) no-repeat center;}
.com_datum_itemContent .turn_w a:hover{ background-color:#e1e1e1;}

.com_datum_itemContent{ width:936px; height:270px; position:relative;}
.com_datum_itemContent .wrap{ overflow:hidden; }
.com_datum_itemList ul{ width:2000px;}
.com_datum_itemList li{ float:left; width:214px; margin-right:25px;}
.com_datum_itemList li img{ width:214px; height:140px;}
.com_datum_itemList li h3{ padding:8px 0px;}
.com_datum_itemList li h3 a{ color:#343434; font-size:16px;}
.com_datum_itemList li .p1{ height:16px;}
.com_datum_itemList li .p1 a{ color:#808080; display:inline-block; padding-left:20px;}
.com_datum_itemList li .p1 .a_user{  background:url(images/icon_user.png) no-repeat left top; margin-right:30px; }
.com_datum_itemList li .p1 .a_date{ background:url(images/icon_date.png) no-repeat left top;}
.com_datum_itemList li .p2{ padding-top:8px; line-height:20px;}
.com_datum_itemList li .p2 a{ color:#858585;}

/*********************** 常用资料 *******************************/
.com_datum_wrapper{ width:100%; height:100%; background:url(images/blue_paper.jpg) repeat; /* position:absolute;*/}
.com_datum_content{ width:1087px; margin:28px auto 0px; min-height:715px; position:relative;}
.shadow_L{ position:absolute; left:0px; bottom:0px; background:url(images/shadow_L.png) no-repeat left bottom; width:39px; height:360px;}
.com_datum_contentC{ position:absolute; left:39px; top:0px; background:#fff; border:12px solid #f1f1f1; border-width:10px 12px; width:980px; min-height:690px; height:100%;}
.shadow_R{ position:absolute; left:1043px; bottom:0px; background:url(images/shadow_R.png) no-repeat left bottom; width:44px; height:360px;}


.com_datum_contentLeft{ width:716px; height:100%; overflow:hidden; float:left;border:1px solid #dcdcdc; border-right:0px; min-height:690px; position:relative;}
.com_datum_contentList{ width:262px; height:100%; float:left;  background:#f9f9f9; position:relative; border:1px solid #dcdcdc; border-left:0px;} 
.com_datum_contentListC{ padding:10px 20px;}
.com_datum_listItem{ margin-bottom:20px;}
.com_datum_listItem h3{ margin-bottom:15px; font-size:16px; color:#f99f47; height:35px; padding-left:39px; line-height:35px;  font-weight:700; border-bottom:1px solid #d5d5d5; position:relative;}
.com_datum_listItem h3 .ico_head{ display:inline-block; width:33px; height:35px; background:#f9f9f9 url(images/com_datum_item_headS.png) no-repeat left top; position:absolute; bottom:-1px; left:0px;}
.com_datum_listItem ul li{ line-height:24px;}
.com_datum_listItem ul li a{ color:#5a5a5a;}
.com_datum_listItem ul li.current a,.com_datum_listItem ul li:hover a{ color:#0d99e1; border-left:3px solid #0d99e1; line-height:17px; padding-left:5px;}
.com_datum_contentLeft .com_datum_arrowL ,.com_datum_contentLeft .com_datum_arrowR{ position:fixed; width:46px; height:100%; z-index:1003;}
.com_datum_contentLeft .com_datum_arrowL span,.com_datum_contentLeft .com_datum_arrowR span{ display:none; width:46px; height:104px; position:absolute; cursor:pointer; top:240px;}
.com_datum_contentLeft:hover .com_datum_arrowL span,.com_datum_contentLeft:hover .com_datum_arrowR span{ display:block;}
.com_datum_contentLeft .com_datum_arrowL span{ background:url(images/com_arrow_norL.png) no-repeat center; }
.com_datum_contentLeft .com_datum_arrowR span{  background:url(images/com_arrow_norR.png) no-repeat center;}
.com_datum_contentLeft .com_datum_arrowL span:hover{ background:url(images/com_arrow_curL.png) no-repeat center;}
.com_datum_contentLeft .com_datum_arrowR span:hover{  background:url(images/com_arrow_curR.png) no-repeat center;}
.com_datum_contentLeft .com_datum_arrowL.disable span{ background:url(images/com_arrow_unL.png) no-repeat center; cursor:default;}
.com_datum_contentLeft .com_datum_arrowR.disable span{  background:url(images/com_arrow_unR.png) no-repeat center; cursor:default;}

.exp_preview{  position:absolute; background:#fff; width:660px; }
.exp_preview>div{ padding-left:60px; padding-top:35px;}
.exp_preview h2{ font-size:24px; color:#b16706; margin-bottom:30px;}
.card_item { margin-bottom:30px;}
.card_item h3{ color:#252525; font-size:18px;}
.card_item ul li{ color:#616161; font-size:14px; line-height:26px;}

#prevExpMask{ display:none; cursor:url(images/left.cur),auto; background:rgb(255,255,255); filter:alpha(opacity=0); /*IE滤镜，透明度50%*/-moz-opacity:0; /*Firefox私有，透明度50%*/opacity:0;/*其他，透明度50%*/width:314px; height:100%; position:absolute; left:0px; top:0px; z-index:101;  }

.shadow_B{ background:url(images/shadow_B.png) no-repeat center bottom; height:17px; width:100%;}
.shadow_line{ background:url(images/shadow_line.png) repeat-y; height:100%; width:8px; position:absolute; left:0px; top:0px;}

.com_datum_attach{ background:#f4f4f4; height:34px; line-height:34px; color:#535353; font-size:14px; padding:0px 10px; margin:60px auto 25px;}
.com_datum_attach img{ margin:0px 5px; position:relative; top:5px;}
