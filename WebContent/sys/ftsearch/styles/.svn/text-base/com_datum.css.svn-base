.sim_doc_link{ text-decoration:none; transition:background-color 0.3s ease 0s, border 0.3s ease 0s, color 0.3s ease 0s, opacity 0.3s ease-in-out 0s;}
div,ul,li,h3{ margin:0; padding:0;}
ol,ul{ list-style:none;}
div, a {
    font-family: "微软雅黑", Geneva, "sans-serif" , "微软雅黑" , "宋体";
    font-size: 12px;
}