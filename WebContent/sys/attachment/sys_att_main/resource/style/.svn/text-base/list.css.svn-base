.luiAttSubject img {
	vertical-align: middle;
	width: 20px;
	height: 20px;
}

.luiAttSubject {
	width: 30% !important;
	text-align: left !important;
}

.lui-upload-list-operation>div {
	cursor: pointer;
	width: 18px;
	height: 19px;
	display: inline-block;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 18px 19px;
	margin-right: 13px;
	margin-top: 3px;
}

.luiAttHref {
	color: rgb(0, 0, 238) !important;
	text-decoration: underline;
}

.luiAttTip {
	display: inline-block;
	margin-left: 20px;
	color: red;
}