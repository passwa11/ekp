.swfuploadbutton {
    width: 71px;
    height: 25px;
    line-height: 25px;
    background-color: #47b5e6;
    display: inline-block;
    vertical-align: top;
}

.swfuploadtext,.biguploadtext {
    color: #ffffff;
    font-size: 12px;
    text-align: center;
}

.swfuploadtext {
}

.biguploadbutton {
    margin-left: 10px;
    width: 71px;
    height: 25px;
    line-height: 25px;
    background-color: #47b5e6;
    display: inline-block;
    vertical-align: top;
    cursor: pointer;
}

.uploadinfotext {
    display: inline-block;
    padding: 5px 0px 0px 10px;
}

.upload_list_checkbox {
    width: 15px;
}

.tb_noborder td.upload_list_icon {
    width: 15px;
    padding-left: 10px;
    padding-right: 10px;
}

.upload_list_filename_edit {
    color: #57a7da;
    word-break: break-all;
}

body .tb_noborder td .upload_list_filename_edit {
    color: #333;
    margin: 0 5px 0px;
    vertical-align: middle;
    line-height: 20px;
    height: 20px;
    font-size: 15px;
}

.upload_list_filename_view {
    vertical-align: middle;
}

/*文件名超出省略 */
.upload_list_filename_view, .upload_list_filename_edit {
    height: 20px;
    overflow: hidden;
}

.upload_list_filename_title {
    display: inline-block;
    max-width: 100px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: text-top;
    height: 20px;
    line-height: 20px;
    vertical-align: middle;
}

.upload_list_filename_ext {
    vertical-align: middle;
    display: inline;
    /* margin-right: 40px; */
}

.upload_list_filename_view:hover {
    /* color: #e17d27 */
}

.upload_list_filename_s {
    cursor: pointer;
    color: #57a7da;
    min-width: 150px;
}

.upload_list_operation {
    /* padding-right: 5px!important; */
    white-space: nowrap;
}

.upload_list_operation .upload_opt_icon {
    display: inline-block;
    margin: 0 0 0 8px;
    white-space: nowrap;
    text-align: center;
    position: relative;
}

.upload_list_operation div {
    display: inline-block;
}

.upload_list_operation .upload_opt_tip {
    position: absolute;
    top: -27px;
    left: -30px;
    display: none;
    text-align: center;
    right: -30px;
}

.upload_list_operation .upload_opt_tip .upload_opt_tip_arrow {
    content: "";
    display: inline-block;
    width: 0;
    height: 0;
    border: 4px solid transparent;
    border-top-color: #121212;
    position: absolute;
    left: 0;
    right: 0;
    bottom: -8px;
    margin-left: auto;
    margin-right: auto;
}

.upload_list_operation .upload_opt_tip i {
    font-style: normal;
}

.upload_list_operation .upload_opt_tip .upload_opt_tip_inner {
    display: inline-block;
    background: #121212;
    padding: 0 10px;
    border-radius: 5px;
    color: #fff;
    font-size: 12px;
    line-height: 24px;
    display: inline-block;
}

.upload_list_size {
    right: 0;
    /* width: 80px; */
    text-align: right;
    padding-right: 4px!important;
    color: #999;
    white-space: nowrap;
    font-size: 13px;
    position: absolute;
    top: -2px;
}

.upload_list_downtime {
    width: 100px;
}

.upload_list_status {
    white-space: nowrap;
    margin-left: 4px;
    margin-right: 10px;
}

.upload_list_status i {
    display: inline-block;
    width: 20px;
    height: 20px;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 18px auto;
    vertical-align: -5px;
    margin-right: 5px;
}

.upload_opt_status.success i {
    background-image: url(icon_success@2x.png)
}

.upload_opt_status.fail i {
    background-image: url(icon_fail@2x.png)
}

.upload_list_progress {
    width: 200px;
}

.upload_opt_status {
    font-size: 12px;
    display: inline-block;
}

