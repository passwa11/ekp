.lui_atta_showPic {
	padding: 7px 0;
	background: #fff;
	width: 100%;
	margin-left: auto;
	margin-right: auto;
	display: none;
}
.lui_atta_showPic_container{
	position: relative;
	overflow: hidden;
	height: 565px;
	border: 1px solid #b5b5b5;
}
.lui_atta_showPic_content {
	position: absolute; 
	left: 0px;
	display: block;
}
.lui_atta_showPic tr:last-child td{
	 position:relative; 
	 top:10px;
}
.insignia_left {
	display: block;
	margin-top: 50px;
	width: 150px;
	height: 451px;
	z-index: 5;
	position: absolute;
	background-image: url(about:blank);
}
.insignia_right {
	display: block;
	margin-top: 50px;
	width: 150px;
	height: 451px;
	z-index: 10;
	position: absolute;
	right:15px;
	background-image: url(about:blank);
}
.small_pic_list{
	width:100%;
}
.picList {
	height: 112px;
}
.picList UL LI {
	float: left;
	width:11%
}

.picList UL {
	height: 112px;
	border: 0px;
	list-style-type: none;
	width: 100%;
}
.smallDiv{
	width: 78px;
	height: 104px;
	line-height: 100px;
	text-align: center;
}
#sImg {
	float: right;
	position: relative;
	left: -5px;
	top: -20px;
}
.smallDiv2{
	border: #f0f0f0 2px solid;
	height: 104px;
	width: 100%;
	text-align: center;
	line-height: 100px;
}
.smallPic{
	width: 100%;
	height: 100%;
	vertical-align: middle;
}
.img_small_btnr {
	width: 17px;
	height: 112px;
	text-align: center;
	line-height: 112px;
	background: #f3f3f3;
	float: left;
	/*margin-left:13px;*/
}
.img_small_btnl {
	width: 17px;
	height: 112px;
	text-align: center;
	line-height: 112px;
	background: #f3f3f3;
	float: left;
}
.img_small_btnbar {
	/**width: 14px;**/
	height: 14px;
	margin-top: 42px;
	cursor: pointer;
	position: relative;
}
.img_small_btnbar .d_i_trigR {
	border-left: 10px solid #d0d0d0;
	right: 0;
}

.img_small_btnbar .d_i_trig_coverR {
	border-left: 10px solid #f3f3f3;
	right: 4px;
}
.img_small_btnbar .d_i_trig_coverL {
	border-right: 10px solid #f3f3f3;
	left: 4px;
}
.img_small_btnbar .d_i_trigL {
	border-right: 10px solid #d0d0d0;
	left: 0;
}

.img_small_btnbar .d_i_maintrig {
	width: 0;
	height: 0;
	line-height: 0;
	border-top: 10px dashed transparent;
	border-bottom: 10px dashed transparent;
	position: absolute;
	top: 50%;
	margin-top: -10px;
}