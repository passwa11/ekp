.muiAttViewerContainer {
	position: relative;
	text-align: center;
}

.mblBackground {
	background-color: #f7f7f7 !important;
}

.muiAttViewerContainer .muiAttViewerContent {
	-webkit-overflow-scrolling: touch;
	overflow: scroll;
	background-color: #f7f7f7;
}

.muiAttViewerContainer .muiAttViewerContent img {
	margin: 0.5rem 0rem;
}

.muiAttViewerContent .muiAttViewerNo {
	position: absolute;
}

.muiAttViewerContent {
	/* padding-top: 30px; */
}

.muiAttViewerContent .muiAttViewerRate {
	position: absolute;
}

.muiAttViewerSlider {
	position: fixed;
	top: 0rem;
	z-index: 20;
	width: 100%;
	background-color: #18b4ed;
	/*background-color: rgba(0, 0, 0, 0.7);*/
	height: 4rem;
}

.muiAttViewerSlider>div {
	position: relative;
}

.muiAttViewerPageSlider {
	position: relative;
}

.muiAttViewerPage {
	position: absolute;
	font-size: 1.2rem;
	display: inline-block;
	color: #fff;
	float: right;
	right: 0.5rem;
	top: 0rem;
}

.muiAttViewerSlider .mblSliderH {
	vertical-align: middle;
	display: inline-block;
	width: 60%;
}

.muiAttViewerSlider .mblSliderH .mblSliderHandle {
	border-radius: 50%;
	width: 2rem;
	height: 2rem;
	margin: -1rem 0 0 -1rem;
	border: 1px solid #838383;
}

.muiAttViewerSlider .mui.mui-back {
	color: #fff;
	font-weight: bolder;
	font-size: 4.5rem!important;
	float: left;
	line-height: 6rem!important;
	padding: 0 1rem 0 2rem;
	width: 8rem;
	text-align: left;
}

.muiAttViewerSlider .mui span {
	font-size: 4rem!important;
	font-weight: normal;
	padding-left: 0.8rem;
}

.muiAttViewerVideo .muiAttViewerSlider,.muiAttViewerAudio .muiAttViewerSlider {
	height: 3rem;
}

.muiAttViewerVideo .muiAttViewerSlider .mui.mui-back {
	padding: 0 1rem;
	font-size: 2rem!important;
	line-height: 3rem;
}

.muiAttViewerAudio .muiAttViewerSlider .mui.mui-back {
	padding: 0 1rem;
	font-size: 2rem;
	line-height: 3rem;
}

.muiAttViewerVideo .muiAttViewerSlider .mui span {
	font-size: 1.8rem;
	padding-left: 0.3rem;
}

.muiAttViewerAudio .muiAttViewerSlider .mui span {
	font-size: 1.8rem;
	padding-left: 0.3rem;
}

.muiAttViewerSlider>div:after {
	display: block;
	content: 'clear';
	clear: both;
	line-height: 0;
	visibility: hidden;
}

.muiAttViewerSlider>div {
	vertical-align: middle;
	display: inline-block;
	width: 100%;
	height: 100%;
}

.muiAttViewerImg,.muiAttViewerVideo,.muiAttViewerAudio {
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
	padding: 0;
	height: auto;
	margin: 0;
}

.muiAttViewerContent .muiAttViewerVideo {
	width: 100%;
	max-width: auto;
}

.muiAttViewerAudio {
	width: 100%;
	max-width: auto;
}

.muiAttViewerAudio {
	height: 100px;
}
.audionaming{
	margin:0 auto;	
	width:80%;
	height:20%;
}

.muiContainer{
	background-color: #fff;
	margin:6px auto;
	overflow:hidden;
	box-shadow: 0px 1px 10px rgba(30, 29, 29, 0.09);
	visibility:hidden
}