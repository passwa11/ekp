.muiAttViewerZip {
	position: relative;	
}

.muiAttViewerZipHead {
	position: fixed;
	left: 0;
	right: 0;
	height: 4rem;
	background-color: #E8F0FD;
	line-height: 4rem;
	z-index: 10;
	white-space: nowrap;
	overflow-y: hidden;
	overflow-x: auto;
	font-size: 0;
	padding: 0 1.2rem;
}

.muiAttViewerZipHead .muiAttViewerLink {
	color: #4285F4;
	font-size: 1.5rem;
	display: inline-block;
	line-height: 4rem;
	vertical-align: middle;
}

.muiAttViewerZipHead .muiAttViewerLink.active {
	color: #2a2a2a;
}


.muiAttViewerZipHead .muiAttViewerLink i {
	margin: 0 0.6rem;
}

.muiAttViewerZipHead .muiAttViewerZipBtn {
	position: absolute;
	left: 0;
	top: 0;
	height: 4rem;
	line-height: 4rem;
	font-size: 2.4rem;
	padding: 0 1.2rem;
}

.muiAttViewerZipHead .muiAttViewerZipBtn:active {
	background-color: #0c82c5;
}

.muiAttViewerZipHead .muiAttViewerZipBtn > * {
	vertical-align: middle;
}

.muiAttViewerZipTitle {
	white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    padding: 0 6rem;
}

.muiAttViewerZipContent {
	padding-top: 4rem;
}

.muiAttViewerZipEmptyList {
	text-align: center;
	height: 16rem;
	line-height: 16rem;
	color: #aaa;
}

.muiAttViewerZipEmptyList > * {
	vertical-align: middle;
}

.muiAttViewerZipEmptyList i {
	margin-right: 0.4rem;
}

.muiAttViewerZipList {
}

.muiAttViewerZipItem {
	display: flex;	
	height: 7rem;
	padding: 0 1.2rem;
	padding-right: 1.6rem;
	flex-flow: row nowrap;
	align-items: center;
	color: #333;
	border-bottom: 1px solid #f1f1f1;
}

.muiAttViewerZipItem:active {
	background-color: #f5f5f5;
}

.muiAttViewerZipItem .muiAttViewerLeftIcon {
	margin-right: 1.2rem;
	font-size: 4rem;
	width: 2.8rem;
	min-width: 2.8rem;
	height: 2.8rem;
	display: inline-block;
	line-height: 2.8rem;
	text-align: center;
}

.muiAttViewerZipItem .muiAttViewerLeftIcon img{
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}


.muiAttViewerZipItem .muiAttViewerRightIcon {
	margin-left: 1rem;
	font-size: 2.2rem;
}

.muiAttViewerZipItem .muiAttViewerIcon {
	background-size: 100% 100%;
	background-image: url(../images/file.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-excel {
	background-image: url(../images/excel.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-folder {
	background-image: url(../images/folder.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-html {
	background-image: url(../images/html.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-jpg {
	background-image: url(../images/jpg.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-pdf {
	background-image: url(../images/pdf.png);
}

.muiAttViewerZipItem .muiAttViewerIcon-ppt {
	background-image: url(../images/ppt.png);
}
.muiAttViewerZipItem .muiAttViewerIcon-text {
	background-image: url(../images/text.png);
}
.muiAttViewerZipItem .muiAttViewerIcon-word {
	background-image: url(../images/word.png);
}
.muiAttViewerZipItem .muiAttViewerIcon-zip {
	background-image: url(../images/zip.png);
}

.muiAttViewerZipItem .muiAttViewerZipLabel {
	flex: 1;
	font-family: PingFangSC-Regular;
}

.muiAttViewerZipItem .muiAttViewerZipTop {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size: 1.5rem;
	color: #333;
	letter-spacing: 0.08rem;
	margin-bottom: 0.5rem;
	max-width: 25rem;
}

.muiAttViewerZipItem .muiAttViewerZipBottom {
	font-size: 1.2rem;
	color: #999;
	letter-spacing: 0.064rem;
	
}

.muiAttViewerZipItem .muiAttViewerZipBottom span:nth-child(2) {
	float: right;
}

.muiRtfPicSlider {
	position: fixed;
} 