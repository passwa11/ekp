/***********************************   快速审批列表相关（Start）    ***********************************/
#scroll_FastReview .fastReviewTodoListTitle{
	padding-left:1rem;
	padding-right:1rem;
	padding-top: 1.4rem;
	padding-bottom:1rem;
	font-weight: bold;
	width: 50%;
}
#scroll_FastReview .fastReviewTodoList{
    margin-top: 1rem;
}
#scroll_FastReview .muiFastReviewTodoListItem{
    padding-bottom: 1rem;
    position: relative;
}

#scroll_FastReview .muiFastReviewTodoListItem:after{
	content: " ";
    position: absolute;
    z-index: 2;
    bottom: 0;
    left: 0;
    right: 0;
    border-top: .1rem solid #EEEEEE;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}

#scroll_FastReview .muiPortalClickItem:not(:first-child)>.muiFastReviewTodoListItem{
	padding-top: .6rem;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewToTitleLabel{
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    -webkit-line-clamp: 4;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    box-orient: vertical;
    -webkit-box-pack: center;
    box-pack: center;
    -webkit-box-align: start;
    box-align: start;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo{
    position: relative;
    margin-top: .6rem;
    min-height: 2.6rem;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo .muiFastReviewTodoPersonHeadIcon{
    position: absolute;
    width: 2.4rem;
    height: 2.4rem;
    border-radius: 50%;
    border: 1px solid #c9dfff;
    background-color: #f8f8f8;
    overflow: hidden;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo .muiFastReviewTodoPersonHeadIcon img{
	width: 100%;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo .muiFastReviewTodoTextInfo{
    position: relative;
    padding-left: 3.6rem;
    padding-top: .4rem;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo .muiFastReviewTodoTextInfo span{
	display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 100%;
}

.muiFastReviewTodoListItem .muiFastReviewTodoContentContainer .muiFastReviewTodoBaseInfo .muiFastReviewTodoTextInfo span:not(:last-child){
	padding-right: 1rem;
}

.muiFastReviewTodoListItem .muiFastReviewTodoOperateBtnContainer{
	position: relative;
    text-align: right;
    padding-top: .6rem;
}

.muiFastReviewTodoListItem .muiFastReviewTodoOperateBtnContainer .muiProcessBtn{
    display: inline-block;
    background-color: #4285F4;
    border: 1px solid #4285F4;
    color: #FFFFFF;
    border-radius: .2rem;
    padding-left: 1.2rem;
    padding-right: 1.2rem;
    padding-top: .4rem;
    padding-bottom: .4rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoOperateBtnContainer .muiProcessBtn.muiProcessBtnGray{
	background-color: #FFFFFF;
	color: #2A304A;
	border: 1px solid #999999;
}
.muiFastReviewTodoListItem .muiFastReviewTodoOperateBtnContainer .muiProcessBtn:not(:first-child){
	margin-left: 1rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoOperateBtnContainer .muiFastReviewTodoOperatedTag{
	display: inline-block;
	background-color: #F4F4F4;
	border: 1px solid #F4F4F4;
    color: #999999;
    border-radius: .2rem;
    padding-left: 1.2rem;
    padding-right: 1.2rem;
    padding-top: .4rem;
    padding-bottom: .4rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentTable{
	clear: both;
    width: 100%;
    display: table;
    border: 0;
    table-layout: fixed;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentTable>div{
	vertical-align: middle;
    height: 100%;
    border: 0;
    display: table-cell;
    font-size: 1.4rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentLeft{
	width: 1.4rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentRight{
	padding-left: 1rem;
}
.muiFastReviewTodoListItem .muiFastReviewTodoSelArea,.muiFastReviewTodoSelAllNode{
	width: 1.4rem;
    height: 1.4rem;
    line-height: 1.8rem;
    border: .1rem solid #dfe1e0;
    text-align: center;
    border-radius: 2px;
    top: 0.5rem;
    position: absolute;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentLeft .muiFastReviewTodoSeled,.muiFastReviewTodoSeledAll{
	background-color: #4285f4;
    color: #fff;
    border: 0;
    border-radius: 2px;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentLeft .muiFastReviewTodoSeled>.mui,.muiFastReviewTodoSeledAll>.mui{
	font-size: 1rem;
    font-weight: bold;
}
.muiFastReviewTodoListItem .muiFastReviewTodoContentLeft .mui-checked.muiFastReviewTodoSelected,.muiFastReviewTodoSelectedAll {
    font-size: 1rem;
    position: relative;
    top: -0.1rem;
}
.muiFastReviewTodoHead{
	display: table;
	width: 100%;
}
.muiFastReviewTodoHead>div{
	display: table-cell;
}
.muiFastReviewTodoOptArea{
	float: right;
    padding-right: 2rem;
}
.muiFastReviewTodoSelAllArea{
	display: inline-block;
}
.muiFastReviewTodoSelAllArea .muiFastReviewTodoSelAllNode{
	display: inline-block;
    position: inherit;
}
.muiFastReviewTodoReviewNode{
	padding-left: 2rem;
	color: #4285f4;
}
.muiAllReview .muiAllReviewReturn{
	height: 5rem;
    line-height: 5rem;
    font-size: 1.6rem;
    color: #999fb7;
    padding: 0 1.5rem;
    border-bottom: 1px solid #e2e2e2;
}
.muiAllReview .muiAllReviewUsagesRow{
    padding: 0 1.5rem;
}
.muiAllReview .muiAllReviewTitleNode{
	font-size: 1.4rem;
    background-color: transparent;
    color: var(--muiFontColorInfo);
    padding: .5rem 0 1rem;
}
.muiAllReview .commonUsagesDiv .iconArea{
	margin-left: auto;
    margin-right: auto;
    margin-bottom: 5px;
    width: 2.5rem;
    height: 2.5rem;
    line-height: 2.5rem;
    text-align: center;
    color: #4285f4;
    border-radius: 50%;
    display: block;
    position: relative;
}
.muiAllReview .commonUsagesDiv .iconTitle{
	text-align: center;
    font-size: 12px;
    display: block;
    color: #666;
}
.muiAllReview .commonUsagesDiv .handingWay{
	padding: 10px 0;
    vertical-align: middle;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    position: relative;
    display: inline-block;
    width: 20%;
    -webkit-flex: 1;
    flex: 1;
    -webkit-box-flex: 1;
}
.muiAllReview .commonUsagesDiv .handingWay .mui{
	background: url(./icon-usage.svg) no-repeat;
    background-position: center;
    width: 2rem;
    height: 2rem;
    line-height: 2.5rem;
}
.muiAllReview .muiAllReviewUsagesRow .muiTextarea{
	padding: 1rem .8rem;
    background: #f8f8f8;
    border-radius: 4px;
    min-height: 150px;
}
.muiFormSelect .muiCheckItem .muiCheckBlock, .muiFormSelect .muiRadioItem .muiRadioCircle {
    display: none;
}
.muiAllReview .muiTabBarGrid{
	position: fixed;
    bottom: 0;
}
/***********************************   快速审批列表相关（End）    ***********************************/