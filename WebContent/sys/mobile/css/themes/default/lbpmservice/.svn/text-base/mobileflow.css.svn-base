@charset "utf-8";

/* 流程状态数据统计列表 Starts
   ========================================================================== */
.mui_flowstate_card{
  margin: 0;
  padding: .3rem;
  list-style: none;
  background-color: #fff;
}
.muiTemplateView .mui_flowstate_card{
  margin: -.6rem -.6rem 0;
}

.mui_flowstate_card *,
.mui_flowstate_card *:after,
.mui_flowstate_card *:before{
  box-sizing: border-box;
}
.mui_flowstate_card:after,
.mui_flowstate_card_pane:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.mui_flowstate_card_item{
  padding: .3rem;
  width: 33.333333%;
  min-height: 1px;
  float: left;
}
.mui_flowstate_card_pane{
  padding: .5rem .6rem;
  text-align: left;
  color: #333;
  text-decoration: none;
  border-radius: 2px;
  border: 1px solid #e5e5e5;
  background-color: #fff;
  display: block;
  overflow: hidden;
}
a.mui_flowstate_card_pane:active{
  background-color: #f8f8f8;
}

.mui_flowstate_card_pane .icnbox{
  width: 3.6rem;
  height: 3.6rem;
  line-height: 3.6rem;
  text-align: center;
  color: #fff;
  font-size: 2rem;
  border-radius: 50%;
  background-color: #f8f8f8;
  display: block;
  overflow: hidden;
  float: left;
}
.mui_flowstate_card_pane .icnbox img{
  width: 100%;
  height: 100%;
  display: block;
}
.mui_flowstate_card_pane .icnbox .mui {
  line-height: inherit;
  position: relative;
  top: -2px;
}
.mui_flowstate_card_item:nth-child(6n+1) .mui_flowstate_card_pane .icnbox {
  background-color: #d4997b;
}
.mui_flowstate_card_item:nth-child(6n+2) .mui_flowstate_card_pane .icnbox{ background-color: #82b2d1; }
.mui_flowstate_card_item:nth-child(6n+3) .mui_flowstate_card_pane .icnbox{ background-color: #82b2d1; }
.mui_flowstate_card_item:nth-child(6n+4) .mui_flowstate_card_pane .icnbox{ background-color: #92c18f; }
.mui_flowstate_card_item:nth-child(6n+5) .mui_flowstate_card_pane .icnbox{ background-color: #92c18f; }
.mui_flowstate_card_item:nth-child(6n+6) .mui_flowstate_card_pane .icnbox{ background-color: #c0a1cd; }

.mui_flowstate_card_pane .card_content{
  margin-left: 4rem;
}
.mui_flowstate_card_pane .card_num{
  line-height: 1;
  color: #333;
  font-family: Arial;
  font-size: 2.4rem;
  display: block;
}
.mui_flowstate_card_pane .card_txt{
  margin-bottom: 0;
  color: #999;
  font-size: 1rem;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* 流程状态数据统计列表 Ends
   ========================================================================== */


/* 流程状态数据卡片 Starts
   ========================================================================== */
.mui_flowstate_data_group{
  padding: 1.2rem .8rem;
  position: relative;
}
.mui_flowstate_data_group *,
.mui_flowstate_data_group *:after,
.mui_flowstate_data_group *:before{
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.mui_flowstate_data_group:before{
  content: '';
  border-top: 1px solid #e5e5e5;
  position: absolute;
  left: .8rem;
  right: .8rem;
  top: 0;
}
.mui_flowstate_data{
  border-radius: 5px;
  border: 1px solid #e5e5e5;
  background-color: #fff;
}
.mui_flowstate_data:not(:last-child){
  margin-bottom: .6rem;
}
.mui_flowstate_data .color_warning{ color: #e43d41; }
.mui_flowstate_data .color_success{ color: #1bb61f; }
.mui_flowstate_data_heading{
  padding: .5rem .5rem .2rem 5rem;
  position: relative;
}
.mui_flowstate_data_heading:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.mui_flowstate_data_heading .heading_avatar{
  margin-top: .2rem;
  margin-left: -4.5rem;
  width: 4rem;
  height: 4rem;
  border-radius: 50%;
  background-color: #f8f8f8;
  overflow: hidden;
  float: left;
}
.mui_flowstate_data_heading .heading_avatar > img{
  width: 100%;
  height: 100%;
  display: block;
}
.mui_flowstate_data_heading .heading_content{
  padding: .5rem 0;
  padding-right: 5rem;
  border-bottom: 1px solid #e5e5e5;
}
.mui_flowstate_data_heading .heading_title,
.mui_flowstate_data_heading .heading_subhead{
  font-weight: normal;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.mui_flowstate_data_heading .heading_title{
  color: #333;
  font-size: 1.5rem;
}
.mui_flowstate_data_heading .heading_subhead{
  color: #18b4ed;
  font-size: 1.2rem;
}

.mui_flowstate_data_heading .mui_flowstate_data_btn{
  position: absolute;
  right: .5rem;
  top: 50%;
  margin-top: -15px;

}
.mui_flowstate_data_btn {
  display: inline-block;
  padding: 3px 10px;
  margin-bottom: 0;
  color: #18b4ed;
  font-size: 1.5rem;
  font-weight: normal;
  line-height: 1.42857143;
  text-align: center;
  text-decoration: none;
  white-space: nowrap;
  vertical-align: middle;
  touch-action: manipulation;
  cursor: pointer;
  user-select: none;
  background-image: none;
  background-color: #fff;
  border: 1px solid #18b4ed;
  border-radius: 4px;
  outline: 0;
}
.mui_flowstate_data_btn:active{
  background-color: rgba(24, 180, 237, .1);
}
.mui_flowstate_data_content{
  padding: .5rem;
}

.mui_flowstate_data_timeaxis{
  margin: 0;
  padding: 0 0 0 2rem;
  list-style: none;
}
.mui_flowstate_data_timeaxis > li{
  padding-left: 1rem;
  border-left: 1px solid #e5e5e5;
  position: relative;
}
.mui_flowstate_data_timeaxis > li:before{
  content: '';
  width: 9px;
  height: 9px;
  display: inline-block;
  border-radius: 50%;
  border: 1px solid #e5e5e5;
  background-color: #fff;
  position: absolute;
  top: 4px;
  left: -6px;
  z-index: 2;
}
.mui_flowstate_data_timeaxis > li:not(:last-child){
  padding-bottom: .5rem;
}
.mui_flowstate_data_timeaxis > li:first-child:after{
  content: '';
  width: 3px;
  height: 10px;
  background-color: #fff;
  display: inline-block;
  position: absolute;
  left: -1px;
  top: 0;
  z-index: 0;
}
.mui_flowstate_data_timeaxis > li:last-child:after{
  content: '';
  width: 3px;
  height: 10px;
  background-color: #fff;
  display: inline-block;
  position: absolute;
  left: -1px;
  bottom: 0;
  z-index: 0;
}

.mui_flowstate_data_timeaxis > li .title,
.mui_flowstate_data_timeaxis > li .content{
  color: #333;
  font-size: 1.2rem;
}
.mui_flowstate_data_timeaxis > li .title{
  color: #999;
}
/* 流程状态数据卡片 Ends
   ========================================================================== */
