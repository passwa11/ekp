.muiSimple>tbody td:after {
	left: 0;
}
.muiSimple>tbody tr:last-child td:after {
	display: none;
}

.muiFormEleTip{
	margin-bottom: 1rem;
}

.muiFormEleTitle {
	color: var(--muiFontColorInfo);
	font-size: 1.4rem;
	margin-bottom: 1rem;
}


.muiFormEleWrap {
	box-sizing: border-box;
	position: relative;
	font-size: 1.4rem;
}

.muiFormEleWrap .mui-close {
	color: #fff;
	background-color: #aaa;
	border-radius: 50%;
	width: 1.2rem;
	height: 1.2rem;
	font-size: 0.8rem;
	line-height: 1.4rem;
	text-align: center;
	display: none;
}

/*文档内容信息*/
.muiDocFrame {
	margin: 0.8rem 1rem 0rem;
}

.muiDocFrame .muiDocContent .muiFieldRtf {
	padding-right: 0;
}
.muiDocFrame .muiDocContent .muiFieldRtf>p>em{
	font-style: italic;
}
.muiDocSubjectMain{
	text-align: left;
    padding: 0rem 1.2rem;
}

/*标题*/
.muiDocSubject {
	margin: 1.5rem 0;
	color: var(--muiFontColorInfo);
	font-size: var(--muiFontSizeMA);
	overflow: hidden;
	line-height: 2.4rem;
	word-break: break-word;
	font-weight: bold;
	display: inline-block;
}

.muiDocSubject .muiEssence {
	position: relative;
	top: -0.3rem;
}

/*简要信息*/
.muiDocInfo {
	color: #979DB1;
	font-size: var(--muiFontSizeMS);
	line-height: 1.5rem;
}
.muiDocInfo > span {
	padding-right: 1rem;
	font-family: "landray-number", -apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
}
/* 小标题*/
.muiTabStyle{
	position:relative;
	margin-top: 2rem;
	padding-left: 1rem;
	font-size: 1.4rem;
	color: #191C2C;
}

.muiTabStyle:before{
	position: absolute;
	content: "";
	border-radius: 2px;
    height: 1.2rem;
    width: 3px;
    float: left;
    background-color: #4285F4;
    display: inline-block;
    margin-right: 10px;
    margin-top: 4px;
    left: 0;
    top: 0;
}

/*文档摘要区域*/
.muiDocSummary {
    margin: 2rem 0 1.5rem;
    text-align: justify;
    line-height: 20px;
    font-size: var(--muiFontSizeS);
    word-wrap: break-word;
    word-break: break-all;
    color: #979DB1;
}

/*文档摘要*/
.muiDocSummarySign {
	text-indent: 0rem;
	padding: 0.1rem 0.4rem;
	color: #A7A7A7;
	display: inline-block;
	border: 1px solid #CDCDCD;
	border-radius: 4px;
}

/*文档正文*/
.muiDocContent {
	color: #1b1b1b;
	word-break: break-all;
}

/*@富文本-开始*/
.muiFieldRtf blockquote {
	padding-left: 10px;
	border-left: 3px solid #ddd;
}

.muiFieldRtf.muiFieldRtfEdit {
	overflow: initial;
	padding: 0;
}
.muiFieldRtf.muiFieldRtfEdit>div>div.muiFormRequiredShow {
	bottom: 0px;
}


/*还原ul,ol本身的样式*/
.muiFieldRtf a{
	text-decoration: underline;	
}
.muiFieldRtf ul, .muiFieldRtf ol {
	padding-left: 20px;
}

.muiFieldRtf ul li {
	list-style: disc;
}

.muiFieldRtf i {
	font-style: italic;
}

.muiFieldRtf ol li {
	list-style-type: decimal;
}

.muiFieldRtf strong {
	font-weight: bold;
}
.muiFieldRtf em strong span{
	font-style: italic;
}
.muiFieldRtf {
	padding: 1rem 0;
	font-size: 1.4rem;
	word-wrap: break-word;
	word-break: break-word;
	overflow: hidden;
	white-space: normal;
	-webkit-user-select: auto;
}
.muiFieldRtf img {
	max-width: 100% !important;
}

.muiFieldRtf p {
	margin: 2px 0;
	font-size: 1.4rem;
}

.ck-editor__editable { min-height: 100px; }


/*@富文本-结束*/

/*必填*/
.muiFormRequired {
	display: none;
	color: #FF605B;
	height: 2rem;
	line-height: 2rem;
	position: absolute;
	right: .5rem;
	z-index: 8;
	font-size: inherit;
	padding-top: .3rem;
}

.muiFormRequiredShow{
	display: block;
}

/*检验-开始*/
.muiValidate {
	position: relative;
	margin-top: 0.5rem;
	text-align: left;
}

.muiValidateShape {
	width: 4px;
	height: 4px;
	position: absolute;
	top: 0.9rem;
	border-radius: 50%;
	background-color: #FF605B;
	border: 0;
	margin: 0;
}

.muiValidateMsg {
	display: inline-block;
	color: #FF605B;
	font-size: 1.1rem;
	word-wrap: break-word;
	word-break: break-all;
}

.muiValidateInfo {
	padding-left: 1rem;
}
/*检验-结束*/

.muiSelInput::-webkit-input-placeholder {
	font-size: 1.4rem;
}

.muiSelInputRight {
	position: absolute;
	right: 0;
	color: #d5d5d5;
	display: none;
}

.muiSelInputRight.muiSelInputRightShow{
	display: block;
}

.muiSelInputRight{
	right: 2rem;
}

.muiSelInputRight>span {
	color: var(--muiFontColor);
	z-index: 3;
	position: relative;
	display: inline-block;
}

.muiSelInputRight .mui, .muiSelInputRight .fontmuis {
	color: #C7C7CC;
	transform: scale(0.8);
}

.muiSelInputRight .mui.mui-forward {
	color: #aaa;
}

.muiFormStatusView {
	display: block;
	color: var(--muiFontColorInfo);
}

.muiFormStatusView .muiSelInput {
	font-size: 1.4rem;
}

.muiFormStatusEdit .muiSelInput:empty:after {
	content: attr(placeholder);
	color: #C4C6CF;
	display: block;
	height: 100%;
	/*font-size: 1.4rem;*/
	white-space: initial;
	word-wrap: break-word;
    word-break: break-word;
}

.muiValidateWrap .muiFormItem{
	padding-right: 2rem;
}

.muiSelInputRightShow+.muiSelInput{
	padding-right: 4rem;
}

/*地图控件预输入信息与后面的定位文字重叠问题*/
.muiSelInputRightShow.muiSelInputRightText+.muiMapInput.muiSelInput{
	padding-right: 6.5rem;
}

/*@newMui开头，竖向布局*/
.newMui.muiFormStatusView {
	min-height: auto;
}


/*@oldMui开头，横向布局*/

.oldMui .muiFieldRtf.muiFormEleWrap {
	padding: 0.5rem 0;
}

/*以下未整理*/

/*图标对齐*/
.actionView .detailNode .muiFieldItem>div.muiFieldOpt, .muiFormContent .muiSimple>tbody>tr>td:first-child .muiFieldItem>.muiFieldOpt
