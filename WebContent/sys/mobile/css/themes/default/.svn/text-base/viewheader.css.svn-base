/*默认表头*/
.muiDefaultHeader{
    position: relative;
    margin-bottom: -0.1rem !important;
    background-color:#FFFFFF !important; 
    padding-bottom: 3.8rem;
}
.muiDefaultHeader .muiDefaultBg{
	background: #6e98dd url(images/default_bg.jpg) no-repeat left top;
	height: 10rem;
	width:100%;
    background-size: cover;
    
}
.muiDefaultHeader .muiDefaultIcon{
    width: 4rem;
    height: 4rem;
    border-radius: 50%;
    position: absolute;
    left: 1.4rem;
    bottom: 2rem;
}

.muiDefaultHeader .muiDefaultLabel{
    position: absolute;
    left: 5.6rem;
    bottom: 2rem;
    color: #868686;
    font-size: 1.2rem;
    padding-top: 0.2rem;
}
.muiDefaultHeader .muiDefaultCreated{
    color: #868686;
    position: absolute;
    right: 1rem;
    font-weight: normal;
    font-size: 1.2rem;
    bottom: 2rem;
    padding-top: 0.2rem;
}
/*封面显示*/
.muiPaperHeader{
	height:19.3rem; 
	width:100%;
	position:relative;
	background-size:cover;
	margin-bottom: 0rem !important;
}
.muiPaperDefHeader{
	background:url(images/cover_bg.jpg) center center no-repeat;
}

.muiPaperHeader .muiPaperTitle{
	padding:5px 1rem;  position:absolute; bottom:0; left:0; width:100%; box-sizing:border-box;-moz-box-sizing:border-box; -webkit-box-sizing:border-box; color:#fff;  background:rgba(0,0,0,0.5); 
}
.muiPaperHeader .muiPaperTitle span{
	max-height: 3.6rem;
	line-height: 1.9rem;word-break:break-all; word-wrap:break-word;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
/*文档显示*/
.muiDocumentHeader{
	background:url(images/document_bg.jpg) no-repeat center;
	background-size:cover;
	position:relative;
	overflow : hidden;
	padding-top: 2rem;
    padding-bottom: 1.5rem;
    padding-right: 1rem;
    margin-bottom: 0rem !important;
}
.muiDocumentHeader .muiDocumentLeftInfo{
    position: absolute;
    left: 1rem;
    top: 2rem;
}
.muiDocumentHeader .muiDocumentLeftInfo .muiDocumentIconArea{
	display: inline-block;
    width: 5rem;
    height: 5rem;
    border-radius: 50%;
    border: 3px solid rgba(255,255,255,1);
}
.muiDocumentHeader .muiDocumentLeftInfo .muiDocumentIconArea .muiDocumentIcon{
    width: 5rem;
    height: 5rem;
    border-radius: 50%;
}
.muiDocumentHeader .muiDocumentRightInfo{
	position: relative;
    margin-left: 7.5rem;
}
.muiDocumentHeader .muiDocumentRightInfo .muiDocumentTitle{
	margin-bottom: .2rem;
    text-align: left;
    color: #fff;
    font-size: 1.8rem;
    font-weight: 700;
    word-break: break-all;
    min-height: 5.6rem;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
}
.muiDocumentHeader .muiDocumentRightInfo .muiDocumentSummary{
	position: relative;
    list-style: none;
    padding-left: 0;
    margin: 0;
}
.muiDocumentHeader .muiDocumentRightInfo .muiDocumentSummary li{
    margin-top: .2rem;
    margin-right: 1rem;
    line-height: 1;
    color: #fff;
    font-size: 1.2rem;
    display: inline-block;
}
/*流程状态显示*/
.muiProcessHeader{
	display: table; width: 100%; padding:0; position:relative;
	margin-bottom:0rem !important;
}
.muiProcessHeader .muiProcessStatus{
 	width: 6rem; padding-right: 1rem; 
}
.muiProcessHeader .muiProcessStatus .mui{
 	vertical-align: top;
  	font-size: 9rem;
  	color: #fd6868;
  	position: absolute;
  	top: 0rem;
  	opacity: 1;
  	right: 0;
  	z-index: 1;
}
.muiProcessHeader .muiProcessStatus .mui-process00{
    color: #aaa;
}
.muiProcessHeader .muiProcessStatus .mui-process00:before{
	content: "\C147";
}
.muiProcessHeader .muiProcessStatus .mui-process30{
    color: #fd6868;
}
.muiProcessHeader .muiProcessStatus .mui-process30:before{
	content: "\C146";
}

/*头像显示*/
.muiPersonHeader{
	padding:0.3rem 0rem;
	display: inline-block;
}
.muiPersonHeader .muiPersonIcon{
	width: 4rem;
    height: 4rem;
    border-radius: 50%;
}

.muiFlowBack{background-color: #F7F7F7; }
.muiFlowInfoW {margin-bottom:0.5rem; background: #fff; padding-left:1.2rem;text-align:left;}
.muiFlowInfoW .muiFlowTable tr td{ border: none !important;}
.muiFlowInfoW .muiFlowTable .muiFlowCreator{color:#999;font-size: 1.3rem; padding-top:0.2rem; text-align: center;word-break:break-all;}
.muiFlowInfoW .muiFlowTable .muiFlowSummary{color:#999999;font-size: 1.3rem;}
.muiFlowInfoW .muiFlowTable .muiFlowIconTd{ width:4rem; padding-right:1rem; }
.muiFlowInfoW .muiFormSubject{padding-top:1.5rem; color: #2e2e2e;font-size: 2rem; overflow: hidden; line-height: 2.4rem;word-break: break-all;font-weight: bold;}
.muiFlowInfoW .muiNavbarContainer,.muiFlowFixedItem .muiNavbarContainer{ background: #fff; border-bottom: 1px solid #f7f7f7;}
.muiFlowInfoW .muiFlowHeader { display: table; width: 100%; padding: 0.8rem 0; position:relative; }
.newMui .muiFlowInfoW .muiFlowHeader{padding: 0.8rem 0 0 0;}
.muiFlowInfoW .muiFlowHeader > div { display: table-cell; vertical-align: middle; }
.muiFlowInfoW .muiFlowHeader .muiProcessIcon {  width: 4.2rem; padding:0 0.8rem; }
.muiFlowInfoW .muiFlowHeader .muiProcessIcon img{ width:4.2rem; height:4.2rem;}
.muiFlowInfoW .muiFlowHeader .muiProcessTitle { font-size: 1.4rem; }
.muiFlowInfoW .muiFlowHeader .muiProcessTitle .muiFormEleWrap {
  display: flex;
  align-items: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  padding-left: 0;
  
}
.muiProcessStatus { filter:alpha(opacity=0);-moz-opacity:0; opacity:0; width: 6rem; height: 3.8rem; padding-right: 1rem; position: absolute; top:1.5rem;right: 0.2rem;}
.muiProcessStatus .mui {
  vertical-align: top;
  font-size: 9rem;
  color: #fd6868;
  position: absolute;
  top: -1rem;
  right: 0;
}
.muiProcessStatus.stamp{ filter:alpha(opacity=100);-moz-opacity:1; opacity:1; animation:stamp 0.2s;-webkit-animation:stamp 0.2s; transition:all 0.1s ease 0s;}

.muiDiscardStatus { filter:alpha(opacity=0);-moz-opacity:0; opacity:0; width: 6rem; height: 3.8rem; padding-right: 1rem; }
.muiDiscardStatus .mui { font-size: 9rem; color: #aaa;  }
.muiDiscardStatus .mui:before{ position:relative;} 

.muiDiscardStatus.stamp { }

.muiDiscardStatus.stamp{ filter:alpha(opacity=100);-moz-opacity:1; opacity:1; animation:stamp 0.2s;-webkit-animation:stamp 0.2s; transition:all 0.1s ease 0s;}
@-webkit-keyframes stamp {
    0% {-webkit-transform: scale(2);}
 100% {-webkit-transform: scale(1);}
}
@keyframes stamp {
     0% { transform:scale(2);}
  100% {transform:scale(1);}
}