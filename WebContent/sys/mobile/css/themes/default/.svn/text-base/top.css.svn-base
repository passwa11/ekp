.muiTop {
	width: 5rem;
	height: 5rem;
	line-height: 5rem;
	vertical-align: middle;
	text-align: center;
	position: fixed;
	right: 1.25rem;
	bottom: 5.5rem;
	border-radius: 50%;
	background: #CACACA;
	box-shadow: 0 6px 10px 0 rgba(59,68,93,0.20);
	cursor: pointer;
	display: none;
	z-index: 99;
}

.muiTop>i {
	color: #fff;
	font-size:var(--muiFontSizeL);
	vertical-align: middle;
}
/* 群协作入口 */
.muiCooperate {
	width: 42px;
	height: 42px;
	line-height: 42px;
	vertical-align: middle;
	text-align: center;
	position: fixed;
	bottom: 14.5rem;
	right: 5px;
	border-radius: 50%;
	background: #4285f4;
	background-size: 42px;
	cursor: pointer;
	display: block;
	line-height: 42px;
	z-index: 99;
	color: #fff;
	font-size: 1.2rem;
}

/*addBottomTipMixins 样式*/
.addBottomTip{
	color:#C1C5D2;
	background:#F5F6FB;
	text-align:center;
}

.addBottomTip .addBootomTipContent{
	padding:3rem;
	padding-bottom: 5rem;
}
.addBootomTipContent .notice{
	font-size:1.4rem;
	position:relative;
}
.addBootomTipContent .notice:before,.addBootomTipContent .notice:after{
	content: '';
    display: block;
    width: 5.9rem;
    border-top: 1px solid rgba(179,179,179,.45);
    position: absolute;
    -webkit-transform: scaleY(.5)

}
.addBootomTipContent .notice:before{
	top: 50%;
    left: 50%;
    margin-left: -10rem;
}
.addBootomTipContent .notice:after{
	top: 50%;
    left: 50%;
    margin-left: 4rem;
}

.addBootomTipContent img{
	width:5rem;
}