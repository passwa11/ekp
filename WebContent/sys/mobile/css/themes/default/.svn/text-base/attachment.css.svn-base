/*只有自定义表单移动设计器使用*/

/*附件区域*/
.muiAttachments {
 	border-radius:5px;	
 	overflow: hidden; 
}
/*正常模式下*/
.muiAttachmentsnormal{
	/*border: 1px solid #DBDBDB;*/
}
/*简单模式下*/
.muiAttachmentssimple{
	border: none;
}
/*附件项默认样式*/
.muiAttachmentItem {
	width: 100%;
	clear: both;
	min-height:5.6rem;
 	height:5.6rem;	
	display: table;
}
/*标题项*/
.muiAttTitle{
	height: 3.5rem;  
	min-height:3.5rem;
	line-height: 3.5rem;
	/*background-color: #f3f3f3 !important;*/
	/*border-top: 1px solid #DBDBDB;*/
	border-bottom: 1px solid #DBDBDB;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemIcon{
	height: 3.4rem;
	width: 2rem;
	background:none;
}
.muiAttTitle .muiAttachmentItemIcon i.mui{
	color: #000 !important;
	font-size: 1.6rem;
	line-height: 3.6rem;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemL{
	padding: 0 0 0 0.4rem;
	width: 2rem;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemName{
	font-size: 1.2rem;
	margin-bottom: 0;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemC{
	padding: 0 0 0 0.2rem;
}
.muiAttTitle .muiAttachmentItemR{
	display: none !important;
}
/*列表下附件项*/
.muiAttachmentsnormal .muiAttachmentList .muiAttachmentItem{
	border-bottom: 1px solid #DBDBDB;
	background-color: #FFF;
}
.muiAttachmentsnormal .muiAttachmentList .muiAttachmentItem:last-child{
	/*border:none;*/
}
/*列表下附件项简单模式*/
.muiAttachmentssimple .muiAttachmentList .muiAttachmentItem{
	border:none;
	background-color: #FFF;
}
/*选中效果*/
.muiAttachmentList .muiAttachmentItem.mblListItemSelected{
	background-color: #E4E4E4 !important;	
}

/*附件项内部样式*/
.muiAttachmentItem>div {
	border-collapse: 0rem;
	border-spacing: 0rem;
	padding: 0.5rem 0 0.5rem 0.5rem;
}
.muiAttachmentItem .muiAttachmentItem:first-child .muiAttachmentItemName{
	height: 3.5rem;
	line-height: 3.5rem;
	max-height: 3.5rem;
}
/*附件项名称*/
.muiAttachmentItem .muiAttachmentItemName {
	margin-bottom: 0.2rem;
	width: 100%;
	max-height: 3.2rem;
	line-height:1.6rem;
	vertical-align: middle;
	color: #262626;
	font-size: 1.4rem;
	word-break: break-all;
	word-wrap: break-word;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.muiAttachmentItem .muiAttachmentItemL {
	display: table-cell;
	/*padding-left: 0.8rem;*/
	text-align: center;
	width: 3rem;
	line-height: 3rem;
	vertical-align: middle;
}
.muiAttachmentItem .muiAttachmentItemR {
	width:2.5rem;
	padding-right: .5rem;
	text-align:right;
	vertical-align: middle;
	display: table-cell;
}

.muiAttachmentItem .muiAttachmentItemC {
	display: table-cell;
	max-width: 70%;
	vertical-align:middle;
	padding-left: 0.8rem;
}
/*附件大小*/
.muiAttachmentItem .muiAttachmentItemSize {
	height: 1.5rem;
	line-height: 1.5rem;
	font-size: 1.2rem;
	display: block;
	color: #a4a4a4;
}
/*附件打开图标*/
.muiAttachmentItem .muiAttachmentItemExpand {
	text-align: center;
	color: #7B7B7B;
}
.muiAttachmentItem .muiAttachmentItemExpand.mui{
	font-size: 2rem;
}
/*附件图标*/
.muiAttachmentItem .muiAttachmentItemIcon {
	height: 4.8rem;
	width: 4.8rem;
	overflow: hidden;
}

.muiAttachmentItem .muiAttachmentItemIcon img{
	width: 100%;
	min-height: 100%;
}
.muiAttachmentItem .muiAttachmentItemIcon i{
	width: 100%;
	height:100%;
	line-height: 5rem;
	color: #fff;
}
.mui-file-audio{background:#8eddfc}
.muiAttachmentItem .muiAttachmentItemIcon .mui{ font-size: 4rem;}
.mui-files .muiAttachmentItemIcon{
	background-color: #7c88a1;
}

.mui-files-ppt .muiAttachmentItemIcon{
	background-color: #e49446;
}

.mui-files-img .muiAttachmentItemIcon{
	background-color: #f8f8f8;
}

.mui-files-text .muiAttachmentItemIcon{
	background-color: #4b7bc4;
}

.mui-files-excel .muiAttachmentItemIcon{
	background-color: #61b045;
}

.mui-files-pdf .muiAttachmentItemIcon{
	background-color: #e6546f;
}

.mui-files-word .muiAttachmentItemIcon{
	background-color: #4178d3;
}

.mui-files-zip .muiAttachmentItemIcon{
	background-color: #559bcc;
}

.mui-files-project .muiAttachmentItemIcon{
	background-color: #33ba5f;
}

.mui-files-visio .muiAttachmentItemIcon{
	background-color: #1d6ad6;
}

.mui-files-html .muiAttachmentItemIcon{
	background-color: #7084bd;
}

.mui-files-video .muiAttachmentItemIcon{
	background-color: #7084bd;
}

/*附件编辑*/
.muiAttachmentList{ zoom: 1; padding:0 1.2rem 0 0; border-bottom:1px solid #dbdbdb;}
.muiAttachmentList{border-bottom: 0;}
.splitLine .muiAttachmentList{ padding-left:0px;} /*流程新建输入框*/
.muiAttachmentList:after{ display:block; content:'clear'; clear:both; line-height:0; visibility:hidden;}
.muiAttachmentList .muiAttachmentItem:last-child{ border-bottom:none;}
.muiAttachmentEditItem {
	display:block;
	vertical-align: top;
	float: left;
	box-sizing: border-box;
	width: 20%;
	position: relative;
}
.muiAttachmentEditItem.muiRight{
	float: right;
}
.muiAttachmentEditItem {
	width: 6rem;
}

.muiAttachmentEditItem:after {
	content: '';
	display: inline-block;
	padding-bottom: 100%;
	width: 0.1px;
	vertical-align: middle;
}
.muiAttachmentEditItem .muiAttachmentItemT{
	position: absolute;
	top: 0.5rem;
	bottom: 0.5rem;
	left: 0rem;
	right: 1rem;
	border-radius:4px;
}
.muiAttachmentEditOptItem .muiAttachmentItemT{
	border: 1px solid #d5d5d5;
	background-color: #F9FAFC;
}
.muiAttachmentItemT .muiFieldRequired{
	position: absolute;
	top: 0rem;
	right: 0.4rem;
}
.muiAttachmentEditItem .muiAttachmentItemStatus{
	position:absolute;
	top:0rem;
	left:0rem;
	width:100%;
	height: 100%;
	opacity: 0.9;
	z-index: 3;
	text-align:center;
	border-radius:5px;
	background-color: #EEEEEE;
	line-height: 6rem;
	font-size: 1.2rem;
	white-space: nowrap;
}
.muiAttachmentEditItem .muiAttachmentItemStatus>.mui{ 
	line-height: 6rem;
	position: relative;
	top: -2px;
}
.muiAttachmentEditItem .muiAttachmentItemIcon{
	position:relative;
	width: 100%;
	height:100%;
  text-align:center;
  border-radius:6px;
	overflow: hidden;
	display: flex;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
}
.muiAttachmentEditItem .muiAttachmentItemIcon i{
	line-height: inherit!important;
	color: #fff;
	font-size: 2.5rem;
}
.muiAttachmentEditItem .muiAttachmentItemText{ white-space: nowrap; }
.muiAttachmentEditItem .muiAttachmentItemIcon i {
	line-height: 6rem;
}
.muiAttachmentEditItem .muiAttachmentItemIcon i.muis-new{ color: #b7b7b7;}
.muiAttachmentEditItem .muiAttachmentItemIcon i.mui-insert{ color: #b7b7b7;}
.muiAttachmentEditItem .muiAttachmentUploadFile{
	position:absolute;
	top:0rem;
	left:0rem;
	width:100%;
	height: 100%;
	cursor: pointer;
	filter:alpha(opacity:0);
	opacity: 0;
	z-index: 5;
}

.muiAttachmentEditItem .muiAttachmentItemB{
	overflow: hidden;
	position: absolute;
	left: 0;
	bottom: -1.6rem;
	right: 1rem;
}

.muiAttachmentEditOptItem .muiAttachmentItemB{
	width: 100%;
	text-align: center;
	display: inline-block;
	color: #898989;
	font-size: 1.3rem;
	height: auto;
	line-height: 2rem;
	overflow: hidden;
}

.muiAttachmentEditItem .muiAttachmentItemName {
	width: 100%;
	color: #666;
	font-size: 1.2rem;
	line-height:1.4rem;
	white-space:nowrap;
	overflow: hidden;
	text-align: center;
	text-overflow: ellipsis;
}
.muiAttachmentEditItem .muiAttachmentItemSize {
	text-align: center;
	line-height: 1.2rem;
	font-size: 1rem;
	display: block;
	color: #999;
	white-space:nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	display: none;
}
.muiAttachmentEditItem .muiAttachmentItemDel{
	position:absolute;
	top:-1rem;
	right:-1rem;
	width: 1.5rem;
	height: 1.5rem;
	line-height: 1.4rem;
	vertical-align:middle;
	z-index: 5;
	border-radius:50%;
	text-align:center;
	margin:.6rem;
	background-color: #fff;
}
.muiAttachmentEditItem .muiAttachmentItemDel i{
	line-height:1.5rem;
	font-size: 1.5rem;
	color: #FF605B;
	width: 100%;
	height: 100%;
}
/*附件上传选择*/
.muiAttachmentUploadArea{
	position: absolute;
	top:0rem;
	width:100%;
	height: 100%;
	z-index: 100;
	text-align: center;
}
.muiAttachmentUploadbak{
	position: absolute;
	width:100%;
	height: 100%;
	background-color:#999999;
	opacity: 0.7;
	z-index: 101;
}
.muiAttachmentUploadOpt{
	position: absolute;
	z-index: 102;
	width:100%;
	bottom: 0.5rem;
}

.muiAttachmentUploadSelect,.muiAttachmentUploadCancel{
	background-color:#F7F7F7;
	opacity: 1 important;
	text-align: center;
	color:#017BFF;
	width:100%;
	width:95%;
	margin:0.8rem auto;
	font-weight: bold;
	font-size:1.6rem;
	border-radius:0.6rem;;
}

.muiAttachmentUploadSelect > div, .muiAttachmentUploadCancel{
	height: 4rem;
	line-height: 4rem;
	vertical-align: middle;
}
.muiAttachmentUploadSelect > div {
	border-bottom: 1px solid #DBDBDB;
}
.muiAttachmentUploadSelect > div:last-child {
	border-bottom: none;
}


.textAlign{text-align: center;}
.muiReadAll {
	font-weight: normal;
	display: inline-block;
	width: 90%;
	font-size: 2rem;
	margin: 2.3rem 0rem 1.5rem;
	height: 45px;
	line-height: 45px;
	color: #fff;
	background-color: #5bc91f;
}
.muiReadAll:active{ opacity: .75; }

.muiAttachmentItem .muiReadAllItem{ padding-left:1rem;}
.muiReadAllItem .muiAttachmentItemC{ padding-left:0px;}

.muiReadAll:active{ opacity: .75; }
.muiReadAllItem .muiAttachmentItemC{ padding-left:0px;}
.mblListItemSelected{ background-color: #fff!important; }
.muiOtherOpt{
	padding-right: 1.2rem;
	text-align: right;
}
.muiOtherOpt .muiDownloadBtn,
.muiOtherOpt .muiEditBtn{
	color: #7B7B7B;
	font-size: 1.5rem;
}
.muiOtherOpt .muiDownloadBtn:active,
.muiOtherOpt .muiEditBtn:active{
	opacity: .75;
}
.muiOtherOpt .muiEditBtn > .mui{
	margin-right: .3rem;
}
.muiOtherOpt .mui{
	top: -.1rem;
}
.muiOtherOpt .muiDownloadBtn{}
.muiOtherOpt .mui-top{
	transform: rotate(180deg);
}
.muiOtherOpt .muiEditBtn{
	margin-left: 1.5rem;
}