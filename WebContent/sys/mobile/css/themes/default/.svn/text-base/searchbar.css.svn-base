.muiSearchButton{
	color:#3564aa;
	min-width:4rem;
	height: 100%;
	cursor: pointer;
	font-size: var(--muiFontSizeMA);
}

.muiSearchBarDiv{
	position:absolute;
	top:0px;
	right:0px;
	width:100%;
	z-index:21;
	padding: 0rem 0rem;
	overflow: hidden;
}
.muiSearchBarDiv.mportalSearchBarDiv{
	width: 100%;
}

.muiSearchBarWrap{
	float:right;
	width:85%;
}

.muiSearchResult{
	float:right;
	width:85%;
}

.muiSearchBar{
	vertical-align: middle;
	padding:0 1.2rem;
	position: relative;
	z-index: 14;
	background-color: #fff;
}
.muiSearchBar > form{
	vertical-align: middle;
}
.muiSearchBarContainer{
	display:table;
	width:100%;
	height: 3.4rem;
	line-height: 3.4rem;
    padding-top: .8rem;
    padding-bottom: .8rem;
    margin: 0 auto;
}
.muiSearchBarContainer > div{
	vertical-align: middle;
	display: table-cell;
}
.muiSearchDiv{
	background: #F7F7F7;
	border-radius: .6rem;
	position: relative;
	height: 100%;
}

.muiSearchInput,
.muiSearchClear {
	border: none !important;
	width: 2.5rem;
	font-size: 1.6rem;
	height: 100%;
	line-height: 3.4rem;
	vertical-align: middle;
	display: inline-block;
	background: none !important;
}
.muiSearchIcon{
	color:#D2D3D8;
	display: inline-block;
	line-height: 3.4rem;
	height: 3.4rem;
	width: 3rem;
	font-size:1.6rem;
	vertical-align: middle;
}

.muiSearchIcon,.muiSearchClear{
	position: absolute;
	color:#D2D3D8;
	text-align:center;
}
.muiSearchIcon{
	top:0rem;
	left:0.5rem;
}
.muiSearchClear{
	font-size: 1.4rem;
	top:0rem;
	right:0;
	display: none;
	z-index: 14 ;
}
.muiSearchInput{
	padding:0 2rem 0 3.5rem;
	width:100%;
	text-align:left;
	font-size:1.2rem;
	line-height:2.2rem;
	height:2.2rem;
	vertical-align:middle;
	outline:none;
	-webkit-appearance:none;
	box-shadow:none;
}

.muiSearchInput::-webkit-input-placeholder{
	color: #B1B1B1;
}
.muiSearchInput::-webkit-search-cancel-button {
	display: none;
}
.muiSearchBtnDiv{
	text-align: center;
	width:4.8rem;
	height:100%;
	overflow: hidden;
}
.muiSearchCancelBtn{
	padding:0rem 0.3rem;
    text-align:center;
    vertical-align:middle;
    display:inline-block;
    white-space:nowrap;
    cursor:pointer;
}
.muiSearchPrompt,.muiSearchComplete{
	position: absolute;
	right: 0;
	z-index:21;
	overflow:hidden;
	background-color:rgba(255, 255, 255 , 1);
	padding: 0rem 0rem;
    width: 100%;
    top: 5rem;
    overflow: auto;
    bottom: 0;
}

.muiSearchComplete{
	bottom: 0;
	z-index:21;
}

.muiSearchPrompt{
	opacity: 0;
}

.muiSearchPrompt>div{
	height: 100%;
	width: 100%;
	background-color: #F6F6F6;
}

.muiSearchPrompt .muiSearchOpt{
	text-align: center;
	margin-top: 2.5rem;
}
.muiSearchRecord > div{
	border-bottom:  #e5e5e5 solid 1px;
}

.muiSearchRecord .muis-delete{
	float: right;
    color: #EAEBF0;
    font-size: 2.1rem;
}

.muiSearchOpt >div{
	display: inline-block;
}

.muiSearchOpt .muiSearchClearBtn{
    width:60%;
    margin:0 auto;
	padding: 1rem 0;
    color:#fff;
	background-color: #55B3F4;
    border-radius:0.2rem;
	cursor: pointer;
}

.muiSearchTitle{
	padding: 0rem 1.5rem;
	color:#999FB7;	
	height:3.2rem;
	line-height:3.2rem;
	font-size:1.2rem;
	border-bottom:1px solid  #DADADA;
}

.muiCateSearchArea>.muiCateCancel {
	float: left;
	line-height: 4rem;
	color: #008DD2;
	padding-left: 1rem;
	font-size: 1.4rem;
}

.muiCateSearchArea .muiSearchDiv {
	border: none;
}
.muiCateSearchArea .muiSearchBtnDiv {
	width: 4rem;
	text-align: right;
}
.muiCateSearchArea .muiSearchBtnDiv .muiSearchCancelBtn {
	padding: 0;
	color: #999;
}
.muiCateSearchArea form {
	outline: none;
	-webkit-outline: none;
}
.muiSearchRecord,
.muiSearchHotRecord {
	padding:1rem 1.5rem 2.7rem;
	background-color: #fff;
	margin-bottom: 1rem;
}

.muiSearchRecord{
	display: none;
}
.muiSearchRecord h3,
.muiSearchHotRecord h3 {
	font-size: 1.4rem;
	font-weight: bold;
	position: relative;
	color: #333333;
	line-height: 1.4rem;
	display: inline-block;
	height: 4.7rem;
	line-height: 4.7rem;
}

.muiSearchRecord h3 i,
.muiSearchHotRecord h3 i {
	position: absolute;
    right: 0;
    top: .8rem;
    color: #9a9191;
}

.muiSearchRecord i{
	font-size: 1.4rem;
	color: #999FB7;
	line-height: 1.2rem;
    float: right;
    height: 4.7rem;
    line-height: 4.7rem;
}

.muiSearchRecord ul,
.muiSearchHotRecord ul {
	font-size:1.4rem;
}

.muiSearchRecord ul li{
	height: 1.6rem;
	line-height: 1.6rem;
	font-size: 1.55rem;
	color: #2A304A;
	border-bottom: 1px solid #EEEEEE;
	padding: 1.5rem 0;
	display: flex;
	width: 100%;
}

.muiSearchHotRecord ul li{
	margin-top: 1rem;
	background: #F6F6F6;
	border-radius: 4px;
	color:#2A304A;
    height: 3rem;
    line-height: 3rem;
    text-align: center;
    width: 22%;
    display: inline-block;
    margin-right: 4%;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle; 
}

.muiSearchHotRecord ul li:nth-child(4n){
	margin-right: 0;
}

/* 页面搜索框样式 */
.searchBarModule {
	height: 4rem;
	border-bottom: 1px solid rgba(25, 31, 37, 0.12);
	font-size: 0;
}

.searchBarWrap {
	display: inline-block;
	vertical-align: top;
	width: calc(100% - 50px);
	box-sizing: border-box;
	padding-left: 10px;
	padding-right: 10px;
}

.searchBar_inputWrap {
	border: 1px solid #C2C2C2;
	height: 3rem;
	line-height: 3rem;
	margin-top: 0.7rem;
	border-radius: 5px;
	padding-left: 32px;
	width: 100%;
	box-sizing: border-box;
	position: relative;
	padding-right: 10px;
}

.searchBar_inputWrap input {
	border: none;
	width: 100%;
	vertical-align: middle;
}

.searchBarWrap .mui {
	font-size: 18px;
	color: #c2c2c2;
	position: absolute;
	left: 10px;
	top: 7px;
}

/* 搜索框旁边的筛选器 */
.searchBarModule .filterBtn {
	font-size: 14px;
	display: inline-block;
	vertical-align: middle;
	width: 42px;
	text-align: center;
	line-height: 4rem;
	padding-right: 5px;
	white-space: nowrap;
}

.searchBarModule .filterBtn.muiActive {
	color: #4285F4;
}

.searchBarModule .filterBtn:after {
	content: "";
	display: none;
	width: 12px;
	height: 12px;
	background: url(./images/icon-filter.png) no-repeat left top;
	background-size: 12px;
	margin-left: 2px;
}

/*===== 常用筛选 Starts =====*/

.filterlayer_dialog {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 29;
	padding-bottom: 0;
	box-sizing: border-box;
	opacity: 0;
	transition: opacity 0.3s;
}

.filterlayer_mask {
	background-color: rgba(0, 0, 0, 0.5);
	content: '';
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}

.filterlayer_dialog.muiShow {
	opacity: 1;
}

.muiShow .filterlayer_content {
	transform: translateX(0%);
}
.muiShow .filterlayer_bottom_buttons {
	transform: translateX(0%);
}

.filterlayer_content {
	box-sizing: border-box;
	overflow-y: auto;
	position: absolute;
    top: 0;
    right: 0;
    width: 80%;
    height: 100%;
    background-color: white;
	transform: translateX(100%);
	transition: transform 0.3s;
	padding-bottom: 45px;
}

.filterlayer_content .filterItem {
	border-bottom: 1px solid #f1f1f1;
	padding-bottom: 21px;
	padding-left: 6px;
	padding-right: 6px;
}


.filterlayer_content .filterItemTitle {
	text-align: left;
	font-weight: normal;
	padding: 13px 6px 6px 6px;
	font-size: 14px;
	color: #2A304A;
	white-space: nowrap;
	overflow: hidden;
	width: 100%;
	text-overflow: ellipsis;
	box-sizing: border-box;
	position: relative;
}

.filterlayer_content .filterItemExpander {
    top: 13px;
    right: 6px;
    font-size: 12px;
    color: #d8d8d8;
    position: absolute;
    transform: rotate(90deg);
    transition: transform 0.3s;
}


.filterItem.expand .filterItemExpander {
	transform: rotate(-90deg);
}

.filterlayer_content .filterItemPicker {
	position: relative;
	display: block;
	width: 100%;
}

.filterlayer_content .filterItemPicker_split {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 15px;
	height: 1px;
	background-color: #d8d8d8;
	transform: translate(-50%, -50%);
}

.filterlayer_content .filterItemPicker_wrapper {
	display: flex;
}

.filterlayer_content .filterItemPicker .muiPropertyDatetime {
	flex: 1;
}

.filterlayer_content .filterItemPicker .muiPropertyDatetime:nth-child(1) {
	margin-right: 25px;
	position: relative;
}

.filterlayer_content .filterItemPicker .muiPropertyDatetime .muiFormEleWrap {
	min-width: unset;
}
.filterlayer_content .filterItemPicker .muiPropertyDatetime .muiSelInputRight {
	display: none;
}

.filterlayer_content .filterItemPicker .muiPropertyDatetime .muiSelInput {
	height: 30px;
	border-radius: 15px;
	background-color: #F7F7F8;
	padding: 0 10px;
	box-sizing: border-box;
	font-size: 12px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.filterlayer_content .filterItemPicker .muiPropertyDatetime .muiSelInput:empty:after {
	font-size: 12px;
}

.filterlayer_content .filterItemInput {
	width: 100%;
	font-size: 12px;
    background: #F7F7F8;
    border-radius: 2px;
    border: none;
    height: 30px;
    padding: 0 10px;
    color: #333;
    box-sizing: border-box;
}

.filterItem.expand ul li.overflow {
	display: inline-block;
}


.filterlayer_content ul {
	width: 100%;
	font-size: 0;
}

.filterlayer_content ul li {
	display: inline-block;
	width: 33.33%;
	text-align: center;
	line-height: 32px;
	margin: 4px 0;
	padding: 0 6px;
	box-sizing: border-box;
}


.filterlayer_content ul li.overflow {
	display: none;
}

.filterlayer_content ul a {
	font-size: 12px;
	color: #2A304A;
	display: block;
	background-color: #F7F7F8;
	border-radius: 4px;
	white-space: nowrap;
	text-overflow: ellipsis;
	width: 99%;
	overflow: hidden;
	box-sizing: border-box;
	padding: 0 3px;
	position: relative;
}

.filterlayer_content ul li.selected a,
.filterlayer_content ul li:active a {
	background: #F0F6FF;
	color: #4285F4;
}

.filterlayer_content ul li.selected a::after {
	content: '×';
	text-align: center;
	line-height: 12px;
	font-size: 10px;
	color: white;
	position: absolute;
    border-top-left-radius: 10px;
	right: 0;
	bottom: 0;
	width: 12px;
	height: 12px;
	line-height: 14px;
	overflow: hidden;
	background-color: #4285F4;
}

.filterlayer_bottom_buttons {
	position: absolute;
    bottom: 0;
    right: 0;
    width: 80%;
    background-color: white;
    transition: transform 0.3s;
	transform: translateX(100%);
	display: flex;
	flex-flow: row nowrap;
	box-shadow: 0 -2px 8px 0 rgba(173,173,173,0.50);
}

.filterlayer_bottom_button {
	height: 45px;
	flex: 1;
	line-height: 45px;
	text-align: center;
	font-size: 14px;
	color: #999FB7;
}

.filterlayer_bottom_button.bg_1 {}

.filterlayer_bottom_button.bg_2 {
	background-color: #4285F4;
	color: #fff;
}

.muiTagFilterItem{
	padding: .5rem .7rem;
    margin: 0 .5rem;
    background: #F7F7F8;
    border-radius: 6px;
    color: #2A304A;
}
.muiTagFilterItem.selected{
	background: #F0F6FF;
	color: #4285f4;
}

/*联想*/
.muiSearchCompleteContainer{
	padding: 0 1.5rem;
}

.muiSearchCompleteContainer li{
	font-size: 1.5rem;
	color: #2A304A;
	line-height: 4.7rem;
	height: 4.7rem;
	border-bottom: 1px solid #EEEEEE;
	overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.muiSearchPromptList{
	position: relative;
	background: #fff!important;
}

.mui-ftsearch-hightline{
	color: #6698F6;
}

.muiSearchRText{
	flex-grow:1;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}