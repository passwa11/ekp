.muiViewTop, .muiViewBottom {
	position: -webkit-sticky !important;
	position: sticky !important;
	width: 100%;
	top: 0;
	z-index: 9;
}

.muiViewBottom {
	bottom: 0;
	top: auto;
	position: fixed !important;
	box-shadow: #C4C6CF 0px 6px 16px 2px;
}

/*#120027兼容E人E本*/
@media only screen and (device-width: 768px) and (device-height: 1024px) {
	.muiViewBottom{
		position: relative !important;
	}	
}
.muiViewV {
	overflow-x: hidden;
}

.muiViewH {
	overflow-x: auto;
	white-space: nowrap;
	-webkit-overflow-scrolling: touch;
}

.muiViewH::-webkit-scrollbar {
	display: none;
}

.muiFixed {
	position: fixed !important;
	top: 0;
	z-index: 9;
	width: 100%;
	left: 0;
}
#scrollView{
	transform-style: unset !important;
}