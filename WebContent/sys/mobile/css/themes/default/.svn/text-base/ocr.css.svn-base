.muiOcr{
    display: block;
}
.muiOcr.muiOcrRight{
    text-align: right;
}
.muiOcr.muiOcrLeft{
    text-align: left;
}
.muiOcrContainer{
    display: inline-block;
}
.muiOcrContainer.muiOcrRequiredContainer{
    padding-right: 2rem;
}
/*识别失败 start*/
.muiOcrFailContent{
    width: 100%;
}
.muiOcrFailContent .muiOcrFailDesc{
    width: 21.4rem;
    padding-bottom: 2.5rem;
    margin: 0 auto;
}
.muiOcrFailContent .muiOcrFailDesc p{
    font-family: PingFangSC-Regular;
    font-size: 1.4rem;
    color: #9497A4;
    letter-spacing: 0;
    line-height: 1.8rem;
    font-weight: 400;
    text-align: left;
}
.muiOcrFailContent .muiOcrFailBtns{

}
.muiOcrFailBtns .muiOcrFailBtn{
    height: 4.85rem;
    border-top: 0.05rem solid #E7E7E7;
    font-family: PingFangSC-Regular;
    font-size: 1.6rem;
    letter-spacing: 0;
    text-align: center;
    font-weight: 400;
    line-height: 4.85rem;
}
.muiOcrFailBtns .muiOcrFailBtn.muiOcrRedoBtn{
    color: #4285F4;

}
.muiOcrFailBtns .muiOcrFailBtn.muiOcrUploadBtn{
    color: #4285F4;
}
.muiOcrFailBtns .muiOcrFailBtn.muiOcrCancelBtn{
    color: #9497A4;
}
.muiOcrFailBtns .muiOcrFailBtn.muiOcrHalfBtn{
    float: left;
    width: 50%;
}
.muiOcrFailBtns .muiOcrFailBtn.muiOcrRedoBtn.muiOcrHalfBtn{
    border-left: .05rem solid #e7e7e7;
    box-sizing: border-box;
}
/*识别失败 end*/

/*操作栏 start*/
.muiOcrOptDialog{

}
.muiOcrOptDialog.muiDialogElement .muiDialogElementContainer_bottom{
    background: none;
}
.muiOcrOptContent{
    width: 100%;
}
.muiOcrOptContent .muiOcrOptBtn{
    height: 5rem;
    font-family: PingFangSC-Regular;
    font-size: 1.5rem;
    color: #2A304A;
    letter-spacing: 0;
    text-align: center;
    font-weight: 400;
    text-align: center;
    line-height: 5rem;
    border-bottom: 0.5px solid #E8E8E9;
    width: 100%;
    margin: 0 auto;
    background: #ffffff;
}
.muiOcrOptContent .muiOcrOptBlock{
    border-radius: 1rem;
    margin-left: 1.5rem;
    margin-right: 1.5rem;
    background: #ffffff;
    overflow: hidden;
}
.muiOcrOptContent .muiOcrOptDownBlock{
    margin-top: 1rem;
    margin-bottom: 1.5rem;
}
.muiOcrOptContent .muiOcrOptBtn.muiOcrDelBtn{
    color: #F25643;
}
.muiOcrOptContent .muiOcrOptBtn.muiOcrCancelBtn{
    color: #4285F4;
}
.muiOcrOptContent .muiOcrOptSpace{

}
/*操作栏 end*/

.muiAttItemStatus{
    width: 100%;
    height: 100%;
    opacity: 1;
    z-index: 3;
    text-align: center;
    border-radius: 5px;
    background-color: #EEEEEE;
    line-height: 3.7rem;
    font-size: 1.4rem;
    white-space: nowrap;
    position: relative;
}
.muiAttItemStatus > i{
    display: inline-block;
    position: absolute;
    margin-top: -1.4rem;
    top: 50%;
}

/*默认实现 start*/
.muiOcrDefaultContent{
    width: 16rem;
}

.muiOcrDefaultContent .muiOcrDefaultSelect{
    width: 16rem;
    height: 12rem;
    background-size: 16rem 12rem;
}

.muiOcrDefaultContent .muiOcrDefaultSelect .muiOcrDefaultImg{
    width: 100%;
}

.muiOcrDefaultContent .muiOcrDefaultDesc{
    width: 100%;
    height: 2rem;
    font-family: PingFangSC-Regular;
    font-size: 1.4rem;
    color: #9497A4;
    font-weight: 400;
    text-align: center;
    padding-top: 1rem;
}

.muiOcrDefaultContent .muiOcrDefaultFail{
    padding-top: 0.4rem;
    padding-left: 3.25rem;
    height: 1.8rem;
    font-family: PingFangSC-Regular;
    font-size: 1.2rem;
    color: #FF605B;
    letter-spacing: 0;
    line-height: 1.8rem;
    font-weight: 400;
}

.muiOcrDefaultContent .muiOcrDefaultSelect.muiOcrDefaultJust{

}
.muiOcrDefaultContent .muiOcrDefaultSelect.muiOcrDefaultBack{

}

.muiOcrDefaultNoRemain{
    display: inline-block;
}
.muiOcrDefaultNoRemain .muiOcrDefaultBlock{

}
.muiOcrDefaultNoRemain .muiOcrDefaultBlock .muiOcrDefaultDesc{
    font-family: PingFangSC-Regular;
    font-size: 1.4rem;
    color: #AAACB7;
    letter-spacing: 0;
    text-align: right;
    font-weight: 400;
    display: inline-block;
    margin-right: 0.8rem;
    float: left;
}
.muiOcrDefaultNoRemain .muiOcrDefaultBlock .muiOcrDefaultSelect{
    width: 1.6rem;
    height: 1.6rem;
    background: url("./images/ocr/ocr@1x.svg") no-repeat;
    background-size: contain;
    display: inline-block;
    vertical-align: middle;
}
.muiOcrDefaultNoRemainView.muiOcrDefaultNoRemain .muiOcrDefaultBlock .muiOcrDefaultSelect{
    background: url("./images/ocr/ocr_jinyong@1x.svg") no-repeat;
    background-size: 3.6rem 2rem;
    width: 3.6rem;
    height: 2rem;
}
.muiOcrDefaultNoRemain .muiOcrDefaultFail{
    padding-top: 0.4rem;
    padding-left: 3.25rem;
    height: 1.8rem;
    font-family: PingFangSC-Regular;
    font-size: 1.2rem;
    color: #FF605B;
    letter-spacing: 0;
    line-height: 1.8rem;
    font-weight: 400;
}

.muiOcrDefaultContent.idCard .muiOcrDefaultSelect.muiOcrDefaultJust{
    background-image: url("./images/ocr/idCard@2x.png");
}
.muiOcrDefaultContent.idCard .muiOcrDefaultSelect.muiOcrDefaultBack{
    background-image: url("./images/ocr/idCard_back@2x.png");
}

.muiOcrDefaultContent.businessLicense .muiOcrDefaultSelect.muiOcrDefaultJust{
    background-image: url("./images/ocr/businessLicense@2x.png");
    height: 21rem;
    background-size: 16rem 21rem;
}

.muiOcrDefaultContent.vehicleLicense .muiOcrDefaultSelect.muiOcrDefaultJust{
    background-image: url("./images/ocr/vehicleLicense@2x.png");
}

.muiOcrDefaultContent.drivingLicense .muiOcrDefaultSelect.muiOcrDefaultJust{
    background-image: url("./images/ocr/drivingLicense@2x.png");
}
.muiOcrDefaultContent .muiOcrDefaultSelect.muiOcrNoBackground{
    background: none!important;
    height: auto!important;
}
/*默认实现 end*/

/*ocr校验start*/
.muiOcrImgRequired.muiFormRequired.muiFormRequiredShow{
    top: 35% !important;
    display:block!important;
}
.muiOcr.muiOcrRight ~ .muiValidate{
    float: right;
    margin-right: 2rem;
}
/*ocr校验end*/