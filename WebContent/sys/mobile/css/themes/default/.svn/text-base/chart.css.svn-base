@CHARSET "UTF-8";
.muiChart{
	position: relative;
}
.muiChartToolBar{
	position: absolute;
	bottom: 0;
	z-index : 999;
}
.muiChartToolBar .mui{
	font-size: 2.5rem;
	margin-right: 1rem;
}

.muiChartToolBar .muiChartZoomOutIcon{
	color : #5484ed;
}

.muiChartToolBar .muiChartZoomInIcon{
	color : #ff887c;
}