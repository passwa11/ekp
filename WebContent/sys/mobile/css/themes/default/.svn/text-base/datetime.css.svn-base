
.mblSpinWheel {
    background-image: none;
    border: none;
}

.mblSpinWheel {
    position: relative;
    overflow: hidden;
    height: 200px;
    color: #000000;
}

.mblSpinWheelSlot {
    position: relative;
    top: 0px;
    float: left;
    width: 100px;
    height: 100%;
    border-left: solid 2px #000000;
    border-right: solid 2px #000000;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.mblSpinWheelSlotLabel {
    padding: 0 8px;
    height: 44px;
    overflow: hidden;
    font: bold 24px/44px Helvetica, sans-serif;
}

.mblSpinWheelSlot {
    border: none;
    color: #929292;
}

.mblSpinWheelBar {
    position: absolute;
    top: 79px;
    border: solid 1px #7b8497;
    height: 42px;
    width: 100%;
    clear: both;
    opacity: 0.6;
}

.mblSpinWheelBar {
    background-image: none;
    background-color: rgba(0, 0, 0, 0);
    border: none;
    border-top: solid 1px #929292;
    border-bottom: solid 1px #929292;
}

.mblSpinWheelSlotTouch {
    position: absolute;
    top: 0px;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.mblSpinWheelSlotPanel {
    position: absolute;
    top: 0px;
    width: 100%;
}

.muiDateTimeDialogContent {
	margin: 0 auto;
	font-size: 0;
	padding: 0 1.5rem 0 2.5rem;
	display: flex;
	display: -webkit-flex;
}
.muiDateTimeDialogContent .mblSpinWheelDatePicker {
	width: 80%;
	display: inline-block;
	position: static;
}
.muiDateTimeDialogContent .mblSpinWheelTimePicker {
	width: 40%;
	display: inline-block;
	position: static;
}
.muiDateTimeDialogContent .mblSpinWheelSlot {
	width: 30%;
	z-index: 1;
	top: -5px;
}
.muiDateTimeDialogContent .mblSpinWheelSlot:first-child {
	width: 40%;
	box-sizing: border-box;
	padding-left: 0.5rem;
}
.muiDateTimeDialogContent .mblSpinWheelSlotTouch {
	outline: none;
}
.muiDateTimeDialogContent .mblSpinWheelSlotLabel {
	font-weight: normal;
	height: 3.5rem !important;
	font-size: 1.5rem;
	color: #797979;
	text-align: left;
	padding: 0;
}
.muiDateTimeDialogContent .mblSpinWheelSlotLabelBlue{
	color: #fff;
	font-size: 1.7rem;
	line-height: 3.4rem;
}
.muiDateTimeDialogContent .mblSpinWheelSlotLabelBlue .mblSpainWheelSlotLabelWeek{
	display:inline-block;
	vertical-align:middle;
	margin:0rem;
	padding:0rem;
	font-size: 1.2rem;
	color: #fff;
	position: relative;
	top:-1px;
	margin-left: .5rem;
}
.muiDateTimeDialogContent .mblSpinWheelBar {
	height: 3.2rem !important;
	top: 6.8rem;
	background-color: #4285f4;
	z-index: 0;
}

.muiDateTimeDialogContent .mblSpinWheel {
	height: 18rem;
	margin-bottom: 1rem;
	margin: 0 auto;
}
.muiDateTimeDialogContent .mblSpinWheel .mblSpinWheelBar {
	border-top: 0;
	border-bottom: 0;
	opacity: 1;
	left: 0;
}

.muiDateTimeDialogDisContent .muiDateTimeArea{
	position:relative;
	border: 0;
	border-spacing: 0;
	width: 100%;
	height: 18rem;
	overflow: hidden;
}
.muiDateTimeDialogDisContent .muiDateTimeArea>div{
	position: absolute;
	height: 15rem;
	width:100%;
	top:0rem;
	left:0rem;
	z-index: 1;
	opacity:0;
}
.muiDateTimeDialogDisContent .muiDateTimeChoices{
	width: 98%;
	height:2.5rem;
	line-height:2.5rem;
	border-radius: 5px;
	border: 1px solid #007aff;
	margin: 5px auto 0;
	display: table;
}
.muiDateTimeDialogDisContent .muiDateTimeChoices >div{
	display:table-cell;
	width: 50%;
	text-align: center;
	font-size: 1.3rem;
}
.muiDateTimeDialogDisContent .muiDateTimeChoices >div.checked{
	color: #fff;
	background-color: #007aff;
}
.mblSpainWheelSlotLabelText {
	font-size: 1.2rem;
	margin-left: 0.3rem;
	position: relative;
	top: -1px;
}
.muiFormInputContnet input {
	padding: 0.2rem;
	height: 2rem;
	border: 0;
	background-color: inherit;
	width: 100%;
}

.muiDateTimeField .muiField {
	width: auto;
	padding: 0;
}

.muiFormSelectContnet .muiFormDatetimeText {
	display: inline-block;
}

.muiFormSelectContnet.edit .muiFormDatetimeText{
	padding-right: 1.5rem;
}

.muiFormDatetimeText:empty:after {
	content: attr(placeholder);
	color: #A9A9A9;
}
.muiDateTimeDialog .mblSpinWheelSlotContainer > div.mblSpinWheelSlotPanel {
	-webkit-transform: translate3d(0,0,0); 
}
/*在可视区域的宽度大于401px 的时候被应用(魅族mx3 432px)*/
@media screen and (min-width: 401px) {
	.muiDateTimeDialogContent .mblSpinWheelSlotLabel{
		height: 4rem !important;
	}
	.muiDateTimeDialogContent .mblSpinWheel{
		height: 20rem;
	}
	.muiDateTimeDialogDisContent .muiDateTimeArea{
		height: 20rem;
	}
	.muiDateTimeDialogContent .mblSpinWheelBar{
		height: 4.5rem !important;
		top:7.2rem;
	}
}