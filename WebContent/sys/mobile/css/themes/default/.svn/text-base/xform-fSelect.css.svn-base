.muiCateLists .muiFSelectItem{
	padding:0.3rem 0rem;
	line-height: 2.2rem;
	padding-left : 1rem;
}

.muiCateLists .muiFSelectItem .muiFSelectContent{
	background-color: white;
	padding-left: 0.3rem;
}

.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem{
	border-bottom: 0.1rem solid #f0eff5;
	padding: 0.6rem 1rem 0.6rem 0;
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem:last-child{
	border-bottom:none;
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem .muiFSelectInfo{
	display: table;
	table-layout: fixed; 
	width: 100%;
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem .muiFSelectInfo > div{
	display: table-cell;
	word-break: break-word;
	word-wrap: break-all;
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem .muiFSelectInfo .muiFSelectTitle{
	width:26%;
	color: #878787;
	padding-left:1.2rem
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem:FIRST-CHILD .muiFSelectInfo .muiFSelectValue{
	color: var(--muiFontColorInfo);
    font-size: var(--muiFontSizeM);
	padding-left: .5rem;
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem .muiFSelectInfo .muiFSelectSelArea{
	width: 2.5rem;
	position: relative;
}

.muiFSelectInfo .muiFSelectSelArea .muiFSelectSel{
	position: absolute;
	top:0.3rem;
	right:0.3rem;
	width: 1.6rem;
	height: 1.6rem;
	line-height: 2rem;
	border: 0.1rem solid #dfe1e0;
	background-color: #fff;
}
.muiFSelectInfo .muiFSelectSelArea .muiFSelectSel.muiFSelectSeled{
	border: 0.1rem solid var(--muiFontColor);
}
.muiFSelectInfo .muiFSelectSelArea .muiFSelectSel.muiFSelectSeled,.muiFSelectSelectAllCheckbox.selected{
	opacity: 1;
    transition: all .3s;
}
.muiFSelectInfo .muiFSelectSelMul .muiFSelectSel{
	border-radius: 4px;
}
.muiFSelectInfo .muiFSelectSelSgl .muiFSelectSel{
	border-radius: 50%;
}

.muiFSelectInfo .muiFSelectSelArea .muiFSelectSeled{
	background-color: var(--muiFontColor);
}
.muiFSelectInfo .muiFSelectSelArea .muiFSelectSeled i{
	color: white;
	line-height: 2rem;
	top: -2px;
}

.muiFSelectSec{
	display: block;
  width: 100%;
  height: 7rem;
  overflow: hidden;
  position: relative;
}
.muiFSelectSec:before {
	content: '';
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 1px;
	background-color: #e5e5e5;
	transform: scale(1,.5);
}
.muiFSelectSec .muiFSelectContainer{
	width: 100%;
	height: 100%;
	display: block;
}
.muiFSelectSec .muiFSelectContainer > div{
	height: 100%;
	display: inline-block;
}
.muiFSelectSec .muiFSelectContainer .muiFSelectSecRight{
	float: right;
    margin-top: 1rem;
    margin-right: 1.5rem;
}
.muiFSelectSecBtn{
    top: 0;
    margin: 0rem;
    border: 1px solid transparent;
    background-color: var(--muiFontColor);
    width: 8rem;
    height: 5rem;
    text-align: center;
    line-height: 5rem;
    color: white;
    cursor: pointer;
    display: block;
    transition: background-color .3s;
    font-size: var( --muiFontSizeM);
    border-radius: 6px;
}
.muiFSelectSecBtnDis{
	color: #b5b5b5;
	background-color: transparent;
	position: relative;
}
.muiFSelectSecBtnDis:after {
	content: '';
	position: absolute;
	top: -1px;
	left: 0;
	width: 1px;
	height: 100%;
	background-color: #e5e5e5;
	transform: scaleX(0.5);
	-webkit-transform: scaleX(0.5);
}
.listFSelectItem{
	width:100%;
	text-align: center;
}

.muiFSelectSearchBarList{
	padding:0.6rem 0rem;
}
.muiFSelectSearchBarList .muiSearchBar {
	border-bottom: 0;
}

.muiFSelectSelectAllCheckboxArea{
    width: 7.5rem;
    display: table-cell;
    word-break: break-word;
    padding-left: 1.6rem;
    height: 7rem;
    vertical-align: middle;
}

.muiFSelectSelectAllCheckbox{
	border-radius:4px;
	margin-bottom:-0.3rem;
	width:1.6rem;
	height:1.6rem;
	border:0.1rem solid #dfe1e0;
	background-color:#fff;
	text-align:left;
	display:inline-block;
	margin-right:6px;
	line-height: 2rem;
}
.muiFSelectSelectAllCheckbox.selected{
    border: 0.1rem solid var(--muiFontColor);
    background-color: var(--muiFontColor);
    color: white;
    position: relative;
}
.muiFSelectSelectAllCheckbox.checked{
	color:white;
	font-size:1.3rem;
	line-height:2rem;
	top:-1px;
}
.muiFSelectSelectAllCheckbox.selected>i.muis-form-selected-cor,
.muiFSelectSel.muiFSelectSeled>i.muis-form-selected-cor{
	transform: scale(.7,.7);
    position: absolute;
    top: -2px;
}
.muiFSelectAllText{
	display: inline-block;
    font-size: var(--muiFontSizeM);
    color: var(--muiFontColorMuted);
}
.muiCateLists .muiFSelectItem .muiFSelectContent .muiFSelectItem .muiFSelectInfo .muiFSelectValue.muiFSelectSeled{
	color: var(--muiFontColor);
}