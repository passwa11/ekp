
.muiListPicslideContainer{
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
    border-radius: .6rem;
    z-index: 90;  /** 此z-index是为兼容IPhone滑动过程中左边框圆角消失 **/
}

.muiListPicslideContainer .mblView{
	border-radius: .6rem;
}

.muiListPicslidePaging{
	position:absolute;
	padding:0 0.8rem;
    height:2.5rem;
    line-height:2.5rem;
    text-align:center;
    color:#fff;
    font-size:1.6rem;
    background-color:rgba(208,122,38,0.65);
    left:0;
    bottom:4.5rem;
    z-index: 1;
}
.muiListPicslideCur{
	 font-size:2rem;
}
.muiListPicslideTitle{
	padding:0rem 0.8rem;
    width:60%;
    height:3rem;
    line-height:3rem;
    text-align:left;
    color:#fff;
    font-size:1.8rem;
    background-color:rgba(119,143,155,0.55);
    position:absolute;
    left:0;
    bottom:0.8rem;
    z-index: 1;
    text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}
.muiListPicslideSwitch {
  right: 0;
  text-align: right;
  padding: 0 1rem;
  height: 3.5rem;
  line-height: 3.5rem;
  color: #fff;
  font-size: 1.8rem;
  position: absolute;
  bottom: 0;
  z-index: 2;
  text-overflow: ellipsis;
}
.muiListPicslideSwitch > span {
  width: 5px;
  height: 5px;
  border-radius: 50%;
  background-color: #fff;
  display: inline-block;
  bottom: 2px;
  position: relative;
}
.muiListPicslideSwitch > span:not(:first-child){ margin-left: 5px;}
.muiListPicslideSwitch >span.item_on{ width: 20px; border-radius: 12px; }

.muiListPicslideTitleDot {
  padding: 0 70px 0 .8rem;
  width: 100%;
  height: 3.5rem;
  line-height: 3.5rem;
  text-align: left;
  color: #fff;
  font-size: 1.6rem;
  background-image: linear-gradient(-90deg, rgba(0,0,0,0.00) 0%, rgba(0,0,0,0.6) 100%);
  background-image: -webkit-linear-gradient(-90deg, rgba(0,0,0,0.00) 0%, rgba(0,0,0,0.6) 100%);
  position: absolute;
  left: 0;
  bottom: 0rem;
  z-index: 1;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  border-bottom-left-radius: .6rem;
  border-bottom-right-radius: .6rem;
}
.muiListPicslideItem{
	text-align: center;
	width: 100%;
	height: 100%;
	vertical-align: middle;
}
.muiListPicItemImg{
	margin:auto;
	vertical-align: middle;
	border-radius: .6rem;
}
.muiListPicItemTitle{
	width:100%;
}
  /*针对 Iphone 6 适配 Starts*/
@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
  .muiListPicItemTitle{
    font-size: 1.6rem;
    font-weight: 600;
  }
}
  /*针对 Iphone 6 适配 Ends*/

.muiRtfPicSlider{
	position: absolute;
	top: 0rem;
	left: 0rem;
	background-color:rgba(0, 0, 0 , 0.92);
	-webkit-transition:-webkit-transform 0.5s ease-out;
	transition:transform 0.5s ease-out;
	-webkit-transition:transform 0.5s ease-out;
	transition:-webkit-transform 0.5s ease-out;
	-webkit-transform: scale(0,0) ;
	transform: scale(0,0) ;
	z-index: 20;
}
.muiRtfPicShow{
	-webkit-transform: scale(1,1) !important;
	transform: scale(1,1) !important;
}
