/*附件区域*/
.muiAttachments {
}
/*简单模式下*/
.muiAttachmentssimple{
	border: none;
}
/*附件项默认样式*/
.muiAttachmentItem {
	width: 100%;
	clear: both;
	display: table;
	margin: 1.5rem 0;
	padding: 0 0.5rem;
	box-sizing: border-box;
}

.muiAttachmentItemIcon i{
	width: 100%;
	height:100%;
	line-height:3.7rem;
	display: block;
	background-size: 100%;
}

/*标题项*/
.muiAttTitle{
	height: 3.5rem;  
	min-height:3.5rem;
	line-height: 3.5rem;
	background: #F5F6FB;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemIcon{
	height: 3.4rem;
	width: 2rem;
	background:none;
}
.muiAttTitle .muiAttachmentItemIcon i.mui{
	color: #000 !important;
	font-size: 1.6rem;
	line-height: 3.6rem;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemL{
	padding: 0 0 0 0.4rem;
	width: 2rem;
	display: none;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemName{
	font-size: 1.2rem;
	margin-bottom: 0;
	color: #999FB7;
}
.muiAttTitle.muiAttachmentItem .muiAttachmentItemC{
	margin-bottom: 0;
	vertical-align: middle;
	padding: 0 0 0 0.2rem;
	font-size: var(--muiFontSizeSS);
	color: #999FB7;
	letter-spacing: 0;
	line-height: 1.2rem;
}
.muiAttTitle .muiAttachmentItemR{
	display: none !important;
}
/*列表下附件项*/
.muiAttachmentsnormal .muiAttachmentList .muiAttachmentItem{
	border-bottom: 1px solid #DBDBDB;
	background-color: #FFF;
}
.muiAttachmentsnormal .muiAttachmentList .muiAttachmentItem:last-child{
	/*border:none;*/
}
/*列表下附件项简单模式*/
.muiAttachmentssimple .muiAttachmentList .muiAttachmentItem{
	border:none;
	background-color: #FFF;
}
/*选中效果*/
.muiAttachmentList .muiAttachmentItem.mblListItemSelected{
	background-color: #E4E4E4 !important;	
}

/*附件项内部样式*/
.muiAttachmentItem .muiAttachmentItem:first-child .muiAttachmentItemName{
	height: 3.5rem;
	line-height: 3.5rem;
	max-height: 3.5rem;
}
/*附件项名称*/
.muiAttachmentItem .muiAttachmentItemName {
	margin-bottom: 0.5rem;
	width: 100%;
	max-height: 3rem;
	vertical-align: middle;
	font-size: 1.4rem;
	color: #3E4665;
	letter-spacing: 0;
	text-align: left;
	line-height: 1.5rem;
	word-break: break-all;
	word-wrap: break-word;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.muiAttachmentItem .muiAttachmentItemL {
	display: table-cell;
	text-align: center;
	width: 3.2rem;
	line-height: 3.2rem;
	vertical-align: top;
}
.muiAttachmentItem .muiAttachmentItemR {
	width:2.5rem;
	padding-right: .5rem;
	text-align:right;
	vertical-align: middle;
	display: table-cell;
}

.muiAttachmentItem .muiAttachmentItemC {
	display: table-cell;
	max-width: 70%;
	vertical-align: top;
	padding-left: 1rem;
}
/*附件大小*/
.muiAttachmentItem .muiAttachmentItemSize {
	font-size: 1.2rem;
	color: #999FB7;
	letter-spacing: 0;
}
/*附件打开图标*/
.muiAttachmentItem .muiAttachmentItemExpand {
	text-align: center;
	color: #7B7B7B;
	font-size: var(--muiFontSizeMA);
}
.muiAttachmentItem .muiAttachmentItemExpand.mui{
	font-size: 2rem;
}
/*附件图标*/
.muiAttachmentItem .muiAttachmentItemIcon {
	height: 3.2rem;
	width: 3.2rem;
	overflow: hidden;
	display: block;
}

.muiAttachmentItem .muiAttachmentItemIcon img{
	width: 100%;
	min-height: 100%;
}

