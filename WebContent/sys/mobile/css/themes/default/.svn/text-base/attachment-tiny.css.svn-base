/*附件编辑*/
.muiAttachmentList{ zoom: 1; padding:0 1.2rem 0 0; border-bottom:1px solid #dbdbdb;}
.muiAttachmentList{border-bottom: 0;}
.splitLine .muiAttachmentList{ padding-left:0px;} /*流程新建输入框*/
.muiAttachmentList:after{ display:block; content:'clear'; clear:both; line-height:0; visibility:hidden;}
.muiAttachmentList .muiAttachmentItem:last-child{ border-bottom:none;}
.muiAttachmentEditItem {
	display:inline-block;
	vertical-align: top;
	box-sizing: border-box;
	position: relative;
}
.muiAttachmentEditItem.muiRight{
}
.muiAttachmentEditItem:after {
	content: '';
	display: inline-block;
	padding-bottom: 100%;
	width: 0.1px;
	vertical-align: middle;
}
.muiAttachmentEditItem .muiAttachmentItemT{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	border-radius:4px;
}
.muiAttachmentEditOptItem .muiAttachmentItemT{
	border: none;
	background-color: #F7F8FA;
}
.muiAttachmentItemT .muiFieldRequired{
	position: absolute;
	top: 0rem;
	right: 0.4rem;
}
.muiAttachmentEditItem .muiAttachmentItemStatus{
	position:absolute;
	top:0rem;
	left:0rem;
	width:100%;
	height: 100%;
	opacity: 0.9;
	z-index: 3;
	text-align:center;
	border-radius:5px;
	background-color: #EEEEEE;
	line-height: 3.7rem;
	font-size: 1.2rem;
	white-space: nowrap;
}

.muiAttachmentEditItem .muiAttachmentItemStatus>i{
	display: inline-block;
}

.muiAttachmentEditItem .muiAttachmentItemIcon{
	position:relative;
	width: 100%;
	height:100%;
  text-align:center;
  border-radius:6px;
	overflow: hidden;
	display: flex;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
}
.muiAttachmentEditItem .muiAttachmentItemText{ white-space: nowrap; }
.muiAttachmentEditItem .muiAttachmentItemIcon i.muis-new{ color: #b7b7b7;}
.muiAttachmentEditItem .muiAttachmentUploadFile{
	position:absolute;
	top:0rem;
	left:0rem;
	width:100%;
	height: 100%;
	cursor: pointer;
	filter:alpha(opacity:0);
	opacity: 0;
	z-index: 5;
}

.muiAttachmentEditItem .muiAttachmentItemB{
	overflow: hidden;
	position: absolute;
	left: 0;
	bottom: -2rem;
	right: 0;
}

.muiAttachmentEditOptItem .muiAttachmentItemB{
	width: 100%;
	text-align: center;
	display: inline-block;
	color: #898989;
	font-size: 1.3rem;
	height: auto;
	line-height: 2rem;
	overflow: hidden;
}

.muiAttachmentEditItem .muiAttachmentItemName {
	width: 100%;
	font-size: var(--muiFontSizeSS);
	color: #2A304A;
	letter-spacing: 0;
	text-align: center;
	line-height: 1.8rem;
	white-space:nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	
}
.muiAttachmentEditItem .muiAttachmentItemSize {
	text-align: center;
	line-height: 1.2rem;
	font-size: 1rem;
	display: block;
	color: #999;
	white-space:nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	display: none;
}
.muiAttachmentEditItem .muiAttachmentItemDel{
	position:absolute;
	top:-1rem;
	right:-1rem;
	width: 1.5rem;
	height: 1.5rem;
	line-height: 1.4rem;
	vertical-align:middle;
	z-index: 5;
	border-radius:50%;
	text-align:center;
	margin:.6rem;
	background-color: #fff;
}
.muiAttachmentEditItem .muiAttachmentItemDel i{
	line-height:1.5rem;
	font-size: 1.5rem;
	color: #FF605B;
	width: 100%;
	height: 100%;
}
/*附件上传选择*/
.muiAttachmentUploadArea{
	position: fixed;
	top:0rem;
	width:100%;
	height: 100%;
	z-index: 100;
	text-align: center;
}
.muiAttachmentUploadbak{
	position: absolute;
	width:100%;
	height: 100%;
	background-color:#999999;
	opacity: 0.7;
	z-index: 101;
}
.muiAttachmentUploadOpt{
	position: absolute;
	z-index: 102;
	width:100%;
	bottom: 0.5rem;
}

.muiAttachmentUploadSelect,.muiAttachmentUploadCancel{
	background-color:#F7F7F7;
	opacity: 1 important;
	text-align: center;
	color:#017BFF;
	width:100%;
	width:95%;
	margin:0.8rem auto;
	font-weight: bold;
	font-size:1.6rem;
	border-radius:0.6rem;;
}

.muiAttachmentUploadSelect > div, .muiAttachmentUploadCancel{
	height: 4rem;
	line-height: 4rem;
	vertical-align: middle;
}
.muiAttachmentUploadSelect > div {
	border-bottom: 1px solid #DBDBDB;
}
.muiAttachmentUploadSelect > div:last-child {
	border-bottom: none;
}


.textAlign{text-align: center;}
.muiReadAll {
	font-weight: normal;
	display: inline-block;
	width: 90%;
	font-size: 2rem;
	margin: 2.3rem 0rem 1.5rem;
	height: 45px;
	line-height: 45px;
	color: #fff;
	background-color: #5bc91f;
}
.muiReadAll:active{ opacity: .75; }

.muiReadAllItem .muiAttachmentItemC{ padding-left:0px;}

.muiReadAll:active{ opacity: .75; }
.muiReadAllItem .muiAttachmentItemC{ padding-left:0px;}
.mblListItemSelected{ background-color: #fff!important; }
.muiOtherOpt{
	padding-right: 1.2rem;
	text-align: right;
}
.muiOtherOpt .muiDownloadBtn,
.muiOtherOpt .muiEditBtn{
	color: #7B7B7B;
	font-size: 1.5rem;
}
.muiOtherOpt .muiDownloadBtn:active,
.muiOtherOpt .muiEditBtn:active{
	opacity: .75;
}
.muiOtherOpt .muiEditBtn > .mui{
	margin-right: .3rem;
}
.muiOtherOpt .mui{
	top: -.1rem;
}
.muiOtherOpt .muiDownloadBtn{}
.muiOtherOpt .mui-top{
	transform: rotate(180deg);
}
.muiOtherOpt .muiEditBtn{
	margin-left: 1.5rem;
}

.muiAttDialogContainer{
	bottom: 0px;
	position: absolute;
	left: 1rem;
	right: 1rem;
	overflow: hidden;
}

.muiAttDialogContent,.muiAttDialogBottom{
	background-color: #fff;
	border-radius: 10px;
	overflow: hidden;
}

.muiAttDialogButton,.muiAttDialogBottom{
	height: 5.8rem;
	display: inline-block;
	text-align: center;
	vertical-align: middle;
	line-height: 5.8rem;
	font-size: 1.6rem;
	color: var(--muiFontColor);
	width: 100%;
	position: relative;
}

.muiAttDialogButton:AFTER{
	content: "";
	transform: scaleY(0.5);
	bottom: 0;
	border-top: 1px solid #EEEEEE;
	position: absolute;
	left: 0;
	right: 0;
}

.muiAttDialogBottom{
	margin: 1rem 0;
	color: var(--muiFontColorMuted);
	height: 4.8rem;
	line-height: 4.8rem;
}
