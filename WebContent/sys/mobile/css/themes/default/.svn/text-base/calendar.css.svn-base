.muiCalendarTable {
}

*{-webkit-tap-highlight-color:rgba(255,255,255,0);}

.muiCalendarTable tr:last-child td{ padding-bottom:0.6rem; }
.muiCalendarHead .muiCalendarMonthContent {
	width: 18rem;
	display: block;
	margin: 0 auto;
	position: absolute;
	left: 3rem;
	right: 3rem;
}

.muiCalendarHead {
	min-height: 49px!important;
	color: #b8b8b8;
	text-align: center;
	background-color: #fff;
}
.muiCalendarHead>div{ position:relative; top:0.3rem;}
.muiCalendarHead:after {
	display: block;
	content: 'clear';
	clear: both;
	line-height: 0;
	visibility: hidden;
}
.muiCalendarHead .calendar-today{ font-size:1.8rem;  color:#4285f4;}
.muiCalendarHead span.mui {
	font-size: 1.8rem;
	color: #4285f4;
}
.muiCalendarHead span.mui.mui-listView{ font-size:2rem;}
.muiCalendarHead span.mui-attachment.mui,
.muiCalendarHead span.mui-group.mui{
	font-size: 2.2rem;
	padding-top: 1rem;
}

.muiCalendarHead>div {
	display: inline-block;
}

.muiCalendarHead .muiCalendarHeadFirst {
	float: left;
	padding-left: 0.2rem;
}

.muiCalendarHead .muiCalendarHeadLast {
	float: right;
}
.muiCalendarHead .muiCalendarHeadFirst span {
	padding-left: 1rem;
	width: 3rem;
	text-align: center;
	font-size:  2.2rem;
}
.muiCalendarHead .muiCalendarHeadLast span {
	padding-left: 0.3rem;
	padding-right: 1.2rem;
	text-align: center;
	float: left;
}
.muiCalendarHead .muiCalendarHeadLast span.calendar-today {
	padding-bottom: .8rem;
	padding-right: 1.7rem;
	text-align: center;
	padding-top:0.6rem;
}

  /* 大字体兼容适配 Starts */
html.fontSize2 .muiCalendarHead .muiCalendarHeadLast span,
html.fontSize3 .muiCalendarHead .muiCalendarHeadLast span,
html.fontSize4 .muiCalendarHead .muiCalendarHeadLast span{
	padding-right: 0;
}
html.fontSize2 .muiCalendarHead .muiCalendarHeadLast span.calendar-today,
html.fontSize3 .muiCalendarHead .muiCalendarHeadLast span.calendar-today,
html.fontSize4 .muiCalendarHead .muiCalendarHeadLast span.calendar-today{
	padding-right: .2rem;
}

  /* 大字体兼容适配 Ends */


.muiCalendarHead>div:first-child {
	float: left;
}

.muiCalendarHead span {
	padding: 1rem .3rem;
	display: inline-block;
}

.muiCalendarTable .muiCalendarWeek th {
	text-align: center;
	font-size: 1.2rem;
	padding: 0.8rem;
	color: #4d4d4d;
}

.muiCalendarTable tbody tr:last-child>td {
	border-bottom: 1px solid #e8e8e8;
}

.muiCalendarTable td .muiCalendarDate {
	border: 1px solid transparent;
	font-size: 1.5rem;
	display: inline-block;
	border-radius: 50%;
	width: 2.5rem;
	height: 2.5rem;
	line-height: 2.5rem;
	font-size: 1.5rem;
	position: relative;
	z-index: 1;
}

.muiCalendarTable td .muiCalendarCurrentDate {
  border-color: #fd8641;
}
.muiCalendarTable td .muiCalendarDateCurr.selected {
  color: #fff;
  border-color: #fd8641;
  background-color: #fd8641;
}
.muiCalendarTable td .muiCalendarCurrentDate + .muiCalendarNotify {
  background-color: #fd8641;
}
.muiCalendarTable td .muiCalendarDateCurr.selected ~ .muiCalendarNotify {
	background-color: #fff;
}
.muiCalendarTable td .muiCalendarHoliday{
  color: #4285f4;
}

.muiCalendarTable td .muiCalendarDateSign {
	/* position: absolute;
	font-size: 0.83rem;
	left: 50%;
	top: -4px;
	-webkit-transform: translateX(-2rem) scale(0.8333);
			transform: translateX(-2rem); */
	width: auto;
	height: auto;
	line-height: 1em;
	text-align: left;
	color: #f95a5a;
	font-size: 12px;
	border-radius: 0;	
	position: absolute;
	left: 1px;
	top: 1px;
	right: 1px;
	bottom: 1px;
	z-index: 0;
	border: 0;
}
.muiHoliday {
	background-color: #fcecea;
}
.muiWork {
	background-color: #ecf2fd;
}

.muiCalendarNotify {
	background-color: #FD8641;
	width: 4px;
	height: 4px;
	display: inline-block;
	top: 2.2rem;
	left: 50%;
	border-radius: 50%;
	position: absolute;
	margin-left: -2px;
}

.muiCalendarTable td {
	position: relative;
}

.muiCalendarDatePre {
}

.muiCalendarDateNext {
}

.muiCalendarMonth {
	color: #000;
}

.muiCalendarMonth.selected{
	background-color: #4285f4;
  	opacity: .7;
  	border-radius: 5px;
}

.muiCalendarBottom {
	padding: 0rem 0rem 0.5rem;
	background-color: #fff;
}

.muiCalendarBottom>div {
}
.muiCalendarBottom .muiCalendarOpt{
}

.muiCalendarBottom .muiCalendarBottomLeft {
	color: #4285f4;
	font-size: 3.5rem;
	display: inline-block;
	width: 4rem;
	text-align: center;
}

.muiCalendarBottom .muiCalendarBottomCenter {
	padding-left: 0.5rem;
	display: inline-block;
}

.muiCalendarBottom .muiCalendarBottomCenter>div {
	font-size: 1.2rem;
	color: #898989;
}

.muiCalendarBottom .muiCalendarBottomRight {
	margin-top: .5rem;
	text-align: center;
	vertical-align: middle;
	display: inline-block;
	position: relative;
	float: right;
}
.muiCalendarBottom .muiCalendarBottomRight > .mui {
	color: #fff;
	font-size: 1.6rem;
	width: 3.2rem;
	height: 3.2rem;
	line-height: 3.2rem;
	border-radius:50%;
	background-color: #4285f4;
  -webkit-text-stroke-width: 0.5px;
}
.muiCalendarBottom .muiCalendarBottomRight > .mui:active{ opacity: .75; }

.muiCalendarBottom .muiCalendarBottomInfo>div {
	display: inline-block;
	font-size: 1.2rem; padding-top:0.2rem;
}

.muiCalendarScrollableView .mblScrollableViewContainer {
	min-width: 100%;
}

.muiCalendarScrollableView {
	overflow: hidden;
}

.muiCalendarContainer>div {
	/* background-color: #000 !important; */
}

.muiCalendarWeekTable {
	position: absolute;
	z-index: 99;
	-webkit-transform: translate3d(0, -14rem, 0); 
	height: 7.8rem;
	background: #fff;
	width: 100%;
	margin-top: 1px;
	margin-bottom: 1px;
}

.muiCalendarWeekTable tr td{ padding-bottom:6px;}
.muiCalendarHead>div.muiCalendarHeadLast{
	top:.6rem;
}

.muiCalendarMonthContent > span{
	font-size: 1.6rem;
}

/* 日程start */
.mui_ekp_portal_date_manage{
	margin-top:4rem;
}
.mui_ekp_portal_date>ul.schedule{
	display: table;
	width: 100%;
}
.mui_ekp_portal_date>ul.schedule>li{
	text-align: center;
	width: 14%;
	float: left;
}
.mui_ekp_portal_date .mui_ekp_portal_date_week>li{
	color: #C9CAD1;
	height: 2rem;
	line-height: 2rem;
}
.mui_ekp_portal_date_days>li{
	color: #3E4665;
	border-radius: 4px;
}
.mui_ekp_portal_date_days>li.mui_ekp_portal_today{
	background-color: #F5F9FF;
	border: 1px solid #DFEBFF;
}
.mui_ekp_portal_date_num{
	font-family: "landray-number", -apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
	color: #3E4665;
	position: relative;
	top:5px;
}
.active .mui_ekp_portal_date_num{
