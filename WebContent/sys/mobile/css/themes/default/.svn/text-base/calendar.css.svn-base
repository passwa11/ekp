/* 日程end */

.calendarAddBtn {
	width: 4rem;
	height: 4rem;
	border-radius: 2rem;
	right: 2rem;
	bottom: 8rem;
	position: absolute;
	background: var(--muiFontColor);
	text-align: center;
	line-height: 4rem;
	font-size: var(--muiFontSizeXL);
}
.calendarAddBtn i {
	color: #fff;
}
.muiCalendarGroupShare span {
	font-size: var(--muiFontSizeMS);
}
.muiCalendarGroupShare i {
	font-size: var(--muiFontSizeSS);
    -webkit-transform: scale(0.4);
    transform: scale(0.4);
    display: inline-block;
}

/* 会议rightNode 点击弹窗 */
.muiCalendarRightNodeDialog{
	position: fixed;
    top: 0;
    bottom: 0;
    width: 100%;
    opacity: 0;
    transition: opacity .5s;
}

.muiCalendarRightNodeDialog.muiCalendarRightNodeDialogShow{
	opacity: 1;
	z-index:100;
}

.muiCalendarRightNodeDialog .muiCalendarRightNodeCover{
	width: 100%;
	height: 100%;
	opacity: 0.7;
	background: #000000;
	position: absolute;
}

.muiCalendarRightNodeClose{
	position: absolute;
	bottom: 4rem;
	width: 100%;
    text-align: center;
}

.muiCalendarRightNodeClose i{
	display: inline-block;
	font-size: 4rem;
	color: #fff;
}

.muiCalendarRightNodeMenu{
	position: absolute;
	transform: translate(-50%,-50%);
	top: 50%;
	left: 50%;
	width: 100%;
	display: flex;
    flex-wrap: wrap;
}

.muiCalendarRightNodeMenu .muiCalendarRightNodeMenuItem{
	color: #fff;
	margin: 2rem 0;
	text-align: center;
}

.muiCalendarRightNodeMenu img{
	width:12rem;
	height:18rem;
}

.muiCalendarRightNodeMenuItem i{
	display: inline-block;
	width: 6.5rem;
	height: 6.5rem;
	line-height: 6.5rem;
    text-align: center;
    font-size:var(--muiFontSizeXXXXL);
    border-radius: 50%;
}
.muiCalendarRightNodeMenuItemText{
	font-size: var(--muiFontSizeMS);
	margin-top: 1rem;
}