}
.active .mui_ekp_portal_data_bg{
	background-color: #4285F4!important;
}
.mui_ekp_portal_date_lunar{
	color: #C9CAD1;
	position: relative;
	top: 3px;
}
.active .mui_ekp_portal_date_lunar{
	color: #BFD7FF;
}
.schedule .active.today .mui_ekp_portal_date_lunar {
	color: #fff;
	opacity: 0.5;
}
.work_condition{
	position: absolute;
	font-size: 9px;
	top: 6px;
	margin-left:2px;
}
.work_condition.rest{
	color: #FFA58B;
}
.work_condition.work{
	color: #4285F4;
}
.active .work_condition.rest,.active .work_condition.work{
	color:#fff;
}
.journey_status.journey{
	position: absolute;
	display: inline-block;
	width: 4px;
	height:4px;
	border-radius: 4px;
	background-color: #4285F4;
	bottom:2px;
	left: 21px;
}
.active .journey_status.journey{
	background-color: #fff;
}
.mui_ekp_portal_date_title{
	height: 4rem;
}
.mui_ekp_portal_date_title .muis-to-left{
	margin-right: 1px;
}
.mui_ekp_portal_date_title .muis-to-right{
	margin-left: 1px;
}
.mui_ekp_portal_date_title>span{
	font-family: "landray-number",fontnumber, -apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
	float: left;
	font-weight: 600;
    height: 3rem;
    line-height: 3rem;
    display: block;
}
.mui_ekp_portal_date_datails{
	padding:0 1.25rem;
	margin: 2rem 0;
}
.mui_ekp_portal_date_datails>li{
	position: relative;
	border-left:1px solid #4285F4;
	padding:0 0 2.2rem 1.8rem;
}
.mui_ekp_portal_date_datails>li.muiListNoData{
	padding: 0;
	background: #fff;
	line-height: 16rem;
}
.mui_ekp_portal_date_datails>li.mblListItemSelected {
	background: none !important;
}
.mui_ekp_portal_date_datails>li:last-child {
	border-left-color:transparent
}
.mui_ekp_portal_date_datails>.muiListNoData:after {
	width: 0;
	height: 0;
	border: 0;
}
.mui_ekp_portal_date_datails>li:after {
	content: "";
	display: inline-block;
	width: 7px;
	height: 7px;
	border:3px solid #4285F4;
	border-radius: 7px;
	position: absolute;
	top: 0px;
	left: -7px;
	background-color: #ffffff;
}
.mui_ekp_portal_date_datails>li.outdate:after{
	border:3px solid #E8E8E8;
}
.mui_ekp_portal_date_datails>li.outdate {
	border-left-color: #E8E8E8;
}
.mui_ekp_portal_date_datails>li.outdate>span,
.mui_ekp_portal_date_datails>li.outdate>p {
	color: #ADAFBA;
}
.mui_ekp_portal_date_datails>li{
	position: relative;
	border-left:1px solid #4285F4;
	padding:0 0 2.2rem 1.8rem;
}
.mui_ekp_portal_date_datails>li.outdate {
	border-left-color: #E8E8E8;
}
.mui_ekp_portal_date_datails>li:last-child {
	border-left-color:transparent
}
.mui_ekp_portal_date_datails>li:after {
	content: "";
	display: inline-block;
	width: 7px;
	height: 7px;
	border:3px solid #4285F4;
	border-radius: 7px;
	position: absolute;
	top: 0px;
	left: -7px;
	background-color: #ffffff;
}
.mui_ekp_portal_date_datails>li>span{
	margin-bottom: .2rem;
	display: block;
    font-family: "landray-number", -apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
    color: #3E4665;
    position: relative;
    top: -6px;
}
.mui_ekp_portal_date_datails>li>p {
	color: #2A304A;
	overflow: hidden;
	text-overflow: ellipsis;
