	"dojo/_base/lang",
	"dojo/_base/array",
	'dojo/topic',
	"dojo/request",
	"dojo/on",
	"dojo/dom", "dojo/dom-construct", "dojo/dom-style", "dojo/dom-class",
	"dijit/_WidgetBase",
	"dijit/_Container",
	"dijit/_Contained"
], function (declare, lang, array, topic, request, on, dom, domCtr, domStyle, domClass,
	WidgetBase, Container, Contained) {
