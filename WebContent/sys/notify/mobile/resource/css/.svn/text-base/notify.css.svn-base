/*---------------------------------------- 待审列表 ----------------------------------------*/

.lui_notify_pending{
	background: url(../../../resource/images/pendding.png?v=1.0) no-repeat 50% 30%;
    display: inline-block;
    vertical-align: middle;
    height: 16px;
    width: 16px;
    margin-bottom:3px;
}
.muiCardItem .figure .title .warn{margin-right:0px;}
.muiCardSearchBarDiv{
	position:absolute;
	top:0px;
	left:0px;
	width: 100%;
	z-index:10;
	padding: 0rem 0rem;
	background-color: #f7f7f7;
	overflow: hidden;
}
.muiCardSearchBar{
	width:100%;
	height:3.8rem;
	vertical-align: middle;
	border-bottom:1px solid #e8e8e8;
}
.muiCardSearchBarContainer{
	display:table;
	width:100%;
	box-sizing:border-box;
    -moz-box-sizing:border-box; /* Firefox */
    -webkit-box-sizing:border-box; /* Safari */
	height: 2.8rem;
	line-height: 2.8rem;
	padding:0.5rem
	
}
.muiCardSearchBarContainer > div{
	vertical-align: middle;
	display: table-cell;
}
.muiCardSearchDiv{
	position: relative;
	border: 0.1rem solid #d7d7d7;
	background-color: #fff;
	border-radius: 0.4rem;
	height: 100%;
	overflow: hidden;
}
.muiCardSearchIcon,.muiCardSearchInput,.muiCardSearchClear{
	border: none !important;
	width: 2.5rem;
	font-size:2rem;
	height: 100%;
	line-height: 2.8rem;
	vertical-align: middle;
	display: inline-block;
	background:none !important;
}
.muiCardSearchIcon,.muiCardSearchClear{
	position: absolute;
	color: #93989d;
	text-align:center;
}
.muiCardSearchIcon{
	top:0rem;
	left:0.5rem;
}
.muiCardSearchClear{
	top:0rem;
	right:0;
	display: none;
	z-index: 14 !important;
}
.muiCardSearchInput{
	position: absolute;
	padding:0rem 2rem 0rem 3.2rem;
	top:0rem;
	width:100%;
	text-align:left;
	font-size:1.4rem;
	line-height:2.8rem;
	height:2.8rem;
	vertical-align:middle;
	outline:none;
	background-color:none;
	-webkit-appearance:none;
	-webkit-box-shadow:none;
	appearance:none;
	box-shadow:none;
}
.muiSearchBar .muiSearchType {
	width: 6.5rem;
	background: #FFFFFF;
}

  /* 大字体兼容适配 Starts */
html.fontSize2 .muiSearchBar .muiSearchType,
html.fontSize3 .muiSearchBar .muiSearchType,
html.fontSize4 .muiSearchBar .muiSearchType{
	width: 7.2rem;
}
  /* 大字体兼容适配 Ends */

.notifySearchType.muiFormEleWrap.popup{
	height:2.8rem;
}

.oldMui .notifySearchType.muiFormEleWrap.showTitle .muiSelInput{
	line-height: 2.8rem;
}
.oldMui .notifySearchType .muiSelInputRight{
	line-height: 2.8rem;
	margin-top: -1.3rem;
}

.notifySearchType .muiSelInput:empty:after{
	line-height: 2.8rem;
}
.notifySearchType{
	min-height: 0px!important;
	height: 100%;
}
.notifySearchType .muiSelInputRight .mui-forward{font-size: 1.6rem;}
.notifySearchType .muiSelInput{text-align: center;}
.muiSearchType .muiFormEleWrap.popup {
	position: relative;
}
.muiSearchType .oldMui.muiFormEleWrap.showTitle .muiSelInput {
	padding-left: 1rem;
	text-align: left;
	line-height: 3rem;
}

.muiSearchInput {
    padding: 0 2rem 0 3.5rem;
    width: 100%;
    text-align: left;
    line-height: 2rem;
    height: 3.4rem;
    vertical-align: middle;
    outline: none;
    -webkit-appearance: none;
    box-shadow: none;
}

.muiNotifySearchTitle{
	background-color: #F7F7F8;
	border: 0;
}

/***********************************   消息二级页面搜索工具栏相关（Start）  ***********************************/
.muiNotifySearchBar{
    position: relative;	
	padding-left:1.2rem;
	padding-right:1.2rem;
	padding-top: 1rem;
	padding-bottom: 1rem;
}
.muiNotifySearchBar:after{
    content: " ";
    position: absolute;
    z-index: 2;
    bottom: 0;
    left: 0;
    right: 0;
    border-bottom: .1rem solid #EEEEEE;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.muiNotifySearchBarContainer{
	position: relative;
}
.muiNotifySearchInputContainer{
	background: #f7f7f7;
    border-radius: 5px;
    position: relative;
    height: 100%;
    margin-left: 6rem;
    margin-right: 4.8rem;
}

.muiNotifySearchTypeContainer{
	position: absolute;
    top: 0;
    left: 0;
	width: 6rem;
	height: 100%;
	z-index: 1;
}

.muiNotifySearchType{
	height: 100%;
}

.muiNotifySearchIcon, .muiNotifySearchClear {
    position: absolute;
    display: inline-block;
    text-align: center;
    color: #d2d3d8;
    height: 3.4rem;
    line-height: 3.4rem;
    vertical-align: middle;
}
.muiNotifySearchIcon{
    width: 3rem;
    font-size: 1.6rem;
}
.muiNotifySearchClear{
    width: 2.6rem;
    font-size: 1.6rem;
    right: 0;
    display:none;
}    

.muiCardSearchBtnDiv{
	position: absolute;
    top: 0;
    right: 0;
	text-align: right;
	width:4.8rem;
	z-index: 1;
    height: 3.4rem;
    line-height: 3.4rem;
}
.muiCardSearchCancelBtn{
    text-align:center;
    vertical-align:middle;
    display:inline-block;
    white-space:nowrap;
    cursor:pointer;
}
.muiNotifyTypeSelectContainer{
	height: 100%;
}

.muiNotifyTypeSelectContainer>.muiNotifyTypeSelectText,
.muiNotifyTypeSelectContainer>.muiNotifyTypeSelectStatusContainer{
    display: inline-block;
    height: 3.4rem;
    line-height: 3.4rem;
}
.muiNotifyTypeSelectContainer .muiNotifyTypeSelectText{
}
.muiNotifyTypeSelectContainer .muiNotifyTypeSelectStatusContainer{
	margin-left: .4rem;
}
.muiNotifyTypeSelectContainer .muiNotifyTypeSelectStatusContainer>i{
	display: inline-block;
	font-size: 1rem;
    -webkit-transform: scale(0.6);
    -moz-transform: scale(0.6);
    -o-transform: scale(0.6);
    transform: scale(0.6);	
}

body div.muiNotifyTypeSelectOptionsDialog{
    position: absolute;
	width: 100%;
    z-index: 99999;
    background-color: rgba(0,0,0,0.5);
    -webkit-transition: -webkit-transform .5s ease-out;
}

body div.muiNotifyTypeSelectOptionsDialog .muiNotifyTypeSelectOptionsContainer{
	position: relative;
    background-color: #FFFFFF;
    text-align: center;
    width: 100%;
    left: 0;
    padding-top: 1rem;
    margin-top: -1px;
}

body div.muiNotifyTypeSelectOptionsDialog .muiNotifyTypeSelectOptionsContainer .muiNotifyTypeSelectOption{
	position: relative;
	padding: 1.6rem 1.2rem;
}

body div.muiNotifyTypeSelectOptionsDialog .muiNotifyTypeSelectOptionsContainer .muiNotifyTypeSelectOption:before{
	content: " ";
    position: absolute;
    z-index: 2;
    top: 0;
    left: 0;
    right: 0;
    border-top: .1rem solid #EEEEEE;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
/***********************************   消息二级页面搜索工具栏相关（End）  ***********************************/



/***********************************   消息二级页面列表相关（Start）  ***********************************/
.notifyCardList .muiNotifyCardItem{
	background-color: #FFFFFF;
	box-shadow: 0 0 3rem 0 rgba(153,179,210,0.20);
	border-radius: .6rem;
    margin-top: .8rem;
}

.notifyCardList .muiNotifyCardItem:last-child{
	margin-bottom: .8rem;
}

.notifyCardList .muiNotifyCardItem .warn{
	color: #F46642;
}

.notifyCardList .muiNotifyCardItemLock .muiNotifyContentContainer{
    background: url(../images/notify-data-bg-lock.png) right bottom .4rem no-repeat;
    background-size: 4rem 5.6rem;
}


.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer{
	padding-top: .6rem;
	padding-bottom: .6rem;
    padding-left: 1rem;
    padding-right: 1rem;	
    
}    

.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyTitleLabel{
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    color: var(--muiFontColorInfo);
    font-size: var(--muiFontSizeM);    
    -webkit-line-clamp: 4;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    box-orient: vertical;
    -webkit-box-pack: center;
    box-pack: center;
    -webkit-box-align: start;
    box-align: start;
}

.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo{
	position: relative;
    color: var(--muiFontColorMuted);
    font-size: var(--muiFontSizeS);	
    margin-top: .4rem;
    min-height: 2.6rem;
}

.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo .muiNotifyPersonHeadIcon{
    position: absolute;
    width: 2.4rem;
    height: 2.4rem;
    border-radius: 50%;
    border: 1px solid #c9dfff;
    background-color: #f8f8f8;
    overflow: hidden; 
}
.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo .muiNotifyPersonHeadIcon img{
	width: 100%;
}

.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo .muiNotifyTextInfo{
    position: relative;
    font-size: var(--muiFontSizeS);
    padding-left: 3.6rem;
    padding-top: .4rem;
}
.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo .muiNotifyTextInfo span{
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 100%;
}
.notifyCardList .muiNotifyCardItem .muiNotifyContentContainer .muiNotifyBaseInfo .muiNotifyTextInfo span:not(:last-child){
	padding-right: 1rem;
}

.notifyCardList .muiNotifyCardItem .muiNotifyOperateBtnContainer{
	position: relative;
    color: var(--muiFontColor);
    font-size: var(--muiFontSizeS);	
    text-align: right;
    padding-left: 1rem;
    padding-right: 1rem;    
    padding-bottom: .6rem;
}

.notifyCardList .muiNotifyCardItem .muiNotifyOperateBtnContainer .muiNotifyOperateBtn{
    display: inline-block;
    border-radius: .2rem;
    padding-left: 1.2rem;
    padding-right: 1.2rem;
    padding-top: .4rem;
    padding-bottom: .4rem;
}

.notifyCardList .muiNotifyCardItem .muiNotifyOperateBtnContainer .muiNotifyOperateBtn.muiNotifyGoToBtn{
	border: 1px solid #999999;
	color: #2A304A;
}

.notifyCardList .muiNotifyCardItem .muiNotifyOperateBtnContainer .muiNotifyOperateBtn.muiNotifySetReadBtn{
	background-color: #4285F4;
	border: 1px solid #4285F4;
    color: #FFFFFF;
}

.notifyCardList .muiNotifyCardItem .muiNotifyOperateBtnContainer .muiNotifyOperateBtn:not(:first-child){
	margin-left: 1rem;
}

/***********************************   消息二级页面列表相关（End）  ***********************************/