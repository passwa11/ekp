/* 图标  */
.lui_notify_todo_list_body .lui_dataview_classic_heading .lui_refreshIcon, .lui_notify_todo_list_body .lui_dataview_classic_heading .lui_fastApproveIcon {
    display: inline-block;
    background-repeat: no-repeat;
    background-size: cover;
    width: 16px;
    height: 16px;
    cursor: pointer;
    margin: 0px 8px 0 10px;
    position: relative;
    top: 4px;
}

/* 刷新图标 */
.lui_notify_todo_list_body .lui_dataview_classic_heading .lui_refreshIcon {
    background-image: url(../images/ldux/refresh@2x.png);
}
@media \0screen\,screen\9 {
    .lui_notify_todo_list_body .lui_dataview_classic_heading .lui_refreshIcon {
        background-image: url(../images/ldux/refresh.png);
    }
}
.lui_notify_todo_list_body .lui_dataview_classic_heading .lui_refreshIcon img {
    display: none;
}

/* 快速审批图标 */
.lui_notify_todo_list_body .lui_dataview_classic_heading .lui_fastApproveIcon {
    background-image: url(../images/ldux/fastreview@2x.png);
}
