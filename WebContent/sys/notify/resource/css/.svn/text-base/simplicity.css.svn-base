    display: none;
}

.lui_notify_todo_list_body .lui_dataview_classic_heading .lui_refreshIcon a, .lui_notify_todo_list_body .lux_dataview_classic_heading .lui_refreshIcon a, .lui_notify_todo_list_body .lui_dataview_classic_heading .lui_fastApproveIcon a, .lui_notify_todo_list_body .lux_dataview_classic_heading .lui_fastApproveIcon a {
    background: none;
    width: 100%;
    height: 100%;
