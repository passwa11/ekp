/* 日历日程样式 开始 */

.lui_plump_calendar--box {
  width: 364px;
  height: 504px;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,0.05);
  border-radius: 6px;
  color:#333;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}
.lui_plump_calendar--content {
  padding: 10px 17px;
  position: relative;
}
.lui_plump_calendar--header {
  height: 49px;
  position: relative;
}
.lui_plump_calendar--header div {
  display: inline-block;
  color: #666;
}
.lui_pch_title {
  font-size: 32px;
}
.lui_pch_date_switch {
  margin-left:17px; 
}
.lui_pch_date_prevSelect {
    width: 8px;
    height: 8px;
    display: inline-block;
    background: url('../images/prev-arrow.png') no-repeat;
    background-size: contain;
    cursor: pointer;
}
.lui_pc_date_nextSelect {
    width: 8px;
    height: 8px;
    display: inline-block;
    background: url('../images/next-arrow.png') no-repeat;
    background-size: contain;
    cursor: pointer;
}
.lui_pch_button {
  position: absolute;
  right: 0px;
  top: 50%;
  transform: translate(0,-50%);
}
.lui_pch_button .lui_pch_currentDate {
  width: 27px;
  height:17px;
  display: inline-block;
  border:1px solid #aaa;
  border-radius: 4px;
  color: #C2C2C2;
  text-align: center;
  position: relative;
  bottom: 0px;
  vertical-align: text-bottom;
  line-height: 17px;
}
.lui_pch_renovate {
  width: 27px;
  height:17px;
  display: inline-block;
  border:1px solid #aaa;
  border-radius: 4px;
  background: url('../images/refresh.png') no-repeat center;
  background-size: contain;
  margin-left: 5px;
  vertical-align: text-bottom;
}
.lui_plump_calendar--dateMain table{
  width: 100%;
  text-align: center;
}
.lui_plump_calendar--dateMain table thead {
  opacity: 0.5;
  font-size: 12px;
  color: #999999;
}
.lui_plump_calendar--dateMain table td{
  padding: 5px 0px;
}
.lui_plump_calendar--dateMain table td div{
  width: 28px;
  height: 28px;
  margin: 0 auto;
  position: relative;
  line-height: 28px;
  border: 1px solid transparent;
}
.lui_plump_calendar--dateMain .lui_pcd_table--tr td:first-child,.lui_plump_calendar--dateMain .lui_pcd_table--tr td:last-child{
  color: #C2C2C2;
}
.lui_pcd_dropdowm--button {
  text-align: center;
  margin-top: 4px;
}
.lui_pcd_dropdowm--button i{
  width: 17px;
  height: 15px;
  display: inline-block;
  background: url('../images/arrow_down.png') no-repeat;
  background-size: cover;
  cursor: pointer;
  transition-duration: .5s
}
.lui_pcd_dropdowm--button::after {
  content: ' ';
  width: 100%;
  height: 1px;
  background: #eee;
  display: block;
  position: absolute;
  left: 0;
}
.lui_plump_calendar--timeAxis {
  padding: 10px 0px 0px 22px;
  position: relative;
}
.lui_pct_ui--div {
  font-size: 16px;
  color: #333333;
  margin-bottom: 8px;
}
.lui_pct_ui--div a {
  border: 1px solid #4285F4;
  border-radius: 2px;
  display: inline-block;
  font-size: 12px;
  color: #4285F4;
  padding: 0px 7px;
  margin-left: 11px;
  vertical-align: text-bottom;
}
.lui_pct_ul--li {
  position: relative;
  margin-bottom: 20px;
}
.lui_pct_ul--li .lui_pct_ui--item{
  background: #F7F9FB;
  border-radius: 4px;
  width: 100%;
  height: 32px;
  line-height: 32px;
  padding-left: 10px;
  box-sizing: border-box;
}
.lui_pct_ul--li::before{
  content: ' ';
  height: 80px;
  width: 1px;
  display: inline-block;
  background: rgba(204, 204, 204,.5);
  position: absolute;
  left: -17px;
  top: 10px;
}
.lui_pct_ui__i {
  position: absolute;
  left: -21px;
  background: rgba(170, 170, 170);
  width: 8px;
  height: 8px;
  border-radius: 50%;
  top: 6.5px;
  
}
.lui_pct_ui--div i::before{
  content:' ';
  width: 14px;
  height: 14px;
  background: rgba(170, 170, 170,.4);
  display: inline-block;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top:50%;
  transform: translate(-50%,-50%);
}
.lui_pct_ui--div .lui_pct_uid_green {
  border-color:#22B77D;
  color: #22B77D;
}
.lui_plump_calendar--dateMain .lui_pcd_div--circleStyle {
  border-radius: 50%;
  /* border: 1px solid rgba(66, 133, 244,.5); */
  border: 1px solid rgba(66, 133, 244,.5);
  /* background: rgb(66, 133, 244); */
  /* color: #fff; */
}
.lui_plump_calendar--dateMain .lui_pcd_div--currentStyle {
  border: 1px solid rgba(66, 133, 244,.5);
  background: rgb(66, 133, 244);
  color: #fff;
  border-radius: 50%;
}
.lui_plump_calendar--dateMain .lui_pcd_div--redPointStyle::before {
  content: ' ';
  width: 6px;
  height: 6px;
  background: rgb(66, 133, 244);
  display: inline-block;
  border-radius: 50%;
  position: absolute;
  right: 2px;
  top: 2px;
}
.lui_pct_ul--li:last-child::before{
  content: normal;
}
.lui_pcd--shadeBox {
  height: 60px;
  overflow: hidden;
  transition-duration: .5s
}
.lui_pcd_timeAixs--actions::before {
  background: #4285F4
}
.lui_pct_ui__i--actions {
  background: #4285F4
}
.lui_pct_ui__i--actions::before {
  background:rgba(66, 133, 244,.4) !important;
}
.lui_plump_calendar--footer {
  position: absolute;
  bottom:0px;
  background: #fff;
  width: 100%;
  /* background: linear-gradient(to bottom, transparent , #fff); */
}
.lui_pcf_button {
  width: 100%;
  text-align: right;
}
.lui_pcf_newCalendar,.lui_pcf_more {
  width: 30px;
  height: 22px;
  display: inline-block;
  border: 1px solid #DDDDDD;
  border-radius: 4px;
}
.lui_pcf_newCalendar {
  background:url('../images/add.png') no-repeat center;
  background-size: contain;
}
.lui_pcf_more {
  margin-left: 10px;
  margin-right: 17px;
  background:url('../images/more.png') no-repeat center;
  background-size: contain;
}
.lui_pce_iconStyle i{
  background:url('../images/empty.png') no-repeat center;
  background-size: contain;
  display: block;
 width: 64px;
 height: 64px;
 margin: 0 auto;
}
.lui_pce_iconStyle {
  width: 100%;
  top: 50%;
 left:50%;
 transform: translate(-50%,-50%);
 position: absolute;
}
.lui_plump_calendar--empty {
   width: 100%;
  height: 300px;
  position: relative;
}
.lui_pce_is_title {
  font-size: 12px;
color: #333333;
text-align: center;
}
.lui_pce_is_text {
  font-size: 12px;
text-align: center;
color: #999999;
margin-top: 6px;
}

/* 日历日程样式 结束 */

/* 新地图 样式 开始 */
.lui_new_map_box {
  width: 100%;
  min-height: 400px;
  background: #FFFFFF;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,0.05);
  border-radius: 6px;
  margin: 0 auto;
  padding-top: 5px;
  box-sizing: border-box;
}
.lui_new_map_content {
  width: 100%;
  min-height:400px;
  background-size: cover!important;
  margin: 0 auto;
  vertical-align: center;
  position: relative;
  padding-bottom: 50px; 
}
.lui_new_map_nav {
}
.lui_nmn_navList_ul {
  text-align: center;
}
.lui_nmn_nu_item {
  display: inline-block;
  width: 128px;
  height: 54px;
  line-height: 54px;
  /* background: #EAF4FF; */
  text-align: center;
  font-size: 16px;
  color: #999999;
  cursor: pointer;
  transition-duration: .3s;
  /* color: #4285F4; */
}
.lui_nmn_nu_item_active {
  background: #EAF4FF;
  color: #4285F4;
  border-bottom: 1px solid #4285F4;
}
.lui_nmn_nu_item:hover {
  color: #4285F4;
}
.lui_nmn_nu_item:last-child {
}
.lui_new_map_label {
  padding-top: 19px;
}
.lui_nmn_labelList_ul {
  text-align: center;
}

.lui_new_map{
	display: none;
}
.lui_nmn_lu_item {
  display: inline-block;
  border: 1px solid #979797;
  border-radius: 14px;
  width: 96px;
  text-align: center;
  color: #999999;
  margin-right: 14px;
  cursor: pointer;
  transition-duration: .3s;
}
.lui_nmn_lu_item-active {
  background: #4285F4;
  color:#fff !important;
}
.lui_nmn_lu_item:last-child {
  margin-right: 0px;
}
.lui_nmn_lu_item:hover {
  color: #4285F4;
  border: 1px solid #4285F4;
}
.lui_new_map_cardList {
  margin-top: 40px;
  text-align: center;
  display: none;
}
.lui_nm_cardList_ul {
  text-align: center;
  display: inline-block;
  vertical-align: top;
  margin-right: 20px;
}
.lui_nm_cardList_ul:last-child {
  margin-right: 0px;
}
.lui_nm_clu_card {
  width: 180px;
  background: #FFFFFF;
  box-shadow: 0 2px 5px 0 rgba(0,12,98,0.10);
  border-radius: 6px;
  display: inline-block;
  overflow: hidden;
}
.lui_nm_cluc_title {
  height: 46px;
  line-height: 46px;
  background: url('../images/headBg.png') no-repeat center;
  color: #fff;
  font-size: 16px;
  margin-bottom: 10px;
}
.lui_nm_cluc_itemList {
  
}
.lui_nm_cluc_il_item {
  height: 30px;
  line-height: 30px;
  transition-duration: .3s;
}
.lui_nm_cluc_il_item:hover {
  background: #F2F2F2;
  color: #4285F4;
  cursor: pointer;
}
.lui_nm_cluc_il_item a  {
  color: #666;
  width: 100%;
  height: 100%;
  display: block;
  transition-duration: .3s;
}
.lui_nm_cluc_il_item a:hover {
  color: #4285F4;
}

.lui_new_map ul {
  list-style: none;
}
/* 新地图 样式 结束 */
