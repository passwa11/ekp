/* create 210910 更新门户配置样式 */
/* === 选择标签类型 === */
body a:hover {
    color: #346ac3;
}

.lui_portal_select_tab_iframe {
    /* display: inline-block; */
    text-align: center;
    margin-top: 110px;
    width: 629px;
    margin: 110px auto 0;
}

.lui_portal_select_tab_iframe li {
    display: inline-block;
    width: 290px;
    height: 180px;
    border: 1px solid #ddd;
    cursor: pointer;
    margin: 0 10px;
}

.lui_portal_select_tab_iframe li.select {
    border-color: #4285F4;
    color: #4285F4;
}

.lui_portal_select_tab_iframe img {
    width: 260px;
    margin-top: 63px;
}

.lui_portal_select_tab_iframe input {
    opacity: 0;
}

.lui_portal_select_tab_iframe p {
    margin-top: 30px;
}
body .lui_form_content{
    padding:0;
}

/* === 弹窗样式修改 ===*/
body .lui_dialog_frame {
    /* background: #4285F4; */
    border: none;
    border-radius: 4px;
    overflow: hidden;
}

body .lui_dialog_head_right:hover {
    background-color: transparent;
}

/* body .lui_dialog_head {
    background: #4285F4;
} */
body .lui_dialog_head_left {
    /* color: #fff; */
}

body .lui_toolbar_frame_float {
    background: #fff;
}

body .lui_tabpanel_vertical_light_content_c [data-lui-type='lui/panel!Content'] {
    padding: 10px 20px 0 20px;
}

body .lui_toolbar_frame_float .lui_toolbar_btn .lui_widget_btn_txt {
    /* border-radius: 6px; */
    /* overflow:hidden; */
}

body .lui_tabpanel_vertical_light_content_c .lui_portlet_operations {
    display: none;
}

