/*nav导航样式start*/
.muiNavitem.muiNavitemBadge{
	padding: 0 1.5rem;
	margin:0!important;
}
.muiNavitem .mblDomButtonRedBadge{
	display: none;
}
.muiNavitem .muiNavitemSpanCount{
	vertical-align: top;
	display: inline-block;
    overflow: unset!important;
    height: 85%;
    position: relative;
    color: #999FB7;
}
.muiNavitem.muiNavitemSelected .muiNavitemSpanCount{
	color:#2A304A;
}
.muiNavitem.muiNavitemSelected .muiNavitemBottom{
	position: relative;
    bottom: 3px;
}
.muiAccessLogNavItem>.muiNavitemSpan{
	display: none;
}
.muiNavitem .muiNavitemDiv{
	height: 100%;
}
.muiNavitemDiv span.muiNavitemSpan{
	display: inline-block;
}
/*nav导航样式end*/

/*列表样式start*/
.muiAccordionPanelTitle .panelTitileRight{
	position: absolute;
	top:0;
	right:0;
	color: #4285F4;
}
.panelTitileRight i{
	width: 1.1rem;
    height: 1rem;
    display: inline-block;
    margin-right: 0.4rem;
    background: url(../images/detail_log@2x.png) no-repeat;
    background-size: cover;
}
.muiAccessLogListItem{
	width: 25%;
    float: left;
    margin-top: 1.5rem;
}
.muiAccessLogListItem.fullItem{
	width: 100%;
	float: none;
	margin-top: auto;
	border-bottom: 1px solid #E7E7E7;
	height: auto;
	padding: 1rem 1.5rem;
    box-sizing: border-box;
}
.muiAccessLogListItem .itemBaseInfo{
	height: 5rem;
}
.muiAccessLogListItem .personIcon{
	height: 5rem;
    width: 5rem;
    border: 1px solid #d5d5d5;
    background-color: #f8f8f8;
    border-radius: 50px;
    overflow: hidden;
    margin: 0 auto;
}
.muiAccessLogListItem .personIcon>img{
	width: 100%;
	height: 100%;
}
.muiAccessLogListItem .personName{
	text-align: center;
    font-size: 1.4rem;
    margin-top: 0.7rem;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.muiAccessLogListItem .itemBaseInfo .baseInfoLeft{
	float: left;
}
.baseInfoLeft .personIcon{
	float: left;
}
.baseInfoLeft .personInfo{
	float: left;
	height: 5rem;
    margin-left: 1rem;
    position: relative;
}
.personInfo .personInfoName{
	font-size: 1.7rem;
    color: #2A304A;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    max-width: 10rem;
}
.personInfo .personInfoDept{
	font-size: 1.3rem;
    color: #9497A4;
    position: absolute;
    bottom: 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    max-width: 10rem;
}
.muiAccessLogListItem .itemBaseInfo .baseInfoRight{
	float: right;
	height: 5rem;
    position: relative;
}
.baseInfoRight .timeInfo{
	font-size: 1.3rem;
    color: #9497A4;
}
.baseInfoRight .statusInfo{
	font-size: 1.2rem;
    letter-spacing: 0;
    text-align: center;
    line-height: 18px;
    border-radius: 18px;
    width: 4.6rem;
    position: absolute;
    bottom: 0;
    right: 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    padding: 0 5px;
}
.statusInfo.process{
	color: #4285F4;
	border: 1px solid #4285F4;
}
.statusInfo.publish{
	color: #2CCC74;
	border: 1px solid #2CCC74;
}
.baseInfoRight .ipInfo{
	font-size: 1.3rem;
    color: #9497A4;
    margin-top: 1.4rem;
}
.muiAccessLogListItem .itemFileInfo{
	padding-left: 6rem;
    margin-top: 0.7rem;
}
.itemFileInfo .itemFile{
	padding: 0.6rem 0.9rem;
    background: #F5F6FB;
    border-radius: 6px;
}
.itemFile .fileIcon{
	width: 1.6rem;
    height: 1.6rem;
    display: inline-block;
    background: url(../images/icon_file@2x.png) no-repeat;
    background-size: cover;
    margin-right: 0.5rem;
    position: relative;
    top: 0.2rem;
}
.itemFile .fileTitle{
	font-size: 1.5rem;
	color: #2A304A;
}
/*列表样式end*/
.muiDialogElement .muiDialogElementDiv_bottom, .muiDialogElement .muiDialogElementDiv_top{
	border-bottom: 1px solid #E7E7E7;
}
