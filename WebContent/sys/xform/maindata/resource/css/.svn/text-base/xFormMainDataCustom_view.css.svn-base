@CHARSET "UTF-8";

.xform_main_data_custom_input{
	width:80%;
	border:0px;
	color:grey;
	font-size:12px;
	padding-left:4px;
	height:14px;
}
.xform_main_data_custom_pageUl{
	list-style:none;
	float:right;
	width:110px;
	margin:10px 0px 0px 0px;
}

.xform_main_data_custom_pageUl li{
	display:none;
	cursor:pointer;
	float:right;
	padding:0 5px;
}
.xform_main_data_custom_editDiv{
	display:none;
}
.xform_main_data_custom_editDiv_bg{
	position: absolute; 
	top: 0%; 
	left: 0%; 
	width: 100%; 
	height: 100%; 
	background-color: black; 
	z-index:1001; 
	-moz-opacity: 0.7; 
	opacity:.70; 
	filter: alpha(opacity=70);
}

.xform_main_data_custom_editDiv_contentWrap{
	 position: absolute; 
	 top: 40%; 
	 left: 22%; 
	 width: 53%; 
	 padding: 8px; 
	 background-color: white; 
	 z-index:1002; 
	 overflow: auto;
}

.xform_main_data_custom_search{
	display:inline;
	margin-left:15px;
}

.xform_main_data_custom_search .xform_main_data_custom_search_input{
	width:200px;
	height:24px;
	border:0px;
	border-bottom:1px solid #b4b4b4;
}