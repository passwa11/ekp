@charset "utf-8";

/* 主数据 PC-端 */


/* 主数据框架 */

.lui_keydata_frame {}

.lui_keydata_sidebar {
	background: #4285F4;
	width: 100%;
	position: relative;
}

.lui_keydata_infoBar {
	width: 80px;
	position: absolute;
	top: 30px;
	left: 40px;
}

.lui_keydata_link {
	color: #fff;
}

.lui_keydata_opBar .icon_more,
.lui_keydata_opBar .icon_code {
	display: inline-block;
	width: 16px;
	height: 16px;
	background-repeat: no-repeat;
	background-position: center;
	vertical-align: middle;
	margin-right: 5px;
}

.lui_keydata_opBar .icon_more {
	background-image: url(../images/icon_more.png)
}

.lui_keydata_opBar .icon_code {
	background-image: url(../images/icon_code.png)
}

.lui_keydata_opBar .lui_keydata_code:hover .lui_keydata_qcode {
	display: inline-block;
}

.lui_keydata_qcode:before {
	display: inline-block;
	content: "";
	width: 0;
	height: 0;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 6px solid #fff;
	position: absolute;
	top: -6px;
	right: 15px;
}

.lui_keydata_qcode {
	display: none;
	width: 100px;
	height: 100px;
	padding: 10px;
	background: #fff;
	position: absolute;
	right: 0;
	top: 25px;
}

.lui_keydata_qcode img {
	width: 100%;
	height: 100%;
}

.lui_keydata_link:hover,
.lui_keydata_code:hover {
	color: #fff;
}

.lui_keydata_info_avatar {
	display: inline-block;
	width: 80px;
	height: 80px;
	border-radius: 50%;
}

.lui_keydata_info_avatar img {
	max-width: 100%;
	max-height: 100%;
	border-radius: 50%;
}

.lui_keydata_info_name {
	margin: 0;
	font-size: 24px;
	color: #fff;
	margin-bottom: 20px;
}

.lui_keydata_category {
	background: #F8E71C;
	border-radius: 2px;
	padding: 1px 2px;
	font-size: 10px;
	color: #4285F4;
	margin-left: 10px;
	position: relative;
	top: -12px;
}

.lui_keydata_info_list {
	display: table;
}

.lui_keydata_content,
.lui_keydata_opBar {
	display: table-cell;
}

.lui_keydata_opBar {
	z-index: 1;
	vertical-align: top;
	white-space: nowrap;
}

.lui_keydata_opBar {}

.lui_keydata_code {
	color: #fff;
	margin-left: 30px;
	position: relative;
	display: inline-block;
	padding-right: 30px;
}

.lui_keydata_code #top {
	display: inline-block;
}

.lui_keydata_code canvas {
	width: 100%;
	height: 100%;
}


/* 隐藏滚动条 */

.lui_keydata_scroll_mask {}

.lui_keydata_body_frame {
	margin-left: 150px;
	margin-top: 10px;
	margin-right: 30px;
}

.lui_keydata_info_list {
	padding-left: 150px;
	padding-bottom: 30px;
	padding-top: 40px;
}

.lui_keydata_info_list ul {
	table-layout: fixed;
	font-size: 0;
}

.lui_keydata_info_list ul .keydata_info_label {
	font-style: normal;
	color: rgba(255, 255, 255, .7);
	color: #fff\9;
	width: 85px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	margin-right: 5px;
}

.lui_keydata_info_list ul li .keydata_info_txt {
	color: #fff;
	padding-right: 40px;
}

.lui_keydata_info_list ul li {
	list-style: none;
	display: inline-block;
	width: 30%;
}

.lui_keydata_info_list ul li .lui_keydata_info_innner {
	padding-right: 40px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	margin-bottom: 15px;
}

.lui_keydata_info_list ul li.keydata_img_li {}

.lui_keydata_info_list ul li .keydata_info_label,
.lui_keydata_info_list ul li .keydata_info_txt {
	display: inline-block;
	vertical-align: middle;
	line-height: 20px;
}

.lui_keydata_moreBtn {
	border: 1px solid #fff;
	border-radius: 4px;
	padding: 3px 4px;
	font-size: 12px;
	font-style: normal;
	height: 10px;
	cursor: pointer;
	line-height: 10px;
	display: inline-block;
	margin-left: 5px;
}

.lui_keydata_info_list ul li .keydata_img {}

.lui_keydata_info_list ul li .keydata_img img {}


/* 切换选项卡 Starts */

.lui_keydata_body_frame .lui_zone_nav_bar_new {}

.lui_keydata_body_frame .lui_zone_nav_bar_new li a {
	color: #333;
	height: 28px;
	line-height: 28px;
	margin-top: 8px;
	font-size: 14px;
}

.lui_keydata_body_frame .lui_zone_mbody {
	min-width: auto!important;
	margin-top: 0;
}

.lui_keydata_body_frame .lui_zone_nav_box_new {
	margin-bottom: 0;
}

.lui_keydata_body_frame .lui_zone_nav_box_new.com_bgcolor_d {
	background: #f7f7f7;
	border-top: 1px solid #e7e7e7;
	border-bottom: 1px solid #4285f4;
	border-radius: 0;
	height: 36px;
}

.lui_keydata_body_frame .nav_link .nav_link_body {
	width: 100%!important;
	background: #f7f7f7;
}

.lui_keydata_body_frame .lui_zone_nav_bar_new li:first-child {
	margin-left: 5px;
}

.lui_keydata_body_frame .lui_zone_nav_bar_new li {
	width: auto;
	margin-right: 5px;
	height: 36px;
}

.lui_keydata_body_frame .lui_zone_nav_bar_new li:hover a:hover {
	color: #fff;
}

.lui_keydata_body_frame .lui_zone_nav_bar_new li a:hover,
.lui_keydata_body_frame .lui_zone_nav_bar_new li a.com_btn_link {
	height: 28px;
	line-height: 28px;
	background: #4285f4;
	color: #fff!important;
	margin-top: 8px;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	border: none;
	text-decoration: none;
}

.lui_keydata_body_frame .nav_link .left_row,
.lui_keydata_body_frame .nav_link .right_row {
	height: 36px;
	line-height: 36px;
	background-color: #f1f1f1;
	color: transparent;
	top: 0;
	border-right: 1px solid #e7e7e7;
	border-left: 1px solid #e7e7e7;
}

.lui_keydata_body_frame .nav_link .nav_link_frame {
	top: 0;
	bottom: 0;
	left: 12px;
	right: 12px;
}

.lui_keydata_body_frame .nav_link .left_row:before,
.lui_keydata_body_frame .nav_link .right_row:before {
	display: inline-block;
	content: "";
	width: 0;
	height: 0;
	border: 4px solid transparent;
	position: absolute;
	top: 14px;
}

.lui_keydata_body_frame .nav_link .right_row:before {
	border-left-color: #999;
	left: 3px;
}

.lui_keydata_body_frame .nav_link .left_row:before {
	border-right-color: #999;
	left: -2px;
}

.lui_keydata_body_frame .lui_zone_nav_bar_new li.line {
	display: none;
}


/* 切换选项卡 Ends */


/* 响应式 Starts */

.lui_keydata_info_list ul li {
	width: 50%;
	font-size: 14px;
}


/*屏幕宽度在1367和1793之间*/

@media screen and (min-device-width:1367px) and (max-device-width:1920px) {
	.lui_keydata_info_list ul li {
		width: 33.333%
	}
}


/*屏幕宽度大于在1921 */

@media screen and (min-device-width:1921px) {
	.lui_keydata_info_list ul li {
		width: 25%;
	}
}

/* 响应式 Ends */




/* 数据卡片-用于数据展示 Starts
   ========================================================================== */
.lui_keydata_card{
  padding: 20px 20px 20px 99px;
  display: block;
  border-radius: 4px;
  border: 1px solid #ddd;
  background-color: #fff;
  overflow: hidden;
  transition-duration: .3s;
  -webkit-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
     -moz-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
      -ms-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
       -o-box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
          box-shadow: 0 2px 8px -2px rgba(0, 0, 0, .125);
}
a.lui_keydata_card:hover{
  -wekbit-transform: translate3d(0, -2px, 0);
          transform: translate3d(0, -2px, 0);
}
a.lui_keydata_card:active{
  background-color: #f8f8f8;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
          box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
}
.lui_keydata_card:after,
.lui_keydata_card_list:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.lui_keydata_card_avatar{
  margin-left: -79px;
  width: 64px;
  height: 64px;
  border-radius: 50%;
  border: 1px solid #4285f4;
  background-color: #f8f8f8;
  display: block;
  overflow: hidden;
  float: left;
}
.lui_keydata_card_avatar > img{
  width: 100%;
  height: 100%;
  display: block;
}
.lui_keydata_card_tag{
  padding-right: 2px;
  padding-left: 8px;
  color: #fff;
  font-size: 12px;
  height: 18px;
  line-height: 18px;
  background-color: #ffb93e;
  max-width: 60px;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
  position: absolute;
  right: -20px;
  top: 0;
}
.lui_keydata_card_tag:before{
  content: '';
	border-width: 9px 5px;
	border-style: dashed dashed dashed solid;
	border-color: transparent transparent transparent #fff;
	position: absolute;
	left: 0;
	top: 0;
}
.lui_keydata_card_tag.tag-warning{ background-color: #ffb93e;}
.lui_keydata_card_tag.tag-danger{ background-color: #ff5154;}
.lui_keydata_card_tag.tag-success{ background-color: #22b07b;}
.lui_keydata_card_tag.tag-purple{ background-color: #c77cdb;}

.lui_keydata_card_nodata{
  margin: 0;
  padding: 10px 0;
  color: #666;
  font-size: 12px;
}

.lui_keydata_card_content{
  min-height: 105px;
  position: relative;
}
.lui_keydata_card_title{
  margin: 0;
  padding-bottom: 5px;
  padding-right: 30px;
  line-height: 20px;
  color: #333;
  font-size: 16px;
  font-weight: normal;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lui_keydata_card_list{
  margin: 0;
  padding: 0;
  list-style: none;
  display: table;
  border-collapse: collapse;
}
.lui_keydata_card_list > li{
  display: table-row;
  line-height: 1;
  color: #333;
  font-size: 0;
  *white-space: -1px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lui_keydata_card_list > li .title,
.lui_keydata_card_list > li .txt{
  display: table-cell;
  vertical-align: top;
  padding-top: 8px;
  font-size: 12px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lui_keydata_card_list > li .title{
  padding-right: 8px;
  max-width: 80px;
  color: #999;
}
.lui_keydata_card_list > li .txt{
  /* max-width: 100%; */
  max-width: 200px; /* 表格右列限宽缩略文字 */
}
.lui_keydata_card_list > li .icon-star{
  width: 12px;
  height: 12px;
  vertical-align: top;
}
.lui_keydata_card_footer{
  margin-top: 20px;
  margin-right: -20px;
  margin-bottom: -20px;
  margin-left: -99px;
  padding: 9px 10px;
  line-height: 20px;
  text-align: center;
  border-bottom-left-radius: 3px;
  border-bottom-right-radius: 3px;
  border-top: 1px solid #ddd;
  background-color: #fafafa;
}
.lui_keydata_card .card_btn_more{
  color: #666;
  font-size: 12px;
}
.lui_keydata_card .card_btn_more:hover{
  color: #4285f4;
}

/* 数据卡片-用于数据展示 Ends
   ========================================================================== */


/* 数据图表 Starts
   ========================================================================== */
.lui_keydata_panel_chart{
	margin-bottom: 10px;
	border: 1px solid #ddd;
	position: relative;
}
.lui_keydata_panel_chart_heading{}
.lui_keydata_panel_chart:after,
.lui_keydata_panel_chart_content:after,
.lui_keydata_chart_content:after{
	content: '';
	display: table;
	visibility: hidden;
	clear: both;
}
.lui_keydata_form_select{
	padding: 2px 10px;
	min-width: 100px;
	color: #333;
	font-size: 14px;
	border: 1px solid #ddd;
	border-radius: 3px;
	-webkit-appearance: none;
	position: absolute;
	right: 15px;
	top: 15px;
	z-index: 1;
}
.lui_keydata_panel_chart_body{
	padding-left: 215px;
	position: relative;
}
.lui_keydata_panel_chart_body .lui_keydata_chart_heading{
	float: left;
	margin-left: -215px;
}
.lui_keydata_chart_heading{
	width: 195px;
	padding: 35px 10px;
	position: relative;
}
.lui_keydata_borderL:after,
.lui_keydata_chart_heading:after{
	content: '';
	display: block;
	width: 1px;
	background-color: #fafafa\9;
	background-image:-webkit-linear-gradient(top, transparent 0%, #ddd 50%, transparent 100%);
	background-image:-moz-linear-gradient(top, transparent 0%, #ddd 50%, transparent 100%);
	background-image:-o-linear-gradient(top, transparent 0%, #ddd 50%, transparent 100%);
	background-image:-ms-linear-gradient(top, transparent 0%, #ddd 50%, transparent 100%);
	background-image:linear-gradient(top, transparent 0%, #ddd 50%, transparent 100%);
	position: absolute;
	top: 20px;
	bottom: 20px;
	right: 0;
}
.lui_keydata_chart_heading > .icnbox{
	margin: 0 auto;
	width: 120px;
	height: 120px;
	text-align: center;
	overflow: hidden;
}
.lui_keydata_chart_heading > .icnbox img{
	width: 100%;
	height: 100%;
}
.lui_keydata_chart_heading > .num,
.lui_keydata_chart_heading > .txt{
	line-height: 1;
	text-align: center;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.lui_keydata_chart_heading > .num{
	margin-top: 15px;
	margin-bottom: 8px;
	color: #4285f4;
	font-size: 40px;
}
.lui_keydata_chart_heading > .txt{
	color: #666;
	font-size: 14px;
}

.lui_keydata_chart_heading.countNumbox{
	padding-top: 70px;
	text-align: center;
}
.lui_keydata_chart_heading.countNumbox:after{
	display: none;
}
.lui_keydata_chart_heading.countNumbox > .countNum{
	vertical-align: baseline;
}
.lui_keydata_chart_heading.countNumbox > .countNum em{
	margin-right: 3px;
	font-size: 42px;
	font-style: normal;

}

.lui_keydata_chart_heading.countNumbox > .txt{
	padding: 0 15px;
	height: 26px;
	line-height: 26px;
	border-radius: 20px;
	background-color: #d9e7fd;
	display: inline-block;
}




.lui_keydata_chart_content{}
.lui_keydata_panel_chart .col-row{
	width: 100%;
	min-height: 1px;
	float: left;
}
.lui_keydata_panel_chart .col-half{
	width: 50%;
	min-height: 1px;
	float: left;
}


.lui_keydata_chart_pane{
	padding: 10px;
	background-color: #fff;
}
.lui_keydata_chart_pane img{
	max-width: 100%; 
	max-height: 100%; 
}

/* 数据图表 Ends
   ========================================================================== */
