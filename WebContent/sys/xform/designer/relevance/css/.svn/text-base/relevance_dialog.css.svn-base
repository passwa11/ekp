.txtlistpath{ display:block; background:repeat-x left bottom; height:20px; padding:0 0 5px 0; color:#2792C6; line-height:20px; margin:10px 0 0 0; text-align:left;}
.relevance_dialog_selectbox_ul{
 	position: relative;
    margin: 0 10px;
    zoom:1;
    list-style:none;
}
.relevance_dialog_selectbox_ul > li{
	padding:13px 0;
    width: 33%;
    float: left;
    cursor: pointer!important;
    position: relative;
}
.relevance_dialog_selectbox_ul > li > span{
	width:80%;
	padding-left:7px;
	overflow: hidden; /*自动隐藏文字*/
	text-overflow: ellipsis;/*文字隐藏后添加省略号*/
	white-space: nowrap;/*强制不换行*/
	float: left;
}
.relevance_dialog_wrap{
	padding-left:4px;
}

.relevance_dialog_selectedbox_del{
	position: absolute;
	width: auto;
	cursor: pointer;
	color: #37ace1;
	text-decoration: underline;
	float: left;
	display:inline;
	padding-left:8px;
}
.relevance_main_buttons_container{
	text-align:center;
	border-top:solid 1px #797874;
	position: fixed;
	left:0;
	right:0;
	bottom:0;
	padding:10px 0px;
	background-color:#fff;
}
.relevance_dialog_loading{
	position:absolute;
	left:420px;
	top:200px;
	height:50px;
	width:50px;
	background:url(../icon/loading.gif) no-repeat center;
}
.relevance_dialog_moduleSelect{
	position:absolute;
	right:8px;
	top:3px;
	padding-left:24px;
	border:1px solid #b4b4b4;
	border-radius:5px;
}
.relevance_dialog_moduleSelect .relevance_dislog_moduleSelect_input{
	width:270px;
	height:24px;
	padding-left:5px;
	padding-right:24px;
	color:#4285f4;
	border:0px;
	border-radius:5px;
}
.relevance_dialog_moduleSelect .relevance_dislog_moduleSelect_delWord{
	background:url(../icon/relevance_del_gray.png) no-repeat;
	width:15px;
	height:15px;
	cursor:pointer;
	position:absolute;
	right:8px;
	top:4px;
}
.relevance_dialog_moduleSelect .relevance_dislog_moduleSelect_delWord:hover{
	background-image:url(../icon/relevance_del.png) !important;
}

.relevance_dialog_moduleSelect .relevance_dislog_moduleSelect_select{
	border: 0;
	cursor: pointer;
    padding:0;
	width: 24px;
	height: 24px;
	background: transparent url(../icon/search_btn.png) no-repeat center;
	position: absolute;
	left: 0px;
	top: 0px;
    z-index:1;
}
.relevance_dialog_moduleSelect .relevance_dislog_moduleSelect_select:hover{
	background:transparent url(../icon/search_btn_on.png) no-repeat center;
}

.relevance_dialog_selectbox_deleteAll{
	display:inline-block;
	float:right;
	cursor: pointer;
	color: #ff7200;
	padding-right:25px;
}

.relevance_search_select{
	position: absolute;
    top: 3px;
    right: 350px;
    height: 26px;
    border-radius: 4px;
    border: 1px solid #b4b4b4;
}

.relevance_startTime_Field{
	position:absolute;
	top:3px;
	right:10px;
	height:26px;
}