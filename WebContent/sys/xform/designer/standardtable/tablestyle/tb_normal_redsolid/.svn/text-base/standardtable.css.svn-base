/*表格所有框线红色，字段红色实线*/
.tb_normal_redsolid { width:100%;text-align: left; padding: 8px !important; margin: 0px auto; border-collapse: collapse; background-color: rgb(255, 255, 255); }
.tb_normal_redsolid   tr  td { padding: 8px; border: 1px solid #f00 !important;word-break: break-word; }
.tb_normal_redsolid   tr  .td_normal_title {}
.tb_normal_redsolid .inputsgl {
	height: auto;
	border-bottom: 1px solid red !important;
	/**钉钉高级审批版，控件四边框 **/
	border-color: red !important;
}

/**钉钉高级审批版，控件四边框 **/
.tb_normal_redsolid  * :not(span){
  border-color: red !important;
}

/*动态下拉框 xform_relation_select 外层已设置border,内层不需要设置border-bottom*/
.tb_normal_redsolid .xform_relation_select .inputsgl {
	border-bottom:none !important;
}

/*地址本 组织架构元素不需要设置红色边框*/
.tb_normal_redsolid .xform_address .mf_container .mf_list .mf_item {
	border-color: transparent !important;
}

/*高级地址本 组织架构元素不需要设置红色边框*/
.tb_normal_redsolid .xform_new_address .mf_container .mf_list .mf_item{
	border-color: transparent !important;
}

.tb_normal_redsolid .cke_toolbox_collapser{
  border-color: #bcbcbc !important;
}

.tb_normal_redsolid .inputselectsgl {
	height: 20px;
	line-height: 20px;
	border-bottom-color: red;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	display: inline-block;
	position: relative;
	cursor: pointer;
}
.tb_normal_redsolid   tr  td select{border: 1px solid red;}
.tb_normal_redsolid   tr  td textarea{border: 1px solid red !important;}
/*明细表所以框线红色*/
.tb_normal_redsolid   tr  td .tb_normal tr td{border: 1px solid red !important;}
/*还原被覆盖的无边框样式,防止单选按钮和多选按钮出现边框*/
.tb_normal_redsolid .tb_noborder{background-color:#FFFFFF !important;margin:0px !important;border-collapse:collapse !important;padding:0px !important;border:0px !important;}
.tb_normal_redsolid .tb_noborder tr td {padding:0px !important;border:0px !important;}
.tb_normal_redsolid .tb_noborder tr   {padding:0px !important;border:0px !important;}
.tb_normal_redsolid .validation-table td{border:0px solid red !important;}

.tb_normal_redsolid .freeze,.tb_normal_redsolid  .freeze_left_col,.tb_normal_redsolid  .freeze_right_col,.tb_normal_redsolid  .freeze_first_row,.tb_normal_redsolid  .freeze_last_row {
    outline: 1px solid red !important;
}