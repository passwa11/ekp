/*表格所有框线黑色，字段默认实线*/
.tb_normal_blacksolid { width:100%;text-align: left; padding: 8px !important; margin: 0px auto; border-collapse: collapse; background-color: rgb(255, 255, 255); }
.tb_normal_blacksolid  tr  td { border: 1px solid #000 !important; padding: 8px; word-break: break-word; border-left: 1px solid #000; border-right: 1px solid #000;}
.tb_normal_blacksolid   tr  .td_normal_title {}
.tb_normal_blacksolid .inputsgl, .tb_normal_blacksolid .inputselectsgl {
	height: auto;
  	min-height: 20px;
	border: 0;
	border-bottom: 1px solid #959595;
	border-color: #959595 !important;
}
.tb_normal_blacksolid *:not(.select_div_box .fs-arrow) {
	/* #137194-16主表样式选择“样式8”，PC、移动端都是灰色，不是黑色，15正常 */
	/*border-color: #000 !important;*/
}

.xform_address .mf_container ol.mf_list {
    top: 0px;
}

.tb_normal_blacksolid   tr  td select{border: 1px solid #959595;}
.tb_normal_blacksolid   tr  td textarea{border: 1px solid #959595 !important;}
/*明细表所有框线黑色*/
.tb_normal_blacksolid   tr  td .tb_normal tr td{border: 1px solid #000;}
/*还原被覆盖的无边框样式,防止单选按钮和多选按钮出现边框*/
.tb_noborder{background-color:#FFFFFF !important;margin:0px !important;border-collapse:collapse !important;padding:0px !important;border:0px !important;}
.tb_noborder tr td {padding:0px !important;border:0px !important;}
.tb_noborder tr   {padding:0px !important;border:0px !important;}
.validation-table td{border:0px solid red !important;}