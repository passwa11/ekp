.tb_normal_solidborder_radius_bgcolor,.tb_normal_solidborder_radius_bgcolor table {
	width:100%; 
	text-align: left; 
	margin: 15px auto!important; 
	border: 1px solid #D4D6DB; 
	border-collapse: separate; 
	border-spacing: 0; 
	background-color: rgb(255, 255, 255); 
	border-radius: 4px; 
}
table.tb_normal_solidborder_radius_bgcolor>tbody>tr>td, table.tb_normal_solidborder_radius_bgcolor table>tbody>tr>td{ 
	padding: 8px!important; 
	border-right: 1px solid #D4D6DB!important;
	border-bottom: 1px solid #D4D6DB!important;
	border-left: 0px solid #D4D6DB!important;
    border-top: 0px solid #D4D6DB!important;
	word-break: break-all; 
}
table.tb_normal_solidborder_radius_bgcolor table.muiDetailTable>tbody>tr>td.muiDetailTableNo {
	padding: 0px!important; 
}
.tb_normal_solidborder_radius_bgcolor tr td.td_normal_title,.tb_normal_solidborder_radius_bgcolor tr td.muiTitle{
	background-color: #F5F6FB!important;
}
.tb_normal_solidborder_radius_bgcolor table tr td.td_normal_title,.tb_normal_solidborder_radius_bgcolor table tr td.muiTitle{
	background-color: #F5F6FB!important;
}
.tb_normal_solidborder_radius_bgcolor tr:first-child td:first-child,.tb_normal_solidborder_radius_bgcolor table tr:first-child td:first-child{
	border-top-left-radius: 4px;
}
.tb_normal_solidborder_radius_bgcolor tr:last-child td:first-child,.tb_normal_solidborder_radius_bgcolor table tr:last-child td:first-child{
	border-bottom-left-radius: 4px;
}
.tb_normal_solidborder_radius_bgcolor tr td:last-child,.tb_normal_solidborder_radius_bgcolor table tr td:last-child { 
	border-right: 0!important;
}
.tb_normal_solidborder_radius_bgcolor tr:last-child td,.tb_normal_solidborder_radius_bgcolor table tr:last-child td { 
	border-bottom: 0!important;
}
.tb_normal_solidborder_radius_bgcolor .inputsgl, .tb_normal_solidborder_radius_bgcolor table .inputsgl {
	height: auto;
	border-bottom: 1px solid #D4D6DB !important;
	/**钉钉高级审批版，控件四边框 **/
	border-color: #D4D6DB !important;
}

.tb_normal_solidborder_radius_bgcolor > * {
	border-color: #D4D6DB !important;
}

.tb_normal_solidborder_radius_bgcolor tr td .tb_normal tr td,.tb_normal_solidborder_radius_bgcolor table tr td .tb_normal tr td{
	border: 1px solid #D4D6DB !important;
}
