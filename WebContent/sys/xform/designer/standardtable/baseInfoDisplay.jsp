<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ include file="/sys/ui/jsp/common.jsp" %>
<div ondblclick="Designer.instance.bindEvent(event, 'builder', 'dblclick');" class="muiXformBaseInfo">
	<table style="width:100%" id="baseInfoTable" class="muiSimple" cellpadding="0" cellspacing="0">
	    <%-- 主题  --%>
		<tr>
			<td class="muiTitle" column="0" row="0" style="padding-left: 1.2rem;width:30%"><bean:message key="sysForm.baseInfo.style.subject" bundle="sys-xform-base"/></td>
			<td class="muiContent" column="1" row="0"></td>
		</tr>
		<%-- 模板名称  --%>
		<tr>
			<td class="muiTitle" column="0" row="1" style="padding-left: 1.2rem;width:30%"><bean:message key="sysForm.baseInfo.style.templateName" bundle="sys-xform-base"/></td>
			<td class="muiContent" column="1" row="1"></td>
		</tr>
		<%-- 申请人  --%>
		<tr>
			<td class="muiTitle" column="0" row="2" style="padding-left: 1.2rem;width:30%"><bean:message key="sysForm.baseInfo.style.creator" bundle="sys-xform-base"/></td>
			<td class="muiContent" column="1" row="2"></td>
		</tr>
	</table>
</div>
<div class="muiXformSplit"></div>
