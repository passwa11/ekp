/** 通用模板**/

body {
	overflow-x: hidden;
}

.cm-common-template {
  width: 920px;
}

.cm-common-template .common-content {
  padding-left: 10px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

.cm-common-template .common-content .cards {
  float: left;
  margin: 10px;
  width: 280px;
  height: 92px;
  padding: 27px 10px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
          box-shadow: 0 0 20px 0 rgba(153, 179, 210, 0.16);
  border-radius: 4px;
}

.cm-common-template .common-content .cards .left {
  width: 38px;
  margin-right: 10px;
  vertical-align: bottom;
  background: url("../img/icon_biaodan@2x.png");
  background-size: 38px 38px;
  height: 38px;
  display: inline-block;
  background: url("../img/icon_biaodan.png")\9;
}

.cm-common-template .common-content .cards .center {
  display: inline-block;
}

.cm-common-template .common-content .cards .center p {
  font-size: 14px;
  color: #333333;
  margin-bottom: 4px;
  max-width: 140px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: top;
}

.cm-common-template .common-content .cards .center a {
  font-size: 12px;
  color: #4285F4;
}

.cm-common-template .common-content .cards .center a::after {
  content: "";
  display: inline-block;
  width: 10px;
  height: 10px;
  background: url("../img/chakan.png");
  background-size: 10px 10px;
}

.cm-common-template .common-content .cards .right {
  float: right;
  margin-top: 7px;
  width: 62px;
  height: 21.6px;
  line-height: 21.6px;
  background: #4285F4;
  border: 1px solid #4285F4;
  border-radius: 2px;
  color: #fff;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
  -webkit-transition: all .3s;
  transition: all .3s;
  font-size: 12px;
}

/* ------------ 02-引用通用模板 Ends ------------ */