.muiFormRelevanceBtnPlus {
	border: 1px solid #4285F4;
    color: #4285F4;
    border-radius: 2rem;
    display: inline-block;
    padding: .4rem .4rem .3rem .4rem;
    font-size: var(--muiFontSizeXS);
    margin-top: .4rem;
    float: right;
    line-height: 1.2rem;
    margin-right: 1.5rem;
}
.muiFormRelevanceBtnPlus.BtnPlusVertical{
	float: left;
}
.muiFormRelevanceBtnText{
	padding: 0 .4rem;
}
.muiFormRelevanceHideBox{
	height: 12rem;
    width: 100%;
    background-image: linear-gradient(-180deg,rgba(255,255,255,0) 0%,#fff 80%);
    background-image: -webkit-gradient(linear,left top, left bottom,from(rgba(255,255,255,0)),color-stop(80%, #fff));
    margin-top: -12rem;
    position: relative;
    padding-top: 0px;
}
.hiden{
	height: 12rem;
	overflow: hidden;
}
.muiFormRelevanceListBox{
	flex-shrink: 1;
	width: 100%
}
.muiFormRelevanceBtnHide{
	color: #D8D8D8;
    width: 100%;
    height: 2.5rem;
    text-align: center;
    text-align: -webkit-center;
    padding-top: .5rem;
}
.muiForemRelevanceHidArticle {
    height: 12rem;
    overflow: hidden;
}
.muiFormRelevanceDocLi{
	list-style-type: none;
	padding:4px 0px;
	font-size: 1.5rem;
	color: #4285F4;
}
.muiFormRelevanceDocLi .muiFormRelevanceDocLink{
	font-size: var(--muiFontSizeM);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    width: calc(100% - 2.6rem);
    text-align: left;
}
.muiFormRelevanceDocLi .muiFormRelevanceDocDelIcon{
	width: calc(100% - 5.6rem);
}
.muiFormRelevanceBtnIcon{
    font-size: var(--muiFontSizeXS);
    transform: scale(.7,.7);
    transform-origin: .7rem .2rem;
}