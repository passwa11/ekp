.newAuditNote .handingWay{
	vertical-align: middle;
	white-space: nowrap;
	overflow: hidden;
	text-overflow:ellipsis;
	position: absolute;
	right: 0.5rem;
	display: block;
	margin: 0;
	top:50%;
	margin-top:-19px;
	font-size: 2rem;
  -webkit-flex: 1;        /* Chrome */ 
					flex: 1;        /* NEW, Spec - Opera 12.1, Firefox 20+ */
  -webkit-box-flex: 1;    /* OLD - iOS 6-, Safari 3.1-6 */  
  z-index : 10000;
}

.newAuditNote .handingWayWrap .handingWay{
	margin-top:0px;
}

.newAuditNote .handingWayWrap{
	width: 100%;
}

.newAuditNote .handingWay .iconTitle {
	text-align: center;
	font-size: 12px;
	display: block;
	color: #666;;
}

.newAuditNote .handingWay .iconArea{
	width:90px;
	height:24px;
	border: 1px solid #0D93D6;
	border-radius: 100px;
	font-size:1.5rem;
	text-align:center;
	color:#37aee9;
	margin-left:1rem;
}

.newAuditNote li{
	margin: 0;
	padding: 0 10px;
	list-style: none;
	display: inline-block;
	margin: 0.3rem 1rem;
	padding: 0.3rem 0;
	vertical-align: top;
	list-style: none;
} 

.newAuditNote .img_wrapper{
	position: relative;
	width: 7.7rem;
	height: auto;
	background-color: #EEEEEE;
	border-radius: 5px;
	box-shadow: none;
}

.newAuditNote .img_wrapper .img_content{
	width: 7.7rem;
	height: auto;
	background: none;
	overflow: hidden;
}
.newAuditNote .img_wrapper .img_content img{ 
	width: 100%;
}

.newAuditNote .img_wrapper .btn_canncel_img{
	position: absolute;
	top: -0.5rem;
	right: -0.6rem;
	width: 2rem;
	height: 2rem;
	line-height: 2rem;
	vertical-align: middle;
	z-index: 2;
	border-radius: 50%;
	text-align: center;
	background-color: #FC573B;
	background-size: 16px;
}

div .newAuditNote{
	width:100%;
	display:none;
	position:relative;
	vertical-align:middle;
}