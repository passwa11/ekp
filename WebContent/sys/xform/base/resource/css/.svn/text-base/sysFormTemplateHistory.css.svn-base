
.sys_form_template_history_mouseover{
	background-color: #F0F0F0;
	cursor:pointer;
}

.sys_xform_history_moduleSelect{
	display:inline-block;
	position:absolute;
	line-height:25px;
	height:25px;
	right:3%;
	padding-right:24px;
	border:1px solid #b4b4b4;
	border-radius:5px;
	top:3px;
}
.sys_xform_history_moduleSelect .sys_xform_moduleSelect_input{
	width:300px;
	height:24px;
	/* padding-top:3px; #134333 修复样式问题所以删除*/
	padding-left:5px;
	padding-right:24px;
	color:#b4b4b4;
	border:0px;
	border-radius:5px;
}
.sys_xform_history_moduleSelect .sys_xform_moduleSelect_delWord{
	background:url(../icon/relevance_del_gray.png) no-repeat;
	width:15px;
	height:15px;
	cursor:pointer;
	position:absolute;
	right:8px;
	top:4px;
}
.sys_xform_history_moduleSelect .sys_xform_moduleSelect_delWord:hover{
	background-image:url(../icon/relevance_del.png) !important;
}

.sys_xform_history_moduleSelect .sys_xform_moduleSelect_select{
	border: 0;
	cursor: pointer;
    padding:0;
	width: 24px;
	height: 24px;
	background: transparent url(../icon/search_btn.png) no-repeat center;
	position: absolute;
	right: 0px;
	top: 0px;
    z-index:1;
}
.sys_xform_history_moduleSelect .sys_xform_moduleSelect_select:hover{
	background:transparent url(../icon/search_btn_on.png) no-repeat center;
}
.sys_xform_history_pageUl{
	list-style:none;
	float:right;
	width:100px;
	margin:10px 0px;
}

.sys_xform_history_pageUl li{
	display:none;
	cursor:pointer;
	float:right;
	padding:0 5px;
}

.sys_xform_history_placeholder{position:absolute;top:0;left:0;color:#999;z-index:1;}
