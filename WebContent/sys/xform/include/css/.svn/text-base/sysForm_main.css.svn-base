  background-color: #fff;
  -webkit-box-shadow: 0 0 12px rgba(0, 0, 0, .175);
          box-shadow: 0 0 12px rgba(0, 0, 0, .175);
  position: absolute;
  bottom:90px;
  left: 50%;
  margin-left: -121.5px;
  /* margin-bottom : -3px; */
  text-align: left;
}

