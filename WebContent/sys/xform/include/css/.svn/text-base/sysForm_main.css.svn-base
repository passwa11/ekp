/* 圆环流程图表 Starts
   ========================================================================== */
.lui-flow-rotundity{
  padding: 20px 10px 0;
  line-height: 1.42857143;
  display: inline-block;
  position: relative;
}
.lui-flow-rotundity-list{
  margin: 0;
  padding: 0;
  list-style: none;
  max-height: 90px;
}
.lui-flow-rotundity-list:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.lui-flow-rotundity-list > li{
  padding-bottom: 20px;
  width: 125px;
  /* min-height: 81px; */
  min-height:69px;
  text-align: center;
  float: left;
  position: relative;
  max-height: 90px;
}
.lui-flow-rotundity-list > li > *{
  position: relative;
  z-index: 1;
}
.lui-flow-rotundity-list > li:before{
  content: '';
  display: block;
  border-top: 2px solid #797979;
  position: absolute;
  left: 0;
  right: 0;
  /* top: 16px; */
  top: 10px;
  z-index: 0;
}
.lui-flow-rotundity-list > li:first-child:before,
.lui-flow-rotundity-list > li.first-child:before{
  left: 50%;
}
.lui-flow-rotundity-list > li:last-child:before,
.lui-flow-rotundity-list > li.last-child:before{
  right: 50%;
}
.lui-flow-rotundity-list > li.last-child:after{
  content: '';
  display: block;
  border-left: 2px solid #797979;
  position: absolute;
  left: 50%;
 /*  top: 34px; */
  top: 18px;
  bottom: 0;
  margin-left: -1px;
}
.lui-flow-rotundity-list:nth-child(even) > li{
  float: right;
}
.lui-flow-rotundity-list:nth-child(even) > li:first-child:before,
.lui-flow-rotundity-list:nth-child(even) > li.first-child:before{
  left: 0;
  right: 50%!important;
}
.lui-flow-rotundity-list:nth-child(even) > li:last-child:before,
.lui-flow-rotundity-list:nth-child(even) > li.last-child:before{
  left: 50%;
  right: 0;
}
.lui-flow-rotundity-list:last-child > li:last-child:after,
.lui-flow-rotundity-list:last-child > li.last-child:after{
  display: none;
}

.lui-flow-rotundity-list .spot{
  margin-left: auto;
  margin-right: auto;
  /* width: 30px;
  height: 30px; */
  width: 18px;
  height: 18px;
  border-radius: 50%;
  /* border: 2px solid #fff; */
  background-color: #e4e4e4;
  box-shadow: 2px 3px 12px rgba(0, 0, 0, .35);
  display: block;
  background: url(../../designer/stage_diagram/css/noStart.png) no-repeat;
}
.lui-flow-rotundity-list .spot.spot-blue{
  background-color: #199ed7;
}

.lui-flow-rotundity-list .spot.spot-green{
  background-color: #6aca25;
}

.lui-flow-rotundity-list .spot.spot-orange{
  background-color: #fd9827;
}

.lui-flow-rotundity-list .spot.spot-start{
  background: url(../../designer/stage_diagram/css/start.png) no-repeat;
}

.lui-flow-rotundity-list .spot.spot-finish{
  background: url(../../designer/stage_diagram/css/finish.png) no-repeat;
}

.lui-flow-rotundity-list .spot.spot-current{
  background: url(../../designer/stage_diagram/css/current.png) no-repeat;
}

.lui-flow-rotundity-list .spot.spot-end{
  background: url(../../designer/stage_diagram/css/end.png) no-repeat;
}

.lui-flow-rotundity-list .title,
.lui-flow-rotundity-list .subhead{
  margin: 0;
  text-align: center;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lui-flow-rotundity-list .title{
  margin-top: 10px;
  color: #333;
  font-size: 14px;
}
.lui-flow-rotundity-list .subhead{
  color: #666;
  font-size: 12px;
}

/* 提示框 Starts
