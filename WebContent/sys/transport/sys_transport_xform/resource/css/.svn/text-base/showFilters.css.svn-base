 .displaycss_create_pop {
    position:absolute;
    right: 20px;
    top: 30px;
    background: #FFFFFF;
    box-shadow: 0 1px 3px 0 rgba(0,0,0,0.15);
    border-radius: 0 0 2px 2px;
    z-index: 1;
    width: 124px;
 	height: 0px;
 	border: 0;
    transition: all 0.3s ease;
    overflow: hidden;
 }

 .displaycss_create_pop:hover {
    height: 270px;
 	border: 1px solid #ddd;
 }
 .displaycss_create_pop ul {
 	list-style: none;
 	width: 124px;
 	height: 270px;
 	overflow: auto;
}

.displaycss_create_pop ul li {
	width: 100%;
	font-size: 12px;
    color: #666;
	line-height: 30px;
	padding: 0 8px;
	box-sizing: border-box;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.displaycss_create_pop ul li.active {
	color: #4285f4;
	background: rgba(66,133,244,0.10);
}


