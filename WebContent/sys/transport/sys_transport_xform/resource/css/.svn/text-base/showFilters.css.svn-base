.font-position {
    margin: 20px 0 16px;
    display: inline-block;
    position: relative;
}

.font-position-hover {
	display: none;
	white-space: nowrap;
	position: absolute;
	top: -40px;
    left: -25px;
	font-size: 14px;
    color: #FFFFFF;
	padding: 5px 8px;
	background: rgba(0,0,0,0.75);
	border-radius: 2px;
    box-shadow: 0 3px 6px -4px rgba(0,0,0,0.12), 0 6px 16px 0 rgba(0,0,0,0.08), 0 9px 28px 8px rgba(0,0,0,0.05);
}

.font-position-hover:after {
	content: '';
	position: absolute;
	border-style: solid;
	border-color: rgba(0,0,0,0.75) transparent transparent transparent;
	border-width: 6px;
	bottom: -12px;
    left: 45%;
}

.font-position:hover .font-position-hover {
	display: block;
}

.font-position:after {
	content: '';
	display:inline-block;
	width: 14px;
	height: 14px;
	 background: url(../images/font-tips@2x.png) center center no-repeat;
    background: url(../images/drag.png) no-repeat \9;
    background-size: 14px;
        margin-bottom: -2px;
        cursor: pointer;
}

