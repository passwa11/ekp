}

 .drag:before {
	content: '';
    position: absolute;
    width: 20px;
    height: 20px;
    left: 0px;
    top: 10px;
    background: url(../images/drag@2x.png) no-repeat;
    background: url(../images/drag.png) no-repeat \9;
    background-size: 20px;
}

.expandItem, .muiItem {
	text-align: center;
}

.isExpend {
	width: 14px;
	height: 14px;
	border: 1px solid #DDDDDD;
    border-radius: 2px;
}

.isMuti {
	width: 14px;
	height: 14px;
	border-radius: 2px;
} 

.defaultValue, .defaultValueText {
	width: 100%;
	height: 30px;
	line-height: 30px;
	background: #FFFFFF;
    border: 1px solid #DDDDDD;
