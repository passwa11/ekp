	line-height: 40px;
	position: relative;
	overflow:visible;
}

.lui_custom_list_box_content_col_content > .lui_custom_list_box_content_col_content_line .drag_item {
	cursor: move;
}

.lui_custom_list_box_content_col_content > .lui_custom_list_box_content_col_content_line .drag_item span{
    display: inline-block;
    width: 100%;
