 ul li {
     list-style-type: none;
 }

 .select_label {
     color: #982F4D;
     line-height: 24px;
     padding-right: 10px;
     font-size: 12px;
     font-weight: 700;
 }

 .s_select_box,
 .m_select_box {
     color: #444;
     position: relative;
     cursor: pointer;
     width: 100%;
     font-size: 12px;
 }

 .s_select_txt,
 .m_select_txt {
     padding-left: 10px;
     padding-right: 30px;
     box-sizing: border-box;
     width: 100%;
     line-height: 30px;
     height: 30px;
     cursor: text;
     overflow: hidden;
 }
 
    .s_select_txt:empty:before{
        content: '请选择'; 
        color: #999;
        font-size: 12px;
    }
    .s_select_txt:focus:before{
        content: "";
    }
 
 .m_select_txt .m_option_list_item {
     border: 1px solid #ddd;
     border-radius: 2px;
     margin-right: 6px;
     padding-left: 8px;
     padding-right: 25px;
     position: relative;
     cursor: pointer;
 }
 
 .m_select_txt .m_option_list_item:after {
    content: '×';
    font-size: 16px;
    line-height: 27px;
    margin-left: 5px;
    position: absolute;
 }

 .s_option_list,
 .m_option_list {
     width: 100%;
     border: solid 1px #EDE7D6;
     position: absolute;
     top: 30px;
     left: -1px;
     z-index: 2;
     overflow: hidden;
     display: none;
 }

 .s_option_list li,
 .m_option_list li {
     display: block;
     height: 26px;
     line-height: 26px;
     text-align: left;
     padding: 0 10px;
     box-sizing: border-box;
     width: 100%;
     background: #fff;
     position: relative;
 }

 .s_option_list li:hover,
 .m_option_list li:hover {
     background: #ECF2FD;
     color: #4285f4;
 }

 .s_option_list li.active,
 .m_option_list li.active {
     background: #ECF2FD;
 }

 .s_option_list li.active:after,
 .m_option_list li.active:after {
     content: '';
     position: absolute;
     width: 12px;
     height: 12px;
     right: 10px;
     top: 50%;
     transform: translate(0, -50%);
     background: url(../images/selected@2x.png) no-repeat;
     background: url(../images/selected.png) no-repeat \9;
     background-size: 12px;
 }
 
