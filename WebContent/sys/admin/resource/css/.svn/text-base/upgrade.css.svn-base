    /* 清除浮动 */
.lui-upgrade-btn-group > li{ zoom:1;}
.lui-upgrade-btn-group > li:after{
    display:block;
    content:'clear';
    clear:both;
    line-height:0;
    visibility:hidden;
}


.lui-upgrade-body {
    background-color:#f0f0f0;
}

.lui-upgrade-wizard {
    margin:10px 0 0;
    padding-left:266px;
    height:520px;
    background:url(../images/wizard/upgradeW_bg.png) no-repeat 0 0;
    position:relative;
}
.lui-upgrade-number {
    color:#2c93da;
    font-size:40px;
    font-family:Arial;
    position:absolute;
    left: 30px;
    top: 170px;
}
.lui-upgrade-number span{
    margin-right: 10px;
    font-size:100px;
}
.lui-upgrade-btn-group {}
.lui-upgrade-btn-group > li {
  padding-top: 29px;
  padding-left: 70px;
  height: 108px;
}
.lui-upgrade-btn-group > li a {
  padding-left: 48px;
  color: #fff;
  font-size: 16px;
  background-repeat: no-repeat;
  background-position: 0 0;
  display: block;
  cursor: pointer;
}
.lui-upgrade-btn-group > li a .btn-R {
