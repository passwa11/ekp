.muiPortalMenuSlide {
  overflow-y: hidden;
  position: relative;
}
.mulPortalSlide {
  width: 100%;
}
.muiPortalMenuSlide .muiPortalMenuSlideSwitch {
  right: 0;
  text-align: center;
  padding: .3rem 1rem 1rem;
  width: 100%;
  /*height: 3.5rem;
  line-height: 3.5rem;*/
  color: #fff;
  font-size: 0;
  position: absolute;
  bottom: 0;
  z-index: 2;
  text-overflow: ellipsis;
}
.muiPortalMenuSlideSwitch .muiPortalMenuSlideSwitchSpan {
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background-color: #b0b0b0;
  display: inline-block;
  margin-right: .5rem;
}
.muiPortalMenuSlideSwitch .muiPortalMenuSlideSwitchSpan.on {
  background-color: #25a8e8;
}
.muiPortalMenuSlideItem {
  display: inline-block;
  width: 7rem;
  text-align: center;
}

.muiPortalMenuSlide .MenuModuleIndex0 .muiPortalMenuModule{
	background-color: #5a9cff;
}
.muiPortalMenuSlide .MenuModuleIndex1 .muiPortalMenuModule{
	background-color: #5cd66e;
}
.muiPortalMenuSlide .MenuModuleIndex2 .muiPortalMenuModule{
	background-color: #ffc526;
}
.muiPortalMenuSlide .MenuModuleIndex3 .muiPortalMenuModule{
	background-color: #fe7373;
}
.muiPortalMenuSlide .MenuModuleIndex4 .muiPortalMenuModule{
	background-color: #b39af1;
}
.muiPortalMenuSlide .MenuModuleIndex5 .muiPortalMenuModule{
	background-color: #ff9ad1;
}
.muiPortalMenuSlide .MenuModuleIndex6 .muiPortalMenuModule{
	background-color: #ff8347;
}
.muiPortalMenuSlide .MenuModuleIndex7 .muiPortalMenuModule{
	background-color: #63d3bd;
}


.muiPortalMenuSlideItem .muiPortalMenuModule .mui {
  color: #fff;
}
.mulPortalSlideInner {
  height: 9rem;
}
.muiPortalMenuSlide .muiPortalMenuModule {
  margin: 0 auto;
  width: 42px;
  height: 42px;
  line-height: 42px;
  text-align: center;
  color: #fff;
  font-size: 2.8rem;
  border-radius: 50%;
  background-color: #f8f8f8;
  display: block;
  position: relative;
  font-size: 1.6rem;
}

/* 移动端优化 */
.mui_ekp_portal_item{

}
.mui_ekp_portal_item::-webkit-scrollbar {
	display: none;
}
.mui_ekp_portal_item ul::-webkit-scrollbar {
	display: none;
}
.mui_ekp_portal_item ul>li>a {
	display: inline-block;
}
.mui_ekp_portal_item ul{
	padding: 0 1.25rem;	
}
.mui_ekp_portal_item ul>li{
	width: 20%;
	float: left;
	text-align: center;
	height: 10rem;	
}
.mui_ekp_portal_item ul>li>a>div{
	display: block;
	margin:0 auto;
	width: 5.2rem;
	height: 5.2rem;
	margin-bottom:0.4rem;
}
.mui_ekp_portal_item ul>li>a>span {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
}

.muiPortalClickItem .imgBox {
	line-height: 1.4;
