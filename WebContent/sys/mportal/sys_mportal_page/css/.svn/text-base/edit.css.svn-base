.fdLogoImg {
	display: inline-block;
	max-width: 800px;
}

.fdLogoImg img {
	height: 30px;
}

.fdLogoImg .fdLogoImgBg {
	background-color: #1d9d74;
	text-align: center;
	padding: 5px;
}

@font-face {
	font-family: 'FontMui';
	src:
		url("<%=request.getContextPath()%>/sys/mobile/css/font/fontmui.woff");
}

.mui {
	background-color: #1d9d74;
	color: #fff!important;
	width: 36px;
	height: 36px;
	display: inline-block;
	vertical-align: middle;
	line-height: 36px!important;
	text-align: center;
	font-size: 20px!important;
	cursor: pointer;
}