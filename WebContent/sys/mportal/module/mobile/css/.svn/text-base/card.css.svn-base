.muiModuleCard {
	margin-bottom: 2.25rem;
}

.muiModuleCardTitle {
	padding: 1.25rem;
	font-size: var(--muiFontSizeLA);
	font-weight: bold;
	background-color: #fff;
}

.muiModuleCardMore {
	float: right;
	color: var(--muiFontColorMuted);
	font-size: var(--muiFontSizeS);
	font-weight: 400;
	line-height: 2;
}

.muiModuleCardMoreArrow {
	margin-left: .6rem;	
}
.muiModuleCardContent{
	margin: 0  1.25rem;
	background: #FFFFFF;
	box-shadow: 0 0 30px 0 rgba(153,179,210,0.20);
	border-radius: 10px;
}

.muiModuleTabsContainer {
	float: right;
	line-height: 2rem;
	font-weight: 600;
}

.muiModuleTabsContainer div{
	float: left;
	margin-left: 1rem;
	color: var(--muiFontColorMuted);
}

.muiModuleTabsContainer div.selected {
	color: var(--muiFontColor);
}

.muiModuleTabsMoreContainer {
	height: 5rem;
}

.muiModuleTabsMore {
    width: 10rem;
    height: 3rem;
    line-height: 3.2rem;
    background-color: #f7f7f8;
    border-radius: 40px;
    text-align: center;
    font-size: var(--muiFontSizeSS);
    margin-right: 1rem;
    margin-top: 1rem;
    color: var(--muiFontColorMuted);
    float: right;
}

.muiModuleTabsMore i{
	display: inline-block;
    width: 1.2rem;
    height: 1.2rem;
    background-size: 1.2rem;
    position: relative;
    right: 6px;
}
