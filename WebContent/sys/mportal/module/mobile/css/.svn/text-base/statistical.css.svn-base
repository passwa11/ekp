.muiStaMenu>div {
	display: flex;
}

.muiStaMenuItem {
	flex: 1;
	text-align: center;
	display: inline-block;
	vertical-align: middle;
	color: #4285F4;
}

.muiModuleStatistical {
	padding-bottom: 2rem;
}

.muiStaMenuItemText {
	color: #3E4665;
	letter-spacing: 0;
	text-align: center;
	line-height: 1.2rem;
	font-size: 1.2rem;
	margin-top: 0.8rem;
}

.muiStaMenuItemCount {
	font-family: "landray-number", -apple-system, SF UI Text, Arial, PingFang SC, Hiragino Sans GB, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif;
	font-size: 3.1rem;
	color: #4285F4;
	letter-spacing: 0;
	text-align: center;
	line-height: 3.1rem;
	display: inline-block;
	margin-top: 1.9rem;
	position: relative;
}

.muiStaMenuItemAdd {
	font-size: 2rem;
    position: absolute;
    top: 0;
    line-height: 2rem;
    height: 2rem;
}

.muiStaMenuItemCount.disable {
	color: #DFE2ED;
}

.muiModuleStaTitle {
	font-size: var(--muiFontSizeMS);
	color: var(--muiFontColorInfo);
	font-weight: bold;
	margin: 1.5rem 0 0 1.7rem;
}

.muiStaConciseMenuItem{
    text-align: center;
    display: inline-block;
    vertical-align: middle;
}

.muiStaConciseMenuItem .muiStaMenuItemCount{
	font-size: var(--muiFontSizeXXL);
	color: var(--muiFontColorInfo);
}

.muiStaConciseMenuItem .muiStaMenuItemText{
	font-size: var(--muiFontSizeSS);
	color: var(--muiFontColorMuted);
}