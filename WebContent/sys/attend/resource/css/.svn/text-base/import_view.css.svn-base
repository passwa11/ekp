@CHARSET "UTF-8";
.lui_attend_container{
	padding: 50px 20px 30px;
}
.lui_attendTime_container{
	margin-bottom: 10px;
    text-align: center;
    vertical-align: middle;
}
.lui_attendBottom_container{
	margin: 0 auto;
    padding: 40px;
    width: 855px;
    vertical-align: top;
    text-align: center;
}
.lui_attendTime_container > .lui_attendTime_item{
	margin: 0 73px;
    vertical-align: top;
    display: inline-block;
    position: relative;
}
.lui_attendTime_container > .lui_attendTime_item.lui_date_container {
    margin: 0 50px;
    padding-top: 6px;
    color: #999;
    font-size: 14px;
}
.lui_attendTime_container > .lui_attendTime_item .time, 
.lui_attendTime_container > .lui_attendTime_item .txt {
    color: #999;
    display: block;
}
.lui_attendTime_container > .lui_attendTime_item .txt {
    font-size: 14px;
}
.lui_attendTime_container > .lui_attendTime_item .time {
    margin-bottom: 3px;
    color: #ff8b24;
    font-size: 34px;
}
.lui_attend_container.unStart .lui_attendTime_container > .lui_attendTime_item .time,
.lui_attend_container.finish .lui_attendTime_container > .lui_attendTime_item .time {
    color: #666;
}
.lui_attendTime_container > .lui_attendTime_item.date {
    margin: 0 50px;
    padding-top: 18px;
    color: #666;
}
.lui_attendTime_container > .lui_attendTime_item.lui_date_container:after {
    content: '';
    border-top: 1px dashed #999;
    position: absolute;
    top: 28px;
    left: -80px;
    right: -80px;
}
/*
.lui_attendTime_container > .lui_attendTime_item.lui_date_container:after,
.lui_attendTime_container > .lui_attendTime_item.lui_date_container:before {
    content: '';
    width: 70px;
    border-top: 1px dashed #999;
    position: absolute;
    top: 28px;
}
.lui_attendTime_container > .lui_attendTime_item.lui_date_container:before {
    left: -80px;
}
.lui_attendTime_container > .lui_attendTime_item.lui_date_container:after {
    right: -80px;
}
*/
.lui_attendBottom_container:after {
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.lui_attendBottom_item{
    float: left;
	margin: 0 73px;
    width: 274px;
    height: 294px;
    background-color: #fff;
    -webkit-box-shadow: 0 0 10px #eee;
    -moz-box-shadow: 0 0 10px #eee;
    -ms-box-shadow: 0 0 10px #eee;
    -o-box-shadow: 0 0 10px #eee;
    box-shadow: 0 0 10px #eee;
    border: 1px solid #ddd\9;
    display: inline-block;
    position: relative;
    overflow: hidden;
}
.lui_attendBottom_item_h{
	margin: 0;
	padding: 33px 20px 10px;
	color: #333;
	font-size: 16px;
	font-weight: normal;
	border-bottom: 1px dashed #ddd;
	display: block;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.lui_attendBottom_item:nth-child(2) .lui_attendBottom_item_h {
    border-bottom: none;
    font-size: 14px;
}
.lui_attendBottom_item:nth-child(2) .lui_attendStat_body {
    padding-top: 0;
}
.lui_attendBottom_item_tag {/*     width: 92px; */background-position: 0 0;background-repeat: no-repeat;font-style: normal;display: block;position: absolute;left:0;top: 5px;background: #d2d2d2;line-height: 25px;height: 24px;font-size: 12px;color: #fff;text-align: center;padding:0 10px;padding-right:15px;}
.lui_attendBottom_item_tag:after{
	content:"";
	display:inline-block;
	width:0;
	height:0;
	border:12px solid transparent;
	border-width:12px 6px;
	border-right-color:#fff;
	position:absolute;
	right:0;
	top:0;
}

.lui_attend_container.unStart .lui_attendBottom_item_tag {
/*     background-image: url(../images/unStart.png); */
}
.lui_attend_container.doing .lui_attendBottom_item_tag {
    /* background-image: url(../images/doing.png); */
    background-color: #4285f4;
}
.lui_attend_container.finish .lui_attendBottom_item_tag {
    /* background-image: url(../images/finish.png); */
}
.lui_attendStat_body {
    padding: 15px 15px 0px 15px;
    min-height: 170px;
}
.lui_attendStat_body canvas {
    border: 1px solid #d3d4dc;
    padding: 6px;
    margin-top: 10px;
}
.lui_attendStat_list{
	margin: 0;
    padding: 30px 12px;
    list-style: none;
}
.lui_attendStat_list > li {
    width: 50%;
    float: left;
}
.lui_attendStat_list > li .num, 
.lui_attendStat_list > li .txt {
    color: #999;
    display: block;
}
.lui_attendStat_list > li .num {
    margin: 0 auto 10px;
    width: 78px;
    height: 78px;
    line-height: 78px;
    font-size: 36px;
    border-radius: 50%;
    border: 1px solid #999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.lui_attendStat_list > li .txt {
    font-size: 14px;
}
.lui_attend_container.doing .lui_attendStat_list > li .num{
	border-color : #15a4fa;
	color: #15a4fa;
}
.lui_attend_container.doing .lui_attendStat_list > li.unattend .num{
	border-color : #ff6c6c;
	color: #ff6c6c;
}
.lui_attendStat_list:after {
    content: '';
    display: table;
    visibility: hidden;
    clear: both;
}
.lui_attendStat_footer{
    color: #999;
    font-size: 14px;
}
.lui_attendStat_footer > span{
	margin: 0 10px;
}
.lui_attendListview_select{
    margin: 0 auto 10px;
    max-width: 925px;
    border-bottom: 1px solid #d5d5d5;
    padding-bottom: 10px;
    padding-left: 5px;
}
.lui_attendListview_selectItem{
	cursor: pointer;
    font-size: 16px;
    margin-right: 20px;
}
.lui_attendListview_selectItem:hover{
	color: #15a4fa;
}
.lui_attendListview_selectItem.selected{
	color: #15a4fa;
}
.lui_attendListview_selectSep{
	padding: 10px;
}
.lui_attendStat_download{
	position: relative;
}
.lui_attendStat_download_btn{
    position: absolute;
    margin: 30px auto 0;
    left: 0;
    right: 0;
    font-size: 16px;
    top: -20px;
}
.lui_attendStat_download_btn>a:hover,
.lui_attendStat_download_btn>a {
    color: #37aee9;
    font-size: 16px;
}
.lui_attendStat_download_btn>a:hover {
    text-decoration: underline;
}
.lui_category_item .lui_attendCategoryTitle{
	background:#f6f6f6;
	height: 30px;
    padding: 0 16px;
}
.lui_category_item .lui_left{
    display: inline-block;
    float: left;
    line-height: 30px;
}
.lui_category_item .lui_left .lui_attendCategoryTime{
	padding-left:5px;
}
.lui_category_item .lui_right{
    display: inline-block;
    float: right;
    line-height: 30px;
}
.lui_category_item .lui_right span{
    padding-left: 20px;
    cursor: pointer;
    padding-right: 5px;
    background-repeat: no-repeat;
}
.lui_category_item .lui_attendCategoryStatus{    
	padding: 0px 5px;
    border-radius: 3px;
    color: #fff;
    padding: 3px 4px;
    margin: 0 20px 0 5px;
    background-color: #b9b9b9;
}
.lui_category_item .lui_attendCategoryStatus.doing {
    background-color: #37aee9;
}
.lui_category_item .lui_attendCategoryIcon{
	position: relative;
    cursor: pointer;
    width: 30px;
    height: 30px;
}
.lui_category_item .lui_attendCategoryIcon.up{
    background: url(../images/arrow1.png);
    background-repeat: no-repeat;
    background-position: 50% 50%;
}
.lui_category_item .lui_attendCategoryIcon.down{
    background: url(../images/arrow2.png);
     background-repeat: no-repeat;
    background-position: 50% 50%;
}


.lui_category_item .lui_listview_columntable_table thead {
    background-color: #fff;
}
.lui_category_item .lui_listview_columntable_table thead th {
    background-color: #fff;
    font-size: 12px;
    color: #666;
    border-bottom: 0;
}
.lui_category_item .lui_listview_columntable_table tbody tr {
    border-bottom: 0;
}
.lui_category_item .lui_listview_body .lui_listview_columntable_table tbody tr:last-child {
    border-bottom: 0;
}
.lui_category_item .lui_listview_body .lui_listview_columntable_table {
    border-bottom: 0;
}
.lui_category_item .listview {
    max-width: 925px;
    margin: 0 auto;
}
.lui_category_item {
    border: 1px solid #d3d4dc;
    margin-bottom: 18px;
}
.lui_category_item>div:last-child {
    border-top: 1px solid #d3d4dc;
}
.lui_sysAttend_projection {
  background-color: #37adea;
  max-width: 925px;
  margin: 0 auto 50px;
  padding: 10px;
  line-height: 20px;
  text-align: center;
  color: #fff;
  font-size: 14px;
  cursor: pointer;
  transition-duration: .3s;
}
.lui_sysAttend_projection:hover{ background-color: #0893d7; }
.lui_sysAttend_projection > span{
  padding-left: 30px;
  background: url(../images/icon-projective.png) no-repeat 0 50%;
}
.lui_attend_projection_bg{
	background-image: url(../images/projection.png);
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
.lui_attend_projection_panel {
    /*max-width: 1160px;
    max-height: 760px;
    width: 95%;
    margin: 35px auto 0;*/
    position: absolute;
    left: 5%;
    right: 5%;
    top: 5%;
    bottom: 5%;
    overflow: hidden;
}
.lui_attend_projection_panel:before{
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: #fff;
    opacity: 0.5;
    filter: alpha(Opacity=80);
}
.lui_attend_projection_panel_heading {
    height: 80px;
    background-color: #fff;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1;
}
.lui_attend_projection_panel_title {
    margin: 0;
    font-size: 28px;
    color: #666;
    line-height: 80px;
    font-weight: normal;
}
.lui_attend_projection_panel_body { height: 100%;}
.lui_attend_projection_panel_body:after{
    content: '';
    display: table;
    visibility: hidden;
    clear: both;
}
.lui_attend_projection_panel_left {
    padding-top: 80px;
    width: 50%;
    height: 100%;
    float: left;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.lui_attend_projection_qr_wrap {
    margin: 0 auto;
    width: 450px;
    height: 450px;
    background: url(../images/projection_qr_bg.png) center no-repeat;
    background-size: contain;
    position: relative;
}
.lui_attend_projection_qr {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -30%;
    margin-left: -30%;
    padding: 8px;
    width: 60%;
    background-color: #fff;
}
.lui_attend_projection_qr > canvas{
    width: 100%!important;
    height: 100%!important;
}
.lui_attend_projection_qr_tip {
    position: absolute;
    bottom: -12px;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 24px;
    color: #333;
}
.lui_attend_projection_panel_right {
  padding-top: 80px;
    width: 50%;
    max-height: 600px;
    float: left;
}
.lui_attend_projection_list {
  list-style-type: none;
  margin: 0 0 0 90px;
  padding: 15px 0;
  max-height: 730px;
  overflow-y: auto;
}
.lui_attend_projection_listItem {
    position: relative;
    margin: 10px 0;
    padding-left: 94px;
}
.lui_attend_projection_listItem:after{
  content: '';
  display: table;
  visibility: hidden;
  clear: both;
}
.lui_attend_projection_avatar {
    width: 60px;
    height: 60px;
    border: 2px solid #fff;
    border-radius: 50%;
    overflow: hidden;
    position: absolute;
    top: 50%;
    margin-top: -32px;
    left: 0;
}
.lui_attend_projection_avatar>img {
    width: 100%;
}
.lui_attend_projection_person {
    font-size: 20px;
    color: #333;
    padding-right: 8%;
    line-height: 64px;
    min-width: 80px;
    max-width: 42%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    float: left;
}
.lui_attend_projection_time {
    line-height: 64px;
    font-size: 16px;
    color: #333;
    padding: 0;
    max-width: 50%;
    float: left;
}
.lui_attend_projection_btn{
	width: 51px;
    height: 51px;
    background: url(../images/screen_btn.png) center no-repeat;
    border: none;
    bottom: 20px;
    right: 20px;
    position: absolute;
    cursor: pointer;
    border-radius: 50%;
    transition: box-shadow 0.3s;
    transition-duration: .3s;
}
.lui_attend_projection_btn:hover {
    opacity: .75;
    filter: alpha(opacity=75);
}

.lui_attend_projection_panel_body:after {
    content: '';
    display: table;
    visibility: hidden;
    clear: both;
}


.sign-in-member-name {
    font-size: 20px;
    color: #333;
    padding: 28px 80px 28px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.conf_show_more_w {
	position: relative;
	display: inline-block;
}

.conf_btn_edit .btn_txt {
	margin: 0px 2px;
	color:#2574ad;
	border-bottom:1px solid transparent;
}
.conf_btn_edit .btn_txt:hover{
	color:#123a6b;
	border-bottom-color:#123a6b; 
}
@media screen and (max-width: 1400px){
.sign-in-member-name {
    padding: 22px 80px 22px 0;
}
}