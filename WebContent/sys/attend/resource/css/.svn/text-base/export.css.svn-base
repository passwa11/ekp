/* 附件图标 -- 建议搬迁到附件机制 Ends */
.lui_log_list_open{
    right: 0px;
    /*display: block;*/
}
.lui_log_list_init{
    right: -500px;
}
.lui_log_list {
    position: fixed;
    top: 0;
    width: 400px;
    height: 100%;
    border-radius: 8px;
    box-sizing: border-box;
    border: 1px solid #ebeef5;
    background-color: #fff;
    box-shadow: 0 2px 12px 0 rgb(0 0 0 / 10%);
    transition: opacity .3s,transform .3s,left .3s,right .3s,top .4s,bottom .3s;
    overflow-x: hidden;
}