<div class='sysAttendView' id="item_{sysAttendViewItem}">
	<section class='sysAttendContainer'>
		<ul class='sysAttendTimeContainer'>
			<li class='sysAttendStartTimeContainer'>
				<span class='sysAttendTime' data-dojo-attach-point='domStartTime'></span>
				<span class='sysAttendStartTimeTxt'>{topic11}</span>
			</li>
			<li class='sysAttendStatusContainer'>
				<span class='sysAttendDate' data-dojo-attach-point='domAttendDate'></span>
				<span class='sysAttendStatus' data-dojo-attach-point='domStatus'></span>
			</li>
			<li class='sysAttendEndTimeContainer'>
				<span class='sysAttendTime' data-dojo-attach-point='domEndTime'></span>
				<span class='sysAttendStartTimeTxt'>{topic12}</span>
			</li>
		</ul>
		<section class='sysAttendMainContainer' data-dojo-attach-point='domMainContainer'>
		</section>
		<ul class='sysAttendStatContainer'>
			<li class='sysAttendStatCount'>
				<span class='num domStatCount' data-dojo-attach-point='domStatCount'></span>	
				<span class='txt'>{topic13}</span>
			</li>
			<li class='sysAttendStatDo'>
				<span class='num domStatDo' data-dojo-attach-point='domStatDo'></span>	
				<span class='txt'>{topic14}</span>
			</li>
			<li class='sysAttendStatDo'>
				<span class='num domStatNoraml' data-dojo-attach-point='domStatNoraml'></span>	
				<span class='txt'>{topic15}</span>
			</li>
			<li class='sysAttendStatUndo'>
				<span class='num domStatLate' data-dojo-attach-point='domStatLate'></span>	
				<span class='txt'>{topic16}</span>
			</li>
			<li class='sysAttendStatUndo'>
				<span class='num domStatUndo' data-dojo-attach-point='domStatUndo'></span>	
				<span class='txt'>{topic17}</span>
			</li>
		</ul>
		<section class='sysAttendListContainer' data-dojo-attach-point='domAttendList'>
			<span>{topic18}</span>
			<i class='mui mui-forward'></i>
		</section>
		<section class='sysAttendQRcodeContainer' data-dojo-attach-point='domQRCode'>
			<span>{topic19}</span>
			<i class='mui mui-forward'></i>
		</section>
	</section>
	<section class='sysAttendBtnContainer'>
		<button class='sysAttendBtn warn' data-dojo-attach-point='domFinishAttend'
			type='button'>{topic2}</button>
	</section>
</div>
