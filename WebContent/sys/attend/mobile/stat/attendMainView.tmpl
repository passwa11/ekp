<div id="attendMainView" class="attendMainView" data-dojo-type='sys/attend/mobile/resource/js/list/AttendMainView'
	data-dojo-props="url:'/sys/attend/sys_attend_main/sysAttendMain.do?method=viewMain'">
	<input type="hidden" name="attendMainId" value='{attendMainId}'>
	<input type="hidden" name="attendType" value='{attendType}'>
	<input type="hidden" name="attendFdType" value='{attendFdType}'>
	<input type="hidden" name="creatorId" value='{creatorId}'>
	<input type="hidden" name="time" value='{time}'>
	<div class="muiAttendMainHead muiCommonHead" data-dojo-type="mui/header/Header" data-dojo-props="height:'1.8rem'">
		<div class="leftArea"><span class="muiSignCateName"></span></div>
		<div class="rightArea">
			<div id="mainDate" class="mainDate" 
				 data-dojo-type="mui/form/DateTime"
				 data-dojo-mixins="mui/datetime/_DateMixin"
				 data-dojo-props="valueField:'nowDate',value:'',edit:false,showStatus:'readOnly'"></div>
		</div>
	</div>
</div>