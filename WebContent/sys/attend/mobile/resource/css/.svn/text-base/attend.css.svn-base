	  }
	  100% {
	    opacity: 1;
	    -webkit-transform: scale(1);
	    				transform: scale(1);
	  }
	}

	@keyframes muiIconLoading {
	  0% {
	    opacity: .2;
	    -webkit-transform: scale(.5);
	    				transform: scale(.5);
	  }
	  100% {
	    opacity: 1;
	    -webkit-transform: scale(1);
	    				transform: scale(1);
	  }
	}
	.muiIconLoading{
		font-size: 0;
		text-align: center;
		display: inline-block;
	}
	.muiIconLoading > span{
		margin: 0 1px;
		width: 3px;
		height: 3px;
		border-radius: 50%;
		background-color: #999;
		display: inline-block;
		position: relative;
		-webkit-animation: muiIconLoading .8s ease-in-out 0s alternate infinite;
						animation: muiIconLoading .8s ease-in-out 0s alternate infinite;
	}
	.muiIconLoading > span:nth-child(1){
		-webkit-animation-delay: 0s;
						animation-delay: 0s;
	}
	.muiIconLoading > span:nth-child(2){
		-webkit-animation-delay: .3s;
						animation-delay: .3s;
	}
	.muiIconLoading > span:nth-child(3){
		-webkit-animation-delay: .6s;
						animation-delay: .6s;
	}
	.muiIconLoading.hide{
		display: none;
