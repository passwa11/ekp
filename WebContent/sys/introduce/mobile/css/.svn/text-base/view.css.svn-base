.muiIntroduceTitle {
	background : #fff;
	margin : .8rem;
	height : 2.5rem;
}

.muiIntroduceTitleLeft {
    font-weight: bold;
	font-size : 2rem;
	padding-left : .3rem;
	width : 9rem;
	float : left;
}

.muiIntroduceTitleRight{
	text-decoration: underline;
	color: #37aee9;
	font-size: 1.8rem;
    margin-top: .2rem;
    display: inline-block;
    width: 65%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color : rbg(24, 178, 240);
}

.muiIntroduceContent{
    margin: .8rem;
    height: auto;
    background: #fff;
}

.muiIntroduceContentTitle{
    font-weight: bold;
	font-size: 1.6rem;
    padding-left: .3rem;
    margin-top: 5px;
}

.muiIntroduceContentMsg{
    margin: 5px 0 0 .2rem;
	font-size: 1.6rem;
}








