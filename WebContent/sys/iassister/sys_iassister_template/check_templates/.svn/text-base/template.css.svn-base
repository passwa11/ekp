@charset "UTF-8";

.check_template_wrapper {
	padding-top: 8px;
}

.check_templates_container .btns {
	text-align: left;
}

.btns .el-switch {
	margin-left: 16px;
}

.check_templates_container .check_desc {
	margin: 10px 0px 6px 0px;
}

.check_templates_container .ellipse {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.lui_input .el-input__inner {
	border-top: none;
	border-left: none;
	border-right: none;
	background-color: transparent;
	border-radius: 0px;
}

.check_templates_container .lui_edit {
	background-repeat: no-repeat;
	background-position: center;
	display: inline-block;
	vertical-align: middle;
	width: 32px;
	height: 32px;
	line-height: 1px;
	background-image: url(lui_edit.png);
	cursor: pointer;
}

.check_templates_container .lui_del {
	background-repeat: no-repeat;
	background-position: center;
	display: inline-block;
	vertical-align: middle;
	width: 16px;
	height: 16px;
	line-height: 1px;
	background-image: url(lui_delete.png);
	background-position: 0px 0px !important;
}

.check_templates_container .el-table__header {
	width: 100% !important;
}

.check_templates_container .el-table__body {
	width: 100% !important;
}

.check_templates_container .el-table td, .el-table th {
	padding: 0px !important;
}

.check_templates_container .el-table--border, .el-table--group {
	border-color: rgb(215, 215, 215) !important;
}

.check_templates_container .el-table td, .el-table th.is-leaf {
	border-color: rgb(215, 215, 215) !important;
}

.check_templates_container .el-table--border::after, .el-table--group::after,
	.el-table::before {
	background-color: rgb(215, 215, 215) !important;
}

.params_mapping .cell {
	padding: 6px;
}

.params_mapping .field_container .cell {
	padding: 0px;
}

.params_mapping .field_container .cell .el-select {
	width: 96%;
}

.params_mapping th {
	background-color: rgb(242, 242, 242) !important;
}

.params_mapping .mapping_field {
	padding: 3px;
	display: flex;
	justify-content: center;
}

.mapping_field .el-input {
	width: 90%;
}

.mapping_field .el-input.is-active .el-input__inner, .el-input__inner:focus
	{
	border-color: #C0C4CC;
}

.group_node .el-select {
	width: 100%;
}

.group_templates .cell {
	padding: 3px;
}

.gt_draggable .gt_wrapper {
	border-bottom: 1px dotted darkgray;
	line-height: 36px;
	height: 36px;
	display: flex;
}

.gt_wrapper.can_drag {
	cursor: move;
}

.gt_draggable .gt_wrapper:last-child {
	border-bottom: none;
}

.gt_wrapper .gt_index {
	width: 26px;
}

.gt_wrapper .gt_label {
	width: calc(100% - 26px);
	text-align: left;
	padding: 0px 6px;
}

.group_mode td.group_order.can_drag {
	cursor: move;
}