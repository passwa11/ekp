 /*    弹出窗口  starts
 ========================================================================== */
 .lui-zonePage-modal.in .lui-zonePage-modal-dialog{
  -webkit-transform: translate(0, 0);
    -ms-transform: translate(0, 0);
    -o-transform: translate(0, 0);
    transform: translate(0, 0);
 }
 .lui-zonePage-modal-open .lui-zonePage-modal {
    overflow-x: hidden;
    overflow-y: auto;
}
.lui-zonePage-fade.in {
    opacity: 1;
}
.lui-zonePage-modal {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 50;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
}
.lui-zonePage-fade {
     opacity: 0; 
    -webkit-transition: opacity .15s linear;
    -o-transition: opacity .15s linear;
    transition: opacity .15s linear;
}
.lui-zonePage-modal.lui-zonePage-fade .lui-zonePage-modal-dialog {
    -webkit-transition: -webkit-transform .3s ease-out;
    -o-transition: -o-transform .3s ease-out;
    transition: transform .3s ease-out;
    -webkit-transform: translate(0, -25%);
    -ms-transform: translate(0, -25%);
    -o-transform: translate(0, -25%);
    transform: translate(0, -25%);
}
.lui-zonePage-modal-dialog {
  position: relative;
  width: 60%;
  min-width: 700px;
  margin: 75px auto 10px;
  z-index: 102;
  top: 0px;
}
.lui-zonePage-modal-dialog>img{
  width: 95%;
  height:500px;
  position: absolute;
  left: 0;
  top: 0;
