.muiCateDiaglogContainer {
    background-color: #fcfcfc;
}

.muiTagPdaContainer {
    position: relative;
}

.muiTagPdaContainer .muiFormRequired {
    display: inline-block;
    position: absolute;
    height: 3rem;
    right: -1.4rem;
    top: auto;
    margin-top: 0;
    line-height: 3rem;
    font-size: 1.4rem;
}

.tag_DialogHeader {
    margin-bottom: 5px;
    box-shadow: 0 1px 5px #e5e5e5;
    z-index: 9999;
}

.tag_DialogHeader .muiCateHeaderContent {
    vertical-align: top;
    background-color: #fff;
}

 .tag_DialogHeader .muiCateHeaderTitle {
    color: #333333;
    font-size: 16px;
}
.tag_DialogHeader .muiCateHeaderReturn, .tag_DialogHeader .muiCateHeaderCancel {
    color: var(--muiFontColor);
    font-size: 14px;
}

.tag_DialogHeader .muiCateHeaderReturn i.mui-back:before {
    font-size: 14px;
    font-weight: bold;
    position: relative;
    top: -0.1rem;
}

.tag_DialogHeader .muiCateHeaderReturn i {
    color: var(--muiFontColor);
}

.tag_DialogCategoryHeader {
    height: 5rem;
    display: flex;
    background: #fcfcfc;
}

.tag_DialogCategorySelect {
    height: inherit;
}

.muiTagDialogButton:AFTER {
    content: "";
    transform: scaleY(0.5);
    bottom: 0;
    border-top: 1px solid #EEEEEE;
    position: absolute;
    left: 0;
    right: 0;
}

.muiTagDialogButton, .muiTagDialogBottom {
    height: 5.8rem;
    display: inline-block;
    text-align: center;
    vertical-align: middle;
    line-height: 5.8rem;
    font-size: 1.6rem;
    color: var(--muiFontColor);
    width: 100%;
    position: relative;
}

.muiDialogElement .muiDialogElementContainer_bottom {
    min-height: 18%;
    max-height: 90%;
    border-radius: 1.5rem;
}

.muiDialogElementContainer_bottom {
    top: auto;
    bottom: 68px;
    left: 1rem;
    right: 1rem;
    transform: translate3d(0, 100%, 0);
}

.muiTagDialogBottom {
    margin: 1rem 0;
    color: var(--muiFontColorMuted);
    height: 4.8rem;
    line-height: 4.8rem;
    background: #ffffff;
    border-radius: 1rem;
}

.muiTagDialogContainer {
    bottom: -70px;
    position: absolute;
    left: 0rem;
    right: 0rem;
    overflow: hidden;
}

.muiDialogTagSelect {
    width: calc(100% - 1.2rem);
    height: 3.4rem;
    line-height: 3.4rem;
    margin: 0 auto;
    margin-top: 0.8rem;
    margin-bottom: 0.8rem;
    margin-left: 1.2rem;
    background: #F7F7F7;
    border-radius: 0.6rem;
}

.muiDialogTagSelect .muiSelInputRight {
    right: 1rem;
}

.muiDialogTagSelect .muiSelInput {
    word-break: keep-all;
    white-space: nowrap;
    overflow-x: auto;
    width: calc(100% - 3.5rem);
    height: inherit;
    padding-right: 0;
    padding-left: 1rem;
    font-size: 1.3rem;
}

.muiDialogTagSelect .muiSelInput:empty:after {
    font-size: 1.3rem;
}

.tag_DialogSearchBar .muiSearchInput {
    font-size: 1.3rem;
}

.tag_DialogSearchBar .muiSearchCancelBtn {
