<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.budget.model.FsscBudgetData" 
        table="fssc_budget_data">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdYear" 
            column="fd_year" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPeriodType" 
            column="fd_period_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPeriod" 
            column="fd_period" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBudgetStatus" 
            column="fd_budget_status" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRule" 
            column="fd_rule" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdApply" 
            column="fd_apply" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdElasticPercent" 
            column="fd_elastic_percent" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCompanyGroupCode" 
            column="fd_company_group_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdCompanyCode" 
            column="fd_company_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdCostCenterGroupCode" 
            column="fd_cost_center_group_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdCostCenterCode" 
            column="fd_cost_center_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdBudgetItemCode" 
            column="fd_budget_item_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdProjectCode" 
            column="fd_project_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdInnerOrderCode" 
            column="fd_inner_order_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdWbsCode" 
            column="fd_wbs_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdPersonCode" 
            column="fd_person_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdDeptCode" 
            column="fd_dept_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdBudgetSchemeCode" 
            column="fd_budget_scheme_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdIsKnots" 
            column="fd_is_knots" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdTransferAcount" 
            column="fd_transfer_acount" 
            update="true" 
            insert="true" />
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" />
        <many-to-one 
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompanyGroup" 
            column="fd_company_group_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostCenter" 
            column="fd_cost_center_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBudgetItem" 
            column="fd_budget_item_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdProject" 
            column="fd_project_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdInnerOrder" 
            column="fd_inner_order_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdWbs" 
            column="fd_wbs_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdPerson" 
            column="fd_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBudgetScheme" 
            column="fd_budget_scheme_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostCenterGroup" 
            column="fd_cost_center_group_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
