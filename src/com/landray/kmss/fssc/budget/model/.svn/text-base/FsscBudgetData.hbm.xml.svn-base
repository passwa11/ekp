           name="fdDept" 
           column="fd_dept_id" 
           update="true" 
           insert="true"/>
        <many-to-one 
           name="docCreator" 
           column="doc_creator_id" 
           update="true" 
           insert="true"/>
        <property 
        	name="authReaderFlag" 
        	column="auth_reader_flag"
			update="true" 
			insert="true" 
			not-null="false" 
			length="1" />
		<bag 
			name="authReaders" 
			table="fssc_budget_data_reader" 
			lazy="true">
			<key 
				column="fd_doc_id" />
			<many-to-many 
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag 
			name="authOtherReaders" 
			table="fssc_budget_data_oreader" 
			lazy="true">
			<key 
				column="fd_doc_id" />
			<many-to-many 
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag 
			name="authAllReaders" 
			table="fssc_budget_data_areader" 
			lazy="true">
			<key 
				column="fd_doc_id" />
			<many-to-many 
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>
    </class>
</hibernate-mapping>
