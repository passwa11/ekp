<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseBalance" 
        table="fssc_expense_balance">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdBudgetRate" 
            column="fd_budget_rate" 
            update="true" 
            insert="true"/>
        <property 
            name="docStatus" 
            column="doc_status" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_balance_createTime"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAttNum" 
            column="fd_att_num" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMonth" 
            column="fd_month" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdSubject" 
            column="fd_subject" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="docNumber" 
            column="doc_number" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
         <property 
            name="authChangeAtt" 
            column="auth_change_att" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNocopy" 
            column="auth_att_nocopy" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNodownload" 
            column="auth_att_nodownload" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNoprint" 
            column="auth_att_noprint" 
            update="true" 
            insert="true"/>
        <property
            name="fdVoucherStatus"
            column="fd_voucher_status"
            update="true"
            insert="true"
            length="100"/>
        <property
            name="fdBookkeepingStatus"
            column="fd_bookkeeping_status"
            update="true"
            insert="true"
            length="100"/>
        <property
            name="fdBookkeepingMessage"
            column="fd_bookkeeping_message"
            update="true"
            insert="true"
            length="600"/>
        <!-- 回收站机制字段开始 -->
        <property
			name="docDeleteFlag"
			column="doc_delete_flag"
			update="true"
			insert="true"
			not-null="false"
			length="5" />
		<property
			name="docDeleteTime"
			column="doc_delete_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="docPublishTime"
			column="doc_publish_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docDeleteBy"
			column="doc_delete_by"
			insert="true"
			update="true"
			not-null="false" />
		<!-- 回收站机制字段结束 -->
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostCenter" 
            column="fd_cost_center_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreatorDept" 
            column="doc_creator_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdVoucherType" 
            column="fd_voucher_type_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docTemplate" 
            column="doc_template_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="fssc_expense_balance_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherReaders" 
            table="fssc_expense_balance_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="fssc_expense_balance_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="fssc_expense_balance_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="fssc_expense_balance_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
         <bag 
            name="authAttCopys" 
            table="fssc_expense_balance_attcopy" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttDownloads" 
            table="fssc_expense_balance_attdl" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttPrints" 
            table="fssc_expense_balance_attprint" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <list 
            name="fdDetailList" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.fssc.expense.model.FsscExpenseBalanceDetail"/>
        </list>
    </class>
</hibernate-mapping>
