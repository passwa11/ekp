<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseShareDetail" 
        table="fssc_expense_share_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <many-to-one 
            name="fdProject" 
            column="fd_project_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostCenter" 
            column="fd_cost_center_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostDept" 
            column="fd_cost_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdExpenseItem" 
            column="fd_expense_item_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdHappenDate" 
            column="fd_happen_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRate" 
            column="fd_rate" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdStandardMoney" 
            column="fd_standard_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="200"/>
    </class>
</hibernate-mapping>
