<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseBalanceDetail" 
        table="fssc_expense_balance_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="5"/>
        <many-to-one 
            name="fdExpenseItem" 
            column="fd_expense_item_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdAccount" 
            column="fd_account_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCostCenter" 
            column="fd_cost_center_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdPerson" 
            column="fd_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdDept" 
            column="fd_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCashFlow" 
            column="fd_cash_flow_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdProject" 
            column="fd_project_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBudgetMoney" 
            column="fd_budget_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBudgetInfo" 
            column="fd_budget_info" 
            update="true" 
            insert="true" 
            length="2000"/>
    </class>
</hibernate-mapping>
