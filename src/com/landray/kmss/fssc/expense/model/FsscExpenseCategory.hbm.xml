<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.expense.model.FsscExpenseCategory" 
        table="fssc_expense_category">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdIsProjectShare" 
            column="fd_is_project_share" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdIsProapp" 
            column="fd_is_proapp" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdIsTravelAlone" 
            column="fd_is_travel_alone" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdIsMobile" 
            column="fd_is_mobile" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdIsForeign" 
            column="fd_is_foreign" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdIsFee" 
            column="fd_is_fee" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdFeeTemplateId" 
            column="fd_fee_template_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdFeeTemplateName" 
            column="fd_fee_tempalte_name" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdIsAmortize" 
            column="fd_is_amortize" 
            update="true" 
            insert="true" 
            length="5"/>
        <property 
            name="fdExtendFields" 
            column="fd_extend_fields" 
            update="true" 
            insert="true" 
            length="1000"/>
        <property 
            name="fdBudgetShowType" 
            column="fd_budget_show_type" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_expenseCategory_order"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_category_createTime"/>
        <property 
            name="fdSubjectRule" 
            column="fd_subject_rule" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdSubjectRuleText" 
            column="fd_subject_rule_text" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdExpenseType" 
            column="fd_expense_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdAllocType" 
            column="fd_alloc_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdSubjectType" 
            column="fd_subject_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdIsProject" 
            column="fd_is_project" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdHierarchyId" 
            column="fd_hierarchy_id" 
            update="true" 
            insert="true" 
            length="450" 
            index="idx_category_hierarchyId"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <!-- 所有人不可使用 -->
        <property
            name="authNotReaderFlag"
            column="auth_not_reader_flag"
            update="true"
            insert="true"
            not-null="false"
            length="1" />
        <property 
            name="docXform" 
            column="doc_xform" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docUseXform" 
            column="doc_use_xform" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="hbmParent" 
            class="com.landray.kmss.fssc.expense.model.FsscExpenseCategory" 
            column="fd_parent_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="fssc_expense_category_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="fssc_expense_category_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="fssc_expense_category_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="fssc_expense_category_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <!-- 新闻默认可阅读者 -->
		<bag
			name="authTmpReaders"
			table="fssc_exp_template_def_reader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>

		<!-- 新闻默认可编辑者 -->
		<bag
			name="authTmpEditors"
			table="fssc_exp_template_def_editor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authTmpAttPrints"
			table="fssc_exp_tmp_attprint"
			lazy="true">
			<key column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_print_id" />
		</bag>
		<bag
			name="authTmpAttCopys"
			table="fssc_exp_tmp_attcopy"
			lazy="true">
			<key column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_copy_id" />
		</bag>
		<bag
			name="authTmpAttDownloads"
			table="fssc_exp_tmp_attdl"
			lazy="true">
			<key column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_download_id" />
		</bag>
		<property
			name="authTmpAttNodownload"
			column="auth_tmp_att_nodownload"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authTmpAttNocopy"
			column="auth_tmp_att_nocopy"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authTmpAttNoprint"
			column="auth_tmp_att_noprint"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
    </class>
</hibernate-mapping>
