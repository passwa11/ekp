<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.voucher.model.FsscVoucherDetail" 
        table="fssc_voucher_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="1"/>
        <many-to-one 
            name="fdBaseCostCenter" 
            column="fd_base_cost_center_id" 
            update="true" 
            insert="true"/>
        <many-to-one
            name="fdBaseErpPerson"
            column="fd_base_erp_person_id"
            update="true"
            insert="true"/>
        <many-to-one 
            name="fdBaseCashFlow" 
            column="fd_base_cash_flow_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property
            name="fdVoucherText"
            column="fd_voucher_text"
            update="true"
            insert="true"
            length="200"/>
        <property
            name="fdOrder"
            column="fd_order"
            update="true"
            insert="true"
            length="10"
            not-null="false" />
        <many-to-one 
            name="fdBaseWbs" 
            column="fd_base_wbs_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBaseInnerOrder" 
            column="fd_base_inner_order_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBaseAccounts" 
            column="fd_base_accounts_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBaseSupplier" 
            column="fd_base_supplier_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBaseCustomer" 
            column="fd_base_customer_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBaseProject" 
            column="fd_base_project_id" 
            update="true" 
            insert="true"/>
        <many-to-one
            name="fdBasePayBank"
            column="fd_base_pay_bank_id"
            update="true"
            insert="true"/>
        <property
            name="fdContractCode"
            column="fd_contract_code"
            update="true"
            insert="true"
            length="200"/>
        <many-to-one 
            name="fdDept" 
            column="fd_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
