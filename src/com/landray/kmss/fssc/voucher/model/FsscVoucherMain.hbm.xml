<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.voucher.model.FsscVoucherMain" 
        table="fssc_voucher_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property
            name="fdIsAmortize"
            column="fd_is_amortize"
            update="true"
            insert="true"/>
        <property
            name="fdPushType"
            column="fd_push_type"
            update="true"
            insert="true"
            length="20"/>
        <property
            name="fdVoucherCreateType"
            column="fd_voucher_create_type"
            update="true"
            insert="true"
            length="200"/>
        <property
            name="docFinanceNumber"
            column="doc_finance_number"
            update="true"
            insert="true"
            length="200"/>
        <property 
            name="docNumber" 
            column="doc_number" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="docSubject"
            column="doc_subject"
            update="true"
            insert="true"
            length="200"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="100"/>
        <property
            name="fdModelNumber"
            column="fd_model_number"
            update="true"
            insert="true"
            length="50"/>
        <property
            name="fdModelMoney"
            column="fd_model_money"
            update="true"
            insert="true"/>
        <property
            name="fdVoucherDate" 
            column="fd_voucher_date" 
            update="true" 
            insert="true"/>
        <property
            name="fdBookkeepingStatus"
            column="fd_bookkeeping_status"
            update="true"
            insert="true"
            length="10"/>
        <property 
            name="fdBookkeepingDate" 
            column="fd_bookkeeping_date" 
            update="true" 
            insert="true"/>
        <property
            name="fdBookkeepingMessage"
            column="fd_bookkeeping_message"
            update="true"
            insert="true"
            length="600"/>
        <many-to-one
            name="fdBookkeepingPerson"
            column="fd_bookkeeping_person"
            update="true"
            insert="true"/>
        <property 
            name="fdAccountingYear" 
            column="fd_accounting_year" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdPeriod" 
            column="fd_period" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdNumber" 
            column="fd_number" 
            update="true" 
            insert="true"/>
        <property 
            name="fdVoucherText" 
            column="fd_voucher_text" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_voucherMain_createTime"/>
        <property
            name="docAlterTime"
            column="doc_alter_time"
            update="true"
            insert="true"
            index="idx_voucherMain_alterTime"/>
        <property
            name="fdMergeEntry"
            column="fd_merge_entry"
            update="true"
            insert="true"
            length="20"/>
        <many-to-one
            name="fdBaseVoucherType"
            column="fd_base_voucher_type_id"
            update="true"
            insert="true"/>
        <many-to-one
            name="fdBaseCurrency"
            column="fd_base_currency_id"
            update="true"
            insert="true"/>
        <many-to-one
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
        <many-to-one
            name="docCreator"
            column="doc_creator_id"
            update="true"
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <list 
            name="fdDetail"
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.fssc.voucher.model.FsscVoucherDetail"/>
        </list>
    </class>
</hibernate-mapping>
