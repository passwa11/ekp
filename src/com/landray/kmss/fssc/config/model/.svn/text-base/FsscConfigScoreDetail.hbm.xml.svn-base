<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.fssc.config.model.FsscConfigScoreDetail" 
        table="fssc_config_score_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
         <many-to-one 
            name="fdAddScorePerson" 
            column="fd_add_score_person" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_fsscConfigScoreDetail_docCreateTime"/>
        <property 
            name="docPublishTime" 
            column="doc_publish_time" 
            update="true" 
            insert="true" 
            index="idx_fsscConfigScoreDetail_docPublishTime"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="200"/>
            <property 
            name="fdDesc" 
            column="fd_Desc" 
            update="true" 
            insert="true" 
            length="1000"/>
        <property 
            name="fdScoreUse" 
            column="fd_score_use" 
            update="true" 
            insert="true"/>
        <property 
            name="flag"
            column="flag" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
