<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="com.landray.kmss.elec.device.model.ElecFileLock"
            table="elec_device_file_lock">
        <id
                name="fdId"
                column="fd_id"
                length="200"/>
        <property
                name="version"
                column="fd_version"
                update="true"
                insert="true"/>
    </class>
</hibernate-mapping>