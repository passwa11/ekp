public enum ElecParticipantTargetEnum {
	
	//发送方
	SENDER("合同发起方，一般是系统持有方的用户"),
	
	//接收方
	RECEIVER("可以是任意的参与用户")
	
	;
	
	private String desc;
	
	ElecParticipantTargetEnum(String desc){
		this.desc = desc;
	}

	public String getDesc() {
		return desc;
	}
	
	public String getName(){
		return this.name();
	}
	
}
