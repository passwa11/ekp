<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.organization.model.HrOrganizationPostSeq" 
        table="hr_org_post_seq">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_hrOrgPost1Seq_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_hrOrgPost1Seq_alterTime"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
