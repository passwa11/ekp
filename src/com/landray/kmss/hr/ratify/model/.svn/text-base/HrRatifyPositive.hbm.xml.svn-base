<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <subclass 
        name="com.landray.kmss.hr.ratify.model.HrRatifyPositive" 
        extends="com.landray.kmss.hr.ratify.model.HrRatifyMain"
        discriminator-value="com.landray.kmss.hr.ratify.model.HrRatifyPositive">
        <join table="hr_ratify_positive">
	        <key
				column="fd_id"></key>
	        <property 
	            name="fdPositiveEnterDate" 
	            column="fd_positive_enter_date" 
	            update="true" 
	            insert="true"/>
	        <property 
	            name="fdPositiveTrialPeriod" 
	            column="fd_positive_trial_period" 
	            update="true" 
	            insert="true" 
	            length="200"/>
	        <property 
	            name="fdPositivePeriodDate" 
	            column="fd_positive_period_date" 
	            update="true" 
	            insert="true"/>
	        <property 
	            name="fdPositiveFormalDate" 
	            column="fd_positive_formal_date" 
	            update="true" 
	            insert="true"/>
	        <property 
	            name="fdPositivePostThink" 
	            column="fd_positive_post_think" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <property 
	            name="fdPositiveTrialSummary" 
	            column="fd_positive_trial_summary" 
	            update="true" 
	            insert="true" 
	            length="1000000" 
	            type="com.landray.kmss.common.dao.ClobStringType" 
	            lazy="true"/>
	        <property 
	            name="fdPositiveSelfEval" 
	            column="fd_positive_self_eval" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <property 
	            name="fdPositiveTeacherView" 
	            column="fd_positive_teacher_view" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <property 
	            name="fdPositiveLeaderView" 
	            column="fd_positive_leader_view" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <property 
	            name="fdPositiveHrView" 
	            column="fd_positive_hr_view" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <property 
	            name="fdPositiveRemark" 
	            column="fd_positive_remark" 
	            update="true" 
	            insert="true" 
	            length="2000"/>
	        <many-to-one 
	            name="fdPositiveStaff" 
	            column="fd_positive_staff_id" 
	            update="true" 
	            insert="true"/>
	        <many-to-one 
	            name="fdPositiveDept" 
	            column="fd_positive_dept_id" 
	            update="true" 
	            insert="true"/>
	        <many-to-one 
	            name="fdPositiveLeader" 
	            column="fd_positive_leader_id" 
	            update="true" 
	            insert="true"/>
        </join>
    </subclass>
</hibernate-mapping>
