<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.ratify.model.HrRatifyHistory" 
        table="hr_ratify_history">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPost" 
            column="fd_post" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStartDate" 
            column="fd_start_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdEndDate" 
            column="fd_end_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdLeaveReason" 
            column="fd_leave_reason" 
            update="true" 
            insert="true" 
            length="2000"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
