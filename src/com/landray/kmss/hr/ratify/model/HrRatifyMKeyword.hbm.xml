<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.ratify.model.HrRatifyMKeyword" 
        table="hr_ratify_m_keyword">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docKeyword" 
            column="doc_keyword" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="fdObject" 
            column="fd_object_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
