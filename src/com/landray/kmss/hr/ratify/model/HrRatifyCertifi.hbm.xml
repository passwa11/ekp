<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.ratify.model.HrRatifyCertifi" 
        table="hr_ratify_certifi">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIssuingUnit" 
            column="fd_issuing_unit" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdIssueDate" 
            column="fd_issue_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdInvalidDate" 
            column="fd_invalid_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="2000"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
