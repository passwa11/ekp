<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.ratify.model.HrRatifyOutSign" 
        table="hr_ratify_out_sign">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdMainId" 
            column="fd_main_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdNeedEnterpris" 
            column="fd_need_enterpris" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdExtmsg" 
            column="fd_extmsg" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true"/>
         <many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="200"/>
    </class>
</hibernate-mapping>
