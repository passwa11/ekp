<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.config.model.HrConfigOvertimeConfig" 
        table="hr_config_overtime_config">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_hrConfigOvertimeConfig_docCreateTime"/>
        <property 
            name="fdOvertimeType" 
            column="fd_overtime_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOvertimeWelfare" 
            column="fd_overtime_welfare" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdWorkTime" 
            column="fd_work_time" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
                
                 <bag 
            name="fdOrg" 
            table="hr_config_overtime_org" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
                
            <bag 
            name="fdRank" 
            table="hr_config_overtime_rank" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.hr.organization.model.HrOrganizationRank" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
