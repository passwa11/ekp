<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.hr.staff.model.Areas"
		table="areas" >
		<id
			name="id"
			column="id"
			length="36">
			<generator class="assigned" />
		</id>
		<!-- 简单属性 -->
		<property
			name="cityId"
			column="city_id"
			update="true"
			insert="true"
			length="200" />
		<property
			name="areaId"
			column="area_id"
			update="true"
			insert="true"
			length="200" />
		<property
			name="area"
			column="area"
			update="true"
			insert="true"
			length="10"
			not-null="false" />
	</class>
</hibernate-mapping>
