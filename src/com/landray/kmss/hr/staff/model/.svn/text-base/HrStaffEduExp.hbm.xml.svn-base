<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.hr.staff.model.HrStaffEduExp" 
        table="hr_staff_edu_exp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMajor" 
            column="fd_major" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdEntranceDate" 
            column="fd_entrance_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdGraduationDate" 
            column="fd_graduation_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemark" 
            column="fd_remark" 
            update="true" 
            insert="true" 
            length="2000"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdAcadeRecord" 
            column="fd_acade_record_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
