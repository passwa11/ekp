<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.hr.staff.model.HrStaffPersonExperienceBrief"
		table="hr_staff_person_exp_brief" >
		<!-- 主结构配置 -->
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator class="assigned" />
		</id>
		<!-- 简单属性 -->
		<property
			name="fdContent"
			column="fd_content"
			update="true"
			insert="true"
			length="2000" />
		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="fdCreator"
			column="fd_creator_id"
			insert="true"
			update="true"
			not-null="false" />
		<many-to-one
			name="fdPersonInfo"
			column="fd_person_info_id"
			insert="true"
			update="true"
			not-null="false" />
	</class>
</hibernate-mapping>
