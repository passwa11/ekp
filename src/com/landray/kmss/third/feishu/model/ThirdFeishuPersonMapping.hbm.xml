<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.feishu.model.ThirdFeishuPersonMapping" 
        table="third_feishu_person_mapping">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdFeishuPeMap_creTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdFeishuPeMap_altTime"/>
        <property 
            name="fdOpenId" 
            column="fd_open_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdFeishuPeMap_openId"/>
        <property 
            name="fdEmployeeId" 
            column="fd_employee_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdFeishuPeMap_empId"/>
        <property 
            name="fdLoginName" 
            column="fd_login_name" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdFeishuPeMap_loginNa"/>
        <property 
            name="fdMobileNo" 
            column="fd_mobile_no" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="fdEkp" 
            column="fd_ekp_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
