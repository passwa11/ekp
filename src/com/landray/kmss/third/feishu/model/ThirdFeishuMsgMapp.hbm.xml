<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.feishu.model.ThirdFeishuMsgMapp" 
        table="third_feishu_msg_mapp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdNotifyId" 
            column="fd_notify_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdFeishuMsgM_notifyId"/>
        <property 
            name="fdMessageId" 
            column="fd_message_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdUserId" 
            column="fd_user_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPersonId" 
            column="fd_person_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdFeishuMsgM_personId"/>
        <property 
            name="fdTodoSubject" 
            column="fd_todo_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdContent" 
            column="fd_content" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdLang" 
            column="fd_lang" 
            update="true" 
            insert="true" 
            length="200"/>
    </class>
</hibernate-mapping>
