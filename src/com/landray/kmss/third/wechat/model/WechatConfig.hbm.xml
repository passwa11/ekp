<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.wechat.model.WechatConfig"
		table="wechat_config">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdEkpid"
			column="fd_ekpid"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdRandom"
			column="fd_random"
			update="true"
			insert="true"
			length="36"
			not-null="false" />	
		
		<property
			name="fdQyRandom"
			column="fd_qy_random"
			update="true"
			insert="true"
			length="36"
			not-null="false" />		
			
		<property
			name="fdScene"
			column="fd_scene"
			update="true"
			insert="true"
			length="36"
			not-null="false" />			

		<property
			name="fdOpenid"
			column="fd_openid"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPushProcess"
			column="fd_push_process"
			update="true"
			insert="true"
			length="32"
			not-null="false" />

		<property
			name="fdPushRead"
			column="fd_push_read"
			update="true"
			insert="true"
			length="32"
			not-null="false" />

		<property
			name="fdUrlAccess"
			column="fd_url_access"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdNickname"
			column="fd_nickname"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdImage"
			column="fd_image"
			update="true"
			insert="true"
			length="500"
			not-null="false" />
			
		 <property
			name="fdAlterTime"
			column="fd_alter_time"
			update="true"
			insert="true"
			length="50"
			not-null="false" />	
		
		<property
			name="fdBindFlag"
			column="fd_bindFlag"
			update="true"
			insert="true"
			length="36"
			not-null="false" />		

	</class>
</hibernate-mapping>
