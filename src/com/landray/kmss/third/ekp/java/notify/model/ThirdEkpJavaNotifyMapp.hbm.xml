            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdEkpJaNotMapp_creTime"/>
        <property 
            name="fdNotifyId" 
            column="fd_notify_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdEkpJaNotMapp_notiId"/>
        <property 
            name="fdNotifyData" 
            column="fd_notify_data" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
    </class>
</hibernate-mapping>
