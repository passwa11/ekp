<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.notify.model.ThirdDingNotifyLog" 
        table="third_ding_notify_log_wr">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdRequestId" 
            column="fd_request_id" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdNotifyId" 
            column="fd_notify_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdNotifyData" 
            column="fd_notify_data" 
            update="true" 
            insert="true" 
            length="4000"/>
        <property 
            name="fdSendTime" 
            column="fd_send_time" 
            update="true"
            index="idx_thirdDingNotifyWr_SendTi"
            insert="true"/>
        <property 
            name="fdRtnMsg" 
            column="fd_rtn_msg" 
            update="true" 
            insert="true" 
            length="4000"/>
        <property 
            name="fdRtnTime" 
            column="fd_rtn_time" 
            update="true" 
            insert="true"/>
            
       <property 
            name="fdResult" 
            column="fd_result" 
            update="true" 
            insert="true"
            index="idx_thirdDingNotifyWr_result"/>
            
       <property 
            name="fdExpireTime" 
            column="fd_expire_time" 
            update="true" 
            insert="true"/>
            
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="500"/>
            
        <property
			name="fdSystemId"
			column="fd_system_id"
			update="true"
			insert="true"
			not-null="false"
			index="idx_ThirdDingLog_systemId"
			length="100"/>
    </class>
</hibernate-mapping>
