<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.notify.model.ThirdDingNotifyWorkrecord" 
        table="third_ding_notify_wr">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
       
        <property 
            name="fdNotifyId" 
            column="fd_notify_id" 
            update="true" 
            insert="true" 
            index="idx_thirdDingWr_notifyId"
            length="100"/>
        <property 
            name="fdRecordId" 
            column="fd_record_id" 
            update="true" 
            insert="true" 
            length="100"
            index="idx_thirdDingWr_recordId"/>
        <property 
            name="fdDingUserId" 
            column="fd_ding_user_id" 
            update="true" 
            insert="true" 
            length="50"/>
         <property 
            name="fdSubject" 
            column="fd_subject" 
            update="true" 
            insert="true" 
            length="500"/>
         <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDing_docCreateTime"/>
        <property 
            name="fdEKPDel" 
            column="fd_ekp_del" 
            update="true" 
            insert="true" 
            index="idx_thirdDingWr_fdEKPDel"/>
            
        <property
			name="fdSystemId"
			column="fd_system_id"
			update="true"
			insert="true"
			not-null="false"
			index="idx_ThirdDingWr_systemId"
			length="100"/>
            
        <many-to-one 
		    name="fdUser" 
		    column="fd_user_id" 
		    update="true" 
		    insert="true"/>
    </class>
</hibernate-mapping>
