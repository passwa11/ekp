<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingDtemplate" 
        table="third_ding_dtemplate">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
			name="fdIsAvailable"
			column="fd_is_available"
			update="true"
			insert="true"
			not-null="false" />
        <property 
            name="fdProcessCode" 
            column="fd_process_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAgentId" 
            column="fd_agent_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFlow" 
            column="fd_flow" 
            update="true" 
            insert="true"/>
        <property 
            name="fdDisableFormEdit" 
            column="fd_disable_form_edit" 
            update="true" 
            insert="true"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdCorpId" 
            column="fd_corp_id" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdLang" 
            column="fd_lang" 
            update="true" 
            insert="true" 
            length="100"
            index="idx_thirdDingDtemplate_fdLang"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingDtemp_creTime"/>
        <list 
            name="fdDetail" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.third.ding.model.ThirdDingTemplateDetail"/>
        </list>
    </class>
</hibernate-mapping>
