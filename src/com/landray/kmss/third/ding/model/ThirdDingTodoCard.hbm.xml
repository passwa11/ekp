<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingTodoCard" 
        table="third_ding_todo_card">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdTemplateId" 
            column="fd_template_id" 
            update="true" 
            insert="true" 
            length="50" 
            index="idx_3DTodoCard_fdTemplateId"/>
        <property 
            name="fdCardId" 
            column="fd_card_id" 
            update="true" 
            insert="true" 
            length="50"
            index="idx_thirdDingTdCard_CardId"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_3DTodoCard_docCreateTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_3DTodoCard_docAlterTime"/>
        <property 
            name="fdCardMsg" 
            column="fd_card_msg" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdLang" 
            column="fd_lang" 
            update="true" 
            insert="true" 
            length="20"/>
    </class>
</hibernate-mapping>
