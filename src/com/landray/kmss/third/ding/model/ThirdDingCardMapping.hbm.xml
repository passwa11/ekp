<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingCardMapping" 
        table="third_ding_card_mapping">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCardId" 
            column="fd_card_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property
            name="fdOutTrackId"
            column="fd_out_track_id"
            update="true"
            insert="true"
            length="50"
            index="idx_tdCardMapping_outTrackId"/>
        <property
            name="fdFrom"
            column="fd_from"
            update="true"
            insert="true"
            length="100"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdReceiverUsers" 
            column="fd_receiver_users" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_tdCardMapping_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_tdCardMapping_alterTime"/>
    </class>
</hibernate-mapping>
