<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingDtask" 
        table="third_ding_dtask">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdDingUserId" 
            column="fd_ding_user_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdTaskId" 
            column="fd_task_id" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdEkpTaskId" 
            column="fd_ekp_task_id" 
            update="true" 
            insert="true" 
            length="50" 
            index="idx_thirdDingDtask_ekpTaskId"/>
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true" 
            length="10" 
            index="idx_thirdDingDtask_fdStatus"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingDtask_creTime"/>
        <many-to-one 
            name="fdInstance" 
            column="fd_instance_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdEkpUser" 
            column="fd_ekp_user_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
