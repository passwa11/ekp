<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingIndanceXDetail" 
        table="third_ding_indance_x_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdValue" 
            column="fd_value" 
            update="true" 
            insert="true" 
            length="250"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true" 
            length="11"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
