<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingCardConfig" 
        table="third_ding_card_config">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_tdCardConfig_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_tdCardConfig_alterTime"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCardId" 
            column="fd_card_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="300"/>
        <property 
            name="fdModelNameText" 
            column="fd_model_name_text" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdTemplateName" 
            column="fd_template_name" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdTemplateId"
            column="fd_template_id"
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdConfig" 
            column="fd_config" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true" 
            length="200"/>
    </class>
</hibernate-mapping>
