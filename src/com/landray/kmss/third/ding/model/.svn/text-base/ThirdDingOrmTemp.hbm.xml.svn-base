<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.model.ThirdDingOrmTemp" 
        table="third_ding_orm_temp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_thirdDingOrmTemp_fdOrder"/>
        <property 
            name="fdTemplateId" 
            column="fd_template_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdProcessCode" 
            column="fd_process_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdProcessName" 
            column="fd_process_name" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdDingTemplateType" 
            column="fd_ding_template_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdStartFlow" 
            column="fd_start_flow" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingOrmTemp_creTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingOrmTemp_altTime"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <list 
            name="fdDetail" 
            lazy="true" 
            cascade="all-delete-orphan" 
            inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.third.ding.model.ThirdDingOrmDe"/>
        </list>
    </class>
</hibernate-mapping>
