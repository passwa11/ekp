<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.ding.model.DingCodeModel"
		table="ding_code_model">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdCode"
			column="fd_code_id"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
		<property
			name="fdUserid"
			column="fd_userid"
			update="true"
			insert="true"
	
			not-null="false"
			length="100" />
		<property
			name="fdExpiresTime"
			column="fd_expires_time"
			update="true"
			insert="true"
			not-null="false"/>
	</class>
</hibernate-mapping>
