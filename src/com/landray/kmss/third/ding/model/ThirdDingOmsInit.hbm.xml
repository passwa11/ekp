<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.ding.model.ThirdDingOmsInit"
		table="third_ding_oms_init">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdPath"
			column="fd_path"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdIsOrg"
			column="fd_is_org"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdDingId"
			column="fd_ding_id"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdDingStatus"
			column="fd_ding_status"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdEkpStatus"
			column="fd_ekp_status"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdHandleStatus"
			column="fd_handle_status"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
				name="fdAccountType"
				column="fd_account_type"
				update="true"
				insert="true"
				not-null="false"
				length="20" />

		<many-to-one
			name="fdEkp"
			column="fd_ekp_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
