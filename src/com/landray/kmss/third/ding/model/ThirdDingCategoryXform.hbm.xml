            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingCateXf_creTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingCateXf_altTime"/>
        <property 
            name="fdTemplateId" 
            column="fd_template_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdRoomId" 
            column="fd_room_id" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
