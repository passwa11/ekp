<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.ding.scenegroup.model.ThirdDingRobotmsgLog" 
        table="third_ding_robotmsg_log">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdResult" 
            column="fd_result" 
            update="true" 
            insert="true"/>
        <property 
            name="fdReqData" 
            column="fd_req_data" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdResData" 
            column="fd_res_data" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdUrl" 
            column="fd_url" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdErrMsg" 
            column="fd_err_msg" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdReqTime" 
            column="fd_req_time" 
            update="true" 
            insert="true" 
            index="idx_thirdDingRobLog_reqTime"/>
        <property 
            name="fdResTime" 
            column="fd_res_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdTitle" 
            column="fd_title" 
            update="true" 
            insert="true" 
            length="1000"/>
        <property 
            name="fdExpireTime" 
            column="fd_expire_time" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdRobot" 
            column="fd_robot_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
