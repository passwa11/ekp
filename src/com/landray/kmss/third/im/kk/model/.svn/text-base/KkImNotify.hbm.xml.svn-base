<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.im.kk.model.KkImNotify"
		table="kk_im_notify">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdNotifyId"
			column="fd_notify_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" 
			index="ix_kk_im_no_id"/>

		<property
			name="fdUserId"
			column="fd_user_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdUserName"
			column="fd_user_name"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdSubject"
			column="fd_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdSendUrl"
			column="fd_send_url"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdNotifyData"
			column="fd_notify_data"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />

		<property
			name="fdRtnMsg"
			column="fd_rtn_msg"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdSendTime"
			column="fd_send_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdFirstTime"
			column="fd_first_Time"
			update="true"
			insert="true"
			not-null="false" />


	</class>
</hibernate-mapping>
