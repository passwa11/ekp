<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.im.kk.queue.model.KkNotifyQueueLock"
		table="kk_notify_queue_lock">
		<id
			name="fdId"
			column="fd_id"
			length="100">
			<generator class="assigned" />
		</id>
		<property
			name="version"
			column="fd_version"
			update="true"
			insert="true"
			length="10"
			not-null="false" />
		
	</class>
</hibernate-mapping>