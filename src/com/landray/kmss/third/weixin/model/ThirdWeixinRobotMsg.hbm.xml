<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinRobotMsg" 
        table="third_weixin_robot_msg">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdRobotId" 
            column="fd_robot_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinRobM_robotId"/>
        <property 
            name="fdRobotName" 
            column="fd_robot_name" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdCreatorUserid" 
            column="fd_creator_userid" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinRobM_cTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinRobM_aTime"/>
    </class>
</hibernate-mapping>
