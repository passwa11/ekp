<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.weixin.model.ThirdWeixinOmsPost"
		table="third_weixin_oms_post">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdPostId"
			column="fd_post_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdPersonIds"
			column="fd_person_ids"
			update="true"
			insert="true"
			length="4000"
			not-null="false" />

		<property
			name="fdWxHandler"
			column="fd_wx_handler"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdWxworkHandler"
			column="fd_wxwork_handler"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true"
			length="1000000" />

	</class>
</hibernate-mapping>
