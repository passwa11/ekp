<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinAppchat" 
        table="third_weixin_appchat">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdChatName" 
            column="fd_chat_name" 
            update="true" 
            insert="true" 
            length="500" 
            index="idx_thirdWxAppchat_chatName"/>
        <property 
            name="fdChatId" 
            column="fd_chat_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWxAppchat_chatId"/>
        <property 
            name="fdChatMsg" 
            column="fd_chat_msg" 
            update="true" 
            insert="true" 
            length="4000"/>
        <property 
            name="fdOwnerId" 
            column="fd_owner_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWxAppchat_ownerId"/>
        <property 
            name="fdUserList" 
            column="fd_user_list" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdIsDissolve" 
            column="fd_is_dissolve" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWxAppchat_cTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWxAppchat_aTime"/>
        <property 
            name="fdOwnerFdid" 
            column="fd_owner_fdid" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdWxAppchat_ownerId"/>
        <property
            name="fdCorpId"
            column="fd_corp_id"
            update="true"
            insert="true"
            length="100"
            index="idx_thirdWxAppchat_corpId"/>
    </class>
</hibernate-mapping>
