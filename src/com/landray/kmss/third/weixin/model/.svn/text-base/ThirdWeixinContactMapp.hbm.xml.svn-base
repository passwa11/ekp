<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinContactMapp" 
        table="third_weixin_contact_mapp">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdContactName" 
            column="fd_contact_name" 
            update="true" 
            insert="true" 
            length="500" 
            index="idx_thirdWeixinConMapp_cName"/>
        <property 
            name="fdContactUserId" 
            column="fd_contact_user_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinConMapp_cUId"/>
        <property 
            name="fdExternalId" 
            column="fd_external_id" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdWeixinConMapp_exId"/>
        <property 
            name="fdTagId" 
            column="fd_tag_id" 
            update="true" 
            insert="true" 
            length="1024"
            index="idx_thirdWeixinConMapp_tagId"/>
        <property 
            name="fdTagName" 
            column="fd_tag_name" 
            update="true" 
            insert="true" 
            length="4000"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinConMapp_cTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinConMapp_aTime"/>
        <property 
            name="fdCorpId" 
            column="fd_corp_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdOrgTypeId"
            column="fd_org_type_id"
            update="true"
            insert="true"
            length="200"
            index="idx_thirdWeixinConMapp_otId"/>
        <property
                name="fdIsDelete"
                column="fd_is_delete"
                update="true"
                insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
