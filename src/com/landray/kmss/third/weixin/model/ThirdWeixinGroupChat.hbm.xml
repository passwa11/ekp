<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.weixin.model.ThirdWeixinGroupChat" 
        table="third_weixin_group_chat">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdRoomName" 
            column="fd_room_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRoomCreator" 
            column="fd_room_creator" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdRoomCreateTime" 
            column="fd_room_create_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRoomNotice" 
            column="fd_room_notice" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdCorpId" 
            column="fd_corp_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinGc_corpId"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinGc_cTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWeixinGc_aTime"/>
        <property 
            name="fdIsDissolve" 
            column="fd_is_dissolve" 
            update="true" 
            insert="true"/>
        <property 
            name="fdOwnerFdid" 
            column="fd_owner_fdid" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdWeixinGc_ownerId"/>
        <property 
            name="fdRoomId" 
            column="fd_room_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWeixinGc_roomId"/>
        <bag 
            name="fdMember" 
            table="third_weixin_group_chat_mem" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.third.weixin.model.ThirdWeixinAccount" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
