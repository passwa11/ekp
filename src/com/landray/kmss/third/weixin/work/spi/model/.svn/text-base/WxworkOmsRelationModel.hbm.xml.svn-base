<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.weixin.work.spi.model.WxworkOmsRelationModel"
		table="wxwork_oms_relation_model">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdEkpId"
			column="fd_ekp_id"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdAppPkId"
			column="fd_app_pk_id"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdAppKey"
			column="fd_app_key"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
				name="fdOpenId"
				column="fd_open_id"
				update="true"
				insert="true"
				not-null="false"
				length="100" />
	</class>
</hibernate-mapping>
