<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.third.weixin.work.model.ThirdWeixinWork"
		table="third_weixin_work">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="250"
			not-null="false" />

		<property
			name="fdAgentid"
			column="fd_agentid"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdSecret"
			column="fd_secret"
			update="true"
			insert="true"
			length="100"
			not-null="false" />
			
		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdSystemUrl"
			column="fd_system_url"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdCallbackUrl"
			column="fd_callback_url"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<property
			name="fdToken"
			column="fd_token"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdAeskey"
			column="fd_aeskey"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdAccesstoken"
			column="fd_accesstoken"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="300"
			not-null="false" />
			
		<property
			name="fdUrlPrefix"
			column="fd_url_prefix"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
