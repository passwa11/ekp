        <property 
            name="fdExpireTime" 
            column="fd_expire_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWxAuthLog_expireTime"/>
        <property 
            name="fdReqDataOri" 
            column="fd_req_data_ori" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdReqDataDecyed" 
            column="fd_req_data_decyed" 
            update="true" 
            insert="true" 
            length="1000000" 
