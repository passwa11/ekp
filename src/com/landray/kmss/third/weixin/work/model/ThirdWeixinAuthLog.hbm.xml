            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWxAuthLog_creTime"/>
        <property 
            name="fdUserId" 
            column="fd_user_id" 
            update="true" 
            insert="true" 
            length="200" 
