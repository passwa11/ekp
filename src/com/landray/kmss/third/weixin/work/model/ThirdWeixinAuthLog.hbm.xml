        <property 
            name="fdResDataOri" 
            column="fd_res_data_ori" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdResDataEncryed" 
            column="fd_res_data_encryed" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdLogType" 
            column="fd_log_type" 
            update="true" 
            insert="true" 
            length="200"/>
