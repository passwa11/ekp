<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="com.landray.kmss.third.weixin.work.model.ThirdWeixinWorkGroup"
            table="third_weixin_work_group">
        <id
                name="fdId"
                column="fd_id"
                length="36"/>
        <property
                name="fdModelName"
                column="fd_model_name"
                update="true"
                insert="true"
                length="800"
                index="idx_wxgroup_modelName"/>
        <property
                name="fdModelId"
                column="fd_model_id"
                update="true"
                insert="true"
                length="36"
                index="idx_wxgroup_modelId"/>
        <property
                name="fdGroupId"
                column="fd_group_id"
                update="true"
                insert="true"
                length="200"
                index="idx_wxgroup_groupId"/>
    </class>
</hibernate-mapping>
