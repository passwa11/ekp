<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.welink.model.ThirdWelinkPersonMapping" 
        table="third_welink_person_mapping">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdWLPersonMapp_aTime"/>
        <property 
            name="fdWelinkId" 
            column="fd_welink_id" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWLPersonMapp_WLId"/>
        <property 
            name="fdLoginName" 
            column="fd_login_name" 
            update="true" 
            insert="true" 
            length="100" 
            index="idx_thirdWLPersonMap_lgName"/>
        <property 
            name="fdMobileNo" 
            column="fd_mobile_no" 
            update="true" 
            insert="true" 
            length="30" 
            index="idx_thirdWLPersonMapp_mbNo"/>
        <property 
            name="fdWelinkUserId" 
            column="fd_welink_user_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdWLPersonMapp_WLId"/>
        <many-to-one 
            name="fdEkpPerson" 
            column="fd_ekp_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
