<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.payment.model.ThirdPaymentCallLog" 
        table="third_payment_call_log">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="500" 
            index="idx_thirdPaymentCL_modelName"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="36" 
            index="idx_thirdPaymentCL_modelId"/>
        <property 
            name="fdKey" 
            column="fd_key" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdPaymentCL_fdKey"/>
        <property 
            name="fdOrderDesc" 
            column="fd_order_desc" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdTotalMoney" 
            column="fd_total_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdOrderNo" 
            column="fd_order_no" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdPayType" 
            column="fd_pay_type" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentCL_CTime"/>
        <property 
            name="fdPaymentService" 
            column="fd_payment_service" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCallMethod" 
            column="fd_call_method" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdReqData" 
            column="fd_req_data" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdResData" 
            column="fd_res_data" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdErrorMsg" 
            column="fd_error_msg" 
            update="true" 
            insert="true" 
            length="1000000" 
            type="com.landray.kmss.common.dao.ClobStringType" 
            lazy="true"/>
        <property 
            name="fdCallResult" 
            column="fd_call_result" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
