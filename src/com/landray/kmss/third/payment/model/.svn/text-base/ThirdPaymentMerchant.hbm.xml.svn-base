<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.third.payment.model.ThirdPaymentMerchant" 
        table="third_payment_merchant">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentMerch_CTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentMerch_ATime"/>
        <property 
            name="fdMerchId" 
            column="fd_merch_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdPaymentMer_merchId"/>
        <property 
            name="fdMerchType" 
            column="fd_merch_type" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdMerchName" 
            column="fd_merch_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCorpName" 
            column="fd_corp_name" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdMerchStatus" 
            column="fd_merch_status" 
            update="true" 
            insert="true" 
            index="idx_thirdPaymentMer_status"/>
        <property 
            name="fdPayWay" 
            column="fd_pay_way" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_thirdPaymentMer_payWay"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="third_payment_merchant_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="third_payment_merchant_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherReaders" 
            table="third_payment_merchant_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="third_payment_merchant_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="third_payment_merchant_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="third_payment_merchant_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
