package com.landray.kmss.third.pda.model;

import com.landray.kmss.sys.appconfig.model.BaseAppConfig;

/**
 * pda版本配置
 * 
 */

public class PdaVersionConfig extends BaseAppConfig {

	public PdaVersionConfig() throws Exception {
		super();
	}

	@Override
    public String getJSPUrl() {
		return "";
	}

	/**
	 * 版本号
	 */
	public String getMenuVersion() {
		return getValue("menuVersion");
	}

	public void setMenuVersion(String menuVersion) {
		setValue("menuVersion", menuVersion);
	}

	/**
	 * 图片版本
	 */
	public String getIconVersion() {
		return getValue("iconVersion");
	}

	public void setIconVersion(String iconVersion) {
		setValue("iconVersion", iconVersion);
	}
	
	/**
	 * 主页配置版本
	 */
	public String getHomePageVersion() {
		return getValue("homePageVersion");
	}

	public void setHomePageVersion(String iconVersion) {
		setValue("homePageVersion", iconVersion);
	}
}
