<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.landray.kmss.third.pda.model.PdaDataExtendConfig"
		table="pda_data_extend_config">
		
		<id name="fdId" column="fd_id" length="36">
			<generator class="assigned" />
		</id>

		<property name="fdName" column="fd_name" update="true"
			insert="true" not-null="false" length="200" />

		<property name="fdKey" column="fd_key" update="true" insert="true"
			not-null="false" length="200" />

        <property name="fdType" column="fd_type" update="true"
			insert="true" not-null="false" length="200" />
			
		<property name="fdValue" column="fd_value" update="true"
			insert="true" not-null="false" length="1000" />

	</class>
</hibernate-mapping>
