<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataExpenseItem" 
        table="eop_basedata_expense_item">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdTripType"
            column="fd_trip_type"
            update="true"
            insert="true"
            length="50"/>
        <property
            name="fdDayCalType"
            column="fd_day_cal_type"
            update="true"
            insert="true"
            length="50"/>
        <property
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="20" 
            index="idx_expenseItem_fdCode"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_expenseItem_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_expenseItem_alterTime"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_expenseItem_order"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
        <many-to-one
			name="hbmParent"
			class="com.landray.kmss.eop.basedata.model.EopBasedataExpenseItem"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />
        <bag 
            name="fdBudgetItems" 
            table="eop_basedata_expense_item_pro" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataBudgetItem" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdAccounts" 
            table="eop_basedata_expense_item_acc" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataAccounts" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_expenseitem_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
