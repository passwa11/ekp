<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataCardInfo"
        table="eop_basedata_card_info">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdCorNum"
            column="fd_cor_num"
            update="true" 
            insert="true" 
            length="19"/>
        <property
            name="fdCorChiName"
            column="fd_cor_chi_name"
            update="true"
            insert="true"
            length="50"/>
        <property 
            name="fdActNum"
            column="fd_act_num"
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdAcctNbr"
            column="fd_acct_nbr"
            update="true" 
            insert="true" 
            length="16"/>
        <property 
            name="fdCardNumber"
            column="fd_card_number"
            update="true" 
            insert="true" 
            length="19"/>
        <property
            name="fdHolderChiName"
            column="fd_holder_chi_name"
            update="true"
            insert="true"
            length="50"/>
        <property 
            name="fdHolderEngName"
            column="fd_holder_eng_name"
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdEmpNumber"
            column="fd_emp_number"
            update="true" 
            insert="true" 
            length="16"/>
        <property 
            name="fdActivationCode"
            column="fd_activation_code"
            update="true" 
            insert="true" 
            length="1"/>
        <property 
            name="fdActivationDate"
            column="fd_activation_date"
            update="true" 
            insert="true"/>
        <property 
            name="fdCirculationFlag"
            column="fd_circulation_flag"
            update="true" 
            insert="true" 
            length="1"/>
        <property 
            name="fdCancelDate"
            column="fd_cancel_date"
            update="true" 
            insert="true"/>
        <property
            name="fdIsAvailable"
            column="fd_is_available"
            update="true"
            insert="true"/>
        <property
            name="docCreateTime"
            column="doc_create_time"
            update="true"
            insert="true"
            index="idx_eopCardInfo_docCreateTime"/>
        <property
            name="docAlterTime"
            column="doc_alter_time"
            update="true"
            insert="true"
            index="idx_eopCardInfo_docAlterTime"/>
        <many-to-one
            name="docCreator"
            column="doc_creator_id"
            update="true"
            insert="true"/>
        <many-to-one
            name="docAlteror"
            column="doc_alteror_id"
            update="true"
            insert="true"/>
        <many-to-one
            name="fdHolder"
            column="fd_holder_id"
            update="true"
            insert="true"/>
        <many-to-one
            name="fdCompany"
            column="fd_company_id"
            update="true"
            insert="true"/>
    </class>
</hibernate-mapping>
