<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataStandardScheme" 
        table="eop_basedata_standard_scheme">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdForbid" 
            column="fd_forbid" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="fdTarget" 
            column="fd_target" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdDimension" 
            column="fd_dimension" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdType" 
            column="fd_type" 
            update="true" 
            insert="true" 
            length="10"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_standardScheme_order"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_standardScheme_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_standardScheme_alterTime"/>
		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
         <many-to-one
			name="hbmParent"
			class="com.landray.kmss.eop.basedata.model.EopBasedataStandardScheme"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdItems" 
            table="eop_basedata_standardsch_it"
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataExpenseItem" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_standardsch_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
