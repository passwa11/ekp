<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataPaymentDetail" 
        table="eop_basedata_payment_detail">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <many-to-one 
            name="fdPayWay" 
            column="fd_pay_way_id" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPaymentMoney" 
            column="fd_payment_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPayeeName" 
            column="fd_payee_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPayeeAccount" 
            column="fd_payee_account" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPayeeBankName" 
            column="fd_payee_bank_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPlanPaymentDate" 
            column="fd_plan_payment_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdExchangeRate" 
            column="fd_exchange_rate" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdPayBank" 
            column="fd_pay_bank_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCompany" 
            column="fd_company_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
