<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataPayBank" 
        table="eop_basedata_pay_bank">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdCode"
            column="fd_code"
            update="true"
            insert="true"
            length="200"/>
        <property 
            name="fdBankName" 
            column="fd_bank_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankNo" 
            column="fd_bank_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <property
             name="fdBankType"
             column="fd_bank_type"
             update="true"
             insert="true"
             length="50"/>
        <property 
            name="fdBankAccount" 
            column="fd_bank_account" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdUse" 
            column="fd_use" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_payBank_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_payBank_alterTime"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
         <property 
            name="fdPayBankBelong" 
            column="fd_pay_bank_belong" 
            update="true" 
            insert="true" 
            length="200"/>
         <property 
            name="fdAccountAreaId" 
            column="fd_account_area_id" 
            update="true" 
            insert="true" 
            length="200"/>
         <property 
            name="fdAccountAreaName" 
            column="fd_account_area_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="fdAccounts" 
            column="fd_accounts_id" 
            update="true" 
            insert="true" />
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_paybank_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
