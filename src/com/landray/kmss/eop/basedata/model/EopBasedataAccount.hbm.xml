<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataAccount" 
        table="eop_basedata_account">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBankName" 
            column="fd_bank_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankNo" 
            column="fd_bank_no" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdBankAccount" 
            column="fd_bank_account" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdIsDefault" 
            column="fd_is_default" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_eop_account_createTime"/>
          <property 
            name="fdAccountArea" 
            column="fd_account_area" 
            update="true" 
            insert="true" 
             length="100" />     
            
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_eop_account_alterTime"/>
        <many-to-one 
            name="fdPerson" 
            column="fd_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
