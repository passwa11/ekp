<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataWbs" 
        table="eop_basedata_wbs">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_wbs_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_wbs_alterTime"/>
        <many-to-one
			name="hbmParent"
			class="com.landray.kmss.eop.basedata.model.EopBasedataWbs"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdProject" 
            column="fd_project_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_wbs_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
