<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataMateCate"
        table="eop_basedata_mate_cate">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_mateCate_order"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdDesc" 
            column="fd_desc" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_mateCate_createTime"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="hbmParent" 
            class="com.landray.kmss.eop.basedata.model.EopBasedataMateCate"
            column="fd_parent_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
