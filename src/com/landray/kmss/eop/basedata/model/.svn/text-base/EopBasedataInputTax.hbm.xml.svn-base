<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="com.landray.kmss.eop.basedata.model.EopBasedataInputTax"
            table="eop_basedata_input_tax">
        <id
                name="fdId"
                column="fd_id"
                length="36"/>
        <property
                name="fdIsAvailable"
                column="fd_is_available"
                update="true"
                insert="true"/>
        <property
                name="docCreateTime"
                column="doc_create_time"
                update="true"
                insert="true"
                index="idx_inputTax_createTime"/>
        <property
                name="docAlterTime"
                column="doc_alter_time"
                update="true"
                insert="true"
                index="idx_inputTax_alterTime"/>
        <property
                name="fdTaxRate"
                column="fd_tax_rate"
                update="true"
                insert="true"/>
        <property
                name="fdIsInputTax"
                column="fd_is_input_tax"
                update="true"
                insert="true"/>
        <property
                name="fdDesc"
                column="fd_desc"
                update="true"
                insert="true"
                length="2000"/>
        <many-to-one
                name="docCreator"
                column="doc_creator_id"
                update="true"
                insert="true"/>
        <many-to-one
                name="docAlteror"
                column="doc_alteror_id"
                update="true"
                insert="true"/>
        <many-to-one
                name="fdItem"
                column="fd_item_id"
                update="true"
                insert="true"/>
        <many-to-one
                name="fdAccount"
                column="fd_account_id"
                update="true"
                insert="true"/>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_inputtax_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
