<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataStandard" 
        table="eop_basedata_standard">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdMoney" 
            column="fd_money" 
            update="true" 
            insert="true"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_standard_createTime"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true" 
            index="idx_standard_alterTime"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true" 
            index="idx_standard_order"/>
        <many-to-one 
            name="fdLevel" 
            column="fd_level_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdArea" 
            column="fd_area_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdVehicle" 
            column="fd_vehicle_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBerth" 
            column="fd_berth_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdItem" 
            column="fd_item_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docAlteror" 
            column="doc_alteror_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCurrency" 
            column="fd_currency_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdSpecialItem" 
            column="fd_special_item" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdPerson" 
            column="fd_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdDept" 
            column="fd_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdCity" 
            column="fd_city_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="fdCompanyList" 
            table="eop_basedata_standard_com" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.eop.basedata.model.EopBasedataCompany" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
