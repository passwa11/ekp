<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataCustomerContact"
        table="eop_basedata_cust_contact">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPosition" 
            column="fd_position" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdPhone" 
            column="fd_phone" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdEmail" 
            column="fd_email" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdAddress" 
            column="fd_address" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdRemarks" 
            column="fd_remarks" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdIsfirst" 
            column="fd_isfirst" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
