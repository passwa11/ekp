<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataMaterial"
        table="eop_basedata_material">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdCode" 
            column="fd_code" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_material_createTime"/>
        <property 
            name="fdSpecs" 
            column="fd_specs" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPrice" 
            column="fd_price" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemarks" 
            column="fd_remarks" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property
            name="fdErpCode"
            column="fd_erp_code"
            update="true"
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdUnit" 
            column="fd_unit_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdType" 
            column="fd_type_id" 
            update="true" 
            insert="true"/>
       	<many-to-one 
            name="hbmParent" 
            class="com.landray.kmss.eop.basedata.model.EopBasedataMaterial"
            column="fd_parent_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
