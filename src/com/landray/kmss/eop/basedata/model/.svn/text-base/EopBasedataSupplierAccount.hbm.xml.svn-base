<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataSupplierAccount" 
        table="eop_basedata_supplier_account">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdAccountName" 
            column="fd_account_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankName" 
            column="fd_bank_name" 
            update="true" 
            insert="true" 
            length="300"/>
        <property 
            name="fdBankNo" 
            column="fd_bank_no" 
            update="true" 
            insert="true" 
            length="20"/>
        <property 
            name="fdBankAccount" 
            column="fd_bank_account" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdAccountAreaCode" 
            column="fd_account_area_code" 
            update="true" 
            insert="true" 
            length="200"/>
        <property
            name="fdAccountAreaName" 
            column="fd_account_area_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBankSwift" 
            column="fd_bank_swift" 
            update="true" 
            insert="true" 
            length="50"/>
        <property 
            name="fdReceiveCompany" 
            column="fd_receive_company" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdReceiveBankName" 
            column="fd_receive_bank_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdReceiveBankAddress" 
            column="fd_receive_bank_address" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdInfo" 
            column="fd_info" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdSupplierArea" 
            column="fd_supplier_area" 
            update="true" 
            insert="true" 
            length="2"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
        <property 
            name="docIndex" 
            column="doc_index" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
