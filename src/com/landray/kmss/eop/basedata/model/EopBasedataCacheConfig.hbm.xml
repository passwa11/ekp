<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.eop.basedata.model.EopBasedataCacheConfig" 
        table="eop_basedata_cache_config">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdProperty" 
            column="fd_property" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdValue" 
            column="fd_value" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdLastLongTime" 
            column="fd_last_long_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdExpiresIn" 
            column="fd_expires_in" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRandom" 
            column="fd_random" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="fdAppType" 
            column="fd_app_type" 
            update="true" 
            insert="true" 
            length="40"/>
    </class>
</hibernate-mapping>
