<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.organization.model.SysOrganizationStaffingLevel"
		table="sys_org_staffing_level">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			access="com.landray.kmss.sys.organization.lang.SysOrgCommonAccessor"/>

		<property
			name="fdLevel"
			column="fd_level"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdIsDefault"
			column="fd_is_default"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
			
		<property
			name="fdImportInfo"
			column="fd_import_info"
			update="true"
			insert="true"
			not-null="false"
			length="200"
			index="idx_sysOrgStaffLevel_imptInfo" />

	</class>
</hibernate-mapping>
