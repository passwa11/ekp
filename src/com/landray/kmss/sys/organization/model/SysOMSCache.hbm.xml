<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.organization.model.SysOMSCache"
		table="sys_oms_cache">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdOrgElementId"
			column="fd_org_element_id"
			update="true"
			insert="true"
			length="200" />
		<property
			name="fdAppName"
			column="fd_app_name"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdOpType"
			column="fd_op_type"
			update="true"
			insert="true" />
	</class>
</hibernate-mapping>
