package com.landray.kmss.sys.organization.eco;


/**
 * 我的所属部门范围
 *
 * @author panyh
 */
public class SysOrgMyDeptRange extends SysOrgBaseRange {
    /**
     * 类型：4，岗位；8，人员
     */
    private int type;
    /**
     * 名称：姓名或岗位名称
     */
    private String name;

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
