<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerPolicy"
		table="sys_rest_server_policy">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false"/>

		<property
			name="fdPolicy"
			column="fd_policy"
			update="true"
			insert="true"
			length="5"
			not-null="false"/>

		<property
			name="fdLoginId"
			column="fd_login_id"
			update="true"
			insert="true"
			length="50"
			not-null="false"/>

		<property
			name="fdPassword"
			column="fd_password"
			update="true"
			insert="true"
			length="50"
			not-null="false" />
			
		<property
			name="fdAppname"
			column="fd_appname"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdHeadername"
			column="fd_headername"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdSecretKey"
			column="fd_secret_key"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdAuthType"
			column="fd_auth_type"
			update="true"
			insert="true"
			length="5"
			not-null="false" />

		<property
			name="fdAccessIp"
			column="fd_access_ip"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="fdService"
			table="sys_rest_server_main_user"
			lazy="false">
			<key
				column="fd_user_id" />
			<many-to-many
				class="com.landray.kmss.sys.restservice.server.model.SysRestserviceServerMain"
				column="fd_service_id" />
		</bag>

	</class>
</hibernate-mapping>
