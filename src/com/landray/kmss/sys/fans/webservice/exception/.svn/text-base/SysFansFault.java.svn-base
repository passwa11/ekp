package com.landray.kmss.sys.fans.webservice.exception;

import java.io.Serializable;

public class SysFansFault implements Serializable {
	private static final long serialVersionUID = -3897830601032299172L;
	private String sysFansFaultMessage;
	private String sysFansFaultCode;

	public String getSysFansFaultMessage() {
		return sysFansFaultMessage;
	}

	public void setSysFansFaultMessage(String sysFansFaultMessage) {
		this.sysFansFaultMessage = sysFansFaultMessage;
	}

	public String getSysFansFaultCode() {
		return sysFansFaultCode;
	}

	public void setSysFansFaultCode(String sysFansFaultCode) {
		this.sysFansFaultCode = sysFansFaultCode;
	}
}
