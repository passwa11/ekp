<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.oms.model.SysOmsTempPost" 
        table="sys_oms_temp_post">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdOrder" 
            column="fd_order" 
            update="true" 
            insert="true"/>
        <property 
            name="fdCreateTime" 
            column="fd_create_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdParentid" 
            column="fd_parentid" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPostId" 
            column="fd_post_id" 
            update="true" 
            insert="true" 
            length="200"
            index="idx_sysOmsTempPost_fdPostId"/>
        <property 
            name="fdAlterTime" 
            column="fd_alter_time" 
            update="true" 
            insert="true"/>
        <property 
            name="fdIsAvailable" 
            column="fd_is_available" 
            update="true" 
            insert="true"/>
        <property 
            name="fdTrxId" 
            column="fd_trx_id" 
            update="true" 
            insert="true" 
            length="200" 
            index="idx_sysOmsTempPost_fdTrxId"/>
            
            
         <property 
            name="fdFailReason" 
            column="fd_fail_reason" 
            update="true" 
            insert="true"/>
            
         <property 
            name="fdExtend" 
            column="fd_extend" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
