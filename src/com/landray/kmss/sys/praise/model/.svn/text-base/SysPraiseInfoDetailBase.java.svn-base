package com.landray.kmss.sys.praise.model;

import com.landray.kmss.common.model.BaseModel;

public abstract class SysPraiseInfoDetailBase extends BaseModel{
	
	/**
	 * 点赞数
	 */
	private Integer fdPraiseNum = 0;
	
	/**
	 * 被赞数
	 */
	private Long fdPraisedNum = 0L;
	
	/**
	 * 踩数
	 */
	private Integer fdOpposeNum = 0;
	
	/**
	 * 被踩数
	 */
	private Long fdOpposedNum = 0L;
	
	/**
	 * 打赏次数
	 */
	private Integer fdPayNum = 0;
	
	/**
	 * 被赏次数
	 */
	private Long fdReceiveNum = 0L;
	
	/**
	 * 打赏财富值
	 */
	private Integer fdRichPay = 0;
	
	/**
	 * 收到的财富值
	 */
	private Long fdRichGet = 0L;

	public Integer getFdPraiseNum() {
		return fdPraiseNum;
	}

	public void setFdPraiseNum(Integer fdPraiseNum) {
		this.fdPraiseNum = fdPraiseNum;
	}

	public Long getFdPraisedNum() {
		return fdPraisedNum;
	}

	public void setFdPraisedNum(Long fdPraisedNum) {
		this.fdPraisedNum = fdPraisedNum;
	}

	public Integer getFdOpposeNum() {
		return fdOpposeNum;
	}

	public void setFdOpposeNum(Integer fdOpposeNum) {
		this.fdOpposeNum = fdOpposeNum;
	}

	public Long getFdOpposedNum() {
		return fdOpposedNum;
	}

	public void setFdOpposedNum(Long fdOpposedNum) {
		this.fdOpposedNum = fdOpposedNum;
	}

	public Integer getFdRichPay() {
		return fdRichPay;
	}

	public void setFdRichPay(Integer fdRichPay) {
		this.fdRichPay = fdRichPay;
	}

	public Long getFdRichGet() {
		return fdRichGet;
	}

	public void setFdRichGet(Long fdRichGet) {
		this.fdRichGet = fdRichGet;
	}

	public Integer getFdPayNum() {
		return fdPayNum;
	}

	public void setFdPayNum(Integer fdPayNum) {
		this.fdPayNum = fdPayNum;
	}

	public Long getFdReceiveNum() {
		return fdReceiveNum;
	}

	public void setFdReceiveNum(Long fdReceiveNum) {
		this.fdReceiveNum = fdReceiveNum;
	}
	
	
}
