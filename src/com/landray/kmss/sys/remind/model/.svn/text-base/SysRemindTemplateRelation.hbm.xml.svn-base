<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.remind.model.SysRemindTemplateRelation" 
        table="sys_remind_template_relation">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36">
            <generator class="assigned" />
        </id>
		<property
			name="fdTemplateName"
			column="fd_template_name"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdModuleUrl"
			column="fd_module_url"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdTemplateProperty"
			column="fd_template_property"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
	</class>
</hibernate-mapping>
