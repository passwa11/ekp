<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.remind.model.SysRemindMainTask" 
        table="sys_remind_main_task">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36">
            <generator class="assigned" />
        </id>
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			not-null="false"
			length="200"/>
		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			not-null="false"
			length="36"/>
		<property
			name="fdSubject"
			column="fd_subject"
			update="true"
			insert="true"
			not-null="false"
			length="450"/>
		<property
			name="fdRunTime"
			column="fd_run_time"
			update="true"
			insert="true"
			not-null="false"/>
		<property
			name="fdRemindId"
			column="fd_remind_id"
			update="true"
			insert="true"
			not-null="false"
			length="36"/>
		<property
			name="fdTriggerId"
			column="fd_trigger_id"
			update="true"
			insert="true"
			not-null="false"
			length="36"/>
		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			not-null="false"
			length="100"/>
			
		<bag
			name="fdTaskLogs"
			lazy="true"
			cascade="all-delete-orphan">
			<key column="fd_task_id" />
			<one-to-many class="com.landray.kmss.sys.remind.model.SysRemindMainTaskLog" />
		</bag>
	</class>
</hibernate-mapping>
