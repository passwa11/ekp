package com.landray.kmss.sys.filestore.constant;

public class ConvertState {
	
	/**
	 * 无效队列
	 */
	public static final String CONVERT_STATE_TASK_INVALID  = "taskInvalid";
	
	/**
	 * 未分配任务
	 */
	public static final String CONVERT_STATE_TASK_UNASSIGNED = "taskUnAssigned";
	
	/**
	 * 任务已经分配
	 */
	public static final String CONVERT_STATE_TASK_ASSIGNED = "taskAssigned";
	
	/**
	 * 转换完成结果是true
	 */
	public static final String CONVERT_STATE_TASK_CONVERT_FINISH_TRUE = "convertFinishTrue";
	
	/**
	 * 转换成功，状态不为 true
	 */
	public static final String CONVERT_STATE_TASK_CONVERT_FINISH_FAIL = "convertFinishFail";
	
	/**
	 * 转换成功（只是兼容历史使用时没有将这个结果分为true或false）
	 * 建议使用CONVERT_STATE_TASK_CONVERT_FINISH_TRUE 和  CONVERT_STATE_TASK_CONVERT_FINISH_FAIL
	 */
	public static final String CONVERT_STATE_TASK_CONVERT_FINISH = "convertFinish";
	
	/**
	 * 转换结果
	 */
	public static final String CONVERT_STATE_CONVERT_FINISH_RESULT = "convertFinishResult";
	
	/**
	 * 超时
	 */
	public static final String CONVERT_STATE_CONVERT_ELAPSE_TIME = "convertElapsedTime";
	
	/**
	 * 获取失败的信息
	 */
	public static final String CONVERT_FAILURE_INFO = "failureInfo";
	
	/**
	 * 失败类型
	 */
	public static final String CONVERT_FAILURE_TYPE = "failureType";
	
	/**
	 * 工具失败
	 */
	public static final String CONVERT_FAILURE_TOOL = "tool";
	
	/**
	 * 超时失败
	 */
	public static final String CONVERT_FAILURE_TIME_OUT = "timeout";
	
	/**
	 * 内存溢出失败
	 */
	public static final String CONVERT_FAILURE_OOM = "oom";
	
	/**
	 * 无效
	 */
	public static final String CONVERT_KEY_INVALID = "invalid";
	
	public static final String CONVERT_OTHER_FINISH = "otherConvertFinish";

	/**
	 * 转换失败
	 */
	public static final String CONVERT_OTHER_FINISH_FAILURE = "otherConvertFinishFailure";


	private ConvertState() {
		
	}
}
