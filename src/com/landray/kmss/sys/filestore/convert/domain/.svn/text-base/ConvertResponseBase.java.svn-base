package com.landray.kmss.sys.filestore.convert.domain;

import com.landray.kmss.sys.filestore.convert.service.ConvertResponseHandle;

/**
 * 响应对象
 */
public class ConvertResponseBase {
    private ConvertResponseHandle convertResponseHandle;
    // 过期时间
    private Long expireTime;

    // 任务ID
    private String taskId;
    // 结果
    private String result;

    public ConvertResponseHandle getConvertResponseHandle() {
        return convertResponseHandle;
    }

    public ConvertResponseBase setConvertResponseHandle(ConvertResponseHandle convertResponseHandle) {
        this.convertResponseHandle = convertResponseHandle;
        return this;
    }

    public Long getExpireTime() {
        return expireTime;
    }

    public ConvertResponseBase setExpireTime(Long expireTime) {
        this.expireTime = expireTime;
        return this;
    }

    public String getTaskId() {
        return taskId;
    }

    public ConvertResponseBase setTaskId(String taskId) {
        this.taskId = taskId;
        return this;
    }

    public String getResult() {
        return result;
    }

    public ConvertResponseBase setResult(String result) {
        this.result = result;
        return this;
    }
}
