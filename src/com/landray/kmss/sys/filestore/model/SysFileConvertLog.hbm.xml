<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysFileConvertLog"
		table="sys_file_convert_log">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdQueueId"
			column="fd_queue_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdConvertKey"
			column="fd_convert_key"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdConvertStatus"
			column="fd_convert_status"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdStatusTime"
			column="fd_status_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdStatusInfo"
			column="fd_status_info"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

	</class>
</hibernate-mapping>
