<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.filestore.model.SysAttCatalog"
		table="sys_att_catalog">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdPath"
			column="fd_path"
			update="true"
			insert="true"
			length="350"
			not-null="false" />

		<property
			name="fdIsCurrent"
			column="fd_is_current"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

	</class>
</hibernate-mapping>
