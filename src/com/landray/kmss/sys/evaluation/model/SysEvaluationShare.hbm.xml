<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.evaluation.model.SysEvaluationShare" 
        table="sys_evaluation_share">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdShareTime" 
            column="fd_share_time" 
            update="true" 
            insert="true"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="1500"/>
        <property 
            name="fdShareReason" 
            column="fd_share_reason" 
            update="true" 
            insert="true" 
            length="1500"/>
        <property 
            name="fdShareMode" 
            column="fd_share_mode" 
            update="true" 
            insert="true"/>
        <property 
            name="goalPersonids" 
            column="goal_personids" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdGroupId" 
            column="fd_group_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdTopic" 
            column="fd_topic" 
            update="true" 
            insert="true" 
            length="500"/>
        <property 
            name="fdModelId" 
            column="fd_model_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdModelName" 
            column="fd_model_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdShareType" 
            column="fd_share_type" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
