<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.attachment.model.SysAttachmentWps" 
        table="sys_attachment_wps">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="sysAttMainId" 
            column="fd_attmain_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="userId" 
            column="fd_user_id" 
            update="true" 
            insert="true" 
            length="36"/>
        <property 
            name="expirationTime" 
            column="fd_expiration_time" 
            update="true" 
            insert="true"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true"/>
        <property 
            name="docAlterTime" 
            column="doc_alter_time" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
