package com.landray.kmss.sys.handover.interfaces.config;

/**
 * 工作交接记录集
 * 
 * @author 缪贵荣
 * 
 */
public class HandoverRecord {

	/**
	 * 唯一标识
	 */
	private String id;

	/**
	 * 链接
	 */
	private String url;

	/**
	 * 数据
	 */
	private String[] datas;

	public HandoverRecord(String id, String[] datas) {
		super();
		this.id = id;
		this.datas = datas;
	}

	public HandoverRecord(String id, String url, String[] datas) {
		super();
		this.id = id;
		this.url = url;
		this.datas = datas;
	}

	public String getId() {
		return id;
	}

	public String getUrl() {
		return url;
	}

	public String[] getDatas() {
		return datas;
	}

}
