<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.handover.model.SysHandoverConfigAuthLogDetail"
		table="sys_handover_auth_log_detail">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			length="36" />
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<property
			name="fdModelMessageKey"
			column="fd_model_message_key"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<property
			name="fdModelSubject"
			column="fd_model_subject"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />
	    <property
			name="fdModelUrl"
			column="fd_model_url"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
			
		<many-to-one
			name="fdMain"
			column="fd_main_id"
			insert="true"
			update="true" />
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"/>
		<property
			name="fdAuthType"
			column="fd_auth_type"
			update="true"
			insert="true"
			length="50"/>
		<property
			name="fdNodeName"
			column="fd_node_name"
			update="true"
			insert="true"
			length="200" />

	</class>
</hibernate-mapping>
