<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.handover.model.SysHandoverConfigMain"
		table="sys_handover_config_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdFromId"
			column="fd_from_id"
			update="true"
			insert="true"
			length="36" />

		<property
			name="fdFromName"
			column="fd_from_name"
			update="true"
			insert="true"
			length="200"/>

		<property
			name="fdToId"
			column="fd_to_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdToName"
			column="fd_to_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"/>
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"/>
			
		<property
			name="handoverType"
			column="handover_type"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdContent"
			column="fd_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
		    type="com.landray.kmss.common.dao.ClobStringType"/>	   
		<property
			name="total"
			column="total"
			update="true"
			insert="true"
			not-null="false" />	
		<property
			name="fdState"
			column="fd_state"
			update="true"
			insert="true"
			not-null="false" />	
		<property
			name="execMode"
			column="exec_mode"
			update="true"
			insert="true"
			not-null="false" />
		<bag
			name="configLogs"
			lazy="true"
			inverse="true"
			cascade="all-delete-orphan">
			<key column="fd_main" />
			<one-to-many
				class="com.landray.kmss.sys.handover.model.SysHandoverConfigLog" />
		</bag>
		<bag
			name="configAuthLogs"
			lazy="true"
			inverse="true"
			cascade="all-delete-orphan">
			<key column="fd_main_id" />
			<one-to-many
				class="com.landray.kmss.sys.handover.model.SysHandoverConfigAuthLogDetail" />
		</bag>
	</class>
</hibernate-mapping>
