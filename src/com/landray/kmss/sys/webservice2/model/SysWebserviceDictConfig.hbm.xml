<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.webservice2.model.SysWebserviceDictConfig"
		table="sys_webservice_dict_config">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
			
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />	
			
		<property
			name="fdMainDisplay"
			column="fd_main_display"
			update="true"
			insert="true"
			length="100000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdListDisplay"
			column="fd_list_display"
			update="true"
			insert="true"
			length="100000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />			

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdPrefix"
			column="fd_prefix"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<many-to-one
			name="fdModule"
			column="fd_module_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
