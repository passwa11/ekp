<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.webservice2.model.SysWebserviceRestConfig"
		table="sys_webservice_rest_config">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdPrefix"
			column="fd_prefix"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdDes"
			column="fd_des"
			update="true"
			insert="true"
			length="2000"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="fdDictItems"
			cascade="all-delete-orphan"
			lazy="true" order-by="fd_order asc">
			<key
				column="fd_module_id" />
			<one-to-many
				class="com.landray.kmss.sys.webservice2.model.SysWebserviceDictConfig" />
		</bag>

	</class>
</hibernate-mapping>
