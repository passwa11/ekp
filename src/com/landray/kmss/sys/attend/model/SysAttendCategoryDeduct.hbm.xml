<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendCategoryDeduct"
		table="sys_attend_category_deduct">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdStartTime"
			column="fd_start_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdEndTime"
			column="fd_end_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="fdThreshold"
			column="fd_threshold"
			update="true"
			insert="true"
			length="10"
			not-null="false" />
			
		<property
			name="fdDeductHours"
			column="fd_deduct_hours"
			update="true"
			insert="true"
			not-null="false" />
		
		<many-to-one
			name="fdCategory"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="false" />
		


	</class>
</hibernate-mapping>
