<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendCategoryRule"
		table="sys_attend_category_rule">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdMode"
			column="fd_mode"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdLimit"
			column="fd_limit"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdInTime"
			column="fd_in_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdInRemind"
			column="fd_in_remind"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdInRemindTime"
			column="fd_in_remind_time"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdOutRemind"
			column="fd_out_remind"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdOutRemindTime"
			column="fd_out_remind_time"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdLeftTime"
			column="fd_left_time"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdLateTime"
			column="fd_late_time"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdOutside"
			column="fd_outside"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<many-to-one
			name="fdCategory"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
