<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.attend.model.SysAttendHisCategoryContent"
        table="sys_attend_category_content">
        <id
                name="fdId"
                column="fd_id"
                length="36">
            <generator
                    class="assigned" />
        </id>

        <property
                name="fdCategoryContent"
                column="fd_category_content"
                update="true"
                insert="true"
                length="1000000"
                not-null="false"
                type="com.landray.kmss.common.dao.ClobStringType"
                lazy="true" />


    </class>
</hibernate-mapping>
