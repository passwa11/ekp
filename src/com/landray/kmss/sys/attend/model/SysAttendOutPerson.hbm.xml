<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.attend.model.SysAttendOutPerson"
		table="sys_attend_out_person">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdPhoneNum"
			column="fd_phone_num"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="false" />

	</class>
</hibernate-mapping>
