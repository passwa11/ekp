<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.time.model.SysTimeHolidayPach"
		table="sys_time_holiday_pach">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"/>

		<property
			name="fdPachTime"
			column="fd_pach_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="fdHoliday"
			column="fd_holiday_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdDetail"
			column="fd_detail_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
