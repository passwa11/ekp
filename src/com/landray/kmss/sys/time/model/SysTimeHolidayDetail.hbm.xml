<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.time.model.SysTimeHolidayDetail"
		table="sys_time_holiday_detail">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />

		<property
			name="fdStartDay"
			column="fd_start_day"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdEndDay"
			column="fd_end_day"
			update="true"
			insert="true"
			not-null="false" />
			
			<property
			name="fdPatchHolidayDay"
			column="fd_patch_holiday_day"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdPatchDay"
			column="fd_patch_day"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="fdHoliday"
			column="fd_holiday_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
