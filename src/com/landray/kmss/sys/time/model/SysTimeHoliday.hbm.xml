<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.time.model.SysTimeHoliday"
		table="sys_time_holiday">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			index="idx_systimeholiday_authAreaId" />
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="fdHolidayDetailList"
			inverse="false"
			order-by="fd_start_day"
			cascade="all-delete-orphan"
			lazy="false">
			<key column="fd_holiday_id" />
			<one-to-many class="com.landray.kmss.sys.time.model.SysTimeHolidayDetail" />
		</bag>


	</class>
</hibernate-mapping>
