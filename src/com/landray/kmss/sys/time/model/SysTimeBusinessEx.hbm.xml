<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.sys.time.model.SysTimeBusinessEx" 
        table="sys_time_business_ex">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_sysTimeBusEx_createTime"/>
        <property 
            name="fdReturnDate" 
            column="fd_return_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdExchangeDate" 
            column="fd_exchange_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdStatus" 
            column="fd_status" 
            update="true" 
            insert="true"/>
        <property 
            name="fdMsg" 
            column="fd_msg" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdFlowId" 
            column="fd_flow_id" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdFlowName" 
            column="fd_flow_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdChangeType" 
            column="fd_change_type" 
            update="true" 
            insert="true"/>
        <property name="fdDocUrl"
        	column="fd_doc_url"
        	update="true"
        	insert="true"
        	length="200"
        ></property>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdApplyPerson" 
            column="fd_apply_person_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdExchangePerson" 
            column="fd_exchange_person_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
