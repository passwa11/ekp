package com.landray.kmss.sys.time.util;

/**
 * 请假时长、分钟数跟天的数据存储对象
 * @author
 */
public class SysTimeLeaveTimeDto {
    /**
     * 分钟数
     */
    private Integer leaveTimeMins;
    /**
     * 天（根据当天的标准工时转换后的小数结果）
     */
    private Float leaveTimeDays;

    public Integer getLeaveTimeMins() {
        if(leaveTimeMins ==null){
            leaveTimeMins =0;
        }
        return leaveTimeMins;
    }

    public void setLeaveTimeMins(Integer leaveTimeMins) {
        this.leaveTimeMins = leaveTimeMins;
    }

    public Float getLeaveTimeDays() {
        if(leaveTimeDays ==null){
            leaveTimeDays =0F;
        }
        return leaveTimeDays;
    }

    public void setLeaveTimeDays(Float leaveTimeDays) {
        this.leaveTimeDays = leaveTimeDays;
    }
}
