<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.portal.model.SysPortalTree"
		table="sys_portal_tree">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false"
			access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />

		<property
			name="fdContent"
			column="fd_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property name="fdType" column="fd_type" update="true"
			insert="true" length="2" not-null="false" />
			
		<property
			name="fdAnonymous"
			column="fd_anonymous"
			update="true"
			insert="true"
			not-null="false"
			length="1"
			index="idx_sysPortalTree_fdAnonymous" />	
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
		<bag
			name="fdEditors"
			table="sys_portal_tree_editors"
			lazy="true">
			<key column="fd_tree_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />
	</class>
</hibernate-mapping>
