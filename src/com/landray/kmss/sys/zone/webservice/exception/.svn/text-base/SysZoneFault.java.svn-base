package com.landray.kmss.sys.zone.webservice.exception;

import java.io.Serializable;

public class SysZoneFault implements Serializable {

	private static final long serialVersionUID = -7834470630154933485L;
	
	private String sysZoneFaultMessage;
	private String sysZoneFaultCode;

	public String getSysZoneFaultMessage() {
		return sysZoneFaultMessage;
	}

	public void setSysZoneFaultMessage(String sysZoneFaultMessage) {
		this.sysZoneFaultMessage = sysZoneFaultMessage;
	}

	public String getSysZoneFaultCode() {
		return sysZoneFaultCode;
	}

	public void setSysZoneFaultCode(String sysZoneFaultCode) {
		this.sysZoneFaultCode = sysZoneFaultCode;
	}
}
