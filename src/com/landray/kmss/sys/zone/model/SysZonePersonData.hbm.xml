<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.zone.model.SysZonePersonData"
		table="sys_zone_person_data">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />
			
		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<many-to-one
			name="fdDataCate"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdPerson"
			column="fd_person_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
