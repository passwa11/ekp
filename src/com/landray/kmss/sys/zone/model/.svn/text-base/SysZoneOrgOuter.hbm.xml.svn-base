<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.zone.model.SysZoneOrgOuter"
		table="sys_zone_org_outer">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
		<property
			name="fdPostDesc"
			length="200"
			column="fd_post_desc"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdMobileNo"
			column="fd_mobile_no"
			update="true"
			insert="true"
			not-null="false"
			length="150" />
		<property
			name="fdWorkPhone"
			column="fd_work_phone"
			update="true"
			insert="true"
			not-null="false"
			length="50" />
		<property
			name="fdEmail"
			column="fd_email"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdMemo"
			length="255"
			column="fd_memo"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdOtherInfo"
			length="255"
			column="fd_other_info"
			update="true"
			insert="true"
			not-null="false" />
	</class>
</hibernate-mapping>
