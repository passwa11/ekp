<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.sys.zone.model.SysZoneOrgRelation"
		table="sys_zone_org_relation">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdOrgId"
			column="fd_org_id"
			update="true"
			insert="true"
			not-null="false"
			length="36" />
			
		<property
			name="fdOrgMemo"
			column="fd_org_memo"
			update="true"
			insert="true"
			not-null="false"
			length="255" />
			
		<property
			name="fdOrgType"
			column="fd_org_type"
			update="true"
			insert="true"
			not-null="false"
			length="16" />
		<property
			name="fdCategoryId"
			length="36"
			column="fd_cate_id"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			not-null="false"
			length="2" />
	</class>
</hibernate-mapping>
