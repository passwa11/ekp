<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.archives.model.KmArchivesMain" 
        table="km_archives_main">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_archivesMain_createTime"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
         <property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			not-null="false"
			length="36" />
        <property 
            name="docNumber" 
            column="doc_number" 
            update="true" 
            insert="true" 
            length="100"/>
        <property 
            name="docStatus" 
            column="doc_status" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="docReadCount" 
            column="doc_read_count" 
            update="true" 
            insert="true"/>
        <property 
            name="fdValidityDate" 
            column="fd_validity_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdFileDate" 
            column="fd_file_date" 
            update="true" 
            insert="true"/>
         <property 
            name="fdDestroyed" 
            column="fd_destroyed" 
            update="true" 
            insert="true"/>
        <property 
            name="fdIsPreFile" 
            column="fd_is_prefile" 
            update="true" 
            insert="true"/>
        <property 
            name="fdVolumeYear" 
            column="fd_volume_year" 
            update="true" 
            insert="true"/>
        <property 
            name="fdPeriod" 
            column="fd_period" 
            update="true" 
            insert="true"/>
        <property 
            name="fdUnit" 
            column="fd_unit" 
            update="true" 
            insert="true"/>
        <property 
            name="fdLibrary" 
            column="fd_library" 
            update="true" 
            insert="true"/>
        <property 
            name="fdDenseLevel" 
            column="fd_dense_level" 
            update="true" 
            insert="true"/>
        <property 
            name="fdRemarks" 
            column="fd_remarks" 
            update="true" 
            insert="true" 
            length="4000"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="false" />
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeAtt" 
            column="auth_change_att" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNocopy" 
            column="auth_att_nocopy" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNodownload" 
            column="auth_att_nodownload" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNoprint" 
            column="auth_att_noprint" 
            update="true" 
            insert="true"/>
        <!-- 软删除部署 -->
      	<property
			name="docDeleteFlag"
			column="doc_delete_flag"
			update="true"
			insert="true"
			not-null="false"
			length="5" />
		<property
			name="docDeleteTime"
			column="doc_delete_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docDeleteBy"
			column="doc_delete_by"
			insert="true"
			update="true"
			not-null="false" />
		<many-to-one
			name="fdDense" 
			not-null="false"
			update="true" 
            insert="true"
			column="dense_fevel_id"/>
        <!-- 扩展数据 -->
		<property
			name="extendFilePath"
			column="extend_file_path"
			update="true"
			insert="true"
			not-null="false"
			length="200" />
		<property
			name="extendDataXML"
			column="extend_data_xml"
			update="true"
			insert="true"
			not-null="false"
			length="1000000"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />
		
		<!-- 是否是新版本 -->
		<property
			name="docIsNewVersion"
			column="doc_is_new_version"
			update="true"
			insert="true"
			not-null="false" />
		<!-- 版本锁定 -->
		<property
			name="docIsLocked"
			column="doc_is_locked"
			update="true"
			insert="true"
			not-null="false" />
		<!-- 主版本号 -->
		<property
			name="docMainVersion"
			column="doc_main_version"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<!--辅版本号 -->
		<property
			name="docAuxiVersion"
			column="doc_auxi_version"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<!-- 原文档打印状态 -->
		<property
			name="fdPrintState"
			column="fd_print_state"
			update="true"
			insert="true"
			not-null="false"
			length="1" />

		<!-- 历史版本 -->
		<bag
			name="docHistoryEditions"
			inverse="true">
			<key column="doc_origin_docid"></key>
			<one-to-many
				class="com.landray.kmss.km.archives.model.KmArchivesMain" />
		</bag>
		<!--主文档-->
		<many-to-one
			name="docOriginDoc"
			column="doc_origin_docid"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.km.archives.model.KmArchivesMain" />
		
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docTemplate" 
            column="doc_template_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdStorekeeper" 
            column="fd_storekeeper_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authFileReaders" 
            table="km_archives_main_freader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authReaders" 
            table="km_archives_main_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="km_archives_main_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherReaders" 
            table="km_archives_main_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="km_archives_main_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="km_archives_main_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="km_archives_main_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttCopys" 
            table="km_archives_main_attcopy" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttDownloads" 
            table="km_archives_main_attdl" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttPrints" 
            table="km_archives_main_attprint" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
