<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.archives.model.KmArchivesBorrow" 
        table="km_archives_borrow">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="docStatus" 
            column="doc_status" 
            update="true" 
            insert="true" 
            length="2"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_archivesBorrow_createTime"/>
        <property 
            name="docSubject" 
            column="doc_subject" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="fdBorrowDate" 
            column="fd_borrow_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdBorrowReason" 
            column="fd_borrow_reason" 
            update="true" 
            insert="true" 
            length="2000"/>
        <property 
            name="fdRemarks" 
            column="fd_remarks" 
            update="true" 
            insert="true" 
            type="com.landray.kmss.common.dao.ClobStringType"/>
        <property 
            name="authChangeReaderFlag" 
            column="auth_change_reader_flag" 
            update="true" 
            insert="true"/>
        <property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="false" />
        <property 
            name="authRBPFlag" 
            column="auth_rbp_flag" 
            update="true" 
            insert="true"/>
        <property
			name="docDeleteFlag"
			column="doc_delete_flag"
			update="true"
			insert="true"
			not-null="false"
			length="5" />
		<property
			name="docDeleteTime"
			column="doc_delete_time"
			update="true"
			insert="true"
			not-null="false" />
		<many-to-one
			name="docDeleteBy"
			column="doc_delete_by"
			insert="true"
			update="true"
			not-null="false" />
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeEditorFlag" 
            column="auth_change_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authEditorFlag" 
            column="auth_editor_flag" 
            update="true" 
            insert="true"/>
        <property 
            name="authChangeAtt" 
            column="auth_change_att" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNocopy" 
            column="auth_att_nocopy" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNodownload" 
            column="auth_att_nodownload" 
            update="true" 
            insert="true"/>
        <property 
            name="authAttNoprint" 
            column="auth_att_noprint" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docTemplate" 
            column="doc_template_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docDept" 
            column="doc_dept_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdBorrower" 
            column="fd_borrower_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="km_archives_borrow_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="km_archives_borrow_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <list 
            name="fdBorrowDetails"
            lazy="true" 
          	cascade="all-delete-orphan"
          	inverse="false">
            <key 
                column="doc_main_id"/>
            <list-index 
                column="doc_index"/>
            <one-to-many 
                class="com.landray.kmss.km.archives.model.KmArchivesDetails"/>
        </list>
        <bag 
            name="authOtherReaders" 
            table="km_archives_borrow_oreader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="km_archives_borrow_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authOtherEditors" 
            table="km_archives_borrow_oeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="km_archives_borrow_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttCopys" 
            table="km_archives_borrow_attcopy" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttDownloads" 
            table="km_archives_borrow_attdl" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAttPrints" 
            table="km_archives_borrow_attprint" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
