<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.archives.model.KmArchivesAppraiseDetails" 
        table="km_archives_appraise_details">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdOriginalDate" 
            column="fd_original_date" 
            update="true" 
            insert="true"/>
        <property 
            name="fdAfterAppraiseDate" 
            column="fd_after_appraise_date" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="fdArchives" 
            column="fd_archives_id" 
            update="true" 
            insert="true"/>
        <many-to-one 
            name="docMain" 
            column="doc_main_id" 
            update="true" 
            insert="true"/>
    </class>
</hibernate-mapping>
