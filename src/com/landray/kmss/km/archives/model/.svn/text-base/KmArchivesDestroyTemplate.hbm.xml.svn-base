<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.archives.model.KmArchivesDestroyTemplate" 
        table="km_archives_destroy_template">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property 
            name="fdName" 
            column="fd_name" 
            update="true" 
            insert="true" 
            length="200"/>
        <property 
            name="docCreateTime" 
            column="doc_create_time" 
            update="true" 
            insert="true" 
            index="idx_archivesDestTpl_creatTime"/>
        <property 
            name="authReaderFlag" 
            column="auth_reader_flag" 
            update="true" 
            insert="true"/>
        <property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="fdDefaultFlag"
			column="fd_default_flag"
			update="true"
			insert="true"
			length="4"
			not-null="false" 
			index="idx_archivesDestTpl_defFlag" />
        <many-to-one 
            name="docCreator" 
            column="doc_creator_id" 
            update="true" 
            insert="true"/>
        <bag 
            name="authReaders" 
            table="km_archives_des_temp_reader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authEditors" 
            table="km_archives_des_temp_editor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllReaders" 
            table="km_archives_des_temp_areader" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
        <bag 
            name="authAllEditors" 
            table="km_archives_des_temp_aeditor" 
            lazy="true">
            <key 
                column="fd_source_id"/>
            <many-to-many 
                class="com.landray.kmss.sys.organization.model.SysOrgElement" 
                column="fd_target_id"/>
        </bag>
    </class>
</hibernate-mapping>
