<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.signature.model.KmSignatureDocumentMain"
		table="km_signature_doc_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<!-- 自动编号 -->
		<property
			name="fdDocumentId"
			column="fd_document_id"
			update="true"
			insert="true"
			length="10" />
		<!-- 文档编号 -->
		<property
			name="fdRecordId"
			column="fd_record_id"
			update="true"
			insert="true"
			length="50"
			not-null="false" />
		<!-- 公文编号 -->
		<property
			name="fdDocNo"
			column="fd_doc_no"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 用户名称 -->
		<property
			name="fdUserName"
			column="fd_user_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 密级 -->
		<property
			name="fdSecurity"
			column="fd_security"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 起草人 -->
		<property
			name="fdDraft"
			column="fd_draft"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 审核人 -->
		<property
			name="fdAuditor"
			column="fd_auditor"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 标题 -->
		<property
			name="fdTitle"
			column="fd_title"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 抄送 -->
		<property
			name="fdCopyTo"
			column="fd_copy_to"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 主题 -->
		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />
		<!-- 打印份数 -->
		<property
			name="fdCopies"
			column="fd_copies"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		<!-- 日期时间 -->
		<property
			name="fdDateTime"
			column="fd_date_time"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

	</class>
</hibernate-mapping>
