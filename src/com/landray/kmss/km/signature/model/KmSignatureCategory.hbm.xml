<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.signature.model.KmSignatureCategory"
		table="km_signature_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		<!-- 名称 -->
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200" />
		<!-- 排序号 -->
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />
		<!-- 层级ID -->
		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
		<!-- 创建时间 -->
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true" />

		<many-to-one
			name="hbmParent"
			class="com.landray.kmss.km.signature.model.KmSignatureCategory"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
<!-- 简单分类继承字段  start -->
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" />
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authTmpAttNodownload"
			column="auth_tmp_att_nodownload"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authTmpAttNocopy"
			column="auth_tmp_att_nocopy"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authTmpAttNoprint"
			column="auth_tmp_att_noprint"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		<property
			name="authChangeReaderFlag"
			column="fd_change_reader_flag"
			update="true"
			insert="true" />
		<property
			name="authChangeEditorFlag"
			column="fd_change_editor_flag"
			update="true"
			insert="true" />
		<property
			name="authChangeAtt"
			column="fd_change_att"
			update="true"
			insert="true" />
		<bag
			name="authTmpReaders"
			table="km_signature_tmp_cate_reader"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authTmpEditors"
			table="km_signature_tmp_cate_editor"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authReaders"
			table="km_signature_cate_reader"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authEditors"
			table="km_signature_cate_editor"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authTmpAttPrints"
			table="km_signature_cate_attprint"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_print_id" />
		</bag>
		<bag
			name="authTmpAttCopys"
			table="km_signature_cate_attcopy"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_copy_id" />
		</bag>
		<bag
			name="authTmpAttDownloads"
			table="km_signature_cate_attdl"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_download_id" />
		</bag>
		<bag
			name="authAllEditors"
			table="km_signature_cate_aeditor"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		<bag
			name="authAllReaders"
			table="km_signature_cate_areader"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		<bag
			name="authOtherReaders"
			table="km_signature_cate_oreader"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag
			name="authOtherEditors"
			table="km_signature_cate_oeditor"
			lazy="true">
			<key
				column="fd_category_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
<!-- 简单分类继承字段  end -->
	</class>
</hibernate-mapping>
