<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.calendar.model.KmCalendarDetails"
		table="km_calendar_details">
		<id
			name="fdId"
			column="fd_id"
			length="36"/>
		<property
			name="fdIsDelete"
			column="fd_is_delete"
			update="true"
			insert="true"/>
		<property
			name="fdLabelId"
			column="fd_label_id"
			update="true"
			insert="true"/>
		<many-to-one
			name="fdCalendar"
			column="fd_calendar_id"
			update="true"
			insert="true"/>
		<many-to-one
			name="fdPerson"
			column="fd_person_id"
			update="true"
			insert="true"/>
	</class>
</hibernate-mapping>
