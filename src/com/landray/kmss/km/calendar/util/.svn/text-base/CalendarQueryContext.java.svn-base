package com.landray.kmss.km.calendar.util;

import java.util.Date;
/**
 *日程查询上下文 
 */
public class CalendarQueryContext {
	
	//来源
	private String fdAppkey;
	
	//类型:笔记？日程？
	private String calType;
	
	//日程开始时间
	private Date rangeStart;
	
	//日程结束时间
	private Date rangeEnd;
	
	//是否包含重复日程
	private boolean includeRecurrence;
	
	//查询人员
	private String personsIds;
	
	//查询标签
	private String labelIds;

	// 群组
	private String groupId;

	public String getIsShare() {
		return isShare;
	}

	public void setIsShare(String isShare) {
		this.isShare = isShare;
	}

	//共享日程
	private String isShare;
	
	//是否作权限过滤
	private boolean ifAuth=true;

	private Integer pageno = null;

	private Integer rowsize = null;

	public String getFdAppkey() {
		return fdAppkey;
	}

	public void setFdAppkey(String fdAppkey) {
		this.fdAppkey = fdAppkey;
	}

	public String getCalType() {
		return calType;
	}

	public void setCalType(String calType) {
		this.calType = calType;
	}

	public Date getRangeStart() {
		return rangeStart;
	}

	public void setRangeStart(Date rangeStart) {
		this.rangeStart = rangeStart;
	}

	public Date getRangeEnd() {
		return rangeEnd;
	}

	public void setRangeEnd(Date rangeEnd) {
		this.rangeEnd = rangeEnd;
	}

	public boolean isIncludeRecurrence() {
		return includeRecurrence;
	}

	public void setIncludeRecurrence(boolean includeRecurrence) {
		this.includeRecurrence = includeRecurrence;
	}

	public String getPersonsIds() {
		return personsIds;
	}

	public void setPersonsIds(String personsIds) {
		this.personsIds = personsIds;
	}

	public String getLabelIds() {
		return labelIds;
	}

	public void setLabelIds(String labelIds) {
		this.labelIds = labelIds;
	}

	public String getGroupId() {
		return groupId;
	}

	public void setGroupId(String groupId) {
		this.groupId = groupId;
	}

	public boolean isIfAuth() {
		return ifAuth;
	}

	public void setIfAuth(boolean ifAuth) {
		this.ifAuth = ifAuth;
	}

	public Integer getPageno() {
		return pageno;
	}

	public void setPageno(Integer pageno) {
		this.pageno = pageno;
	}

	public Integer getRowsize() {
		return rowsize;
	}

	public void setRowsize(Integer rowsize) {
		this.rowsize = rowsize;
	}
	
}
