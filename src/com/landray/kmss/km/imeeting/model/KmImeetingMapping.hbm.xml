<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class 
        name="com.landray.kmss.km.imeeting.model.KmImeetingMapping" 
        table="km_imeeting_mapping">
        <id 
            name="fdId" 
            column="fd_id" 
            length="36"/>
        <property
			name="fdModelId"
 		   column="fd_model_id"
			update="true"
			insert="true"
			not-null="false"
	        length="36" />
		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			not-null="false"
			length="100"/>
		 <property
			name="fdAppKey"
 		   column="fd_app_key"
			update="true"
			insert="true"
			not-null="false"
	        length="36" />
	     <property
			name="fdExtData"
			column="fd_ext_data"
			update="true"
			insert="true"
			length="3000"
			not-null="false" />
		<property
			name="fdThirdSysId"
			column="fd_third_sysid"
			update="true"
			insert="true"
			not-null="false"
			length="100" />
    </class>
</hibernate-mapping>
