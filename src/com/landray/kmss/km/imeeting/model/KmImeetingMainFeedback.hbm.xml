<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingMainFeedback"
		table="km_imeeting_main_feedback">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdFromType"
			column="fd_from_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />	
			
		<property
			name="fdAgendaId"
			column="fd_agenda_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />	
		<property
			name="fdUnitName"
			column="fd_unit_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />	
			
		<property
			name="fdOperateType"
			column="fd_operate_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdReason"
			column="fd_reason"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdFeedbackCount"
			column="fd_feedback_count"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdMeeting"
			column="fd_meeting_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdInvitePerson"
			column="fd_invite_person"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
			name="docAttend"
			column="doc_attend_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="clientType"
			column="client_type"
			update="true"
			insert="true"
			not-null="false"
			length="4" />
			
		<property
			name="fdAttendAgendaId"
			column="fd_attend_agenda_id"
			update="true"
			insert="true"
			not-null="false"
			length="600" />
			
	</class>
</hibernate-mapping>
