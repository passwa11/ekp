<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingResCategory"
		table="km_imeeting_res_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<!-- <many-to-one
			name="fdParent"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" /> -->
			
		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

		<property
			name="authAttNocopy"
			column="auth_att_nocopy"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

		<property
			name="authAttNodownload"
			column="auth_att_nodownload"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

		<property
			name="authAttNoprint"
			column="auth_att_noprint"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
		
		<property
			name="authNotReaderFlag"
			column="auth_not_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			<property
			name="authChangeReaderFlag"
			column="fd_change_reader_flag"
			update="true"
			insert="true" />
		<property
			name="authChangeEditorFlag"
			column="fd_change_editor_flag"
			update="true"
			insert="true" />
			
		<many-to-one
			name="hbmParent"
			class="com.landray.kmss.km.imeeting.model.KmImeetingResCategory"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />	
			
			
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			insert="true"
			update="true"
			not-null="false" />
		
		<bag
			name="authEditors"
			table="km_imeeting_res_cate_editor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		
		<bag
			name="authAllEditors"
			table="km_imeeting_res_cate_aeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>

		<bag
			name="authReaders"
			table="km_imeeting_res_cate_reader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		
		<bag
			name="authAllReaders"
			table="km_imeeting_res_cate_areader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
	
		<bag
			name="authOtherReaders"
			table="km_imeeting_res_cate_oreader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<bag
			name="authOtherEditors"
			table="km_imeeting_res_cate_oeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>

		<bag
			name="defReaders"
			table="km_imeeting_res_cate_defreader"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="def_reader_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />
	
	</class>
</hibernate-mapping>
