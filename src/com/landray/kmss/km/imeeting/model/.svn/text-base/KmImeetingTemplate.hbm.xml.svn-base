<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingTemplate"
		table="km_imeeting_template">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			access="com.landray.kmss.sys.language.interfaces.SysLangPropCommonAccessor" />
		
		<property
			name="fdIsAvailable"
			column="fd_is_available"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdPeriodType"
			column="fd_period_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdHoldTime"
			column="fd_hold_time"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdNotifyDay"
			column="fd_notify_day"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="syncDataToCalendarTime"
			column="sync_data_to_calendar_time"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdOtherAttendPerson"
			column="fd_other_attend_person"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true" />
		
		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<!-- 修改人修改时间 -->
		<many-to-one
			name="docAlteror"
			column="doc_alteror_id"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdNeedMultiRes"
			column="fd_need_multi_res"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
		
		<bag
			name="authReaders"
			table="km_imeeting_tmp_reader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_Reader_id" />
		</bag>
		
		<bag
			name="authEditors"
			table="km_imeeting_tmp_editor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_editor_id" />
		</bag>
		
		<bag
			name="authOtherReaders"
			table="km_imeeting_tmp_oreader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>

		<bag
			name="authOtherEditors"
			table="km_imeeting_tmp_oeditor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
	
		<bag
			name="authAllReaders"
			table="km_imeeting_tmp_areader"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_reader_id" />
		</bag>

		<bag
			name="authAllEditors"
			table="km_imeeting_tmp_aeditor"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_all_editor_id" />
		</bag>
		
		<bag
			name="authTmpReaders"
			table="km_imeeting_tmp_main_reader"
			lazy="true">
			<key column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_reader_id" />
		</bag>
		
		<property
			name="authTmpAttNodownload"
			column="auth_tmp_att_nodownload"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="authTmpAttNocopy"
			column="auth_tmp_att_nocopy"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="authTmpAttNoprint"
			column="auth_tmp_att_noprint"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="fdVoteCategoryId"
			column="fd_vote_category_id"
			insert="true"
			update="true"
			not-null="false"
			length="36" />	
		
		<bag
			name="authTmpAttDownloads"
			table="km_imeeting_tmp_att_main_attdl"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_download_id" />
		</bag>

		<bag
			name="authTmpAttCopys"
			table="km_imeeting_tmp_main_attcopy"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_copy_id" />
		</bag>

		<bag
			name="authTmpAttPrints"
			table="km_imeeting_tmp_main_attprint"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_print_id" />
		</bag>
	
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
			
		<property
			name="authRBPFlag"
			column="fd_rbp_flag"
			update="true"
			insert="true"/>
			
		<property
			name="authChangeReaderFlag"
			column="auth_change_reader_flag"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
			
		<property
			name="authChangeAtt"
			column="auth_change_att"
			update="true"
			insert="true"
			length="1"
			not-null="false" />
			
		<many-to-one
			name="fdEmcee"
			column="fd_emcee_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docDept"
			column="doc_dept_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdHost"
			column="fd_host_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdSummaryInputPerson"
			column="fd_summary_input_person"
			insert="true"
			update="true"
			not-null="false" />
		
		<property
			name="fdSummaryContent"
			column="fd_summary_content"
			update="true"
			insert="true"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true"
			length="1000000" />

		<bag
			name="fdAttendPersons"
			table="km_imeeting_template_attend"
			lazy="true">
			<key
				column="fd_templ_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_attend_id" />
		</bag>
		
		<bag
			name="docProperties"
			table="km_imeeting_tmp_property"
			lazy="true">
			<key
				column="fd_template_id" />
			<many-to-many
				class="com.landray.kmss.sys.category.model.SysCategoryProperty"
				column="doc_property_id" />
		</bag>
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />
			
		<property
			name="fdSignEnable"
			column="fd_sign_enable"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

	</class>
</hibernate-mapping>
