<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingSeatTemplate"
		table="km_imeeting_seat_template">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"/>
			
		<property
			name="fdSeatDetail"
			column="fd_seat_detail"
			update="true"
			insert="true"
			not-null="false"
			length="1000000"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />
			
		<property
			name="fdSeatCount"
			column="fd_seat_count"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdCols"
			column="fd_cols"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdRows"
			column="fd_rows"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
	</class>
</hibernate-mapping>
