<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingOuterScreen"
		table="km_imeeting_outer_screen">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdCode"
			column="fd_code"
			update="true"
			insert="true"
			not-null="false" />
			
		<many-to-one
			name="fdRes"
			column="fd_res_id"
			insert="true"
			update="true"
			not-null="false" />
			
	</class>
</hibernate-mapping>
