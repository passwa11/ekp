<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.imeeting.model.KmImeetingBook"
		table="km_imeeting_book">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"/>

		<property
			name="fdHoldDate"
			column="fd_hold_date"
			update="true"
			insert="true"/>

		<property
			name="fdFinishDate"
			column="fd_finish_date"
			update="true"
			insert="true"/>
			
		<property
			name="fdHoldDuration"
			column="fd_hold_duration"
			update="true"
			insert="true"
			length="8"
			not-null="false" />

		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="fdRecurrenceStr"
			column="fd_recurrence_str"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdRecurrenceLastStart"
			column="fd_recurrence_last_start"
			update="true"
			insert="true"
			not-null="false" />	
	
		<property
			name="fdRecurrenceLastEnd"
			column="fd_recurrence_last_end"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdHasExam"
			column="fd_has_exam"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<property
			name="fdExamRemark"
			column="fd_exam_remark"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
		
		<property
			name="isNotify"
			column="is_notify"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"/>

		<many-to-one
			name="fdPlace"
			column="fd_place_id"
			insert="true"
			update="true"/>
			
		<many-to-one
			name="fdExamer"
			column="fd_examer_id"
			insert="true"
			update="true"
			not-null="false" />
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			class="com.landray.kmss.sys.authorization.model.SysAuthArea" 
			index="idx_kmDocKnowledge_authAreaId" />

	</class>
</hibernate-mapping>
