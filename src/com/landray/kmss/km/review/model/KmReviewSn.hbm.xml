<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.km.review.model.KmReviewSn"
		table="km_review_sn">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdMaxNumber"
			column="fd_max_number"
			update="true"
			insert="true"
			length="8"
			not-null="false" />

		<property
			name="fdDate"
			column="fd_date"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdTemplateId"
			column="fd_template_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdPrefix"
			column="fd_prefix"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />	
			
		<many-to-one
			name="authArea"
			column="auth_area_id"
			insert="true"
			update="true"
			not-null="false"
			index="idx_kmReviewSn_authAreaId" />

	</class>
</hibernate-mapping>
