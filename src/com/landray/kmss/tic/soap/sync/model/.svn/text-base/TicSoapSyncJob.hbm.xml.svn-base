<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tic.soap.sync.model.TicSoapSyncJob"
		table="tic_soap_sync_job">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdKey"
			column="fd_key"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdSubject"
			column="fd_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdJobService"
			column="fd_job_service"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdJobMethod"
			column="fd_job_method"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdLink"
			column="fd_link"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdParameter"
			column="fd_parameter"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdCronExpression"
			column="fd_cron_expression"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdEnabled"
			column="fd_enabled"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsSysJob"
			column="fd_is_sys_job"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRunType"
			column="fd_run_type"
			update="true"
			insert="true"
			length="4"
			not-null="false" />

		<property
			name="fdRunTime"
			column="fd_run_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRequired"
			column="fd_required"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdTriggered"
			column="fd_triggered"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdQuartzEkp"
			column="fd_quartz_ekp"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdUseExplain"
			column="fd_use_explain"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdParentId"
			column="fd_parent_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="fdSoapInfo"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_quartz_id" />
			<one-to-many
				class="com.landray.kmss.tic.soap.sync.model.TicSoapSyncTempFunc" />
		</bag>

	</class>
</hibernate-mapping>
