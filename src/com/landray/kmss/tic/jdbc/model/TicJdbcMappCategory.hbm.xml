<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tic.jdbc.model.TicJdbcMappCategory"
		table="tic_jdbc_mapp_category">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="40"
			not-null="false" />

		<many-to-one
			name="hbmParent"
			column="fd_parent_id"
			class="com.landray.kmss.tic.jdbc.model.TicJdbcMappCategory"
			insert="true"
			update="true"
			not-null="false" />
			
			

	</class>
</hibernate-mapping>
