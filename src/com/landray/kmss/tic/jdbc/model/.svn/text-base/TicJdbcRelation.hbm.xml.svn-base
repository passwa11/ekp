<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.tic.jdbc.model.TicJdbcRelation"
		table="tic_jdbc_relation">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdUseExplain"
			column="fd_use_explain"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdSyncType"
			column="fd_sync_type"
			update="true"
			insert="true"
			length="3000"
			not-null="false" />

		<many-to-one
			name="ticJdbcMappManage"
			column="tic_jdbc_mapp_manage"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="ticJdbcTaskManage"
			column="tic_jdbc_task_manage"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
