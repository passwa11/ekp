		<property
			name="fdQuartzEkp"
			column="fd_quartz_ekp"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdLink"
			column="fd_link"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdUseExplain"
			column="fd_use_explain"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="docCategory"
			column="doc_category"
			insert="true"
			update="true"
			not-null="false" />

		<bag
			name="ticJdbcRelationList"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="tic_jdbc_task_manage" />
			<one-to-many
				class="com.landray.kmss.tic.jdbc.model.TicJdbcRelation" />
		</bag>

	</class>
</hibernate-mapping>
