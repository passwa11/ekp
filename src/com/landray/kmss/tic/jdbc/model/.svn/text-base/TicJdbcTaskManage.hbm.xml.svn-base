			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsRequired"
			column="fd_is_required"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsTriggered"
			column="fd_is_triggered"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdCronExpression"
			column="fd_cron_expression"
			update="true"
